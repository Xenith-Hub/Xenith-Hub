    searchBottomRightCover = Util.Create("Frame", {
        Name = "BottomRightCover",
        BackgroundColor3 = Xan.CurrentTheme.Background,
        Position = UDim2.new(1, -12, 1, -12),
        Size = UDim2.new(0, 12, 0, 12),
        BorderSizePixel = 0,
        Visible = false,
        ZIndex = 250,
        Parent = mainFrame
    })
    
