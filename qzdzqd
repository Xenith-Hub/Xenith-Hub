                Util.Create("UIStroke", {
                    Name = "Border",
                    Color = Xan.CurrentTheme.InputBorder,
                    Transparency = 1,
                    Thickness = 1
                })
            })
            
            signupPasswordInput = Util.Create("TextBox", {
                Name = "Password",
