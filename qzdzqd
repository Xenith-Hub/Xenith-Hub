                function getGameIcon(gameName)
                    if profilePage.Games and profilePage.Games[gameName] then
                        gameData = profilePage.Games[gameName]
                        if gameData.Icon then return gameData.Icon end
                    end
                    return GameIcons[gameName] or Icons.Home
                end
                
                function getGameBanner(gameName)
                    if profilePage.Games and profilePage.Games[gameName] and profilePage.Games[gameName].BannerImage then
                        return profilePage.Games[gameName].BannerImage
                    end
                    return profilePage.BannerImage or ""
                end
                
