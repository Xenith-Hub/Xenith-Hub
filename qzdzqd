                })
                
                pulseTaskId = "statusPulse_" .. tostring(statusDot)
                pulseActive = false
                    pulseUp = true
                function startStatusPulse()
                    if pulseActive then return end
                    pulseActive = true
                    pulseUp = true
                    RenderManager.AddTask(pulseTaskId, function()
                        if not statusDot or not statusDot.Parent then
                            RenderManager.RemoveTask(pulseTaskId)
                            pulseActive = false
                            return
                        end
                        current = statusDot.BackgroundTransparency
                        if pulseUp then
                            statusDot.BackgroundTransparency = math.max(current - 0.02, 0)
                            if statusDot.BackgroundTransparency <= 0 then pulseUp = false end
                        else
                            statusDot.BackgroundTransparency = math.min(current + 0.02, 0.6)
                            if statusDot.BackgroundTransparency >= 0.6 then pulseUp = true end
                        end
                    end, { frameSkip = 2 })
                end
                function stopStatusPulse()
                    RenderManager.RemoveTask(pulseTaskId)
                    pulseActive = false
                end
                
                featuresHeader = Util.Create("TextLabel", {
                    Name = "FeaturesHeader",
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 290),
                    Size = UDim2.new(1, 0, 0, 18),
                    Font = Enum.Font.GothamBold,
                    Text = "Features",
                    TextColor3 = Xan.CurrentTheme.Text,
                    TextSize = 14,
                    TextTransparency = 1,
                    TextXAlignment = Enum.TextXAlignment.Left,
                    ZIndex = 105,
                    Parent = contentArea
                })
                
                featuresValue = Util.Create("TextLabel", {
                    Name = "FeaturesValue",
                    BackgroundTransparency = 1,
                    Position = UDim2.new(0, 0, 0, 310),
                    Size = UDim2.new(1, 0, 0, 60),
                    Font = Enum.Font.Gotham,
                    Text = "",
                    TextColor3 = Xan.CurrentTheme.TextSecondary,
                    TextSize = 12,
