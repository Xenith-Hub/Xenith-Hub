        Util.Tween(searchCloseSearchBtn, 0.15, { ImageColor3 = Xan.CurrentTheme.AccentLight })
    end)
    searchCloseSearchBtn.MouseLeave:Connect(function()
        Util.Tween(searchCloseSearchBtn, 0.15, { ImageColor3 = Xan.CurrentTheme.Accent })
    end)
    
