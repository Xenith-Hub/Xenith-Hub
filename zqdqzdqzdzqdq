        SettingsSection:Keybind({
            Name = "Interface Keybind",
            Blacklist = false,
            Default = SavedKeybind,
            OnBinded = function(Bind)
                WindowFunctions:SetKeybind(Bind)
                ShowKeybindNotification(tostring(Bind.Name))
            end
        }, "Keybind")
    end
end]],
  ["Xenith/Xenith/UserInterface/Imports/Settings/InsertThemeSettings.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibraryUI = Requirements.XenithLibraryUI
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions
	local ThemeColorSchemes = Requirements.ThemeColorSchemes
	local ThemeTemplates = Requirements.ThemeTemplates
	local PerformanceModes = Requirements.PerformanceModes
	local ThemeUtility = Requirements.ThemeUtility
	local Themes = Requirements.Themes

    local Subtitle = Requirements.Subtitle
    local Exit = Requirements.Exit
	local Minimize = Requirements.Minimize

	local function GetThemeFilePath()
		return XenithLibrary.Folder .. "/settings/startup/ThemeSaved.Xenith"
	end

	local function GetThemeFolderPath()
		return XenithLibrary.Folder .. "/settings/themes"
	end

	local function GetBackupFolderPath()
		return GetThemeFolderPath() .. "/backups"
	end

	local function EnsureFolderExists(Path)
		if not isfolder(Path) then
			makefolder(Path)
		end
	end

	local function EnsureThemeFoldersExist()
		EnsureFolderExists(XenithLibrary.Folder .. "/settings")
		EnsureFolderExists(XenithLibrary.Folder .. "/settings/startup")
		EnsureFolderExists(GetThemeFolderPath())
	end

	function WindowFunctions:GetThemeNames()
		local Names = {}
		for ThemeName, _ in pairs(Themes) do
			table.insert(Names, ThemeName)
		end
		return Names
	end

	function WindowFunctions:GetTheme(ThemeName)
		return Themes[ThemeName] or Themes.Default
	end

	function WindowFunctions:GetCurrentTheme()
		return CurrentTheme
	end

	function WindowFunctions:GetThemeAttribute(ThemeName, Attribute)
		return self:GetTheme(ThemeName)[Attribute]
	end

	function WindowFunctions:GetCurrentThemeAttribute(Attribute)
		return self:GetTheme(self:GetCurrentTheme())[Attribute]
	end

	function WindowFunctions:CreateCustomTheme(ThemeSettings)
		if not ThemeSettings then
			return false
		end

		for Key, Value in pairs(Themes.Default) do
			if ThemeSettings[Key] == nil then
				ThemeSettings[Key] = Value
			end
		end

		Themes.Custom = ThemeSettings

		if CurrentTheme == "Custom" then
			self:ApplyTheme("Custom")
		end

		return true
	end

	function WindowFunctions:ExportTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Custom
		return Services.HttpService:JSONEncode(Theme)
	end

	function WindowFunctions:ImportTheme(ThemeData, ThemeName)
		local Success, DecodedTheme = pcall(Services.HttpService.JSONDecode, Services.HttpService, ThemeData)

		if not Success then
			return false, "Invalid JSON format"
		end

		if not DecodedTheme.Background or not DecodedTheme.TextPrimary then
			return false, "Invalid theme structure"
		end

		ThemeName = ThemeName or "Custom"
		Themes[ThemeName] = DecodedTheme

		return true, "Theme imported successfully"
	end

	function WindowFunctions:GenerateColorScheme(SchemeType, BaseColor)
		if not ThemeColorSchemes[SchemeType] then
			return false
		end

		local NewColors = ThemeColorSchemes[SchemeType](BaseColor)

		for ColorName, ColorValue in pairs(NewColors) do
			if Themes.Custom[ColorName] then
				Themes.Custom[ColorName] = ColorValue
			end
		end

		return true
	end

	function WindowFunctions:ApplyTemplate(TemplateName)
		local Template = ThemeTemplates[TemplateName]
		if not Template then
			return false
		end

		for Property, Value in pairs(Template) do
			Themes.Custom[Property] = Value
		end

		return true
	end

	function WindowFunctions:SetPerformanceMode(ModeName)
		local Mode = PerformanceModes[ModeName]
		if not Mode then
			return false
		end

		for Setting, Value in pairs(Mode) do
			Themes.Custom[Setting] = Value
		end

		return true
	end

	function WindowFunctions:GenerateRandomTheme()
		local function RandomColor()
			return Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
		end

		local function RandomBool()
			return math.random() > 0.5
		end

		Themes.Custom.Background = RandomColor()
		Themes.Custom.Foreground = RandomColor()
		Themes.Custom.AccentPrimary = RandomColor()
		Themes.Custom.AccentSecondary = RandomColor()
		Themes.Custom.TextPrimary = RandomColor()
		Themes.Custom.CornerRadius = math.random(0, 25)
		Themes.Custom.ShadowSize = math.random(0, 50)
		Themes.Custom.GlowSize = math.random(0, 30)
		Themes.Custom.AnimationSpeed = math.random(10, 50) / 100
		Themes.Custom.GradientEnabled = RandomBool()

		return true
	end

	function WindowFunctions:ApplyUICorners(Parent, CornerRadius)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UICorner") and not Child:FindFirstAncestor("CustomElementPanel") and not Child:FindFirstAncestor("CheckBoxButton") then
				Child.CornerRadius = UDim.new(0, CornerRadius)
			end
		end
	end

	function WindowFunctions:ApplyUIStrokes(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UIStroke") and Child.Name ~= "TabSwitcherUIStroke" and Child.Name ~= "MainFrameStroke" and Child.Name ~= "NameStroke" and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.Color = Theme.BorderColor
				if not Child:FindFirstAncestor("CheckBoxButton") then
					Child.Transparency = Theme.StrokeTransparency
				end
				
				Child.Thickness = Theme.StrokeThickness
			end
		end
	end

	function WindowFunctions:ApplyScrollbarStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("ScrollingFrame") and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.ScrollBarImageColor3 = Theme.ScrollbarColor
				Child.ScrollBarImageTransparency = Theme.ScrollbarTransparency
			end
		end
	end

	function WindowFunctions:ApplyTextStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:GetAttribute("NoApplyTheme") then
				continue
			end

			if Child:IsA("TextLabel") or Child:IsA("TextButton") or Child:IsA("TextBox") then
				if Child.Name == "Title" or Child.Name == "ParagraphHeader" or Child.Name == "HeaderText" then
					Child.TextColor3 = Theme.HeaderColor
					Child.TextTransparency = Theme.HeaderTransparency
				elseif Child:IsA("TextBox") then
					Child.TextColor3 = Theme.InputFieldColor

					if Child.Name ~= "HeaderInputSearchBox" and Child.Name ~= "SearchBox" then
						Child.BackgroundTransparency = Theme.InputValueBackground or Theme.InputFieldTransparency
					end

					if Child.BackgroundTransparency < 1 then
						Child.BackgroundColor3 = Theme.Foreground
					end

					if Child.PlaceholderText and Child.PlaceholderText ~= "" then
						Child.PlaceholderColor3 = Theme.TextSecondary
					end
				else
					if not Child.Name:find("Result_") then
						Child.TextColor3 = Theme.TextPrimary
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyImageStyling(Parent, Theme)
		local ExcludedNames = {"Wallpaper", "OutputWallpaper", "InformationWallpaper", "ColorWheel", "Toggle", "ToggleHead", "Banner", "Headshot", "LinkIcon", "InfoIcon"}
		
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:IsA("ImageLabel") or Child:IsA("ImageButton") then
				local IsExcluded = false

				for _, ExcludedName in ipairs(ExcludedNames) do
					if Child.Name == ExcludedName then
						IsExcluded = true
						break
					end
				end

				if not IsExcluded then
					Child.ImageColor3 = Theme.TextSecondary
				end
			end
		end
	end

	function WindowFunctions:ApplyComponentStyling(Parent, Theme)
		local ComponentStyles = {
			Toggle = Theme.ToggleColor,
			CheckBoxButton = Theme.AccentPrimary,
			TogglerHead = Theme.TogglerHeadColor,
			SliderBar = Theme.SliderColor,
			RangeBar = Theme.SliderColor,
			SliderHead = Theme.SliderHeadColor,
			Dropdown = Theme.DropdownColor,
			Option = Theme.DropdownColor,
			Button = Theme.ButtonColor,
			InformationGroupHighlight = Theme.AccentPrimary,
			TabSwitcherIndicator = Theme.AccentPrimary,

			Sidebar = function(Child)
				Child.BackgroundTransparency = Theme.SidebarTransparency
				Child.BackgroundColor3 = Theme.SidebarColor
			end,

			Surface = function(Child)
				Child.BackgroundColor3 = Theme.Surface
				Child.BackgroundTransparency = Theme.SurfaceTransparency
			end,

			Section = Theme.Foreground,
			Notification = Theme.NotificationColor,
			ExternalStatus = Theme.StatusColor,
		}

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("GuiObject") and not Child:IsA("ScrollingFrame") and Child.BackgroundTransparency < 1 and not Child:FindFirstAncestor("CustomElementPanel") then
				local Style = ComponentStyles[Child.Name]
				
				if Style then
					if type(Style) == "function" then
						Style(Child)
					elseif Child:IsA("ImageButton") and (Child.Name == "Toggle" or Child.Name == "TogglerHead" or Child.Name == "SliderBar" or Child.Name == "SliderHead") and not Child:FindFirstAncestor("CustomElementPanel") then
						Child.ImageColor3 = Style
					else
						Child.BackgroundColor3 = Style
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyGradients(Parent, Theme)
		if not Theme.GradientEnabled then
			return
		end

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child.Name == "Background" and Child:IsA("GuiObject") and not Child:FindFirstAncestor("CustomElementPanel") then
				local Gradient = Child:FindFirstChild("UIGradient")
				if not Gradient then
					Gradient = Instance.new("UIGradient")
					Gradient.Parent = Child
				end

				Gradient.Color = ColorSequence.new(Theme.GradientColors)
				Gradient.Rotation = Theme.GradientRotation
			end
		end
	end

	function WindowFunctions:ApplyWallpaper(Base, Theme)
		local function ApplyWallpaperToElement(Element)
			if Element then
				Element.ImageColor3 = Theme.Background:Lerp(Color3.fromRGB(255, 255, 255), 0.2)
				Element.ImageTransparency = Theme.WallpaperEnabled and Theme.WallpaperTransparency or 1
			end
		end

		ApplyWallpaperToElement(Base:FindFirstChild("Wallpaper"))
		
		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			ApplyWallpaperToElement(Terminal:FindFirstChild("OutputWallpaper"))
		end
	end

	function WindowFunctions:ApplyShadows(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			Shadow.ImageTransparency = Theme.ShadowTransparency
		end
	end

	function WindowFunctions:ApplyShadowGradients(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			if Shadow and typeof(Theme.GlowColor) == "ColorSequence" then
				local ShadowGradient = Shadow:FindFirstChildWhichIsA("UIGradient") or Instance.new("UIGradient")
				ShadowGradient.Color = Theme.GlowColor
				ShadowGradient.Parent = Shadow
				Shadow.ImageColor3 = Color3.fromRGB(255, 255, 255)
			elseif Shadow then
				if Shadow:FindFirstChildWhichIsA("UIGradient") then
					Shadow:FindFirstChildWhichIsA("UIGradient"):Destroy()
				end
				Shadow.ImageColor3 = Theme.GlowColor
			end
		end
	end

	function WindowFunctions:ApplyPremiumGradients(Content, Theme)
		if not Content then
			return
		end

		for _, Element in pairs(Content:GetDescendants()) do
			if Element:IsA("UIGradient") and Element.Parent.Name ~= "Execute" and not Element:FindFirstAncestor("CustomElementPanel") then
				Element.Color = Theme.PremiumGradientColor
			end
		end
	end

	function WindowFunctions:ApplyControlButtons(Theme)
		if not Exit or not Minimize then
			return
		end

		Exit.BackgroundColor3 = Theme.ExitColor
		Minimize.BackgroundColor3 = Theme.MinimizeColor
	end

	function WindowFunctions:ApplyMenuGameCards(GameCardFolder, Theme)
		if not GameCardFolder then
			return
		end

		for _, Card in pairs(GameCardFolder:GetChildren()) do
			if not Card:IsA("GuiObject") then
				continue
			end

			local GlowFrame = Card:FindFirstChild("GlowFrame")
			if GlowFrame then
				GlowFrame.BackgroundColor3 = Theme.MinimizeColor
			end
		end
	end

	function WindowFunctions:AlreadyExistantTheme()
		local ThemeFile = GetThemeFilePath()
		if isfile(ThemeFile) then
			return readfile(ThemeFile)
		end
		return false
	end

	function WindowFunctions:ApplyTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Default
		CurrentTheme = ThemeName

		local Base = XenithLibraryUI:FindFirstChild("Base")
		if not Base then
			return false
		end

		Base.BackgroundColor3 = Theme.Background
		Base.BackgroundTransparency = Theme.BackgroundTransparency

		local ApplySubtitle = "Build - " .. Utility:CheckHubStatus(WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")) .. " | " .. GameLoading
		Subtitle.Text = ApplySubtitle

		local BaseUIStroke = Base:FindFirstChild("BaseUIStroke")
		if BaseUIStroke then
			BaseUIStroke.Transparency = Theme.StrokeTransparency
			BaseUIStroke.Color = Theme.BorderColor
			BaseUIStroke.Thickness = Theme.StrokeThickness
		end

		local BaseUICorner = Base:FindFirstChild("BaseUICorner")
		if BaseUICorner then
			BaseUICorner.CornerRadius = UDim.new(0, Theme.CornerRadius)
		end

		self:ApplyUICorners(Base, Theme.CornerRadius)
		self:ApplyUIStrokes(Base, Theme)
		self:ApplyScrollbarStyling(Base, Theme)
		self:ApplyTextStyling(Base, Theme)
		self:ApplyImageStyling(Base, Theme)
		self:ApplyComponentStyling(Base, Theme)
		self:ApplyGradients(Base, Theme)
		self:ApplyControlButtons(Theme)
		self:ApplyWallpaper(Base, Theme)
		self:ApplyShadows(Theme)
		self:ApplyShadowGradients(Theme)

		local Sidebar = Base:FindFirstChild("Sidebar")
		if Sidebar then
			Sidebar.BackgroundColor3 = Theme.SidebarColor
			Sidebar.BackgroundTransparency = Theme.SidebarTransparency
			self:ApplyUICorners(Sidebar, Theme.CornerRadius)
			self:ApplyUIStrokes(Sidebar, Theme)
			self:ApplyTextStyling(Sidebar, Theme)
			self:ApplyComponentStyling(Sidebar, Theme)
		end

		local Content = Base:FindFirstChild("Content")
		if Content then
			self:ApplyPremiumGradients(Content, Theme)
			self:ApplyUICorners(Content, Theme.CornerRadius)
			self:ApplyUIStrokes(Content, Theme)
			self:ApplyTextStyling(Content, Theme)
			self:ApplyComponentStyling(Content, Theme)

			local ElementsScrolling = Content:FindFirstChild("Elements") and Content.Elements:FindFirstChild("ElementsScrolling")
			if ElementsScrolling and ElementsScrolling:FindFirstChild("Middle") then
				local GameCards = ElementsScrolling:FindFirstChild("Middle")
				self:ApplyMenuGameCards(GameCards, Theme)
			end
		end

		local BottomBar = Base:FindFirstChild("BottomBar")
		if BottomBar then
			BottomBar.BackgroundColor3 = Theme.Background
			BottomBar.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(BottomBar, Theme.CornerRadius)
			self:ApplyUIStrokes(BottomBar, Theme)
			self:ApplyTextStyling(BottomBar, Theme)
			self:ApplyComponentStyling(BottomBar, Theme)
		end

		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			Terminal.BackgroundColor3 = Theme.Background
			Terminal.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(Terminal, Theme.CornerRadius)
			self:ApplyUIStrokes(Terminal, Theme)
			self:ApplyTextStyling(Terminal, Theme)
			self:ApplyComponentStyling(Terminal, Theme)
		end

		for _, Dialog in ipairs(Base:GetChildren()) do
			if Dialog.Name == "Dialog" and Dialog:IsA("Frame") then
				local TargetFrame = Dialog.Prompt
				self:ApplyUICorners(TargetFrame, Theme.CornerRadius)
				self:ApplyUIStrokes(TargetFrame, Theme)
				self:ApplyTextStyling(TargetFrame, Theme)
				self:ApplyComponentStyling(TargetFrame, Theme)
			end
		end

		if ThemeName ~= "Custom" then
			pcall(function()
				EnsureThemeFoldersExist()
				writefile(GetThemeFilePath(), ThemeName)
			end)
		end

		return true
	end

	function WindowFunctions:ApplyCurrentTheme()
		self:ApplyTheme(self:AlreadyExistantTheme() or self:GetCurrentTheme() or "Default")
	end

	function WindowFunctions:SaveTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		pcall(function()
			EnsureThemeFoldersExist()
			local ThemeData = self:ExportTheme(ThemeName)
			writefile(GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json", ThemeData)
		end)

		return true
	end

	function WindowFunctions:LoadTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		local Success = pcall(function()
			local FilePath = GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json"
			if isfile(FilePath) then
				local ThemeData = readfile(FilePath)
				local ImportSuccess, Message = self:ImportTheme(ThemeData, ThemeName)
				if ImportSuccess then
					self:ApplyTheme(ThemeName)
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:BackupThemes()
		pcall(function()
			EnsureFolderExists(GetBackupFolderPath())

			local BackupData = {
				Timestamp = os.time(),
				Themes = Themes,
				Version = "2.0"
			}

			local BackupJSON = Services.HttpService:JSONEncode(BackupData)
			local BackupName = "theme_backup_" .. os.date("%Y%m%d_%H%M%S") .. ".json"
			writefile(GetBackupFolderPath() .. "/" .. BackupName, BackupJSON)
		end)

		return true
	end

	function WindowFunctions:RestoreThemes()
		local Success = pcall(function()
			local BackupFolder = GetBackupFolderPath()
			if isfolder(BackupFolder) then
				local BackupFiles = listfiles(BackupFolder)
				if #BackupFiles > 0 then
					local LatestBackup = BackupFiles[#BackupFiles]
					local BackupData = readfile(LatestBackup)
					local DecodedBackup = Services.HttpService:JSONDecode(BackupData)
					Themes = DecodedBackup.Themes
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:LoadStartupTheme()
		pcall(function()
			local ThemeFile = GetThemeFilePath()

			if isfile(ThemeFile) then
				local SavedTheme = readfile(ThemeFile)
				for ThemeName, _ in pairs(Themes) do
					if ThemeName == SavedTheme then
						self:ApplyTheme(SavedTheme)
						break
					end
				end
			end
		end)
	end

	function TabFunctions:InsertThemeSettings(Side)
        local SettingsSection = TabFunctions:Section({Side = Side})
        
        SettingsSection:Header({Name = "Theme Settings"})
        
        local ThemeNames = WindowFunctions:GetThemeNames()
        
        local ThemeDropdown = SettingsSection:Dropdown({
            Name = "Theme Preset",
			IgnoreConfig = true,
            Multi = false,
            Required = true,
            Search = true,
            Premium = true,
            Options = ThemeNames,
            Default = "Default",
            Callback = function(ThemeName)
				WindowFunctions:ApplyTheme(ThemeName)
            end
        }, "ThemeSelector")
        
        SettingsSection:Divider()
        SettingsSection:Header({Name = "Theme Customization"})
        
        local function SwitchToCustomTheme()
            if WindowFunctions:GetCurrentTheme() ~= "Custom" then
                ThemeDropdown:UpdateSelection("Custom")
            end
        end
        
        local function CreateCategoryDropdown()
            return SettingsSection:Dropdown({
                Name = "Customization Category",
				IgnoreConfig = true,
                Multi = false,
                Required = true,
                Options = {"Colors", "Layout", "Effects", "Typography", "Advanced", "ImportExport"},
                Default = "Colors",
                Callback = function(Category)
                    for Flag, Object in pairs(WindowFunctions:ReturnFeatures()) do
                        if type(Flag) == "string" and Object.SetVisibility and Flag:find("Custom_") then
                            local CategoryPrefix = "Custom_" .. Category
                            Object:SetVisibility(Flag:find(CategoryPrefix) ~= nil)
                        end
                    end
                end
            }, "ThemeCustomizationTab")
        end
        
        local function CreateColorPicker(Name, ThemeKey, Flag)
            return SettingsSection:ColorPicker({
                Name = Name,
				IgnoreConfig = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Color)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Color
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return SettingsSection:Slider({
                Name = Name,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Minimum = Min,
                Maximum = Max,
                DisplayMethod = DisplayMethod,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateEffectSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
        end
        
        local function CreateToggle(Name, ThemeKey, Flag, ApplyTheme)
            return SettingsSection:Toggle({
                Name = Name,
				IgnoreConfig = true,
				IgnoreDefault = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Value)
                    if ApplyTheme then
                        SwitchToCustomTheme()
                    end
                    Themes.Custom[ThemeKey] = Value
                    if ApplyTheme then
                        WindowFunctions:ApplyTheme("Custom")
                    end
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateDropdown(Name, ThemeKey, Options, Flag)
            return SettingsSection:Dropdown({
                Name = Name,
                Multi = false,
                Required = true,
                Options = Options,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local CategoryTabs = CreateCategoryDropdown()
        
        local ColorPickers = {
            Background = CreateColorPicker("Background Color", "Background", "Custom_Colors_Background"),
            Foreground = CreateColorPicker("Foreground Color", "Foreground", "Custom_Colors_Foreground"),
            Surface = CreateColorPicker("Surface Color", "Surface", "Custom_Colors_Surface"),
            TextPrimary = CreateColorPicker("Primary Text Color", "TextPrimary", "Custom_Colors_TextPrimary"),
            TextSecondary = CreateColorPicker("Secondary Text Color", "TextSecondary", "Custom_Colors_TextSecondary"),
            TextDisabled = CreateColorPicker("Disabled Text Color", "TextDisabled", "Custom_Colors_TextDisabled"),
            AccentPrimary = CreateColorPicker("Primary Accent Color", "AccentPrimary", "Custom_Colors_AccentPrimary"),
            AccentSecondary = CreateColorPicker("Secondary Accent Color", "AccentSecondary", "Custom_Colors_AccentSecondary"),
            AccentTertiary = CreateColorPicker("Tertiary Accent Color", "AccentTertiary", "Custom_Colors_AccentTertiary"),
            SuccessColor = CreateColorPicker("Success Color", "SuccessColor", "Custom_Colors_SuccessColor"),
            WarningColor = CreateColorPicker("Warning Color", "WarningColor", "Custom_Colors_WarningColor"),
            ErrorColor = CreateColorPicker("Error Color", "ErrorColor", "Custom_Colors_ErrorColor"),
            InfoColor = CreateColorPicker("Info Color", "InfoColor", "Custom_Colors_InfoColor"),
            ShadowColor = CreateColorPicker("Shadow Color", "ShadowColor", "Custom_Colors_ShadowColor"),
            HeaderColor = CreateColorPicker("Header Color", "HeaderColor", "Custom_Colors_HeaderColor"),
            SidebarColor = CreateColorPicker("Sidebar Color", "SidebarColor", "Custom_Colors_SidebarColor"),
            BorderColor = CreateColorPicker("Border Color", "BorderColor", "Custom_Colors_BorderColor"),
            NotificationColor = CreateColorPicker("Notification Color", "NotificationColor", "Custom_Colors_NotificationColor"),
            StatusColor = CreateColorPicker("Status Color", "StatusColor", "Custom_Colors_StatusColor"),
            DialogColor = CreateColorPicker("Dialog Color", "DialogColor", "Custom_Colors_DialogColor"),
            ButtonColor = CreateColorPicker("Button Color", "ButtonColor", "Custom_Colors_ButtonColor"),
            ToggleColor = CreateColorPicker("Toggle Color", "ToggleColor", "Custom_Colors_ToggleColor"),
            TogglerHeadColor = CreateColorPicker("Toggle Head Color", "TogglerHeadColor", "Custom_Colors_TogglerHeadColor"),
            SliderColor = CreateColorPicker("Slider Color", "SliderColor", "Custom_Colors_SliderColor"),
            SliderHeadColor = CreateColorPicker("Slider Head Color", "SliderHeadColor", "Custom_Colors_SliderHeadColor"),
            DropdownColor = CreateColorPicker("Dropdown Color", "DropdownColor", "Custom_Colors_DropdownColor"),
            InputFieldColor = CreateColorPicker("Input Field Color", "InputFieldColor", "Custom_Colors_InputFieldColor"),
            ScrollbarColor = CreateColorPicker("Scrollbar Color", "ScrollbarColor", "Custom_Colors_ScrollbarColor"),
            SelectionColor = CreateColorPicker("Selection Color", "SelectionColor", "Custom_Colors_SelectionColor"),
            HoverColor = CreateColorPicker("Hover Color", "HoverColor", "Custom_Colors_HoverColor"),
            ActiveColor = CreateColorPicker("Active Color", "ActiveColor", "Custom_Colors_ActiveColor"),
            FocusColor = CreateColorPicker("Focus Color", "FocusColor", "Custom_Colors_FocusColor"),
            DisabledOverlay = CreateColorPicker("Disabled Overlay Color", "DisabledOverlay", "Custom_Colors_DisabledOverlay"),
            ParticleColor = CreateColorPicker("Particle Color", "ParticleColor", "Custom_Colors_ParticleColor"),
            MinimizeColor = CreateColorPicker("Minimize Button Color", "MinimizeColor", "Custom_Colors_MinimizeColor"),
            ExitColor = CreateColorPicker("Exit Button Color", "ExitColor", "Custom_Colors_ExitColor")
        }
        
        local LayoutSliders = {
            CornerRadius = CreateLayoutSlider("Corner Radius", "CornerRadius", 0, 25, "Round", "Custom_Layout_CornerRadius"),
            BackgroundTransparency = CreateLayoutSlider("Background Transparency", "BackgroundTransparency", 0, 1, "Hundredths", "Custom_Layout_BackgroundTransparency"),
            SurfaceTransparency = CreateLayoutSlider("Surface Transparency", "SurfaceTransparency", 0, 1, "Hundredths", "Custom_Layout_SurfaceTransparency"),
            StrokeTransparency = CreateLayoutSlider("Stroke Transparency", "StrokeTransparency", 0, 1, "Hundredths", "Custom_Layout_StrokeTransparency"),
            StrokeThickness = CreateLayoutSlider("Stroke Thickness", "StrokeThickness", 0, 5, "Round", "Custom_Layout_StrokeThickness"),
            HeaderTransparency = CreateLayoutSlider("Header Transparency", "HeaderTransparency", 0, 1, "Hundredths", "Custom_Layout_HeaderTransparency"),
            SidebarTransparency = CreateLayoutSlider("Sidebar Transparency", "SidebarTransparency", 0, 1, "Hundredths", "Custom_Layout_SidebarTransparency"),
            InputFieldTransparency = CreateLayoutSlider("Input Field Transparency", "InputFieldTransparency", 0, 1, "Hundredths", "Custom_Layout_InputFieldTransparency"),
            ScrollbarTransparency = CreateLayoutSlider("Scrollbar Transparency", "ScrollbarTransparency", 0, 1, "Hundredths", "Custom_Layout_ScrollbarTransparency"),
            HoverTransparency = CreateLayoutSlider("Hover Transparency", "HoverTransparency", 0, 1, "Hundredths", "Custom_Layout_HoverTransparency"),
            DisabledTransparency = CreateLayoutSlider("Disabled Transparency", "DisabledTransparency", 0, 1, "Hundredths", "Custom_Layout_DisabledTransparency"),
            WallpaperTransparency = CreateLayoutSlider("Wallpaper Transparency", "WallpaperTransparency", 0, 1, "Hundredths", "Custom_Layout_WallpaperTransparency")
        }
        
        local EffectsSliders = {
            ShadowTransparency = CreateEffectSlider("Shadow Transparency", "ShadowTransparency", 0, 1, "Hundredths", "Custom_Effects_ShadowTransparency"),
            GlowTransparency = CreateEffectSlider("Glow Transparency", "GlowTransparency", 0, 1, "Hundredths", "Custom_Effects_GlowTransparency"),
            ShadowSize = CreateEffectSlider("Shadow Size", "ShadowSize", 0, 50, "Round", "Custom_Effects_ShadowSize"),
            GlowSize = CreateEffectSlider("Glow Size", "GlowSize", 0, 50, "Round", "Custom_Effects_GlowSize"),
            AnimationSpeed = CreateEffectSlider("Animation Speed", "AnimationSpeed", 0.05, 1, "Hundredths", "Custom_Effects_AnimationSpeed"),
            BlurIntensity = CreateEffectSlider("Blur Intensity", "BlurIntensity", 0, 100, "Round", "Custom_Effects_BlurIntensity"),
            NoiseScale = CreateEffectSlider("Noise Scale", "NoiseScale", 10, 200, "Round", "Custom_Effects_NoiseScale"),
            NoiseTransparency = CreateEffectSlider("Noise Transparency", "NoiseTransparency", 0, 1, "Hundredths", "Custom_Effects_NoiseTransparency"),
            ParticleCount = CreateEffectSlider("Particle Count", "ParticleCount", 0, 100, "Round", "Custom_Effects_ParticleCount"),
            GradientRotation = CreateEffectSlider("Gradient Rotation", "GradientRotation", 0, 360, "Round", "Custom_Effects_GradientRotation")
        }
        
        local EffectsToggles = {
            WallpaperEnabled = CreateToggle("Enable Wallpaper", "WallpaperEnabled", "Custom_Effects_WallpaperEnabled", false),
            GradientEnabled = CreateToggle("Enable Gradient", "GradientEnabled", "Custom_Effects_GradientEnabled", true),
            NoiseEnabled = CreateToggle("Enable Noise", "NoiseEnabled", "Custom_Effects_NoiseEnabled", true),
            ParticlesEnabled = CreateToggle("Enable Particles", "ParticlesEnabled", "Custom_Effects_ParticlesEnabled", true)
        }
        
        local EffectsDropdowns = {
            WallpaperBlending = CreateDropdown("Wallpaper Blending", "WallpaperBlending", {"Normal", "Multiply", "Overlay", "Screen", "SoftLight", "HardLight"}, "Custom_Effects_WallpaperBlending")
        }
        
        local TypographySliders = {
            FontSize = CreateLayoutSlider("Font Size", "FontSize", 8, 24, "Round", "Custom_Typography_FontSize"),
            LineHeight = CreateLayoutSlider("Line Height", "LineHeight", 1, 3, "Tenths", "Custom_Typography_LineHeight"),
            LetterSpacing = CreateLayoutSlider("Letter Spacing", "LetterSpacing", -2, 5, "Tenths", "Custom_Typography_LetterSpacing")
        }
        
        local TypographyDropdowns = {
            FontFamily = CreateDropdown("Font Family", "FontFamily", {"Inter", "Arial", "Helvetica", "Roboto", "Open Sans", "Source Sans Pro", "Lato", "Nunito", "Poppins", "Ubuntu", "JetBrains Mono", "Roboto Mono", "Courier New", "Comic Sans MS", "Segoe UI"}, "Custom_Typography_FontFamily"),
            FontWeight = CreateDropdown("Font Weight", "FontWeight", {"Thin", "Light", "Regular", "Medium", "SemiBold", "Bold", "ExtraBold", "Black"}, "Custom_Typography_FontWeight")
        }
        
        local TypographyToggles = {
            AntiAliasing = CreateToggle("Anti-Aliasing", "AntiAliasing", "Custom_Typography_AntiAliasing", false),
            SubPixelRendering = CreateToggle("Sub-Pixel Rendering", "SubPixelRendering", "Custom_Typography_SubPixelRendering", false)
        }
        
        local AdvancedDropdowns = {
            ThemeVariant = CreateDropdown("Theme Variant", "ThemeVariant", {"Auto", "Light", "Dark"}, "Custom_Advanced_ThemeVariant"),
            AccentColorSource = CreateDropdown("Accent Color Source", "AccentColorSource", {"Manual", "System", "Auto"}, "Custom_Advanced_AccentColorSource")
        }
        
        local AdvancedToggles = {
            AutoDarkMode = CreateToggle("Auto Dark Mode", "AutoDarkMode", "Custom_Advanced_AutoDarkMode", true)
        }
        
        local UtilityButtons = {
            SaveTheme = SettingsSection:Button({
                Name = "Save Custom Theme",
                Callback = function()
                    if WindowFunctions:SaveTheme("Custom") then
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Custom theme saved successfully!",
                            Lifetime = 3
                        })
                    end
                end
            }, "Custom_ImportExport_SaveTheme"):SetVisibility(false),
            
            ExportTheme = SettingsSection:Button({
                Name = "Copy Theme to Clipboard",
                Callback = function()
                    pcall(function()
                        local ThemeData = WindowFunctions:ExportTheme(WindowFunctions:GetCurrentTheme())
                        setclipboard(ThemeData)
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Theme data copied to clipboard!",
                            Lifetime = 3
                        })
                    end)
                end
            }, "Custom_ImportExport_ExportTheme"):SetVisibility(false),
            
            RandomTheme = SettingsSection:Button({
                Name = "Generate Random Theme",
                Callback = function()
                    SwitchToCustomTheme()
                    WindowFunctions:GenerateRandomTheme()

					WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Random theme generated!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_RandomTheme"):SetVisibility(false),
            
            ResetTheme = SettingsSection:Button({
                Name = "Reset to Default",
                Callback = function()
                    Themes.Custom = table.clone(Themes.Default)
                    WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Theme reset to default values!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_ResetTheme"):SetVisibility(false)
        }
        
        WindowFunctions:Initialize()
        WindowFunctions:ApplyCurrentTheme()
    end
end]],
  ["Xenith/UserInterface/Imports/Settings/InsertThemeSettings.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibraryUI = Requirements.XenithLibraryUI
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions
	local ThemeColorSchemes = Requirements.ThemeColorSchemes
	local ThemeTemplates = Requirements.ThemeTemplates
	local PerformanceModes = Requirements.PerformanceModes
	local ThemeUtility = Requirements.ThemeUtility
	local Themes = Requirements.Themes

    local Subtitle = Requirements.Subtitle
    local Exit = Requirements.Exit
	local Minimize = Requirements.Minimize

	local function GetThemeFilePath()
		return XenithLibrary.Folder .. "/settings/startup/ThemeSaved.Xenith"
	end

	local function GetThemeFolderPath()
		return XenithLibrary.Folder .. "/settings/themes"
	end

	local function GetBackupFolderPath()
		return GetThemeFolderPath() .. "/backups"
	end

	local function EnsureFolderExists(Path)
		if not isfolder(Path) then
			makefolder(Path)
		end
	end

	local function EnsureThemeFoldersExist()
		EnsureFolderExists(XenithLibrary.Folder .. "/settings")
		EnsureFolderExists(XenithLibrary.Folder .. "/settings/startup")
		EnsureFolderExists(GetThemeFolderPath())
	end

	function WindowFunctions:GetThemeNames()
		local Names = {}
		for ThemeName, _ in pairs(Themes) do
			table.insert(Names, ThemeName)
		end
		return Names
	end

	function WindowFunctions:GetTheme(ThemeName)
		return Themes[ThemeName] or Themes.Default
	end

	function WindowFunctions:GetCurrentTheme()
		return CurrentTheme
	end

	function WindowFunctions:GetThemeAttribute(ThemeName, Attribute)
		return self:GetTheme(ThemeName)[Attribute]
	end

	function WindowFunctions:GetCurrentThemeAttribute(Attribute)
		return self:GetTheme(self:GetCurrentTheme())[Attribute]
	end

	function WindowFunctions:CreateCustomTheme(ThemeSettings)
		if not ThemeSettings then
			return false
		end

		for Key, Value in pairs(Themes.Default) do
			if ThemeSettings[Key] == nil then
				ThemeSettings[Key] = Value
			end
		end

		Themes.Custom = ThemeSettings

		if CurrentTheme == "Custom" then
			self:ApplyTheme("Custom")
		end

		return true
	end

	function WindowFunctions:ExportTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Custom
		return Services.HttpService:JSONEncode(Theme)
	end

	function WindowFunctions:ImportTheme(ThemeData, ThemeName)
		local Success, DecodedTheme = pcall(Services.HttpService.JSONDecode, Services.HttpService, ThemeData)

		if not Success then
			return false, "Invalid JSON format"
		end

		if not DecodedTheme.Background or not DecodedTheme.TextPrimary then
			return false, "Invalid theme structure"
		end

		ThemeName = ThemeName or "Custom"
		Themes[ThemeName] = DecodedTheme

		return true, "Theme imported successfully"
	end

	function WindowFunctions:GenerateColorScheme(SchemeType, BaseColor)
		if not ThemeColorSchemes[SchemeType] then
			return false
		end

		local NewColors = ThemeColorSchemes[SchemeType](BaseColor)

		for ColorName, ColorValue in pairs(NewColors) do
			if Themes.Custom[ColorName] then
				Themes.Custom[ColorName] = ColorValue
			end
		end

		return true
	end

	function WindowFunctions:ApplyTemplate(TemplateName)
		local Template = ThemeTemplates[TemplateName]
		if not Template then
			return false
		end

		for Property, Value in pairs(Template) do
			Themes.Custom[Property] = Value
		end

		return true
	end

	function WindowFunctions:SetPerformanceMode(ModeName)
		local Mode = PerformanceModes[ModeName]
		if not Mode then
			return false
		end

		for Setting, Value in pairs(Mode) do
			Themes.Custom[Setting] = Value
		end

		return true
	end

	function WindowFunctions:GenerateRandomTheme()
		local function RandomColor()
			return Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
		end

		local function RandomBool()
			return math.random() > 0.5
		end

		Themes.Custom.Background = RandomColor()
		Themes.Custom.Foreground = RandomColor()
		Themes.Custom.AccentPrimary = RandomColor()
		Themes.Custom.AccentSecondary = RandomColor()
		Themes.Custom.TextPrimary = RandomColor()
		Themes.Custom.CornerRadius = math.random(0, 25)
		Themes.Custom.ShadowSize = math.random(0, 50)
		Themes.Custom.GlowSize = math.random(0, 30)
		Themes.Custom.AnimationSpeed = math.random(10, 50) / 100
		Themes.Custom.GradientEnabled = RandomBool()

		return true
	end

	function WindowFunctions:ApplyUICorners(Parent, CornerRadius)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UICorner") and not Child:FindFirstAncestor("CustomElementPanel") and not Child:FindFirstAncestor("CheckBoxButton") then
				Child.CornerRadius = UDim.new(0, CornerRadius)
			end
		end
	end

	function WindowFunctions:ApplyUIStrokes(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UIStroke") and Child.Name ~= "TabSwitcherUIStroke" and Child.Name ~= "MainFrameStroke" and Child.Name ~= "NameStroke" and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.Color = Theme.BorderColor
				if not Child:FindFirstAncestor("CheckBoxButton") then
					Child.Transparency = Theme.StrokeTransparency
				end
				
				Child.Thickness = Theme.StrokeThickness
			end
		end
	end

	function WindowFunctions:ApplyScrollbarStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("ScrollingFrame") and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.ScrollBarImageColor3 = Theme.ScrollbarColor
				Child.ScrollBarImageTransparency = Theme.ScrollbarTransparency
			end
		end
	end

	function WindowFunctions:ApplyTextStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:GetAttribute("NoApplyTheme") then
				continue
			end

			if Child:IsA("TextLabel") or Child:IsA("TextButton") or Child:IsA("TextBox") then
				if Child.Name == "Title" or Child.Name == "ParagraphHeader" or Child.Name == "HeaderText" then
					Child.TextColor3 = Theme.HeaderColor
					Child.TextTransparency = Theme.HeaderTransparency
				elseif Child:IsA("TextBox") then
					Child.TextColor3 = Theme.InputFieldColor

					if Child.Name ~= "HeaderInputSearchBox" and Child.Name ~= "SearchBox" then
						Child.BackgroundTransparency = Theme.InputValueBackground or Theme.InputFieldTransparency
					end

					if Child.BackgroundTransparency < 1 then
						Child.BackgroundColor3 = Theme.Foreground
					end

					if Child.PlaceholderText and Child.PlaceholderText ~= "" then
						Child.PlaceholderColor3 = Theme.TextSecondary
					end
				else
					if not Child.Name:find("Result_") then
						Child.TextColor3 = Theme.TextPrimary
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyImageStyling(Parent, Theme)
		local ExcludedNames = {"Wallpaper", "OutputWallpaper", "InformationWallpaper", "ColorWheel", "Toggle", "ToggleHead", "Banner", "Headshot", "LinkIcon", "InfoIcon"}
		
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:IsA("ImageLabel") or Child:IsA("ImageButton") then
				local IsExcluded = false

				for _, ExcludedName in ipairs(ExcludedNames) do
					if Child.Name == ExcludedName then
						IsExcluded = true
						break
					end
				end

				if not IsExcluded then
					Child.ImageColor3 = Theme.TextSecondary
				end
			end
		end
	end

	function WindowFunctions:ApplyComponentStyling(Parent, Theme)
		local ComponentStyles = {
			Toggle = Theme.ToggleColor,
			CheckBoxButton = Theme.AccentPrimary,
			TogglerHead = Theme.TogglerHeadColor,
			SliderBar = Theme.SliderColor,
			RangeBar = Theme.SliderColor,
			SliderHead = Theme.SliderHeadColor,
			Dropdown = Theme.DropdownColor,
			Option = Theme.DropdownColor,
			Button = Theme.ButtonColor,
			InformationGroupHighlight = Theme.AccentPrimary,
			TabSwitcherIndicator = Theme.AccentPrimary,

			Sidebar = function(Child)
				Child.BackgroundTransparency = Theme.SidebarTransparency
				Child.BackgroundColor3 = Theme.SidebarColor
			end,

			Surface = function(Child)
				Child.BackgroundColor3 = Theme.Surface
				Child.BackgroundTransparency = Theme.SurfaceTransparency
			end,

			Section = Theme.Foreground,
			Notification = Theme.NotificationColor,
			ExternalStatus = Theme.StatusColor,
		}

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("GuiObject") and not Child:IsA("ScrollingFrame") and Child.BackgroundTransparency < 1 and not Child:FindFirstAncestor("CustomElementPanel") then
				local Style = ComponentStyles[Child.Name]
				
				if Style then
					if type(Style) == "function" then
						Style(Child)
					elseif Child:IsA("ImageButton") and (Child.Name == "Toggle" or Child.Name == "TogglerHead" or Child.Name == "SliderBar" or Child.Name == "SliderHead") and not Child:FindFirstAncestor("CustomElementPanel") then
						Child.ImageColor3 = Style
					else
						Child.BackgroundColor3 = Style
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyGradients(Parent, Theme)
		if not Theme.GradientEnabled then
			return
		end

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child.Name == "Background" and Child:IsA("GuiObject") and not Child:FindFirstAncestor("CustomElementPanel") then
				local Gradient = Child:FindFirstChild("UIGradient")
				if not Gradient then
					Gradient = Instance.new("UIGradient")
					Gradient.Parent = Child
				end

				Gradient.Color = ColorSequence.new(Theme.GradientColors)
				Gradient.Rotation = Theme.GradientRotation
			end
		end
	end

	function WindowFunctions:ApplyWallpaper(Base, Theme)
		local function ApplyWallpaperToElement(Element)
			if Element then
				Element.ImageColor3 = Theme.Background:Lerp(Color3.fromRGB(255, 255, 255), 0.2)
				Element.ImageTransparency = Theme.WallpaperEnabled and Theme.WallpaperTransparency or 1
			end
		end

		ApplyWallpaperToElement(Base:FindFirstChild("Wallpaper"))
		
		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			ApplyWallpaperToElement(Terminal:FindFirstChild("OutputWallpaper"))
		end
	end

	function WindowFunctions:ApplyShadows(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			Shadow.ImageTransparency = Theme.ShadowTransparency
		end
	end

	function WindowFunctions:ApplyShadowGradients(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			if Shadow and typeof(Theme.GlowColor) == "ColorSequence" then
				local ShadowGradient = Shadow:FindFirstChildWhichIsA("UIGradient") or Instance.new("UIGradient")
				ShadowGradient.Color = Theme.GlowColor
				ShadowGradient.Parent = Shadow
				Shadow.ImageColor3 = Color3.fromRGB(255, 255, 255)
			elseif Shadow then
				if Shadow:FindFirstChildWhichIsA("UIGradient") then
					Shadow:FindFirstChildWhichIsA("UIGradient"):Destroy()
				end
				Shadow.ImageColor3 = Theme.GlowColor
			end
		end
	end

	function WindowFunctions:ApplyPremiumGradients(Content, Theme)
		if not Content then
			return
		end

		for _, Element in pairs(Content:GetDescendants()) do
			if Element:IsA("UIGradient") and Element.Parent.Name ~= "Execute" and not Element:FindFirstAncestor("CustomElementPanel") then
				Element.Color = Theme.PremiumGradientColor
			end
		end
	end

	function WindowFunctions:ApplyControlButtons(Theme)
		if not Exit or not Minimize then
			return
		end

		Exit.BackgroundColor3 = Theme.ExitColor
		Minimize.BackgroundColor3 = Theme.MinimizeColor
	end

	function WindowFunctions:ApplyMenuGameCards(GameCardFolder, Theme)
		if not GameCardFolder then
			return
		end

		for _, Card in pairs(GameCardFolder:GetChildren()) do
			if not Card:IsA("GuiObject") then
				continue
			end

			local GlowFrame = Card:FindFirstChild("GlowFrame")
			if GlowFrame then
				GlowFrame.BackgroundColor3 = Theme.MinimizeColor
			end
		end
	end

	function WindowFunctions:AlreadyExistantTheme()
		local ThemeFile = GetThemeFilePath()
		if isfile(ThemeFile) then
			return readfile(ThemeFile)
		end
		return false
	end

	function WindowFunctions:ApplyTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Default
		CurrentTheme = ThemeName

		local Base = XenithLibraryUI:FindFirstChild("Base")
		if not Base then
			return false
		end

		Base.BackgroundColor3 = Theme.Background
		Base.BackgroundTransparency = Theme.BackgroundTransparency

		local ApplySubtitle = "Build - " .. Utility:CheckHubStatus(WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")) .. " | " .. GameLoading
		Subtitle.Text = ApplySubtitle

		local BaseUIStroke = Base:FindFirstChild("BaseUIStroke")
		if BaseUIStroke then
			BaseUIStroke.Transparency = Theme.StrokeTransparency
			BaseUIStroke.Color = Theme.BorderColor
			BaseUIStroke.Thickness = Theme.StrokeThickness
		end

		local BaseUICorner = Base:FindFirstChild("BaseUICorner")
		if BaseUICorner then
			BaseUICorner.CornerRadius = UDim.new(0, Theme.CornerRadius)
		end

		self:ApplyUICorners(Base, Theme.CornerRadius)
		self:ApplyUIStrokes(Base, Theme)
		self:ApplyScrollbarStyling(Base, Theme)
		self:ApplyTextStyling(Base, Theme)
		self:ApplyImageStyling(Base, Theme)
		self:ApplyComponentStyling(Base, Theme)
		self:ApplyGradients(Base, Theme)
		self:ApplyControlButtons(Theme)
		self:ApplyWallpaper(Base, Theme)
		self:ApplyShadows(Theme)
		self:ApplyShadowGradients(Theme)

		local Sidebar = Base:FindFirstChild("Sidebar")
		if Sidebar then
			Sidebar.BackgroundColor3 = Theme.SidebarColor
			Sidebar.BackgroundTransparency = Theme.SidebarTransparency
			self:ApplyUICorners(Sidebar, Theme.CornerRadius)
			self:ApplyUIStrokes(Sidebar, Theme)
			self:ApplyTextStyling(Sidebar, Theme)
			self:ApplyComponentStyling(Sidebar, Theme)
		end

		local Content = Base:FindFirstChild("Content")
		if Content then
			self:ApplyPremiumGradients(Content, Theme)
			self:ApplyUICorners(Content, Theme.CornerRadius)
			self:ApplyUIStrokes(Content, Theme)
			self:ApplyTextStyling(Content, Theme)
			self:ApplyComponentStyling(Content, Theme)

			local ElementsScrolling = Content:FindFirstChild("Elements") and Content.Elements:FindFirstChild("ElementsScrolling")
			if ElementsScrolling and ElementsScrolling:FindFirstChild("Middle") then
				local GameCards = ElementsScrolling:FindFirstChild("Middle")
				self:ApplyMenuGameCards(GameCards, Theme)
			end
		end

		local BottomBar = Base:FindFirstChild("BottomBar")
		if BottomBar then
			BottomBar.BackgroundColor3 = Theme.Background
			BottomBar.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(BottomBar, Theme.CornerRadius)
			self:ApplyUIStrokes(BottomBar, Theme)
			self:ApplyTextStyling(BottomBar, Theme)
			self:ApplyComponentStyling(BottomBar, Theme)
		end

		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			Terminal.BackgroundColor3 = Theme.Background
			Terminal.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(Terminal, Theme.CornerRadius)
			self:ApplyUIStrokes(Terminal, Theme)
			self:ApplyTextStyling(Terminal, Theme)
			self:ApplyComponentStyling(Terminal, Theme)
		end

		for _, Dialog in ipairs(Base:GetChildren()) do
			if Dialog.Name == "Dialog" and Dialog:IsA("Frame") then
				local TargetFrame = Dialog.Prompt
				self:ApplyUICorners(TargetFrame, Theme.CornerRadius)
				self:ApplyUIStrokes(TargetFrame, Theme)
				self:ApplyTextStyling(TargetFrame, Theme)
				self:ApplyComponentStyling(TargetFrame, Theme)
			end
		end

		if ThemeName ~= "Custom" then
			pcall(function()
				EnsureThemeFoldersExist()
				writefile(GetThemeFilePath(), ThemeName)
			end)
		end

		return true
	end

	function WindowFunctions:ApplyCurrentTheme()
		self:ApplyTheme(self:AlreadyExistantTheme() or self:GetCurrentTheme() or "Default")
	end

	function WindowFunctions:SaveTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		pcall(function()
			EnsureThemeFoldersExist()
			local ThemeData = self:ExportTheme(ThemeName)
			writefile(GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json", ThemeData)
		end)

		return true
	end

	function WindowFunctions:LoadTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		local Success = pcall(function()
			local FilePath = GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json"
			if isfile(FilePath) then
				local ThemeData = readfile(FilePath)
				local ImportSuccess, Message = self:ImportTheme(ThemeData, ThemeName)
				if ImportSuccess then
					self:ApplyTheme(ThemeName)
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:BackupThemes()
		pcall(function()
			EnsureFolderExists(GetBackupFolderPath())

			local BackupData = {
				Timestamp = os.time(),
				Themes = Themes,
				Version = "2.0"
			}

			local BackupJSON = Services.HttpService:JSONEncode(BackupData)
			local BackupName = "theme_backup_" .. os.date("%Y%m%d_%H%M%S") .. ".json"
			writefile(GetBackupFolderPath() .. "/" .. BackupName, BackupJSON)
		end)

		return true
	end

	function WindowFunctions:RestoreThemes()
		local Success = pcall(function()
			local BackupFolder = GetBackupFolderPath()
			if isfolder(BackupFolder) then
				local BackupFiles = listfiles(BackupFolder)
				if #BackupFiles > 0 then
					local LatestBackup = BackupFiles[#BackupFiles]
					local BackupData = readfile(LatestBackup)
					local DecodedBackup = Services.HttpService:JSONDecode(BackupData)
					Themes = DecodedBackup.Themes
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:LoadStartupTheme()
		pcall(function()
			local ThemeFile = GetThemeFilePath()

			if isfile(ThemeFile) then
				local SavedTheme = readfile(ThemeFile)
				for ThemeName, _ in pairs(Themes) do
					if ThemeName == SavedTheme then
						self:ApplyTheme(SavedTheme)
						break
					end
				end
			end
		end)
	end

	function TabFunctions:InsertThemeSettings(Side)
        local SettingsSection = TabFunctions:Section({Side = Side})
        
        SettingsSection:Header({Name = "Theme Settings"})
        
        local ThemeNames = WindowFunctions:GetThemeNames()
        
        local ThemeDropdown = SettingsSection:Dropdown({
            Name = "Theme Preset",
			IgnoreConfig = true,
            Multi = false,
            Required = true,
            Search = true,
            Premium = true,
            Options = ThemeNames,
            Default = "Default",
            Callback = function(ThemeName)
				WindowFunctions:ApplyTheme(ThemeName)
            end
        }, "ThemeSelector")
        
        SettingsSection:Divider()
        SettingsSection:Header({Name = "Theme Customization"})
        
        local function SwitchToCustomTheme()
            if WindowFunctions:GetCurrentTheme() ~= "Custom" then
                ThemeDropdown:UpdateSelection("Custom")
            end
        end
        
        local function CreateCategoryDropdown()
            return SettingsSection:Dropdown({
                Name = "Customization Category",
				IgnoreConfig = true,
                Multi = false,
                Required = true,
                Options = {"Colors", "Layout", "Effects", "Typography", "Advanced", "ImportExport"},
                Default = "Colors",
                Callback = function(Category)
                    for Flag, Object in pairs(WindowFunctions:ReturnFeatures()) do
                        if type(Flag) == "string" and Object.SetVisibility and Flag:find("Custom_") then
                            local CategoryPrefix = "Custom_" .. Category
                            Object:SetVisibility(Flag:find(CategoryPrefix) ~= nil)
                        end
                    end
                end
            }, "ThemeCustomizationTab")
        end
        
        local function CreateColorPicker(Name, ThemeKey, Flag)
            return SettingsSection:ColorPicker({
                Name = Name,
				IgnoreConfig = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Color)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Color
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return SettingsSection:Slider({
                Name = Name,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Minimum = Min,
                Maximum = Max,
                DisplayMethod = DisplayMethod,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateEffectSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
        end
        
        local function CreateToggle(Name, ThemeKey, Flag, ApplyTheme)
            return SettingsSection:Toggle({
                Name = Name,
				IgnoreConfig = true,
				IgnoreDefault = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Value)
                    if ApplyTheme then
                        SwitchToCustomTheme()
                    end
                    Themes.Custom[ThemeKey] = Value
                    if ApplyTheme then
                        WindowFunctions:ApplyTheme("Custom")
                    end
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateDropdown(Name, ThemeKey, Options, Flag)
            return SettingsSection:Dropdown({
                Name = Name,
                Multi = false,
                Required = true,
                Options = Options,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local CategoryTabs = CreateCategoryDropdown()
        
        local ColorPickers = {
            Background = CreateColorPicker("Background Color", "Background", "Custom_Colors_Background"),
            Foreground = CreateColorPicker("Foreground Color", "Foreground", "Custom_Colors_Foreground"),
            Surface = CreateColorPicker("Surface Color", "Surface", "Custom_Colors_Surface"),
            TextPrimary = CreateColorPicker("Primary Text Color", "TextPrimary", "Custom_Colors_TextPrimary"),
            TextSecondary = CreateColorPicker("Secondary Text Color", "TextSecondary", "Custom_Colors_TextSecondary"),
            TextDisabled = CreateColorPicker("Disabled Text Color", "TextDisabled", "Custom_Colors_TextDisabled"),
            AccentPrimary = CreateColorPicker("Primary Accent Color", "AccentPrimary", "Custom_Colors_AccentPrimary"),
            AccentSecondary = CreateColorPicker("Secondary Accent Color", "AccentSecondary", "Custom_Colors_AccentSecondary"),
            AccentTertiary = CreateColorPicker("Tertiary Accent Color", "AccentTertiary", "Custom_Colors_AccentTertiary"),
            SuccessColor = CreateColorPicker("Success Color", "SuccessColor", "Custom_Colors_SuccessColor"),
            WarningColor = CreateColorPicker("Warning Color", "WarningColor", "Custom_Colors_WarningColor"),
            ErrorColor = CreateColorPicker("Error Color", "ErrorColor", "Custom_Colors_ErrorColor"),
            InfoColor = CreateColorPicker("Info Color", "InfoColor", "Custom_Colors_InfoColor"),
            ShadowColor = CreateColorPicker("Shadow Color", "ShadowColor", "Custom_Colors_ShadowColor"),
            HeaderColor = CreateColorPicker("Header Color", "HeaderColor", "Custom_Colors_HeaderColor"),
            SidebarColor = CreateColorPicker("Sidebar Color", "SidebarColor", "Custom_Colors_SidebarColor"),
            BorderColor = CreateColorPicker("Border Color", "BorderColor", "Custom_Colors_BorderColor"),
            NotificationColor = CreateColorPicker("Notification Color", "NotificationColor", "Custom_Colors_NotificationColor"),
            StatusColor = CreateColorPicker("Status Color", "StatusColor", "Custom_Colors_StatusColor"),
            DialogColor = CreateColorPicker("Dialog Color", "DialogColor", "Custom_Colors_DialogColor"),
            ButtonColor = CreateColorPicker("Button Color", "ButtonColor", "Custom_Colors_ButtonColor"),
            ToggleColor = CreateColorPicker("Toggle Color", "ToggleColor", "Custom_Colors_ToggleColor"),
            TogglerHeadColor = CreateColorPicker("Toggle Head Color", "TogglerHeadColor", "Custom_Colors_TogglerHeadColor"),
            SliderColor = CreateColorPicker("Slider Color", "SliderColor", "Custom_Colors_SliderColor"),
            SliderHeadColor = CreateColorPicker("Slider Head Color", "SliderHeadColor", "Custom_Colors_SliderHeadColor"),
            DropdownColor = CreateColorPicker("Dropdown Color", "DropdownColor", "Custom_Colors_DropdownColor"),
            InputFieldColor = CreateColorPicker("Input Field Color", "InputFieldColor", "Custom_Colors_InputFieldColor"),
            ScrollbarColor = CreateColorPicker("Scrollbar Color", "ScrollbarColor", "Custom_Colors_ScrollbarColor"),
            SelectionColor = CreateColorPicker("Selection Color", "SelectionColor", "Custom_Colors_SelectionColor"),
            HoverColor = CreateColorPicker("Hover Color", "HoverColor", "Custom_Colors_HoverColor"),
            ActiveColor = CreateColorPicker("Active Color", "ActiveColor", "Custom_Colors_ActiveColor"),
            FocusColor = CreateColorPicker("Focus Color", "FocusColor", "Custom_Colors_FocusColor"),
            DisabledOverlay = CreateColorPicker("Disabled Overlay Color", "DisabledOverlay", "Custom_Colors_DisabledOverlay"),
            ParticleColor = CreateColorPicker("Particle Color", "ParticleColor", "Custom_Colors_ParticleColor"),
            MinimizeColor = CreateColorPicker("Minimize Button Color", "MinimizeColor", "Custom_Colors_MinimizeColor"),
            ExitColor = CreateColorPicker("Exit Button Color", "ExitColor", "Custom_Colors_ExitColor")
        }
        
        local LayoutSliders = {
            CornerRadius = CreateLayoutSlider("Corner Radius", "CornerRadius", 0, 25, "Round", "Custom_Layout_CornerRadius"),
            BackgroundTransparency = CreateLayoutSlider("Background Transparency", "BackgroundTransparency", 0, 1, "Hundredths", "Custom_Layout_BackgroundTransparency"),
            SurfaceTransparency = CreateLayoutSlider("Surface Transparency", "SurfaceTransparency", 0, 1, "Hundredths", "Custom_Layout_SurfaceTransparency"),
            StrokeTransparency = CreateLayoutSlider("Stroke Transparency", "StrokeTransparency", 0, 1, "Hundredths", "Custom_Layout_StrokeTransparency"),
            StrokeThickness = CreateLayoutSlider("Stroke Thickness", "StrokeThickness", 0, 5, "Round", "Custom_Layout_StrokeThickness"),
            HeaderTransparency = CreateLayoutSlider("Header Transparency", "HeaderTransparency", 0, 1, "Hundredths", "Custom_Layout_HeaderTransparency"),
            SidebarTransparency = CreateLayoutSlider("Sidebar Transparency", "SidebarTransparency", 0, 1, "Hundredths", "Custom_Layout_SidebarTransparency"),
            InputFieldTransparency = CreateLayoutSlider("Input Field Transparency", "InputFieldTransparency", 0, 1, "Hundredths", "Custom_Layout_InputFieldTransparency"),
            ScrollbarTransparency = CreateLayoutSlider("Scrollbar Transparency", "ScrollbarTransparency", 0, 1, "Hundredths", "Custom_Layout_ScrollbarTransparency"),
            HoverTransparency = CreateLayoutSlider("Hover Transparency", "HoverTransparency", 0, 1, "Hundredths", "Custom_Layout_HoverTransparency"),
            DisabledTransparency = CreateLayoutSlider("Disabled Transparency", "DisabledTransparency", 0, 1, "Hundredths", "Custom_Layout_DisabledTransparency"),
            WallpaperTransparency = CreateLayoutSlider("Wallpaper Transparency", "WallpaperTransparency", 0, 1, "Hundredths", "Custom_Layout_WallpaperTransparency")
        }
        
        local EffectsSliders = {
            ShadowTransparency = CreateEffectSlider("Shadow Transparency", "ShadowTransparency", 0, 1, "Hundredths", "Custom_Effects_ShadowTransparency"),
            GlowTransparency = CreateEffectSlider("Glow Transparency", "GlowTransparency", 0, 1, "Hundredths", "Custom_Effects_GlowTransparency"),
            ShadowSize = CreateEffectSlider("Shadow Size", "ShadowSize", 0, 50, "Round", "Custom_Effects_ShadowSize"),
            GlowSize = CreateEffectSlider("Glow Size", "GlowSize", 0, 50, "Round", "Custom_Effects_GlowSize"),
            AnimationSpeed = CreateEffectSlider("Animation Speed", "AnimationSpeed", 0.05, 1, "Hundredths", "Custom_Effects_AnimationSpeed"),
            BlurIntensity = CreateEffectSlider("Blur Intensity", "BlurIntensity", 0, 100, "Round", "Custom_Effects_BlurIntensity"),
            NoiseScale = CreateEffectSlider("Noise Scale", "NoiseScale", 10, 200, "Round", "Custom_Effects_NoiseScale"),
            NoiseTransparency = CreateEffectSlider("Noise Transparency", "NoiseTransparency", 0, 1, "Hundredths", "Custom_Effects_NoiseTransparency"),
            ParticleCount = CreateEffectSlider("Particle Count", "ParticleCount", 0, 100, "Round", "Custom_Effects_ParticleCount"),
            GradientRotation = CreateEffectSlider("Gradient Rotation", "GradientRotation", 0, 360, "Round", "Custom_Effects_GradientRotation")
        }
        
        local EffectsToggles = {
            WallpaperEnabled = CreateToggle("Enable Wallpaper", "WallpaperEnabled", "Custom_Effects_WallpaperEnabled", false),
            GradientEnabled = CreateToggle("Enable Gradient", "GradientEnabled", "Custom_Effects_GradientEnabled", true),
            NoiseEnabled = CreateToggle("Enable Noise", "NoiseEnabled", "Custom_Effects_NoiseEnabled", true),
            ParticlesEnabled = CreateToggle("Enable Particles", "ParticlesEnabled", "Custom_Effects_ParticlesEnabled", true)
        }
        
        local EffectsDropdowns = {
            WallpaperBlending = CreateDropdown("Wallpaper Blending", "WallpaperBlending", {"Normal", "Multiply", "Overlay", "Screen", "SoftLight", "HardLight"}, "Custom_Effects_WallpaperBlending")
        }
        
        local TypographySliders = {
            FontSize = CreateLayoutSlider("Font Size", "FontSize", 8, 24, "Round", "Custom_Typography_FontSize"),
            LineHeight = CreateLayoutSlider("Line Height", "LineHeight", 1, 3, "Tenths", "Custom_Typography_LineHeight"),
            LetterSpacing = CreateLayoutSlider("Letter Spacing", "LetterSpacing", -2, 5, "Tenths", "Custom_Typography_LetterSpacing")
        }
        
        local TypographyDropdowns = {
            FontFamily = CreateDropdown("Font Family", "FontFamily", {"Inter", "Arial", "Helvetica", "Roboto", "Open Sans", "Source Sans Pro", "Lato", "Nunito", "Poppins", "Ubuntu", "JetBrains Mono", "Roboto Mono", "Courier New", "Comic Sans MS", "Segoe UI"}, "Custom_Typography_FontFamily"),
            FontWeight = CreateDropdown("Font Weight", "FontWeight", {"Thin", "Light", "Regular", "Medium", "SemiBold", "Bold", "ExtraBold", "Black"}, "Custom_Typography_FontWeight")
        }
        
        local TypographyToggles = {
            AntiAliasing = CreateToggle("Anti-Aliasing", "AntiAliasing", "Custom_Typography_AntiAliasing", false),
            SubPixelRendering = CreateToggle("Sub-Pixel Rendering", "SubPixelRendering", "Custom_Typography_SubPixelRendering", false)
        }
        
        local AdvancedDropdowns = {
            ThemeVariant = CreateDropdown("Theme Variant", "ThemeVariant", {"Auto", "Light", "Dark"}, "Custom_Advanced_ThemeVariant"),
            AccentColorSource = CreateDropdown("Accent Color Source", "AccentColorSource", {"Manual", "System", "Auto"}, "Custom_Advanced_AccentColorSource")
        }
        
        local AdvancedToggles = {
            AutoDarkMode = CreateToggle("Auto Dark Mode", "AutoDarkMode", "Custom_Advanced_AutoDarkMode", true)
        }
        
        local UtilityButtons = {
            SaveTheme = SettingsSection:Button({
                Name = "Save Custom Theme",
                Callback = function()
                    if WindowFunctions:SaveTheme("Custom") then
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Custom theme saved successfully!",
                            Lifetime = 3
                        })
                    end
                end
            }, "Custom_ImportExport_SaveTheme"):SetVisibility(false),
            
            ExportTheme = SettingsSection:Button({
                Name = "Copy Theme to Clipboard",
                Callback = function()
                    pcall(function()
                        local ThemeData = WindowFunctions:ExportTheme(WindowFunctions:GetCurrentTheme())
                        setclipboard(ThemeData)
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Theme data copied to clipboard!",
                            Lifetime = 3
                        })
                    end)
                end
            }, "Custom_ImportExport_ExportTheme"):SetVisibility(false),
            
            RandomTheme = SettingsSection:Button({
                Name = "Generate Random Theme",
                Callback = function()
                    SwitchToCustomTheme()
                    WindowFunctions:GenerateRandomTheme()

					WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Random theme generated!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_RandomTheme"):SetVisibility(false),
            
            ResetTheme = SettingsSection:Button({
                Name = "Reset to Default",
                Callback = function()
                    Themes.Custom = table.clone(Themes.Default)
                    WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Theme reset to default values!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_ResetTheme"):SetVisibility(false)
        }
        
        WindowFunctions:Initialize()
        WindowFunctions:ApplyCurrentTheme()
    end
end]],
  ["UserInterface/Imports/Settings/InsertThemeSettings.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibraryUI = Requirements.XenithLibraryUI
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions
	local ThemeColorSchemes = Requirements.ThemeColorSchemes
	local ThemeTemplates = Requirements.ThemeTemplates
	local PerformanceModes = Requirements.PerformanceModes
	local ThemeUtility = Requirements.ThemeUtility
	local Themes = Requirements.Themes

    local Subtitle = Requirements.Subtitle
    local Exit = Requirements.Exit
	local Minimize = Requirements.Minimize

	local function GetThemeFilePath()
		return XenithLibrary.Folder .. "/settings/startup/ThemeSaved.Xenith"
	end

	local function GetThemeFolderPath()
		return XenithLibrary.Folder .. "/settings/themes"
	end

	local function GetBackupFolderPath()
		return GetThemeFolderPath() .. "/backups"
	end

	local function EnsureFolderExists(Path)
		if not isfolder(Path) then
			makefolder(Path)
		end
	end

	local function EnsureThemeFoldersExist()
		EnsureFolderExists(XenithLibrary.Folder .. "/settings")
		EnsureFolderExists(XenithLibrary.Folder .. "/settings/startup")
		EnsureFolderExists(GetThemeFolderPath())
	end

	function WindowFunctions:GetThemeNames()
		local Names = {}
		for ThemeName, _ in pairs(Themes) do
			table.insert(Names, ThemeName)
		end
		return Names
	end

	function WindowFunctions:GetTheme(ThemeName)
		return Themes[ThemeName] or Themes.Default
	end

	function WindowFunctions:GetCurrentTheme()
		return CurrentTheme
	end

	function WindowFunctions:GetThemeAttribute(ThemeName, Attribute)
		return self:GetTheme(ThemeName)[Attribute]
	end

	function WindowFunctions:GetCurrentThemeAttribute(Attribute)
		return self:GetTheme(self:GetCurrentTheme())[Attribute]
	end

	function WindowFunctions:CreateCustomTheme(ThemeSettings)
		if not ThemeSettings then
			return false
		end

		for Key, Value in pairs(Themes.Default) do
			if ThemeSettings[Key] == nil then
				ThemeSettings[Key] = Value
			end
		end

		Themes.Custom = ThemeSettings

		if CurrentTheme == "Custom" then
			self:ApplyTheme("Custom")
		end

		return true
	end

	function WindowFunctions:ExportTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Custom
		return Services.HttpService:JSONEncode(Theme)
	end

	function WindowFunctions:ImportTheme(ThemeData, ThemeName)
		local Success, DecodedTheme = pcall(Services.HttpService.JSONDecode, Services.HttpService, ThemeData)

		if not Success then
			return false, "Invalid JSON format"
		end

		if not DecodedTheme.Background or not DecodedTheme.TextPrimary then
			return false, "Invalid theme structure"
		end

		ThemeName = ThemeName or "Custom"
		Themes[ThemeName] = DecodedTheme

		return true, "Theme imported successfully"
	end

	function WindowFunctions:GenerateColorScheme(SchemeType, BaseColor)
		if not ThemeColorSchemes[SchemeType] then
			return false
		end

		local NewColors = ThemeColorSchemes[SchemeType](BaseColor)

		for ColorName, ColorValue in pairs(NewColors) do
			if Themes.Custom[ColorName] then
				Themes.Custom[ColorName] = ColorValue
			end
		end

		return true
	end

	function WindowFunctions:ApplyTemplate(TemplateName)
		local Template = ThemeTemplates[TemplateName]
		if not Template then
			return false
		end

		for Property, Value in pairs(Template) do
			Themes.Custom[Property] = Value
		end

		return true
	end

	function WindowFunctions:SetPerformanceMode(ModeName)
		local Mode = PerformanceModes[ModeName]
		if not Mode then
			return false
		end

		for Setting, Value in pairs(Mode) do
			Themes.Custom[Setting] = Value
		end

		return true
	end

	function WindowFunctions:GenerateRandomTheme()
		local function RandomColor()
			return Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
		end

		local function RandomBool()
			return math.random() > 0.5
		end

		Themes.Custom.Background = RandomColor()
		Themes.Custom.Foreground = RandomColor()
		Themes.Custom.AccentPrimary = RandomColor()
		Themes.Custom.AccentSecondary = RandomColor()
		Themes.Custom.TextPrimary = RandomColor()
		Themes.Custom.CornerRadius = math.random(0, 25)
		Themes.Custom.ShadowSize = math.random(0, 50)
		Themes.Custom.GlowSize = math.random(0, 30)
		Themes.Custom.AnimationSpeed = math.random(10, 50) / 100
		Themes.Custom.GradientEnabled = RandomBool()

		return true
	end

	function WindowFunctions:ApplyUICorners(Parent, CornerRadius)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UICorner") and not Child:FindFirstAncestor("CustomElementPanel") and not Child:FindFirstAncestor("CheckBoxButton") then
				Child.CornerRadius = UDim.new(0, CornerRadius)
			end
		end
	end

	function WindowFunctions:ApplyUIStrokes(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("UIStroke") and Child.Name ~= "TabSwitcherUIStroke" and Child.Name ~= "MainFrameStroke" and Child.Name ~= "NameStroke" and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.Color = Theme.BorderColor
				if not Child:FindFirstAncestor("CheckBoxButton") then
					Child.Transparency = Theme.StrokeTransparency
				end
				
				Child.Thickness = Theme.StrokeThickness
			end
		end
	end

	function WindowFunctions:ApplyScrollbarStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("ScrollingFrame") and not Child:FindFirstAncestor("CustomElementPanel") then
				Child.ScrollBarImageColor3 = Theme.ScrollbarColor
				Child.ScrollBarImageTransparency = Theme.ScrollbarTransparency
			end
		end
	end

	function WindowFunctions:ApplyTextStyling(Parent, Theme)
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:GetAttribute("NoApplyTheme") then
				continue
			end

			if Child:IsA("TextLabel") or Child:IsA("TextButton") or Child:IsA("TextBox") then
				if Child.Name == "Title" or Child.Name == "ParagraphHeader" or Child.Name == "HeaderText" then
					Child.TextColor3 = Theme.HeaderColor
					Child.TextTransparency = Theme.HeaderTransparency
				elseif Child:IsA("TextBox") then
					Child.TextColor3 = Theme.InputFieldColor

					if Child.Name ~= "HeaderInputSearchBox" and Child.Name ~= "SearchBox" then
						Child.BackgroundTransparency = Theme.InputValueBackground or Theme.InputFieldTransparency
					end

					if Child.BackgroundTransparency < 1 then
						Child.BackgroundColor3 = Theme.Foreground
					end

					if Child.PlaceholderText and Child.PlaceholderText ~= "" then
						Child.PlaceholderColor3 = Theme.TextSecondary
					end
				else
					if not Child.Name:find("Result_") then
						Child.TextColor3 = Theme.TextPrimary
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyImageStyling(Parent, Theme)
		local ExcludedNames = {"Wallpaper", "OutputWallpaper", "InformationWallpaper", "ColorWheel", "Toggle", "ToggleHead", "Banner", "Headshot", "LinkIcon", "InfoIcon"}
		
		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:FindFirstAncestor("CustomElementPanel") then
				continue
			end

			if Child:IsA("ImageLabel") or Child:IsA("ImageButton") then
				local IsExcluded = false

				for _, ExcludedName in ipairs(ExcludedNames) do
					if Child.Name == ExcludedName then
						IsExcluded = true
						break
					end
				end

				if not IsExcluded then
					Child.ImageColor3 = Theme.TextSecondary
				end
			end
		end
	end

	function WindowFunctions:ApplyComponentStyling(Parent, Theme)
		local ComponentStyles = {
			Toggle = Theme.ToggleColor,
			CheckBoxButton = Theme.AccentPrimary,
			TogglerHead = Theme.TogglerHeadColor,
			SliderBar = Theme.SliderColor,
			RangeBar = Theme.SliderColor,
			SliderHead = Theme.SliderHeadColor,
			Dropdown = Theme.DropdownColor,
			Option = Theme.DropdownColor,
			Button = Theme.ButtonColor,
			InformationGroupHighlight = Theme.AccentPrimary,
			TabSwitcherIndicator = Theme.AccentPrimary,

			Sidebar = function(Child)
				Child.BackgroundTransparency = Theme.SidebarTransparency
				Child.BackgroundColor3 = Theme.SidebarColor
			end,

			Surface = function(Child)
				Child.BackgroundColor3 = Theme.Surface
				Child.BackgroundTransparency = Theme.SurfaceTransparency
			end,

			Section = Theme.Foreground,
			Notification = Theme.NotificationColor,
			ExternalStatus = Theme.StatusColor,
		}

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child:IsA("GuiObject") and not Child:IsA("ScrollingFrame") and Child.BackgroundTransparency < 1 and not Child:FindFirstAncestor("CustomElementPanel") then
				local Style = ComponentStyles[Child.Name]
				
				if Style then
					if type(Style) == "function" then
						Style(Child)
					elseif Child:IsA("ImageButton") and (Child.Name == "Toggle" or Child.Name == "TogglerHead" or Child.Name == "SliderBar" or Child.Name == "SliderHead") and not Child:FindFirstAncestor("CustomElementPanel") then
						Child.ImageColor3 = Style
					else
						Child.BackgroundColor3 = Style
					end
				end
			end
		end
	end

	function WindowFunctions:ApplyGradients(Parent, Theme)
		if not Theme.GradientEnabled then
			return
		end

		for _, Child in ipairs(Parent:GetDescendants()) do
			if Child.Name == "Background" and Child:IsA("GuiObject") and not Child:FindFirstAncestor("CustomElementPanel") then
				local Gradient = Child:FindFirstChild("UIGradient")
				if not Gradient then
					Gradient = Instance.new("UIGradient")
					Gradient.Parent = Child
				end

				Gradient.Color = ColorSequence.new(Theme.GradientColors)
				Gradient.Rotation = Theme.GradientRotation
			end
		end
	end

	function WindowFunctions:ApplyWallpaper(Base, Theme)
		local function ApplyWallpaperToElement(Element)
			if Element then
				Element.ImageColor3 = Theme.Background:Lerp(Color3.fromRGB(255, 255, 255), 0.2)
				Element.ImageTransparency = Theme.WallpaperEnabled and Theme.WallpaperTransparency or 1
			end
		end

		ApplyWallpaperToElement(Base:FindFirstChild("Wallpaper"))
		
		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			ApplyWallpaperToElement(Terminal:FindFirstChild("OutputWallpaper"))
		end
	end

	function WindowFunctions:ApplyShadows(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			Shadow.ImageTransparency = Theme.ShadowTransparency
		end
	end

	function WindowFunctions:ApplyShadowGradients(Theme)
		if not Maid.Shadows then
			return
		end

		for _, Shadow in pairs(Maid.Shadows) do
			if Shadow and typeof(Theme.GlowColor) == "ColorSequence" then
				local ShadowGradient = Shadow:FindFirstChildWhichIsA("UIGradient") or Instance.new("UIGradient")
				ShadowGradient.Color = Theme.GlowColor
				ShadowGradient.Parent = Shadow
				Shadow.ImageColor3 = Color3.fromRGB(255, 255, 255)
			elseif Shadow then
				if Shadow:FindFirstChildWhichIsA("UIGradient") then
					Shadow:FindFirstChildWhichIsA("UIGradient"):Destroy()
				end
				Shadow.ImageColor3 = Theme.GlowColor
			end
		end
	end

	function WindowFunctions:ApplyPremiumGradients(Content, Theme)
		if not Content then
			return
		end

		for _, Element in pairs(Content:GetDescendants()) do
			if Element:IsA("UIGradient") and Element.Parent.Name ~= "Execute" and not Element:FindFirstAncestor("CustomElementPanel") then
				Element.Color = Theme.PremiumGradientColor
			end
		end
	end

	function WindowFunctions:ApplyControlButtons(Theme)
		if not Exit or not Minimize then
			return
		end

		Exit.BackgroundColor3 = Theme.ExitColor
		Minimize.BackgroundColor3 = Theme.MinimizeColor
	end

	function WindowFunctions:ApplyMenuGameCards(GameCardFolder, Theme)
		if not GameCardFolder then
			return
		end

		for _, Card in pairs(GameCardFolder:GetChildren()) do
			if not Card:IsA("GuiObject") then
				continue
			end

			local GlowFrame = Card:FindFirstChild("GlowFrame")
			if GlowFrame then
				GlowFrame.BackgroundColor3 = Theme.MinimizeColor
			end
		end
	end

	function WindowFunctions:AlreadyExistantTheme()
		local ThemeFile = GetThemeFilePath()
		if isfile(ThemeFile) then
			return readfile(ThemeFile)
		end
		return false
	end

	function WindowFunctions:ApplyTheme(ThemeName)
		local Theme = Themes[ThemeName] or Themes.Default
		CurrentTheme = ThemeName

		local Base = XenithLibraryUI:FindFirstChild("Base")
		if not Base then
			return false
		end

		Base.BackgroundColor3 = Theme.Background
		Base.BackgroundTransparency = Theme.BackgroundTransparency

		local ApplySubtitle = "Build - " .. Utility:CheckHubStatus(WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")) .. " | " .. GameLoading
		Subtitle.Text = ApplySubtitle

		local BaseUIStroke = Base:FindFirstChild("BaseUIStroke")
		if BaseUIStroke then
			BaseUIStroke.Transparency = Theme.StrokeTransparency
			BaseUIStroke.Color = Theme.BorderColor
			BaseUIStroke.Thickness = Theme.StrokeThickness
		end

		local BaseUICorner = Base:FindFirstChild("BaseUICorner")
		if BaseUICorner then
			BaseUICorner.CornerRadius = UDim.new(0, Theme.CornerRadius)
		end

		self:ApplyUICorners(Base, Theme.CornerRadius)
		self:ApplyUIStrokes(Base, Theme)
		self:ApplyScrollbarStyling(Base, Theme)
		self:ApplyTextStyling(Base, Theme)
		self:ApplyImageStyling(Base, Theme)
		self:ApplyComponentStyling(Base, Theme)
		self:ApplyGradients(Base, Theme)
		self:ApplyControlButtons(Theme)
		self:ApplyWallpaper(Base, Theme)
		self:ApplyShadows(Theme)
		self:ApplyShadowGradients(Theme)

		local Sidebar = Base:FindFirstChild("Sidebar")
		if Sidebar then
			Sidebar.BackgroundColor3 = Theme.SidebarColor
			Sidebar.BackgroundTransparency = Theme.SidebarTransparency
			self:ApplyUICorners(Sidebar, Theme.CornerRadius)
			self:ApplyUIStrokes(Sidebar, Theme)
			self:ApplyTextStyling(Sidebar, Theme)
			self:ApplyComponentStyling(Sidebar, Theme)
		end

		local Content = Base:FindFirstChild("Content")
		if Content then
			self:ApplyPremiumGradients(Content, Theme)
			self:ApplyUICorners(Content, Theme.CornerRadius)
			self:ApplyUIStrokes(Content, Theme)
			self:ApplyTextStyling(Content, Theme)
			self:ApplyComponentStyling(Content, Theme)

			local ElementsScrolling = Content:FindFirstChild("Elements") and Content.Elements:FindFirstChild("ElementsScrolling")
			if ElementsScrolling and ElementsScrolling:FindFirstChild("Middle") then
				local GameCards = ElementsScrolling:FindFirstChild("Middle")
				self:ApplyMenuGameCards(GameCards, Theme)
			end
		end

		local BottomBar = Base:FindFirstChild("BottomBar")
		if BottomBar then
			BottomBar.BackgroundColor3 = Theme.Background
			BottomBar.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(BottomBar, Theme.CornerRadius)
			self:ApplyUIStrokes(BottomBar, Theme)
			self:ApplyTextStyling(BottomBar, Theme)
			self:ApplyComponentStyling(BottomBar, Theme)
		end

		local Terminal = Base:FindFirstChild("Terminal")
		if Terminal then
			Terminal.BackgroundColor3 = Theme.Background
			Terminal.BackgroundTransparency = Theme.BackgroundTransparency
			self:ApplyUICorners(Terminal, Theme.CornerRadius)
			self:ApplyUIStrokes(Terminal, Theme)
			self:ApplyTextStyling(Terminal, Theme)
			self:ApplyComponentStyling(Terminal, Theme)
		end

		for _, Dialog in ipairs(Base:GetChildren()) do
			if Dialog.Name == "Dialog" and Dialog:IsA("Frame") then
				local TargetFrame = Dialog.Prompt
				self:ApplyUICorners(TargetFrame, Theme.CornerRadius)
				self:ApplyUIStrokes(TargetFrame, Theme)
				self:ApplyTextStyling(TargetFrame, Theme)
				self:ApplyComponentStyling(TargetFrame, Theme)
			end
		end

		if ThemeName ~= "Custom" then
			pcall(function()
				EnsureThemeFoldersExist()
				writefile(GetThemeFilePath(), ThemeName)
			end)
		end

		return true
	end

	function WindowFunctions:ApplyCurrentTheme()
		self:ApplyTheme(self:AlreadyExistantTheme() or self:GetCurrentTheme() or "Default")
	end

	function WindowFunctions:SaveTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		pcall(function()
			EnsureThemeFoldersExist()
			local ThemeData = self:ExportTheme(ThemeName)
			writefile(GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json", ThemeData)
		end)

		return true
	end

	function WindowFunctions:LoadTheme(ThemeName)
		ThemeName = ThemeName or "Custom"

		local Success = pcall(function()
			local FilePath = GetThemeFolderPath() .. "/" .. ThemeName:lower() .. ".json"
			if isfile(FilePath) then
				local ThemeData = readfile(FilePath)
				local ImportSuccess, Message = self:ImportTheme(ThemeData, ThemeName)
				if ImportSuccess then
					self:ApplyTheme(ThemeName)
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:BackupThemes()
		pcall(function()
			EnsureFolderExists(GetBackupFolderPath())

			local BackupData = {
				Timestamp = os.time(),
				Themes = Themes,
				Version = "2.0"
			}

			local BackupJSON = Services.HttpService:JSONEncode(BackupData)
			local BackupName = "theme_backup_" .. os.date("%Y%m%d_%H%M%S") .. ".json"
			writefile(GetBackupFolderPath() .. "/" .. BackupName, BackupJSON)
		end)

		return true
	end

	function WindowFunctions:RestoreThemes()
		local Success = pcall(function()
			local BackupFolder = GetBackupFolderPath()
			if isfolder(BackupFolder) then
				local BackupFiles = listfiles(BackupFolder)
				if #BackupFiles > 0 then
					local LatestBackup = BackupFiles[#BackupFiles]
					local BackupData = readfile(LatestBackup)
					local DecodedBackup = Services.HttpService:JSONDecode(BackupData)
					Themes = DecodedBackup.Themes
					return true
				end
			end
		end)

		return Success
	end

	function WindowFunctions:LoadStartupTheme()
		pcall(function()
			local ThemeFile = GetThemeFilePath()

			if isfile(ThemeFile) then
				local SavedTheme = readfile(ThemeFile)
				for ThemeName, _ in pairs(Themes) do
					if ThemeName == SavedTheme then
						self:ApplyTheme(SavedTheme)
						break
					end
				end
			end
		end)
	end

	function TabFunctions:InsertThemeSettings(Side)
        local SettingsSection = TabFunctions:Section({Side = Side})
        
        SettingsSection:Header({Name = "Theme Settings"})
        
        local ThemeNames = WindowFunctions:GetThemeNames()
        
        local ThemeDropdown = SettingsSection:Dropdown({
            Name = "Theme Preset",
			IgnoreConfig = true,
            Multi = false,
            Required = true,
            Search = true,
            Premium = true,
            Options = ThemeNames,
            Default = "Default",
            Callback = function(ThemeName)
				WindowFunctions:ApplyTheme(ThemeName)
            end
        }, "ThemeSelector")
        
        SettingsSection:Divider()
        SettingsSection:Header({Name = "Theme Customization"})
        
        local function SwitchToCustomTheme()
            if WindowFunctions:GetCurrentTheme() ~= "Custom" then
                ThemeDropdown:UpdateSelection("Custom")
            end
        end
        
        local function CreateCategoryDropdown()
            return SettingsSection:Dropdown({
                Name = "Customization Category",
				IgnoreConfig = true,
                Multi = false,
                Required = true,
                Options = {"Colors", "Layout", "Effects", "Typography", "Advanced", "ImportExport"},
                Default = "Colors",
                Callback = function(Category)
                    for Flag, Object in pairs(WindowFunctions:ReturnFeatures()) do
                        if type(Flag) == "string" and Object.SetVisibility and Flag:find("Custom_") then
                            local CategoryPrefix = "Custom_" .. Category
                            Object:SetVisibility(Flag:find(CategoryPrefix) ~= nil)
                        end
                    end
                end
            }, "ThemeCustomizationTab")
        end
        
        local function CreateColorPicker(Name, ThemeKey, Flag)
            return SettingsSection:ColorPicker({
                Name = Name,
				IgnoreConfig = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Color)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Color
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return SettingsSection:Slider({
                Name = Name,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Minimum = Min,
                Maximum = Max,
                DisplayMethod = DisplayMethod,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateEffectSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
            return CreateLayoutSlider(Name, ThemeKey, Min, Max, DisplayMethod, Flag)
        end
        
        local function CreateToggle(Name, ThemeKey, Flag, ApplyTheme)
            return SettingsSection:Toggle({
                Name = Name,
				IgnoreConfig = true,
				IgnoreDefault = true,
                Default = Themes.Default[ThemeKey],
                Callback = function(Value)
                    if ApplyTheme then
                        SwitchToCustomTheme()
                    end
                    Themes.Custom[ThemeKey] = Value
                    if ApplyTheme then
                        WindowFunctions:ApplyTheme("Custom")
                    end
                end
            }, Flag):SetVisibility(false)
        end
        
        local function CreateDropdown(Name, ThemeKey, Options, Flag)
            return SettingsSection:Dropdown({
                Name = Name,
                Multi = false,
                Required = true,
                Options = Options,
                Default = Themes.Default[ThemeKey],
				IgnoreConfig = true,
                Callback = function(Value)
                    SwitchToCustomTheme()
                    Themes.Custom[ThemeKey] = Value
                    WindowFunctions:ApplyTheme("Custom")
                end
            }, Flag):SetVisibility(false)
        end
        
        local CategoryTabs = CreateCategoryDropdown()
        
        local ColorPickers = {
            Background = CreateColorPicker("Background Color", "Background", "Custom_Colors_Background"),
            Foreground = CreateColorPicker("Foreground Color", "Foreground", "Custom_Colors_Foreground"),
            Surface = CreateColorPicker("Surface Color", "Surface", "Custom_Colors_Surface"),
            TextPrimary = CreateColorPicker("Primary Text Color", "TextPrimary", "Custom_Colors_TextPrimary"),
            TextSecondary = CreateColorPicker("Secondary Text Color", "TextSecondary", "Custom_Colors_TextSecondary"),
            TextDisabled = CreateColorPicker("Disabled Text Color", "TextDisabled", "Custom_Colors_TextDisabled"),
            AccentPrimary = CreateColorPicker("Primary Accent Color", "AccentPrimary", "Custom_Colors_AccentPrimary"),
            AccentSecondary = CreateColorPicker("Secondary Accent Color", "AccentSecondary", "Custom_Colors_AccentSecondary"),
            AccentTertiary = CreateColorPicker("Tertiary Accent Color", "AccentTertiary", "Custom_Colors_AccentTertiary"),
            SuccessColor = CreateColorPicker("Success Color", "SuccessColor", "Custom_Colors_SuccessColor"),
            WarningColor = CreateColorPicker("Warning Color", "WarningColor", "Custom_Colors_WarningColor"),
            ErrorColor = CreateColorPicker("Error Color", "ErrorColor", "Custom_Colors_ErrorColor"),
            InfoColor = CreateColorPicker("Info Color", "InfoColor", "Custom_Colors_InfoColor"),
            ShadowColor = CreateColorPicker("Shadow Color", "ShadowColor", "Custom_Colors_ShadowColor"),
            HeaderColor = CreateColorPicker("Header Color", "HeaderColor", "Custom_Colors_HeaderColor"),
            SidebarColor = CreateColorPicker("Sidebar Color", "SidebarColor", "Custom_Colors_SidebarColor"),
            BorderColor = CreateColorPicker("Border Color", "BorderColor", "Custom_Colors_BorderColor"),
            NotificationColor = CreateColorPicker("Notification Color", "NotificationColor", "Custom_Colors_NotificationColor"),
            StatusColor = CreateColorPicker("Status Color", "StatusColor", "Custom_Colors_StatusColor"),
            DialogColor = CreateColorPicker("Dialog Color", "DialogColor", "Custom_Colors_DialogColor"),
            ButtonColor = CreateColorPicker("Button Color", "ButtonColor", "Custom_Colors_ButtonColor"),
            ToggleColor = CreateColorPicker("Toggle Color", "ToggleColor", "Custom_Colors_ToggleColor"),
            TogglerHeadColor = CreateColorPicker("Toggle Head Color", "TogglerHeadColor", "Custom_Colors_TogglerHeadColor"),
            SliderColor = CreateColorPicker("Slider Color", "SliderColor", "Custom_Colors_SliderColor"),
            SliderHeadColor = CreateColorPicker("Slider Head Color", "SliderHeadColor", "Custom_Colors_SliderHeadColor"),
            DropdownColor = CreateColorPicker("Dropdown Color", "DropdownColor", "Custom_Colors_DropdownColor"),
            InputFieldColor = CreateColorPicker("Input Field Color", "InputFieldColor", "Custom_Colors_InputFieldColor"),
            ScrollbarColor = CreateColorPicker("Scrollbar Color", "ScrollbarColor", "Custom_Colors_ScrollbarColor"),
            SelectionColor = CreateColorPicker("Selection Color", "SelectionColor", "Custom_Colors_SelectionColor"),
            HoverColor = CreateColorPicker("Hover Color", "HoverColor", "Custom_Colors_HoverColor"),
            ActiveColor = CreateColorPicker("Active Color", "ActiveColor", "Custom_Colors_ActiveColor"),
            FocusColor = CreateColorPicker("Focus Color", "FocusColor", "Custom_Colors_FocusColor"),
            DisabledOverlay = CreateColorPicker("Disabled Overlay Color", "DisabledOverlay", "Custom_Colors_DisabledOverlay"),
            ParticleColor = CreateColorPicker("Particle Color", "ParticleColor", "Custom_Colors_ParticleColor"),
            MinimizeColor = CreateColorPicker("Minimize Button Color", "MinimizeColor", "Custom_Colors_MinimizeColor"),
            ExitColor = CreateColorPicker("Exit Button Color", "ExitColor", "Custom_Colors_ExitColor")
        }
        
        local LayoutSliders = {
            CornerRadius = CreateLayoutSlider("Corner Radius", "CornerRadius", 0, 25, "Round", "Custom_Layout_CornerRadius"),
            BackgroundTransparency = CreateLayoutSlider("Background Transparency", "BackgroundTransparency", 0, 1, "Hundredths", "Custom_Layout_BackgroundTransparency"),
            SurfaceTransparency = CreateLayoutSlider("Surface Transparency", "SurfaceTransparency", 0, 1, "Hundredths", "Custom_Layout_SurfaceTransparency"),
            StrokeTransparency = CreateLayoutSlider("Stroke Transparency", "StrokeTransparency", 0, 1, "Hundredths", "Custom_Layout_StrokeTransparency"),
            StrokeThickness = CreateLayoutSlider("Stroke Thickness", "StrokeThickness", 0, 5, "Round", "Custom_Layout_StrokeThickness"),
            HeaderTransparency = CreateLayoutSlider("Header Transparency", "HeaderTransparency", 0, 1, "Hundredths", "Custom_Layout_HeaderTransparency"),
            SidebarTransparency = CreateLayoutSlider("Sidebar Transparency", "SidebarTransparency", 0, 1, "Hundredths", "Custom_Layout_SidebarTransparency"),
            InputFieldTransparency = CreateLayoutSlider("Input Field Transparency", "InputFieldTransparency", 0, 1, "Hundredths", "Custom_Layout_InputFieldTransparency"),
            ScrollbarTransparency = CreateLayoutSlider("Scrollbar Transparency", "ScrollbarTransparency", 0, 1, "Hundredths", "Custom_Layout_ScrollbarTransparency"),
            HoverTransparency = CreateLayoutSlider("Hover Transparency", "HoverTransparency", 0, 1, "Hundredths", "Custom_Layout_HoverTransparency"),
            DisabledTransparency = CreateLayoutSlider("Disabled Transparency", "DisabledTransparency", 0, 1, "Hundredths", "Custom_Layout_DisabledTransparency"),
            WallpaperTransparency = CreateLayoutSlider("Wallpaper Transparency", "WallpaperTransparency", 0, 1, "Hundredths", "Custom_Layout_WallpaperTransparency")
        }
        
        local EffectsSliders = {
            ShadowTransparency = CreateEffectSlider("Shadow Transparency", "ShadowTransparency", 0, 1, "Hundredths", "Custom_Effects_ShadowTransparency"),
            GlowTransparency = CreateEffectSlider("Glow Transparency", "GlowTransparency", 0, 1, "Hundredths", "Custom_Effects_GlowTransparency"),
            ShadowSize = CreateEffectSlider("Shadow Size", "ShadowSize", 0, 50, "Round", "Custom_Effects_ShadowSize"),
            GlowSize = CreateEffectSlider("Glow Size", "GlowSize", 0, 50, "Round", "Custom_Effects_GlowSize"),
            AnimationSpeed = CreateEffectSlider("Animation Speed", "AnimationSpeed", 0.05, 1, "Hundredths", "Custom_Effects_AnimationSpeed"),
            BlurIntensity = CreateEffectSlider("Blur Intensity", "BlurIntensity", 0, 100, "Round", "Custom_Effects_BlurIntensity"),
            NoiseScale = CreateEffectSlider("Noise Scale", "NoiseScale", 10, 200, "Round", "Custom_Effects_NoiseScale"),
            NoiseTransparency = CreateEffectSlider("Noise Transparency", "NoiseTransparency", 0, 1, "Hundredths", "Custom_Effects_NoiseTransparency"),
            ParticleCount = CreateEffectSlider("Particle Count", "ParticleCount", 0, 100, "Round", "Custom_Effects_ParticleCount"),
            GradientRotation = CreateEffectSlider("Gradient Rotation", "GradientRotation", 0, 360, "Round", "Custom_Effects_GradientRotation")
        }
        
        local EffectsToggles = {
            WallpaperEnabled = CreateToggle("Enable Wallpaper", "WallpaperEnabled", "Custom_Effects_WallpaperEnabled", false),
            GradientEnabled = CreateToggle("Enable Gradient", "GradientEnabled", "Custom_Effects_GradientEnabled", true),
            NoiseEnabled = CreateToggle("Enable Noise", "NoiseEnabled", "Custom_Effects_NoiseEnabled", true),
            ParticlesEnabled = CreateToggle("Enable Particles", "ParticlesEnabled", "Custom_Effects_ParticlesEnabled", true)
        }
        
        local EffectsDropdowns = {
            WallpaperBlending = CreateDropdown("Wallpaper Blending", "WallpaperBlending", {"Normal", "Multiply", "Overlay", "Screen", "SoftLight", "HardLight"}, "Custom_Effects_WallpaperBlending")
        }
        
        local TypographySliders = {
            FontSize = CreateLayoutSlider("Font Size", "FontSize", 8, 24, "Round", "Custom_Typography_FontSize"),
            LineHeight = CreateLayoutSlider("Line Height", "LineHeight", 1, 3, "Tenths", "Custom_Typography_LineHeight"),
            LetterSpacing = CreateLayoutSlider("Letter Spacing", "LetterSpacing", -2, 5, "Tenths", "Custom_Typography_LetterSpacing")
        }
        
        local TypographyDropdowns = {
            FontFamily = CreateDropdown("Font Family", "FontFamily", {"Inter", "Arial", "Helvetica", "Roboto", "Open Sans", "Source Sans Pro", "Lato", "Nunito", "Poppins", "Ubuntu", "JetBrains Mono", "Roboto Mono", "Courier New", "Comic Sans MS", "Segoe UI"}, "Custom_Typography_FontFamily"),
            FontWeight = CreateDropdown("Font Weight", "FontWeight", {"Thin", "Light", "Regular", "Medium", "SemiBold", "Bold", "ExtraBold", "Black"}, "Custom_Typography_FontWeight")
        }
        
        local TypographyToggles = {
            AntiAliasing = CreateToggle("Anti-Aliasing", "AntiAliasing", "Custom_Typography_AntiAliasing", false),
            SubPixelRendering = CreateToggle("Sub-Pixel Rendering", "SubPixelRendering", "Custom_Typography_SubPixelRendering", false)
        }
        
        local AdvancedDropdowns = {
            ThemeVariant = CreateDropdown("Theme Variant", "ThemeVariant", {"Auto", "Light", "Dark"}, "Custom_Advanced_ThemeVariant"),
            AccentColorSource = CreateDropdown("Accent Color Source", "AccentColorSource", {"Manual", "System", "Auto"}, "Custom_Advanced_AccentColorSource")
        }
        
        local AdvancedToggles = {
            AutoDarkMode = CreateToggle("Auto Dark Mode", "AutoDarkMode", "Custom_Advanced_AutoDarkMode", true)
        }
        
        local UtilityButtons = {
            SaveTheme = SettingsSection:Button({
                Name = "Save Custom Theme",
                Callback = function()
                    if WindowFunctions:SaveTheme("Custom") then
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Custom theme saved successfully!",
                            Lifetime = 3
                        })
                    end
                end
            }, "Custom_ImportExport_SaveTheme"):SetVisibility(false),
            
            ExportTheme = SettingsSection:Button({
                Name = "Copy Theme to Clipboard",
                Callback = function()
                    pcall(function()
                        local ThemeData = WindowFunctions:ExportTheme(WindowFunctions:GetCurrentTheme())
                        setclipboard(ThemeData)
                        WindowFunctions:Notify({
                            Title = "Theme Settings",
                            Description = "Theme data copied to clipboard!",
                            Lifetime = 3
                        })
                    end)
                end
            }, "Custom_ImportExport_ExportTheme"):SetVisibility(false),
            
            RandomTheme = SettingsSection:Button({
                Name = "Generate Random Theme",
                Callback = function()
                    SwitchToCustomTheme()
                    WindowFunctions:GenerateRandomTheme()

					WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Random theme generated!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_RandomTheme"):SetVisibility(false),
            
            ResetTheme = SettingsSection:Button({
                Name = "Reset to Default",
                Callback = function()
                    Themes.Custom = table.clone(Themes.Default)
                    WindowFunctions:ApplyTheme("Custom")
                    WindowFunctions:Notify({
                        Title = "Theme Settings",
                        Description = "Theme reset to default values!",
                        Lifetime = 3
                    })
                end
            }, "Custom_ImportExport_ResetTheme"):SetVisibility(false)
        }
        
        WindowFunctions:Initialize()
        WindowFunctions:ApplyCurrentTheme()
    end
end]],
  ["Xenith/Xenith/UserInterface/Imports/Terminal/TerminalModule.lua"] = [[local TerminalModule = {}
TerminalModule.__index = TerminalModule

local TerminalConstants = {
	DefaultWidth = 0.375,
	MinWidth = 250,
	MaxWidth = 1920,
	SnapRange = 20,
	ToggleDebounceTime = 0.3,
	FadeOutDelay = 0.2,
	MessageGroupingWindow = 2,
	LogTextSize = 13,
	HeaderTextSize = 18,
	IconSize = 15,
	ContextMenuWidth = 160,
	ContextMenuHeight = 90,
	ContextMenuItemHeight = 30,
	DividerThickness = 1,
	CornerRadius = 10,
	ContentTopOffset = 80,
	ScrollBarThickness = 1
}

local TweenSettings = {
	DefaultTransparency = 0.9,
	HoverTransparency = 0.85,
	ActiveTransparency = 0,
	EasingStyle = Enum.EasingStyle.Sine
}

local OutputSettings = {
	ShowTimestamps = true,
	ShowIcons = true,
	SupportTables = true,
	AutoScroll = true,
	MaxTableDepth = 3,
	TruncateStrings = 50,
	MaxTableSize = 100,
	MaxLogEntries = 100
}

function TerminalModule.new(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI, WindowFunctions)
	local self = setmetatable({}, TerminalModule)
	
	self.ResizingOutput = false
	self.DefaultOutputWidth = 0
	self.InitialMouseX = 0
	self.InitialOutputWidth = 0
	self.InitialLeftEdge = 0
	self.TerminalOffset = 0
	self.ToggleDebounce = false
	self.PrintLogs = true
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
	self.LastMessageTime = 0
	
	self:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self:SetupEventHandlers(WindowFunctions)
	self:InitializeLogging(WindowFunctions)
	
	return self
end

function TerminalModule:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self.Terminal = self:CreateTerminalFrame(Parent, XenithLibrary)
	self.ShadowContainer = self:CreateShadow(Parent, XenithLibrary, XenithLibraryUI)
	self.OutputTab = self:CreateOutputTab()
	self.OutputTabDivider = self:CreateDivider(UDim2.new(0.5, 0, 0, 42.5), UDim2.new(0, 48, 0, 1), 0.5)
	self.ClearLogsButton = self:CreateIconButton("ClearLogsIcon", Assets.IconClearLogs, UDim2.fromScale(0.935, 0.05))
	self.StopLogsButton = self:CreateIconButton("StopLogsIcon", Assets.IconStopLogs, UDim2.fromScale(0.85, 0.05))
	self.DividerInteract = self:CreateDividerInteract()
	self.OutputContent = self:CreateScrollingFrame()
	self.OutputWallpaper = self:CreateWallpaper()
	self.OutputUIScale = self:CreateUIScale()
	self.OutputUICorner = self:CreateUICorner()
	self.OutputUIStroke = self:CreateUIStroke()
	self.OutputDivider = self:CreateDivider(UDim2.new(0, 0, 0, 63), UDim2.new(1, 0, 0, 1), 0)
	self.OutputUIListLayout = self:CreateListLayout()
	self.OutputIcon = self:CreateTopbarIcon(TopbarFolder)
	self.OutputInteract = self:CreateTopbarInteract()
	
	self.DefaultOutputWidth = self.Terminal.AbsoluteSize.X
end

function TerminalModule:CreateTerminalFrame(Parent, XenithLibrary)
	local Terminal = Instance.new("Frame")
	Terminal.Name = "Terminal"
	Terminal.AnchorPoint = Vector2.new(0.5, 0.5)
	Terminal.Size = UDim2.new(TerminalConstants.DefaultWidth, 0, 1, 0)
	Terminal.Position = Utility:SaveOpened() and UDim2.new(1.2, 0, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	Terminal.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	Terminal.BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0
	Terminal.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Terminal.BorderSizePixel = 0
	Terminal.Parent = Parent
	Terminal.ZIndex = -5
	Terminal.Visible = Utility:SaveOpened()
	
	return Terminal
end

function TerminalModule:CreateShadow(Base, XenithLibrary, XenithLibraryUI)
	return XenithLibrary:CreateShadow({
		Object = XenithLibraryUI,
		ReplicateTransparency = true,
		ReplicateVisible = {Base, self.Terminal},
		ObjectReplication = self.Terminal,
		Gradient = {
			Color = ColorSequence.new({
				ColorSequenceKeypoint.new(0, Color3.new(0.4, 0.6, 1)),
				ColorSequenceKeypoint.new(1, Color3.new(0, 0, 0.2))
			})
		}
	})
end

function TerminalModule:CreateOutputTab()
	local OutputTab = Instance.new("TextLabel")
	OutputTab.Name = "OutputTab"
	OutputTab.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
	OutputTab.RichText = true
	OutputTab.Text = "Terminal"
	OutputTab.TextColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.TextSize = TerminalConstants.HeaderTextSize
	OutputTab.TextTransparency = 0.5
	OutputTab.TextTruncate = Enum.TextTruncate.SplitWord
	OutputTab.TextXAlignment = Enum.TextXAlignment.Center
	OutputTab.TextYAlignment = Enum.TextYAlignment.Top
	OutputTab.AnchorPoint = Vector2.new(0, 0.5)
	OutputTab.AutomaticSize = Enum.AutomaticSize.Y
	OutputTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.BackgroundTransparency = 1
	OutputTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputTab.BorderSizePixel = 0
	OutputTab.Position = UDim2.fromScale(0.05, 0.05)
	OutputTab.Size = UDim2.fromScale(0.9, 0)
	OutputTab.Parent = self.Terminal
	OutputTab.ZIndex = 155
	
	return OutputTab
end

function TerminalModule:CreateDivider(Position, Size, AnchorPoint)
	local Divider = Instance.new("Frame")
	Divider.Name = "OutputDivider"
	Divider.AnchorPoint = Vector2.new(AnchorPoint, 0)
	Divider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Divider.BackgroundTransparency = TweenSettings.DefaultTransparency
	Divider.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Divider.BorderSizePixel = 0
	Divider.Position = Position
	Divider.Size = Size
	Divider.Parent = self.Terminal
	
	return Divider
end

function TerminalModule:CreateIconButton(Name, ImageAsset, Position)
	local Button = Instance.new("ImageButton")
	Button.Name = Name
	Button.ZIndex = 5
	Button.Image = ImageAsset
	Button.ImageTransparency = 0.7
	Button.AnchorPoint = Vector2.new(1, 0.5)
	Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Button.BackgroundTransparency = 1
	Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Button.BorderSizePixel = 0
	Button.Position = Position
	Button.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Button.Parent = self.Terminal
	Button.Visible = true
	
	return Button
end

function TerminalModule:CreateDividerInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "DividerOutputInteract"
	Interact.AnchorPoint = Vector2.new(0.5, 0)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Position = UDim2.fromScale(1, 0)
	Interact.Size = UDim2.new(0, 5, 1, 0)
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.Parent = self.Terminal
	
	return Interact
end

function TerminalModule:CreateScrollingFrame()
	local Content = Instance.new("ScrollingFrame")
	Content.Name = "Content"
	Content.AnchorPoint = Vector2.new(1, 0)
	Content.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Content.BackgroundTransparency = 1
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(1, 0, 0, TerminalConstants.ContentTopOffset)
	Content.Size = UDim2.new(1, 0, 1, -TerminalConstants.ContentTopOffset)
	Content.CanvasSize = UDim2.new(0, 0, 0, 0)
	Content.ScrollBarThickness = TerminalConstants.ScrollBarThickness
	Content.ScrollBarImageTransparency = 0.5
	Content.Parent = self.Terminal
	
	return Content
end

function TerminalModule:CreateWallpaper()
	local Wallpaper = Instance.new("ImageLabel")
	Wallpaper.Name = "OutputWallpaper"
	Wallpaper.Image = Assets.StyleOneWallpaper
	Wallpaper.Size = UDim2.new(1, 0, 1, 0)
	Wallpaper.Position = UDim2.new(0, 0, 0, 0)
	Wallpaper.BackgroundTransparency = 1
	Wallpaper.ImageTransparency = 0.65
	Wallpaper.ImageColor3 = Color3.fromRGB(32, 32, 32)
	Wallpaper.ScaleType = Enum.ScaleType.Crop
	Wallpaper.Parent = self.Terminal
	
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputWallpaperUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = Wallpaper
	
	return Wallpaper
end

function TerminalModule:CreateUIScale()
	local Scale = Instance.new("UIScale")
	Scale.Name = "OutputUIScale"
	Scale.Parent = self.Terminal
	
	return Scale
end

function TerminalModule:CreateUICorner()
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = self.Terminal
	
	return Corner
end

function TerminalModule:CreateUIStroke()
	local Stroke = Instance.new("UIStroke")
	Stroke.Name = "OutputUIStroke"
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(255, 255, 255)
	Stroke.Transparency = 0.7
	Stroke.Parent = self.Terminal
	
	return Stroke
end

function TerminalModule:CreateListLayout()
	local Layout = Instance.new("UIListLayout")
	Layout.Name = "OutputUIListLayout"
	Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	Layout.Padding = UDim.new(0, 10)
	Layout.SortOrder = Enum.SortOrder.LayoutOrder
	Layout.Parent = self.OutputContent
	
	Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		self.OutputContent.CanvasSize = UDim2.new(0, 0, 0, Layout.AbsoluteContentSize.Y)
	end)
	
	return Layout
end

function TerminalModule:CreateTopbarIcon(TopbarFolder)
	local Icon = Instance.new("ImageButton")
	Icon.Name = "TerminalIcon"
	Icon.Image = Assets.IconOutputIcon
	Icon.ImageTransparency = 0.7
	Icon.AnchorPoint = Vector2.new(1, 0.5)
	Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Icon.BackgroundTransparency = 1
	Icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Icon.BorderSizePixel = 0
	Icon.Position = UDim2.fromScale(1, 0.5)
	Icon.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Icon.Parent = TopbarFolder
	Icon.Visible = true
	
	return Icon
end

function TerminalModule:CreateTopbarInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "Interact"
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.AnchorPoint = Vector2.new(0.5, 0.5)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.Position = UDim2.fromScale(0.5, 0.5)
	Interact.Size = UDim2.fromOffset(40, 40)
	Interact.Parent = self.OutputIcon
	
	return Interact
end

function TerminalModule:SetupEventHandlers(WindowFunctions)
	self:SetupResizeHandlers()
	self:SetupHoverEffects()
	self:SetupToggleHandler(WindowFunctions)
	self:SetupClearLogsHandler()
	self:SetupStopLogsHandler()
end

function TerminalModule:SetupResizeHandlers()
	self.DividerInteract.MouseButton1Down:Connect(function()
		self.ResizingOutput = true
		self.InitialMouseX = Services.UserInputService:GetMouseLocation().X
		self.InitialOutputWidth = self.Terminal.AbsoluteSize.X
		self.InitialLeftEdge = self.Terminal.AbsolutePosition.X
	end)
	
	Services.UserInputService.InputEnded:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			self.ResizingOutput = false
		end
	end)
	
	Services.UserInputService.InputChanged:Connect(function(Input)
		if self.ResizingOutput and Input.UserInputType == Enum.UserInputType.MouseMovement then
			self:HandleResize(Input)
		end
	end)
end

function TerminalModule:HandleResize(Input)
	local DeltaX = Services.UserInputService:GetMouseLocation().X - self.InitialMouseX
	local NewOutputWidth = self.InitialOutputWidth + DeltaX
	
	if math.abs(NewOutputWidth - self.DefaultOutputWidth) < TerminalConstants.SnapRange then
		NewOutputWidth = self.DefaultOutputWidth
	else
		NewOutputWidth = math.clamp(NewOutputWidth, TerminalConstants.MinWidth, TerminalConstants.MaxWidth)
	end
	
	self.Terminal.Size = UDim2.new(0, NewOutputWidth, self.Terminal.Size.Y.Scale, self.Terminal.Size.Y.Offset)
	
	local CurrentLeftEdge = self.Terminal.AbsolutePosition.X
	local LeftAdjustment = self.InitialLeftEdge - CurrentLeftEdge
	local NewXOffset = self.Terminal.Position.X.Offset + LeftAdjustment
	
	self.Terminal.Position = UDim2.new(
		self.Terminal.Position.X.Scale,
		NewXOffset,
		self.Terminal.Position.Y.Scale,
		self.Terminal.Position.Y.Offset
	)
	
	self.TerminalOffset = NewXOffset
	self.OutputContent.Size = UDim2.new(0, NewOutputWidth, self.OutputContent.Size.Y.Scale, self.OutputContent.Size.Y.Offset)
end

function TerminalModule:SetupHoverEffects()
	self.OutputInteract.MouseEnter:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.OutputInteract.MouseLeave:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = 0.7})
	end)
	
	self.ClearLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.ClearLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = 0.7})
	end)
	
	self.StopLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.StopLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = 0.7})
	end)
end

function TerminalModule:SetupToggleHandler(WindowFunctions)
	self.OutputInteract.InputBegan:Connect(function(Input)
		if not self.ToggleDebounce and (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) then
			local NewState = not self.Terminal.Visible
			self:ToggleOutputScreen(NewState, WindowFunctions)
			self.ToggleDebounce = true
			
			Utility:SaveOpened(true, NewState)
			
			task.delay(TerminalConstants.ToggleDebounceTime, function()
				self.ToggleDebounce = false
			end)
		end
	end)
end

function TerminalModule:SetupClearLogsHandler()
	self.ClearLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self:ClearLogs()
		end
	end)
end

function TerminalModule:SetupStopLogsHandler()
	self.StopLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self.PrintLogs = not self.PrintLogs
		end
	end)
end

function TerminalModule:ToggleOutputScreen(Visible, WindowFunctions)
	local TransparencyGoal = Visible and 0 or 1
	local PositionGoal = Visible and UDim2.new(1.2, self.TerminalOffset, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	
	self.Terminal.Visible = true
	
	for _, Child in ipairs(self.Terminal:GetChildren()) do
		if Child:IsA("GuiObject") and Child.Name:sub(1, 1) == "{" then
			Child.Visible = Visible
		end
	end
	
	Utility:QuickTween(self.Terminal, {
		Position = PositionGoal,
		BackgroundTransparency = XenithLibrary.AcrylicBlur and (Visible and WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency") or 1) or TransparencyGoal
	})
	
	if self.OutputWallpaper then
		Utility:QuickTween(self.OutputWallpaper, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("WallpaperTransparency") or 0.65) or 1
		})
	end
	
	if self.ShadowContainer then
		Utility:QuickTween(self.ShadowContainer, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("ShadowTransparency") or 0) or 1
		})
	end
	
	if self.OutputUIStroke then
		Utility:QuickTween(self.OutputUIStroke, {
			Transparency = Visible and 0.6 or 1
		})
	end
	
	if self.OutputDivider then
		Utility:QuickTween(self.OutputDivider, {
			Transparency = Visible and TweenSettings.DefaultTransparency or 1
		})
	end
	
	for _, Descendant in ipairs(self.Terminal:GetDescendants()) do
		if Descendant:IsA("TextLabel") or Descendant:IsA("TextButton") or Descendant:IsA("TextBox") then
			if Descendant.Name:find("OutputResult_") then
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0 or 1})
			else
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0.5 or 1})
			end
		elseif (Descendant:IsA("ImageButton") or Descendant:IsA("ImageLabel")) and Descendant ~= self.OutputWallpaper then
			Utility:QuickTween(Descendant, {ImageTransparency = Visible and 0.7 or 1})
		end
		
		local Stroke = Descendant:FindFirstChildOfClass("UIStroke")
		if Stroke then
			Utility:QuickTween(Stroke, {Transparency = TransparencyGoal})
		end
	end
	
	if not Visible then
		task.delay(TerminalConstants.FadeOutDelay, function()
			self.Terminal.Visible = false
		end)
	end
end

function TerminalModule:ClearLogs()
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") then
			Child:Destroy()
		end
	end
	
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
end

function TerminalModule:InitializeLogging(WindowFunctions)
	Services.LogService.MessageOut:Connect(function(Message, MessageType)
		if not self.PrintLogs then
			return
		end
		
		self:HandleLogMessage(Message, MessageType, WindowFunctions)
	end)
end

function TerminalModule:HandleLogMessage(Message, MessageType, WindowFunctions)
	if not self.PrintLogs or not self.Terminal.Visible then
		return
	end
	
	local CurrentTime = os.time()
	local FormattedTime = OutputSettings.ShowTimestamps and os.date("[%H:%M:%S] ", CurrentTime) or ""
	local MessageId = tostring(math.random(10000000, 999999999))
	local ProcessedMessage = self:ProcessMessage(Message)
	
	if ProcessedMessage == self.LastMessage and self.LastOutputResult and self.LastOutputResult.Parent and (CurrentTime - self.LastMessageTime) < TerminalConstants.MessageGroupingWindow then
		self:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
		return
	end
	
	self.LastMessageCount = 1
	self.LastMessageTime = CurrentTime
	
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	local OutputResult = self:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	
	self.LastMessage = ProcessedMessage
	self.LastOutputResult = OutputResult
end

function TerminalModule:ProcessMessage(RawMessage)
	local MessageText = tostring(RawMessage)
	return MessageText
end

function TerminalModule:CreateFallbackTableString(RawMessage, MessageText)
	local FallbackSuccess, FallbackResult = pcall(function()
		local TableString = "{"
		local Count = 0
		
		for Key, Value in pairs(RawMessage) do
			if Count > 0 then
				TableString = TableString .. ", "
			end
			
			TableString = TableString .. tostring(Key) .. " = " .. tostring(Value)
			Count = Count + 1
			
			if Count >= 5 then
				TableString = TableString .. ", ..."
				break
			end
		end
		
		TableString = TableString .. "}"
		return "[Table] " .. TableString
	end)
	
	return FallbackSuccess and FallbackResult or "[Table] " .. MessageText
end

function TerminalModule:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
	self.LastMessageCount = self.LastMessageCount + 1
	local DisplayText = ProcessedMessage .. " (x" .. self.LastMessageCount .. ")"
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	
	self.LastOutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. DisplayText
	self.LastOutputResult.TextColor3 = MessageData.Color
end

function TerminalModule:GetMessageData(MessageType, WindowFunctions)
	local UseWindowFunctions = WindowFunctions.GetCurrentThemeAttribute ~= nil

    local Data = {
		Prefix = "Info",
		Icon = OutputSettings.ShowIcons and " " or "",
		Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("InfoColor") or Color3.fromRGB(255, 255, 255)
	}
	
	if MessageType == Enum.MessageType.MessageError then
		Data.Prefix = "Error"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("ErrorColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageWarning then
		Data.Prefix = "Warning"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("WarningColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageOutput then
		Data.Prefix = "Output"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("SuccessColor") or Color3.fromRGB(255, 255, 255)
	end
	
	return Data
end

function TerminalModule:EnforceLogLimit()
	local LogEntries = {}
	
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") and Child.Name:find("OutputResult_") then
			table.insert(LogEntries, Child)
		end
	end
	
	table.sort(LogEntries, function(A, B)
		return A.AbsolutePosition.Y < B.AbsolutePosition.Y
	end)
	
	local ExcessCount = #LogEntries - OutputSettings.MaxLogEntries
	if ExcessCount > 0 then
		for I = 1, ExcessCount do
			if LogEntries[I] then
				LogEntries[I]:Destroy()
			end
		end
	end
end

function TerminalModule:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	local OutputResult = Instance.new("TextButton")
	OutputResult.Name = "OutputResult_" .. MessageId
	OutputResult.FontFace = Font.new(Assets.InterFont)
	OutputResult.RichText = true
	OutputResult.TextSize = TerminalConstants.LogTextSize
	OutputResult.TextTruncate = Enum.TextTruncate.SplitWord
	OutputResult.TextXAlignment = Enum.TextXAlignment.Left
	OutputResult.TextYAlignment = Enum.TextYAlignment.Top
	OutputResult.AnchorPoint = Vector2.new(0, 0.5)
	OutputResult.AutomaticSize = Enum.AutomaticSize.Y
	OutputResult.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputResult.BackgroundTransparency = 1
	OutputResult.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputResult.BorderSizePixel = 0
	OutputResult.Position = UDim2.fromScale(1.3e-07, 0.5)
	OutputResult.Size = UDim2.new(1, -40, 0, 0)
	OutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. ProcessedMessage
	OutputResult.TextColor3 = MessageData.Color
	
	local ContextMenu = self:CreateContextMenu(ProcessedMessage, FormattedTime, MessageData.Prefix, OutputResult)
	ContextMenu.Parent = OutputResult
	
	self:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageData.Color, ContextMenu)
	
	task.spawn(function()
		OutputResult.Parent = self.OutputContent
		
		-- Enforce log limit after adding new entry
		self:EnforceLogLimit()
		
		if OutputSettings.AutoScroll then
			task.wait()
			if self.OutputContent then
				self.OutputContent.CanvasPosition = Vector2.new(0, self.OutputContent.AbsoluteCanvasSize.Y)
			end
		end
	end)
	
	return OutputResult
end

function TerminalModule:CreateContextMenu(ProcessedMessage, FormattedTime, MessageLevel, OutputResult)
	local ContextMenu = Instance.new("Frame")
	ContextMenu.Name = "ContextMenu"
	ContextMenu.BackgroundTransparency = 1
	ContextMenu.BorderSizePixel = 0
	ContextMenu.Size = UDim2.fromOffset(TerminalConstants.ContextMenuWidth, TerminalConstants.ContextMenuHeight)
	ContextMenu.Visible = false
	ContextMenu.ZIndex = 1000
	
	local MenuCorner = Instance.new("UICorner")
	MenuCorner.CornerRadius = UDim.new(0, 4)
	MenuCorner.Parent = ContextMenu
	
	local MenuList = Instance.new("UIListLayout")
	MenuList.SortOrder = Enum.SortOrder.LayoutOrder
	MenuList.Parent = ContextMenu
	
	self:CreateContextMenuItem(ContextMenu, " Copy Message", function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Copy Full", function()
		local FullLog = FormattedTime .. "[" .. MessageLevel .. "] " .. ProcessedMessage
		self:SafeCopy(FullLog)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Delete", function()
		if OutputResult and OutputResult.Parent then
			OutputResult:Destroy()
		end
	end)
	
	return ContextMenu
end

function TerminalModule:CreateContextMenuItem(Parent, Text, Callback)
	local Item = Instance.new("TextButton")
	Item.BackgroundTransparency = 1
	Item.BorderSizePixel = 0
	Item.Size = UDim2.new(1, 0, 0, TerminalConstants.ContextMenuItemHeight)
	Item.Text = Text
	Item.TextColor3 = Color3.fromRGB(255, 255, 255)
	Item.TextSize = 12
	Item.FontFace = Font.new(Assets.InterFont)
	Item.Parent = Parent
	
	Item.MouseButton1Click:Connect(function()
		Parent.Visible = false
		task.spawn(Callback)
	end)
	
	return Item
end

function TerminalModule:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageColor, ContextMenu)
	local OriginalBackgroundColor = OutputResult.BackgroundColor3
	
	OutputResult.MouseEnter:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = Color3.fromRGB(50, 50, 50),
			TextColor3 = Color3.fromRGB(92, 156, 255),
			BackgroundTransparency = TweenSettings.DefaultTransparency
		})
	end)
	
	OutputResult.MouseLeave:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = OriginalBackgroundColor,
			TextColor3 = MessageColor,
			BackgroundTransparency = 1
		})
	end)
	
	OutputResult.MouseButton1Click:Connect(function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	OutputResult.MouseButton2Click:Connect(function()
		ContextMenu.Position = UDim2.fromOffset(10, 10)
		ContextMenu.Visible = not ContextMenu.Visible
	end)
end

function TerminalModule:SafeCopy(Text)
	local Success = pcall(function()
		if setclipboard then
			setclipboard(Text)
			return true
		end
		return false
	end)
	
	if Success then
		if Utility and Utility.Notify then
			Utility:Notify("Copied", "Message copied to clipboard!", 2)
		elseif WindowFunctions and WindowFunctions.Notify then
			WindowFunctions:Notify({
				Title = "Copied",
				Description = "Message copied to clipboard!"
			})
		else
			print("Copied to clipboard!")
		end
	else
		if Utility and Utility.Notify then
			Utility:Notify("Error", "Could not copy to clipboard", 2)
		else
			print("Could not copy to clipboard")
		end
	end
end

function TerminalModule:Destroy()
	if self.Terminal then
		self.Terminal:Destroy()
	end
	
	if self.OutputIcon then
		self.OutputIcon:Destroy()
	end
	
	if self.ShadowContainer then
		self.ShadowContainer:Destroy()
	end
end

return TerminalModule]],
  ["Xenith/UserInterface/Imports/Terminal/TerminalModule.lua"] = [[local TerminalModule = {}
TerminalModule.__index = TerminalModule

local TerminalConstants = {
	DefaultWidth = 0.375,
	MinWidth = 250,
	MaxWidth = 1920,
	SnapRange = 20,
	ToggleDebounceTime = 0.3,
	FadeOutDelay = 0.2,
	MessageGroupingWindow = 2,
	LogTextSize = 13,
	HeaderTextSize = 18,
	IconSize = 15,
	ContextMenuWidth = 160,
	ContextMenuHeight = 90,
	ContextMenuItemHeight = 30,
	DividerThickness = 1,
	CornerRadius = 10,
	ContentTopOffset = 80,
	ScrollBarThickness = 1
}

local TweenSettings = {
	DefaultTransparency = 0.9,
	HoverTransparency = 0.85,
	ActiveTransparency = 0,
	EasingStyle = Enum.EasingStyle.Sine
}

local OutputSettings = {
	ShowTimestamps = true,
	ShowIcons = true,
	SupportTables = true,
	AutoScroll = true,
	MaxTableDepth = 3,
	TruncateStrings = 50,
	MaxTableSize = 100,
	MaxLogEntries = 100
}

function TerminalModule.new(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI, WindowFunctions)
	local self = setmetatable({}, TerminalModule)
	
	self.ResizingOutput = false
	self.DefaultOutputWidth = 0
	self.InitialMouseX = 0
	self.InitialOutputWidth = 0
	self.InitialLeftEdge = 0
	self.TerminalOffset = 0
	self.ToggleDebounce = false
	self.PrintLogs = true
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
	self.LastMessageTime = 0
	
	self:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self:SetupEventHandlers(WindowFunctions)
	self:InitializeLogging(WindowFunctions)
	
	return self
end

function TerminalModule:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self.Terminal = self:CreateTerminalFrame(Parent, XenithLibrary)
	self.ShadowContainer = self:CreateShadow(Parent, XenithLibrary, XenithLibraryUI)
	self.OutputTab = self:CreateOutputTab()
	self.OutputTabDivider = self:CreateDivider(UDim2.new(0.5, 0, 0, 42.5), UDim2.new(0, 48, 0, 1), 0.5)
	self.ClearLogsButton = self:CreateIconButton("ClearLogsIcon", Assets.IconClearLogs, UDim2.fromScale(0.935, 0.05))
	self.StopLogsButton = self:CreateIconButton("StopLogsIcon", Assets.IconStopLogs, UDim2.fromScale(0.85, 0.05))
	self.DividerInteract = self:CreateDividerInteract()
	self.OutputContent = self:CreateScrollingFrame()
	self.OutputWallpaper = self:CreateWallpaper()
	self.OutputUIScale = self:CreateUIScale()
	self.OutputUICorner = self:CreateUICorner()
	self.OutputUIStroke = self:CreateUIStroke()
	self.OutputDivider = self:CreateDivider(UDim2.new(0, 0, 0, 63), UDim2.new(1, 0, 0, 1), 0)
	self.OutputUIListLayout = self:CreateListLayout()
	self.OutputIcon = self:CreateTopbarIcon(TopbarFolder)
	self.OutputInteract = self:CreateTopbarInteract()
	
	self.DefaultOutputWidth = self.Terminal.AbsoluteSize.X
end

function TerminalModule:CreateTerminalFrame(Parent, XenithLibrary)
	local Terminal = Instance.new("Frame")
	Terminal.Name = "Terminal"
	Terminal.AnchorPoint = Vector2.new(0.5, 0.5)
	Terminal.Size = UDim2.new(TerminalConstants.DefaultWidth, 0, 1, 0)
	Terminal.Position = Utility:SaveOpened() and UDim2.new(1.2, 0, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	Terminal.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	Terminal.BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0
	Terminal.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Terminal.BorderSizePixel = 0
	Terminal.Parent = Parent
	Terminal.ZIndex = -5
	Terminal.Visible = Utility:SaveOpened()
	
	return Terminal
end

function TerminalModule:CreateShadow(Base, XenithLibrary, XenithLibraryUI)
	return XenithLibrary:CreateShadow({
		Object = XenithLibraryUI,
		ReplicateTransparency = true,
		ReplicateVisible = {Base, self.Terminal},
		ObjectReplication = self.Terminal,
		Gradient = {
			Color = ColorSequence.new({
				ColorSequenceKeypoint.new(0, Color3.new(0.4, 0.6, 1)),
				ColorSequenceKeypoint.new(1, Color3.new(0, 0, 0.2))
			})
		}
	})
end

function TerminalModule:CreateOutputTab()
	local OutputTab = Instance.new("TextLabel")
	OutputTab.Name = "OutputTab"
	OutputTab.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
	OutputTab.RichText = true
	OutputTab.Text = "Terminal"
	OutputTab.TextColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.TextSize = TerminalConstants.HeaderTextSize
	OutputTab.TextTransparency = 0.5
	OutputTab.TextTruncate = Enum.TextTruncate.SplitWord
	OutputTab.TextXAlignment = Enum.TextXAlignment.Center
	OutputTab.TextYAlignment = Enum.TextYAlignment.Top
	OutputTab.AnchorPoint = Vector2.new(0, 0.5)
	OutputTab.AutomaticSize = Enum.AutomaticSize.Y
	OutputTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.BackgroundTransparency = 1
	OutputTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputTab.BorderSizePixel = 0
	OutputTab.Position = UDim2.fromScale(0.05, 0.05)
	OutputTab.Size = UDim2.fromScale(0.9, 0)
	OutputTab.Parent = self.Terminal
	OutputTab.ZIndex = 155
	
	return OutputTab
end

function TerminalModule:CreateDivider(Position, Size, AnchorPoint)
	local Divider = Instance.new("Frame")
	Divider.Name = "OutputDivider"
	Divider.AnchorPoint = Vector2.new(AnchorPoint, 0)
	Divider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Divider.BackgroundTransparency = TweenSettings.DefaultTransparency
	Divider.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Divider.BorderSizePixel = 0
	Divider.Position = Position
	Divider.Size = Size
	Divider.Parent = self.Terminal
	
	return Divider
end

function TerminalModule:CreateIconButton(Name, ImageAsset, Position)
	local Button = Instance.new("ImageButton")
	Button.Name = Name
	Button.ZIndex = 5
	Button.Image = ImageAsset
	Button.ImageTransparency = 0.7
	Button.AnchorPoint = Vector2.new(1, 0.5)
	Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Button.BackgroundTransparency = 1
	Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Button.BorderSizePixel = 0
	Button.Position = Position
	Button.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Button.Parent = self.Terminal
	Button.Visible = true
	
	return Button
end

function TerminalModule:CreateDividerInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "DividerOutputInteract"
	Interact.AnchorPoint = Vector2.new(0.5, 0)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Position = UDim2.fromScale(1, 0)
	Interact.Size = UDim2.new(0, 5, 1, 0)
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.Parent = self.Terminal
	
	return Interact
end

function TerminalModule:CreateScrollingFrame()
	local Content = Instance.new("ScrollingFrame")
	Content.Name = "Content"
	Content.AnchorPoint = Vector2.new(1, 0)
	Content.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Content.BackgroundTransparency = 1
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(1, 0, 0, TerminalConstants.ContentTopOffset)
	Content.Size = UDim2.new(1, 0, 1, -TerminalConstants.ContentTopOffset)
	Content.CanvasSize = UDim2.new(0, 0, 0, 0)
	Content.ScrollBarThickness = TerminalConstants.ScrollBarThickness
	Content.ScrollBarImageTransparency = 0.5
	Content.Parent = self.Terminal
	
	return Content
end

function TerminalModule:CreateWallpaper()
	local Wallpaper = Instance.new("ImageLabel")
	Wallpaper.Name = "OutputWallpaper"
	Wallpaper.Image = Assets.StyleOneWallpaper
	Wallpaper.Size = UDim2.new(1, 0, 1, 0)
	Wallpaper.Position = UDim2.new(0, 0, 0, 0)
	Wallpaper.BackgroundTransparency = 1
	Wallpaper.ImageTransparency = 0.65
	Wallpaper.ImageColor3 = Color3.fromRGB(32, 32, 32)
	Wallpaper.ScaleType = Enum.ScaleType.Crop
	Wallpaper.Parent = self.Terminal
	
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputWallpaperUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = Wallpaper
	
	return Wallpaper
end

function TerminalModule:CreateUIScale()
	local Scale = Instance.new("UIScale")
	Scale.Name = "OutputUIScale"
	Scale.Parent = self.Terminal
	
	return Scale
end

function TerminalModule:CreateUICorner()
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = self.Terminal
	
	return Corner
end

function TerminalModule:CreateUIStroke()
	local Stroke = Instance.new("UIStroke")
	Stroke.Name = "OutputUIStroke"
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(255, 255, 255)
	Stroke.Transparency = 0.7
	Stroke.Parent = self.Terminal
	
	return Stroke
end

function TerminalModule:CreateListLayout()
	local Layout = Instance.new("UIListLayout")
	Layout.Name = "OutputUIListLayout"
	Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	Layout.Padding = UDim.new(0, 10)
	Layout.SortOrder = Enum.SortOrder.LayoutOrder
	Layout.Parent = self.OutputContent
	
	Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		self.OutputContent.CanvasSize = UDim2.new(0, 0, 0, Layout.AbsoluteContentSize.Y)
	end)
	
	return Layout
end

function TerminalModule:CreateTopbarIcon(TopbarFolder)
	local Icon = Instance.new("ImageButton")
	Icon.Name = "TerminalIcon"
	Icon.Image = Assets.IconOutputIcon
	Icon.ImageTransparency = 0.7
	Icon.AnchorPoint = Vector2.new(1, 0.5)
	Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Icon.BackgroundTransparency = 1
	Icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Icon.BorderSizePixel = 0
	Icon.Position = UDim2.fromScale(1, 0.5)
	Icon.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Icon.Parent = TopbarFolder
	Icon.Visible = true
	
	return Icon
end

function TerminalModule:CreateTopbarInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "Interact"
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.AnchorPoint = Vector2.new(0.5, 0.5)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.Position = UDim2.fromScale(0.5, 0.5)
	Interact.Size = UDim2.fromOffset(40, 40)
	Interact.Parent = self.OutputIcon
	
	return Interact
end

function TerminalModule:SetupEventHandlers(WindowFunctions)
	self:SetupResizeHandlers()
	self:SetupHoverEffects()
	self:SetupToggleHandler(WindowFunctions)
	self:SetupClearLogsHandler()
	self:SetupStopLogsHandler()
end

function TerminalModule:SetupResizeHandlers()
	self.DividerInteract.MouseButton1Down:Connect(function()
		self.ResizingOutput = true
		self.InitialMouseX = Services.UserInputService:GetMouseLocation().X
		self.InitialOutputWidth = self.Terminal.AbsoluteSize.X
		self.InitialLeftEdge = self.Terminal.AbsolutePosition.X
	end)
	
	Services.UserInputService.InputEnded:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			self.ResizingOutput = false
		end
	end)
	
	Services.UserInputService.InputChanged:Connect(function(Input)
		if self.ResizingOutput and Input.UserInputType == Enum.UserInputType.MouseMovement then
			self:HandleResize(Input)
		end
	end)
end

function TerminalModule:HandleResize(Input)
	local DeltaX = Services.UserInputService:GetMouseLocation().X - self.InitialMouseX
	local NewOutputWidth = self.InitialOutputWidth + DeltaX
	
	if math.abs(NewOutputWidth - self.DefaultOutputWidth) < TerminalConstants.SnapRange then
		NewOutputWidth = self.DefaultOutputWidth
	else
		NewOutputWidth = math.clamp(NewOutputWidth, TerminalConstants.MinWidth, TerminalConstants.MaxWidth)
	end
	
	self.Terminal.Size = UDim2.new(0, NewOutputWidth, self.Terminal.Size.Y.Scale, self.Terminal.Size.Y.Offset)
	
	local CurrentLeftEdge = self.Terminal.AbsolutePosition.X
	local LeftAdjustment = self.InitialLeftEdge - CurrentLeftEdge
	local NewXOffset = self.Terminal.Position.X.Offset + LeftAdjustment
	
	self.Terminal.Position = UDim2.new(
		self.Terminal.Position.X.Scale,
		NewXOffset,
		self.Terminal.Position.Y.Scale,
		self.Terminal.Position.Y.Offset
	)
	
	self.TerminalOffset = NewXOffset
	self.OutputContent.Size = UDim2.new(0, NewOutputWidth, self.OutputContent.Size.Y.Scale, self.OutputContent.Size.Y.Offset)
end

function TerminalModule:SetupHoverEffects()
	self.OutputInteract.MouseEnter:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.OutputInteract.MouseLeave:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = 0.7})
	end)
	
	self.ClearLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.ClearLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = 0.7})
	end)
	
	self.StopLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.StopLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = 0.7})
	end)
end

function TerminalModule:SetupToggleHandler(WindowFunctions)
	self.OutputInteract.InputBegan:Connect(function(Input)
		if not self.ToggleDebounce and (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) then
			local NewState = not self.Terminal.Visible
			self:ToggleOutputScreen(NewState, WindowFunctions)
			self.ToggleDebounce = true
			
			Utility:SaveOpened(true, NewState)
			
			task.delay(TerminalConstants.ToggleDebounceTime, function()
				self.ToggleDebounce = false
			end)
		end
	end)
end

function TerminalModule:SetupClearLogsHandler()
	self.ClearLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self:ClearLogs()
		end
	end)
end

function TerminalModule:SetupStopLogsHandler()
	self.StopLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self.PrintLogs = not self.PrintLogs
		end
	end)
end

function TerminalModule:ToggleOutputScreen(Visible, WindowFunctions)
	local TransparencyGoal = Visible and 0 or 1
	local PositionGoal = Visible and UDim2.new(1.2, self.TerminalOffset, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	
	self.Terminal.Visible = true
	
	for _, Child in ipairs(self.Terminal:GetChildren()) do
		if Child:IsA("GuiObject") and Child.Name:sub(1, 1) == "{" then
			Child.Visible = Visible
		end
	end
	
	Utility:QuickTween(self.Terminal, {
		Position = PositionGoal,
		BackgroundTransparency = XenithLibrary.AcrylicBlur and (Visible and WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency") or 1) or TransparencyGoal
	})
	
	if self.OutputWallpaper then
		Utility:QuickTween(self.OutputWallpaper, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("WallpaperTransparency") or 0.65) or 1
		})
	end
	
	if self.ShadowContainer then
		Utility:QuickTween(self.ShadowContainer, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("ShadowTransparency") or 0) or 1
		})
	end
	
	if self.OutputUIStroke then
		Utility:QuickTween(self.OutputUIStroke, {
			Transparency = Visible and 0.6 or 1
		})
	end
	
	if self.OutputDivider then
		Utility:QuickTween(self.OutputDivider, {
			Transparency = Visible and TweenSettings.DefaultTransparency or 1
		})
	end
	
	for _, Descendant in ipairs(self.Terminal:GetDescendants()) do
		if Descendant:IsA("TextLabel") or Descendant:IsA("TextButton") or Descendant:IsA("TextBox") then
			if Descendant.Name:find("OutputResult_") then
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0 or 1})
			else
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0.5 or 1})
			end
		elseif (Descendant:IsA("ImageButton") or Descendant:IsA("ImageLabel")) and Descendant ~= self.OutputWallpaper then
			Utility:QuickTween(Descendant, {ImageTransparency = Visible and 0.7 or 1})
		end
		
		local Stroke = Descendant:FindFirstChildOfClass("UIStroke")
		if Stroke then
			Utility:QuickTween(Stroke, {Transparency = TransparencyGoal})
		end
	end
	
	if not Visible then
		task.delay(TerminalConstants.FadeOutDelay, function()
			self.Terminal.Visible = false
		end)
	end
end

function TerminalModule:ClearLogs()
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") then
			Child:Destroy()
		end
	end
	
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
end

function TerminalModule:InitializeLogging(WindowFunctions)
	Services.LogService.MessageOut:Connect(function(Message, MessageType)
		if not self.PrintLogs then
			return
		end
		
		self:HandleLogMessage(Message, MessageType, WindowFunctions)
	end)
end

function TerminalModule:HandleLogMessage(Message, MessageType, WindowFunctions)
	if not self.PrintLogs or not self.Terminal.Visible then
		return
	end
	
	local CurrentTime = os.time()
	local FormattedTime = OutputSettings.ShowTimestamps and os.date("[%H:%M:%S] ", CurrentTime) or ""
	local MessageId = tostring(math.random(10000000, 999999999))
	local ProcessedMessage = self:ProcessMessage(Message)
	
	if ProcessedMessage == self.LastMessage and self.LastOutputResult and self.LastOutputResult.Parent and (CurrentTime - self.LastMessageTime) < TerminalConstants.MessageGroupingWindow then
		self:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
		return
	end
	
	self.LastMessageCount = 1
	self.LastMessageTime = CurrentTime
	
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	local OutputResult = self:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	
	self.LastMessage = ProcessedMessage
	self.LastOutputResult = OutputResult
end

function TerminalModule:ProcessMessage(RawMessage)
	local MessageText = tostring(RawMessage)
	return MessageText
end

function TerminalModule:CreateFallbackTableString(RawMessage, MessageText)
	local FallbackSuccess, FallbackResult = pcall(function()
		local TableString = "{"
		local Count = 0
		
		for Key, Value in pairs(RawMessage) do
			if Count > 0 then
				TableString = TableString .. ", "
			end
			
			TableString = TableString .. tostring(Key) .. " = " .. tostring(Value)
			Count = Count + 1
			
			if Count >= 5 then
				TableString = TableString .. ", ..."
				break
			end
		end
		
		TableString = TableString .. "}"
		return "[Table] " .. TableString
	end)
	
	return FallbackSuccess and FallbackResult or "[Table] " .. MessageText
end

function TerminalModule:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
	self.LastMessageCount = self.LastMessageCount + 1
	local DisplayText = ProcessedMessage .. " (x" .. self.LastMessageCount .. ")"
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	
	self.LastOutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. DisplayText
	self.LastOutputResult.TextColor3 = MessageData.Color
end

function TerminalModule:GetMessageData(MessageType, WindowFunctions)
	local UseWindowFunctions = WindowFunctions.GetCurrentThemeAttribute ~= nil

    local Data = {
		Prefix = "Info",
		Icon = OutputSettings.ShowIcons and " " or "",
		Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("InfoColor") or Color3.fromRGB(255, 255, 255)
	}
	
	if MessageType == Enum.MessageType.MessageError then
		Data.Prefix = "Error"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("ErrorColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageWarning then
		Data.Prefix = "Warning"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("WarningColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageOutput then
		Data.Prefix = "Output"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("SuccessColor") or Color3.fromRGB(255, 255, 255)
	end
	
	return Data
end

function TerminalModule:EnforceLogLimit()
	local LogEntries = {}
	
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") and Child.Name:find("OutputResult_") then
			table.insert(LogEntries, Child)
		end
	end
	
	table.sort(LogEntries, function(A, B)
		return A.AbsolutePosition.Y < B.AbsolutePosition.Y
	end)
	
	local ExcessCount = #LogEntries - OutputSettings.MaxLogEntries
	if ExcessCount > 0 then
		for I = 1, ExcessCount do
			if LogEntries[I] then
				LogEntries[I]:Destroy()
			end
		end
	end
end

function TerminalModule:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	local OutputResult = Instance.new("TextButton")
	OutputResult.Name = "OutputResult_" .. MessageId
	OutputResult.FontFace = Font.new(Assets.InterFont)
	OutputResult.RichText = true
	OutputResult.TextSize = TerminalConstants.LogTextSize
	OutputResult.TextTruncate = Enum.TextTruncate.SplitWord
	OutputResult.TextXAlignment = Enum.TextXAlignment.Left
	OutputResult.TextYAlignment = Enum.TextYAlignment.Top
	OutputResult.AnchorPoint = Vector2.new(0, 0.5)
	OutputResult.AutomaticSize = Enum.AutomaticSize.Y
	OutputResult.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputResult.BackgroundTransparency = 1
	OutputResult.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputResult.BorderSizePixel = 0
	OutputResult.Position = UDim2.fromScale(1.3e-07, 0.5)
	OutputResult.Size = UDim2.new(1, -40, 0, 0)
	OutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. ProcessedMessage
	OutputResult.TextColor3 = MessageData.Color
	
	local ContextMenu = self:CreateContextMenu(ProcessedMessage, FormattedTime, MessageData.Prefix, OutputResult)
	ContextMenu.Parent = OutputResult
	
	self:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageData.Color, ContextMenu)
	
	task.spawn(function()
		OutputResult.Parent = self.OutputContent
		
		-- Enforce log limit after adding new entry
		self:EnforceLogLimit()
		
		if OutputSettings.AutoScroll then
			task.wait()
			if self.OutputContent then
				self.OutputContent.CanvasPosition = Vector2.new(0, self.OutputContent.AbsoluteCanvasSize.Y)
			end
		end
	end)
	
	return OutputResult
end

function TerminalModule:CreateContextMenu(ProcessedMessage, FormattedTime, MessageLevel, OutputResult)
	local ContextMenu = Instance.new("Frame")
	ContextMenu.Name = "ContextMenu"
	ContextMenu.BackgroundTransparency = 1
	ContextMenu.BorderSizePixel = 0
	ContextMenu.Size = UDim2.fromOffset(TerminalConstants.ContextMenuWidth, TerminalConstants.ContextMenuHeight)
	ContextMenu.Visible = false
	ContextMenu.ZIndex = 1000
	
	local MenuCorner = Instance.new("UICorner")
	MenuCorner.CornerRadius = UDim.new(0, 4)
	MenuCorner.Parent = ContextMenu
	
	local MenuList = Instance.new("UIListLayout")
	MenuList.SortOrder = Enum.SortOrder.LayoutOrder
	MenuList.Parent = ContextMenu
	
	self:CreateContextMenuItem(ContextMenu, " Copy Message", function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Copy Full", function()
		local FullLog = FormattedTime .. "[" .. MessageLevel .. "] " .. ProcessedMessage
		self:SafeCopy(FullLog)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Delete", function()
		if OutputResult and OutputResult.Parent then
			OutputResult:Destroy()
		end
	end)
	
	return ContextMenu
end

function TerminalModule:CreateContextMenuItem(Parent, Text, Callback)
	local Item = Instance.new("TextButton")
	Item.BackgroundTransparency = 1
	Item.BorderSizePixel = 0
	Item.Size = UDim2.new(1, 0, 0, TerminalConstants.ContextMenuItemHeight)
	Item.Text = Text
	Item.TextColor3 = Color3.fromRGB(255, 255, 255)
	Item.TextSize = 12
	Item.FontFace = Font.new(Assets.InterFont)
	Item.Parent = Parent
	
	Item.MouseButton1Click:Connect(function()
		Parent.Visible = false
		task.spawn(Callback)
	end)
	
	return Item
end

function TerminalModule:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageColor, ContextMenu)
	local OriginalBackgroundColor = OutputResult.BackgroundColor3
	
	OutputResult.MouseEnter:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = Color3.fromRGB(50, 50, 50),
			TextColor3 = Color3.fromRGB(92, 156, 255),
			BackgroundTransparency = TweenSettings.DefaultTransparency
		})
	end)
	
	OutputResult.MouseLeave:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = OriginalBackgroundColor,
			TextColor3 = MessageColor,
			BackgroundTransparency = 1
		})
	end)
	
	OutputResult.MouseButton1Click:Connect(function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	OutputResult.MouseButton2Click:Connect(function()
		ContextMenu.Position = UDim2.fromOffset(10, 10)
		ContextMenu.Visible = not ContextMenu.Visible
	end)
end

function TerminalModule:SafeCopy(Text)
	local Success = pcall(function()
		if setclipboard then
			setclipboard(Text)
			return true
		end
		return false
	end)
	
	if Success then
		if Utility and Utility.Notify then
			Utility:Notify("Copied", "Message copied to clipboard!", 2)
		elseif WindowFunctions and WindowFunctions.Notify then
			WindowFunctions:Notify({
				Title = "Copied",
				Description = "Message copied to clipboard!"
			})
		else
			print("Copied to clipboard!")
		end
	else
		if Utility and Utility.Notify then
			Utility:Notify("Error", "Could not copy to clipboard", 2)
		else
			print("Could not copy to clipboard")
		end
	end
end

function TerminalModule:Destroy()
	if self.Terminal then
		self.Terminal:Destroy()
	end
	
	if self.OutputIcon then
		self.OutputIcon:Destroy()
	end
	
	if self.ShadowContainer then
		self.ShadowContainer:Destroy()
	end
end

return TerminalModule]],
  ["UserInterface/Imports/Terminal/TerminalModule.lua"] = [[local TerminalModule = {}
TerminalModule.__index = TerminalModule

local TerminalConstants = {
	DefaultWidth = 0.375,
	MinWidth = 250,
	MaxWidth = 1920,
	SnapRange = 20,
	ToggleDebounceTime = 0.3,
	FadeOutDelay = 0.2,
	MessageGroupingWindow = 2,
	LogTextSize = 13,
	HeaderTextSize = 18,
	IconSize = 15,
	ContextMenuWidth = 160,
	ContextMenuHeight = 90,
	ContextMenuItemHeight = 30,
	DividerThickness = 1,
	CornerRadius = 10,
	ContentTopOffset = 80,
	ScrollBarThickness = 1
}

local TweenSettings = {
	DefaultTransparency = 0.9,
	HoverTransparency = 0.85,
	ActiveTransparency = 0,
	EasingStyle = Enum.EasingStyle.Sine
}

local OutputSettings = {
	ShowTimestamps = true,
	ShowIcons = true,
	SupportTables = true,
	AutoScroll = true,
	MaxTableDepth = 3,
	TruncateStrings = 50,
	MaxTableSize = 100,
	MaxLogEntries = 100
}

function TerminalModule.new(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI, WindowFunctions)
	local self = setmetatable({}, TerminalModule)
	
	self.ResizingOutput = false
	self.DefaultOutputWidth = 0
	self.InitialMouseX = 0
	self.InitialOutputWidth = 0
	self.InitialLeftEdge = 0
	self.TerminalOffset = 0
	self.ToggleDebounce = false
	self.PrintLogs = true
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
	self.LastMessageTime = 0
	
	self:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self:SetupEventHandlers(WindowFunctions)
	self:InitializeLogging(WindowFunctions)
	
	return self
end

function TerminalModule:CreateUI(Parent, TopbarFolder, XenithLibrary, XenithLibraryUI)
	self.Terminal = self:CreateTerminalFrame(Parent, XenithLibrary)
	self.ShadowContainer = self:CreateShadow(Parent, XenithLibrary, XenithLibraryUI)
	self.OutputTab = self:CreateOutputTab()
	self.OutputTabDivider = self:CreateDivider(UDim2.new(0.5, 0, 0, 42.5), UDim2.new(0, 48, 0, 1), 0.5)
	self.ClearLogsButton = self:CreateIconButton("ClearLogsIcon", Assets.IconClearLogs, UDim2.fromScale(0.935, 0.05))
	self.StopLogsButton = self:CreateIconButton("StopLogsIcon", Assets.IconStopLogs, UDim2.fromScale(0.85, 0.05))
	self.DividerInteract = self:CreateDividerInteract()
	self.OutputContent = self:CreateScrollingFrame()
	self.OutputWallpaper = self:CreateWallpaper()
	self.OutputUIScale = self:CreateUIScale()
	self.OutputUICorner = self:CreateUICorner()
	self.OutputUIStroke = self:CreateUIStroke()
	self.OutputDivider = self:CreateDivider(UDim2.new(0, 0, 0, 63), UDim2.new(1, 0, 0, 1), 0)
	self.OutputUIListLayout = self:CreateListLayout()
	self.OutputIcon = self:CreateTopbarIcon(TopbarFolder)
	self.OutputInteract = self:CreateTopbarInteract()
	
	self.DefaultOutputWidth = self.Terminal.AbsoluteSize.X
end

function TerminalModule:CreateTerminalFrame(Parent, XenithLibrary)
	local Terminal = Instance.new("Frame")
	Terminal.Name = "Terminal"
	Terminal.AnchorPoint = Vector2.new(0.5, 0.5)
	Terminal.Size = UDim2.new(TerminalConstants.DefaultWidth, 0, 1, 0)
	Terminal.Position = Utility:SaveOpened() and UDim2.new(1.2, 0, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	Terminal.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	Terminal.BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0
	Terminal.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Terminal.BorderSizePixel = 0
	Terminal.Parent = Parent
	Terminal.ZIndex = -5
	Terminal.Visible = Utility:SaveOpened()
	
	return Terminal
end

function TerminalModule:CreateShadow(Base, XenithLibrary, XenithLibraryUI)
	return XenithLibrary:CreateShadow({
		Object = XenithLibraryUI,
		ReplicateTransparency = true,
		ReplicateVisible = {Base, self.Terminal},
		ObjectReplication = self.Terminal,
		Gradient = {
			Color = ColorSequence.new({
				ColorSequenceKeypoint.new(0, Color3.new(0.4, 0.6, 1)),
				ColorSequenceKeypoint.new(1, Color3.new(0, 0, 0.2))
			})
		}
	})
end

function TerminalModule:CreateOutputTab()
	local OutputTab = Instance.new("TextLabel")
	OutputTab.Name = "OutputTab"
	OutputTab.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
	OutputTab.RichText = true
	OutputTab.Text = "Terminal"
	OutputTab.TextColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.TextSize = TerminalConstants.HeaderTextSize
	OutputTab.TextTransparency = 0.5
	OutputTab.TextTruncate = Enum.TextTruncate.SplitWord
	OutputTab.TextXAlignment = Enum.TextXAlignment.Center
	OutputTab.TextYAlignment = Enum.TextYAlignment.Top
	OutputTab.AnchorPoint = Vector2.new(0, 0.5)
	OutputTab.AutomaticSize = Enum.AutomaticSize.Y
	OutputTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputTab.BackgroundTransparency = 1
	OutputTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputTab.BorderSizePixel = 0
	OutputTab.Position = UDim2.fromScale(0.05, 0.05)
	OutputTab.Size = UDim2.fromScale(0.9, 0)
	OutputTab.Parent = self.Terminal
	OutputTab.ZIndex = 155
	
	return OutputTab
end

function TerminalModule:CreateDivider(Position, Size, AnchorPoint)
	local Divider = Instance.new("Frame")
	Divider.Name = "OutputDivider"
	Divider.AnchorPoint = Vector2.new(AnchorPoint, 0)
	Divider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Divider.BackgroundTransparency = TweenSettings.DefaultTransparency
	Divider.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Divider.BorderSizePixel = 0
	Divider.Position = Position
	Divider.Size = Size
	Divider.Parent = self.Terminal
	
	return Divider
end

function TerminalModule:CreateIconButton(Name, ImageAsset, Position)
	local Button = Instance.new("ImageButton")
	Button.Name = Name
	Button.ZIndex = 5
	Button.Image = ImageAsset
	Button.ImageTransparency = 0.7
	Button.AnchorPoint = Vector2.new(1, 0.5)
	Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Button.BackgroundTransparency = 1
	Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Button.BorderSizePixel = 0
	Button.Position = Position
	Button.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Button.Parent = self.Terminal
	Button.Visible = true
	
	return Button
end

function TerminalModule:CreateDividerInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "DividerOutputInteract"
	Interact.AnchorPoint = Vector2.new(0.5, 0)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Position = UDim2.fromScale(1, 0)
	Interact.Size = UDim2.new(0, 5, 1, 0)
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.Parent = self.Terminal
	
	return Interact
end

function TerminalModule:CreateScrollingFrame()
	local Content = Instance.new("ScrollingFrame")
	Content.Name = "Content"
	Content.AnchorPoint = Vector2.new(1, 0)
	Content.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Content.BackgroundTransparency = 1
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(1, 0, 0, TerminalConstants.ContentTopOffset)
	Content.Size = UDim2.new(1, 0, 1, -TerminalConstants.ContentTopOffset)
	Content.CanvasSize = UDim2.new(0, 0, 0, 0)
	Content.ScrollBarThickness = TerminalConstants.ScrollBarThickness
	Content.ScrollBarImageTransparency = 0.5
	Content.Parent = self.Terminal
	
	return Content
end

function TerminalModule:CreateWallpaper()
	local Wallpaper = Instance.new("ImageLabel")
	Wallpaper.Name = "OutputWallpaper"
	Wallpaper.Image = Assets.StyleOneWallpaper
	Wallpaper.Size = UDim2.new(1, 0, 1, 0)
	Wallpaper.Position = UDim2.new(0, 0, 0, 0)
	Wallpaper.BackgroundTransparency = 1
	Wallpaper.ImageTransparency = 0.65
	Wallpaper.ImageColor3 = Color3.fromRGB(32, 32, 32)
	Wallpaper.ScaleType = Enum.ScaleType.Crop
	Wallpaper.Parent = self.Terminal
	
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputWallpaperUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = Wallpaper
	
	return Wallpaper
end

function TerminalModule:CreateUIScale()
	local Scale = Instance.new("UIScale")
	Scale.Name = "OutputUIScale"
	Scale.Parent = self.Terminal
	
	return Scale
end

function TerminalModule:CreateUICorner()
	local Corner = Instance.new("UICorner")
	Corner.Name = "OutputUICorner"
	Corner.CornerRadius = UDim.new(0, TerminalConstants.CornerRadius)
	Corner.Parent = self.Terminal
	
	return Corner
end

function TerminalModule:CreateUIStroke()
	local Stroke = Instance.new("UIStroke")
	Stroke.Name = "OutputUIStroke"
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(255, 255, 255)
	Stroke.Transparency = 0.7
	Stroke.Parent = self.Terminal
	
	return Stroke
end

function TerminalModule:CreateListLayout()
	local Layout = Instance.new("UIListLayout")
	Layout.Name = "OutputUIListLayout"
	Layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	Layout.Padding = UDim.new(0, 10)
	Layout.SortOrder = Enum.SortOrder.LayoutOrder
	Layout.Parent = self.OutputContent
	
	Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		self.OutputContent.CanvasSize = UDim2.new(0, 0, 0, Layout.AbsoluteContentSize.Y)
	end)
	
	return Layout
end

function TerminalModule:CreateTopbarIcon(TopbarFolder)
	local Icon = Instance.new("ImageButton")
	Icon.Name = "TerminalIcon"
	Icon.Image = Assets.IconOutputIcon
	Icon.ImageTransparency = 0.7
	Icon.AnchorPoint = Vector2.new(1, 0.5)
	Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Icon.BackgroundTransparency = 1
	Icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Icon.BorderSizePixel = 0
	Icon.Position = UDim2.fromScale(1, 0.5)
	Icon.Size = UDim2.fromOffset(TerminalConstants.IconSize, TerminalConstants.IconSize)
	Icon.Parent = TopbarFolder
	Icon.Visible = true
	
	return Icon
end

function TerminalModule:CreateTopbarInteract()
	local Interact = Instance.new("TextButton")
	Interact.Name = "Interact"
	Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
	Interact.Text = ""
	Interact.TextColor3 = Color3.fromRGB(0, 0, 0)
	Interact.TextSize = 14
	Interact.AnchorPoint = Vector2.new(0.5, 0.5)
	Interact.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Interact.BackgroundTransparency = 1
	Interact.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Interact.BorderSizePixel = 0
	Interact.Position = UDim2.fromScale(0.5, 0.5)
	Interact.Size = UDim2.fromOffset(40, 40)
	Interact.Parent = self.OutputIcon
	
	return Interact
end

function TerminalModule:SetupEventHandlers(WindowFunctions)
	self:SetupResizeHandlers()
	self:SetupHoverEffects()
	self:SetupToggleHandler(WindowFunctions)
	self:SetupClearLogsHandler()
	self:SetupStopLogsHandler()
end

function TerminalModule:SetupResizeHandlers()
	self.DividerInteract.MouseButton1Down:Connect(function()
		self.ResizingOutput = true
		self.InitialMouseX = Services.UserInputService:GetMouseLocation().X
		self.InitialOutputWidth = self.Terminal.AbsoluteSize.X
		self.InitialLeftEdge = self.Terminal.AbsolutePosition.X
	end)
	
	Services.UserInputService.InputEnded:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 then
			self.ResizingOutput = false
		end
	end)
	
	Services.UserInputService.InputChanged:Connect(function(Input)
		if self.ResizingOutput and Input.UserInputType == Enum.UserInputType.MouseMovement then
			self:HandleResize(Input)
		end
	end)
end

function TerminalModule:HandleResize(Input)
	local DeltaX = Services.UserInputService:GetMouseLocation().X - self.InitialMouseX
	local NewOutputWidth = self.InitialOutputWidth + DeltaX
	
	if math.abs(NewOutputWidth - self.DefaultOutputWidth) < TerminalConstants.SnapRange then
		NewOutputWidth = self.DefaultOutputWidth
	else
		NewOutputWidth = math.clamp(NewOutputWidth, TerminalConstants.MinWidth, TerminalConstants.MaxWidth)
	end
	
	self.Terminal.Size = UDim2.new(0, NewOutputWidth, self.Terminal.Size.Y.Scale, self.Terminal.Size.Y.Offset)
	
	local CurrentLeftEdge = self.Terminal.AbsolutePosition.X
	local LeftAdjustment = self.InitialLeftEdge - CurrentLeftEdge
	local NewXOffset = self.Terminal.Position.X.Offset + LeftAdjustment
	
	self.Terminal.Position = UDim2.new(
		self.Terminal.Position.X.Scale,
		NewXOffset,
		self.Terminal.Position.Y.Scale,
		self.Terminal.Position.Y.Offset
	)
	
	self.TerminalOffset = NewXOffset
	self.OutputContent.Size = UDim2.new(0, NewOutputWidth, self.OutputContent.Size.Y.Scale, self.OutputContent.Size.Y.Offset)
end

function TerminalModule:SetupHoverEffects()
	self.OutputInteract.MouseEnter:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.OutputInteract.MouseLeave:Connect(function()
		Utility:QuickTween(self.OutputIcon, {ImageTransparency = 0.7})
	end)
	
	self.ClearLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.ClearLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.ClearLogsButton, {ImageTransparency = 0.7})
	end)
	
	self.StopLogsButton.MouseEnter:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = TweenSettings.ActiveTransparency})
	end)
	
	self.StopLogsButton.MouseLeave:Connect(function()
		Utility:QuickTween(self.StopLogsButton, {ImageTransparency = 0.7})
	end)
end

function TerminalModule:SetupToggleHandler(WindowFunctions)
	self.OutputInteract.InputBegan:Connect(function(Input)
		if not self.ToggleDebounce and (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) then
			local NewState = not self.Terminal.Visible
			self:ToggleOutputScreen(NewState, WindowFunctions)
			self.ToggleDebounce = true
			
			Utility:SaveOpened(true, NewState)
			
			task.delay(TerminalConstants.ToggleDebounceTime, function()
				self.ToggleDebounce = false
			end)
		end
	end)
end

function TerminalModule:SetupClearLogsHandler()
	self.ClearLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self:ClearLogs()
		end
	end)
end

function TerminalModule:SetupStopLogsHandler()
	self.StopLogsButton.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			self.PrintLogs = not self.PrintLogs
		end
	end)
end

function TerminalModule:ToggleOutputScreen(Visible, WindowFunctions)
	local TransparencyGoal = Visible and 0 or 1
	local PositionGoal = Visible and UDim2.new(1.2, self.TerminalOffset, 0.5, 0) or UDim2.new(1, 0, 0.5, 0)
	
	self.Terminal.Visible = true
	
	for _, Child in ipairs(self.Terminal:GetChildren()) do
		if Child:IsA("GuiObject") and Child.Name:sub(1, 1) == "{" then
			Child.Visible = Visible
		end
	end
	
	Utility:QuickTween(self.Terminal, {
		Position = PositionGoal,
		BackgroundTransparency = XenithLibrary.AcrylicBlur and (Visible and WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency") or 1) or TransparencyGoal
	})
	
	if self.OutputWallpaper then
		Utility:QuickTween(self.OutputWallpaper, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("WallpaperTransparency") or 0.65) or 1
		})
	end
	
	if self.ShadowContainer then
		Utility:QuickTween(self.ShadowContainer, {
			ImageTransparency = Visible and (WindowFunctions:GetCurrentThemeAttribute("ShadowTransparency") or 0) or 1
		})
	end
	
	if self.OutputUIStroke then
		Utility:QuickTween(self.OutputUIStroke, {
			Transparency = Visible and 0.6 or 1
		})
	end
	
	if self.OutputDivider then
		Utility:QuickTween(self.OutputDivider, {
			Transparency = Visible and TweenSettings.DefaultTransparency or 1
		})
	end
	
	for _, Descendant in ipairs(self.Terminal:GetDescendants()) do
		if Descendant:IsA("TextLabel") or Descendant:IsA("TextButton") or Descendant:IsA("TextBox") then
			if Descendant.Name:find("OutputResult_") then
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0 or 1})
			else
				Utility:QuickTween(Descendant, {TextTransparency = Visible and 0.5 or 1})
			end
		elseif (Descendant:IsA("ImageButton") or Descendant:IsA("ImageLabel")) and Descendant ~= self.OutputWallpaper then
			Utility:QuickTween(Descendant, {ImageTransparency = Visible and 0.7 or 1})
		end
		
		local Stroke = Descendant:FindFirstChildOfClass("UIStroke")
		if Stroke then
			Utility:QuickTween(Stroke, {Transparency = TransparencyGoal})
		end
	end
	
	if not Visible then
		task.delay(TerminalConstants.FadeOutDelay, function()
			self.Terminal.Visible = false
		end)
	end
end

function TerminalModule:ClearLogs()
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") then
			Child:Destroy()
		end
	end
	
	self.LastMessage = nil
	self.LastMessageCount = 0
	self.LastOutputResult = nil
end

function TerminalModule:InitializeLogging(WindowFunctions)
	Services.LogService.MessageOut:Connect(function(Message, MessageType)
		if not self.PrintLogs then
			return
		end
		
		self:HandleLogMessage(Message, MessageType, WindowFunctions)
	end)
end

function TerminalModule:HandleLogMessage(Message, MessageType, WindowFunctions)
	if not self.PrintLogs or not self.Terminal.Visible then
		return
	end
	
	local CurrentTime = os.time()
	local FormattedTime = OutputSettings.ShowTimestamps and os.date("[%H:%M:%S] ", CurrentTime) or ""
	local MessageId = tostring(math.random(10000000, 999999999))
	local ProcessedMessage = self:ProcessMessage(Message)
	
	if ProcessedMessage == self.LastMessage and self.LastOutputResult and self.LastOutputResult.Parent and (CurrentTime - self.LastMessageTime) < TerminalConstants.MessageGroupingWindow then
		self:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
		return
	end
	
	self.LastMessageCount = 1
	self.LastMessageTime = CurrentTime
	
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	local OutputResult = self:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	
	self.LastMessage = ProcessedMessage
	self.LastOutputResult = OutputResult
end

function TerminalModule:ProcessMessage(RawMessage)
	local MessageText = tostring(RawMessage)
	return MessageText
end

function TerminalModule:CreateFallbackTableString(RawMessage, MessageText)
	local FallbackSuccess, FallbackResult = pcall(function()
		local TableString = "{"
		local Count = 0
		
		for Key, Value in pairs(RawMessage) do
			if Count > 0 then
				TableString = TableString .. ", "
			end
			
			TableString = TableString .. tostring(Key) .. " = " .. tostring(Value)
			Count = Count + 1
			
			if Count >= 5 then
				TableString = TableString .. ", ..."
				break
			end
		end
		
		TableString = TableString .. "}"
		return "[Table] " .. TableString
	end)
	
	return FallbackSuccess and FallbackResult or "[Table] " .. MessageText
end

function TerminalModule:UpdateGroupedMessage(ProcessedMessage, MessageType, FormattedTime, WindowFunctions)
	self.LastMessageCount = self.LastMessageCount + 1
	local DisplayText = ProcessedMessage .. " (x" .. self.LastMessageCount .. ")"
	local MessageData = self:GetMessageData(MessageType, WindowFunctions)
	
	self.LastOutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. DisplayText
	self.LastOutputResult.TextColor3 = MessageData.Color
end

function TerminalModule:GetMessageData(MessageType, WindowFunctions)
	local UseWindowFunctions = WindowFunctions.GetCurrentThemeAttribute ~= nil

    local Data = {
		Prefix = "Info",
		Icon = OutputSettings.ShowIcons and " " or "",
		Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("InfoColor") or Color3.fromRGB(255, 255, 255)
	}
	
	if MessageType == Enum.MessageType.MessageError then
		Data.Prefix = "Error"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("ErrorColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageWarning then
		Data.Prefix = "Warning"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("WarningColor") or Color3.fromRGB(255, 255, 255)
	elseif MessageType == Enum.MessageType.MessageOutput then
		Data.Prefix = "Output"
		Data.Icon = OutputSettings.ShowIcons and " " or ""
		Data.Color = UseWindowFunctions and WindowFunctions:GetCurrentThemeAttribute("SuccessColor") or Color3.fromRGB(255, 255, 255)
	end
	
	return Data
end

function TerminalModule:EnforceLogLimit()
	local LogEntries = {}
	
	for _, Child in ipairs(self.OutputContent:GetChildren()) do
		if Child:IsA("TextButton") and Child.Name:find("OutputResult_") then
			table.insert(LogEntries, Child)
		end
	end
	
	table.sort(LogEntries, function(A, B)
		return A.AbsolutePosition.Y < B.AbsolutePosition.Y
	end)
	
	local ExcessCount = #LogEntries - OutputSettings.MaxLogEntries
	if ExcessCount > 0 then
		for I = 1, ExcessCount do
			if LogEntries[I] then
				LogEntries[I]:Destroy()
			end
		end
	end
end

function TerminalModule:CreateLogEntry(ProcessedMessage, MessageData, FormattedTime, MessageId)
	local OutputResult = Instance.new("TextButton")
	OutputResult.Name = "OutputResult_" .. MessageId
	OutputResult.FontFace = Font.new(Assets.InterFont)
	OutputResult.RichText = true
	OutputResult.TextSize = TerminalConstants.LogTextSize
	OutputResult.TextTruncate = Enum.TextTruncate.SplitWord
	OutputResult.TextXAlignment = Enum.TextXAlignment.Left
	OutputResult.TextYAlignment = Enum.TextYAlignment.Top
	OutputResult.AnchorPoint = Vector2.new(0, 0.5)
	OutputResult.AutomaticSize = Enum.AutomaticSize.Y
	OutputResult.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OutputResult.BackgroundTransparency = 1
	OutputResult.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputResult.BorderSizePixel = 0
	OutputResult.Position = UDim2.fromScale(1.3e-07, 0.5)
	OutputResult.Size = UDim2.new(1, -40, 0, 0)
	OutputResult.Text = FormattedTime .. MessageData.Icon .. MessageData.Prefix .. ": " .. ProcessedMessage
	OutputResult.TextColor3 = MessageData.Color
	
	local ContextMenu = self:CreateContextMenu(ProcessedMessage, FormattedTime, MessageData.Prefix, OutputResult)
	ContextMenu.Parent = OutputResult
	
	self:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageData.Color, ContextMenu)
	
	task.spawn(function()
		OutputResult.Parent = self.OutputContent
		
		-- Enforce log limit after adding new entry
		self:EnforceLogLimit()
		
		if OutputSettings.AutoScroll then
			task.wait()
			if self.OutputContent then
				self.OutputContent.CanvasPosition = Vector2.new(0, self.OutputContent.AbsoluteCanvasSize.Y)
			end
		end
	end)
	
	return OutputResult
end

function TerminalModule:CreateContextMenu(ProcessedMessage, FormattedTime, MessageLevel, OutputResult)
	local ContextMenu = Instance.new("Frame")
	ContextMenu.Name = "ContextMenu"
	ContextMenu.BackgroundTransparency = 1
	ContextMenu.BorderSizePixel = 0
	ContextMenu.Size = UDim2.fromOffset(TerminalConstants.ContextMenuWidth, TerminalConstants.ContextMenuHeight)
	ContextMenu.Visible = false
	ContextMenu.ZIndex = 1000
	
	local MenuCorner = Instance.new("UICorner")
	MenuCorner.CornerRadius = UDim.new(0, 4)
	MenuCorner.Parent = ContextMenu
	
	local MenuList = Instance.new("UIListLayout")
	MenuList.SortOrder = Enum.SortOrder.LayoutOrder
	MenuList.Parent = ContextMenu
	
	self:CreateContextMenuItem(ContextMenu, " Copy Message", function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Copy Full", function()
		local FullLog = FormattedTime .. "[" .. MessageLevel .. "] " .. ProcessedMessage
		self:SafeCopy(FullLog)
	end)
	
	self:CreateContextMenuItem(ContextMenu, " Delete", function()
		if OutputResult and OutputResult.Parent then
			OutputResult:Destroy()
		end
	end)
	
	return ContextMenu
end

function TerminalModule:CreateContextMenuItem(Parent, Text, Callback)
	local Item = Instance.new("TextButton")
	Item.BackgroundTransparency = 1
	Item.BorderSizePixel = 0
	Item.Size = UDim2.new(1, 0, 0, TerminalConstants.ContextMenuItemHeight)
	Item.Text = Text
	Item.TextColor3 = Color3.fromRGB(255, 255, 255)
	Item.TextSize = 12
	Item.FontFace = Font.new(Assets.InterFont)
	Item.Parent = Parent
	
	Item.MouseButton1Click:Connect(function()
		Parent.Visible = false
		task.spawn(Callback)
	end)
	
	return Item
end

function TerminalModule:SetupLogEntryInteractions(OutputResult, ProcessedMessage, MessageColor, ContextMenu)
	local OriginalBackgroundColor = OutputResult.BackgroundColor3
	
	OutputResult.MouseEnter:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = Color3.fromRGB(50, 50, 50),
			TextColor3 = Color3.fromRGB(92, 156, 255),
			BackgroundTransparency = TweenSettings.DefaultTransparency
		})
	end)
	
	OutputResult.MouseLeave:Connect(function()
		Utility:QuickTween(OutputResult, {
			BackgroundColor3 = OriginalBackgroundColor,
			TextColor3 = MessageColor,
			BackgroundTransparency = 1
		})
	end)
	
	OutputResult.MouseButton1Click:Connect(function()
		self:SafeCopy(ProcessedMessage)
	end)
	
	OutputResult.MouseButton2Click:Connect(function()
		ContextMenu.Position = UDim2.fromOffset(10, 10)
		ContextMenu.Visible = not ContextMenu.Visible
	end)
end

function TerminalModule:SafeCopy(Text)
	local Success = pcall(function()
		if setclipboard then
			setclipboard(Text)
			return true
		end
		return false
	end)
	
	if Success then
		if Utility and Utility.Notify then
			Utility:Notify("Copied", "Message copied to clipboard!", 2)
		elseif WindowFunctions and WindowFunctions.Notify then
			WindowFunctions:Notify({
				Title = "Copied",
				Description = "Message copied to clipboard!"
			})
		else
			print("Copied to clipboard!")
		end
	else
		if Utility and Utility.Notify then
			Utility:Notify("Error", "Could not copy to clipboard", 2)
		else
			print("Could not copy to clipboard")
		end
	end
end

function TerminalModule:Destroy()
	if self.Terminal then
		self.Terminal:Destroy()
	end
	
	if self.OutputIcon then
		self.OutputIcon:Destroy()
	end
	
	if self.ShadowContainer then
		self.ShadowContainer:Destroy()
	end
end

return TerminalModule]],
