  ["Modules/Sound.lua"] = [[local Sound = {}
Sound.__index = Sound

function Sound.New(FilePath, Speed, Volume, IsLink, Link)
    local self = setmetatable({}, Sound)
    
    if IsLink and not isfile(FilePath) then
        writefile(FilePath, Utility:ExecuteHTTPGet(Link))
    end

    self.SoundAsset = getcustomasset(FilePath)
    self.Speed = Speed or 1
    self.Volume = Volume or 1
    self.Sound = Instance.new("Sound", Services.CoreGui)
    self.Sound.SoundId = self.SoundAsset
    self.Sound.PlaybackSpeed = self.Speed
    self.Sound.Volume = self.Volume
    
    return self
end

