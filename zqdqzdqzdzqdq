        end
        
        local function LoadSavedKeybind()
            local KeybindPath = GetKeybindPath()
            if isfile(KeybindPath) then
                local KeybindName = readfile(KeybindPath)
                return Enum.KeyCode[KeybindName]
            end
            return Enum.KeyCode.LeftAlt
        end
        
        local function ShowKeybindNotification(KeybindName)
            WindowFunctions:Notify({
                Title = WindowFunctions.Settings.Title,
                Description = "Successfully Binded Keybind to - " .. KeybindName,
                Lifetime = 3
