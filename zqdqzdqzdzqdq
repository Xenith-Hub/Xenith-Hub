    KeypadFive = "K5",
    KeypadSix = "K6",
    KeypadSeven = "K7",
    KeypadEight = "K8",
    KeypadNine = "K9",

    Up = "UP",
    Down = "DN",
    Left = "LT",
    Right = "RT",

    Print = "PR",
    Pause = "PA",
    Menu = "MN",

    Unknown = "NA"
