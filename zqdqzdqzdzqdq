  ["Xenith/Xenith/Modules/Services.lua"] = [[local ServiceCache = {}
local GetService = game.GetService

local CloneRefServices = {
    CoreGui = true,
    RunService = true
}

local NoCloneRefServices = {
    Players = true
}

getgenv().Services = setmetatable({}, {
    __index = function(self, ServiceName)
        local Cache = ServiceCache[ServiceName]
        if Cache then
            return Cache
        end
        
        local ShouldCloneRef = CloneRefServices[ServiceName] and not NoCloneRefServices[ServiceName]
        local Service = ShouldCloneRef and cloneref(GetService(game, ServiceName)) or GetService(game, ServiceName)
        
        ServiceCache[ServiceName] = Service
        return Service
    end
})

return Services
]],
