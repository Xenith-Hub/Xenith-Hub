local KeybindElements = {}

local KeyShortcuts = {
    Space = "SP",
    LeftShift = "LS",
    RightShift = "RS",
    LeftControl = "LC",
    RightControl = "RC",
    LeftAlt = "LA",
    RightAlt = "RA",

    MouseButton1 = "M1",
    MouseButton2 = "M2",
    MouseButton3 = "M3",

    CapsLock = "CL",
    NumLock = "NL",
    ScrollLock = "SL",

    Backspace = "BS",
    Return = "RT",
    Escape = "ES",
    Delete = "DL",
    Insert = "IN",
    PageUp = "PU",
    PageDown = "PD",
    Home = "HM",
    End = "EN",

    F1 = "F1",
    F2 = "F2",
    F3 = "F3",
    F4 = "F4",
    F5 = "F5",
    F6 = "F6",
    F7 = "F7",
