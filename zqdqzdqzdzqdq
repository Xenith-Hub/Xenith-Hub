  ["Xenith/Xenith/Modules/Sound.lua"] = [[local Sound = {}
Sound.__index = Sound

function Sound.New(FilePath, Speed, Volume, IsLink, Link)
    local self = setmetatable({}, Sound)
    
    if IsLink and not isfile(FilePath) then
        writefile(FilePath, Utility:ExecuteHTTPGet(Link))
    end

    self.SoundAsset = getcustomasset(FilePath)
    self.Speed = Speed or 1
    self.Volume = Volume or 1
    self.Sound = Instance.new("Sound", Services.CoreGui)
    self.Sound.SoundId = self.SoundAsset
    self.Sound.PlaybackSpeed = self.Speed
    self.Sound.Volume = self.Volume
    
    return self
end

function Sound:Play()
    self.Sound:Play()
end

function Sound:Stop()
    self.Sound:Stop()
end

function Sound:SetSpeed(Speed)
    self.Speed = Speed
    self.Sound.PlaybackSpeed = Speed
end

function Sound:SetVolume(Volume)
    self.Volume = Volume
    self.Sound.Volume = Volume
end

function Sound:Destroy()
    self.Sound:Destroy()
    self.Sound = nil
end

return Sound]],
  ["Xenith/Modules/Sound.lua"] = [[local Sound = {}
Sound.__index = Sound

function Sound.New(FilePath, Speed, Volume, IsLink, Link)
