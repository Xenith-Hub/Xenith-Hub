-- Auto-generated bundle. Do not edit by hand.
local FILES = {
  ["Xenith/Xenith/Modules/Maid.lua"] = [[local Maid = {}
Maid.ClassName = "Maid"

function Maid.New()
	return setmetatable({
		_Tasks = {}
	}, Maid)
end

function Maid.IsMaid(Value)
	return type(Value) == "table" and Value.ClassName == "Maid"
end

function Maid:__index(Index)
	if Maid[Index] then
		return Maid[Index]
	else
		return self._Tasks[Index]
	end
end

function Maid:__newindex(Index, NewTask)
	if Maid[Index] ~= nil then
		error(string.format("Cannot use '%s' as a Maid key", tostring(Index)), 2)
	end

	local Tasks = self._Tasks
	local OldTask = Tasks[Index]

	if OldTask == NewTask then
		return
	end

	Tasks[Index] = NewTask

	if OldTask then
		self:_CleanupTask(OldTask)
	end
end

function Maid:_CleanupTask(Task)
	local TaskType = typeof(Task)

	if TaskType == "function" then
		Task()
	elseif TaskType == "thread" then
		local Success = pcall(function()
			if coroutine.status(Task) ~= "dead" then
				task.cancel(Task)
			end
		end)

		if not Success then
			task.defer(function()
				pcall(task.cancel, Task)
			end)
		end
	elseif TaskType == "RBXScriptConnection" then
		if Task.Connected then
			Task:Disconnect()
		end
	elseif TaskType == "Instance" then
		if Task.Parent ~= nil then
			Task:Destroy()
		end
	elseif TaskType == "table" then
		if type(Task.Destroy) == "function" then
			Task:Destroy()
		elseif type(Task.Disconnect) == "function" then
			Task:Disconnect()
		end
	end
end

function Maid:Add(Task)
	if not Task then
		error("Task cannot be false or nil", 2)
	end

	local TaskId = #self._Tasks + 1
	self._Tasks[TaskId] = Task

	if type(Task) == "table" and (not Task.Destroy) and (not Task.Disconnect) then
		warn("[Maid.Add] - Gave table task without .Destroy or .Disconnect\n\n" .. debug.traceback())
	end

	return Task
end

function Maid:GiveTask(Task)
	if not Task then
		error("Task cannot be false or nil", 2)
	end

	local TaskId = #self._Tasks + 1
	self._Tasks[TaskId] = Task

	if type(Task) == "table" and (not Task.Destroy) and (not Task.Disconnect) then
		warn("[Maid.GiveTask] - Gave table task without .Destroy or .Disconnect\n\n" .. debug.traceback())
	end

	return TaskId
end

function Maid:GivePromise(Promise)
	if not Promise:IsPending() then
		return Promise
	end

	local NewPromise = Promise.resolved(Promise)
	local Id = self:GiveTask(NewPromise)

	NewPromise:Finally(function()
		self[Id] = nil
	end)

	return NewPromise
end

function Maid:AddCleanupTask(Task)
	if typeof(Task) ~= "function" then
		return
	end

	if not self._CleanupTasks then
		self._CleanupTasks = {}
	end
	
	table.insert(self._CleanupTasks, Task)
end

function Maid:DoCleaning()
	local PrecleanupTasks = self._CleanupTasks
	local Tasks = self._Tasks

	if PrecleanupTasks then
		for _, Task in pairs (PrecleanupTasks) do
			Task()
		end
	end

	local TasksToClean = {}
	for Index, Task in pairs(Tasks) do
		TasksToClean[Index] = Task
		Tasks[Index] = nil
	end

	for _, Task in pairs(TasksToClean) do
		self:_CleanupTask(Task)
	end
end

function Maid:Destroy()
	local Tasks = self._Tasks

	local TaskSnapshot = {}
	for Index, Task in pairs(Tasks) do
		table.insert(TaskSnapshot, Task)
		Tasks[Index] = nil
	end

	for _, Task in ipairs(TaskSnapshot) do
		pcall(function()
			self:_CleanupTask(Task)
		end)
	end

	table.clear(self._Tasks)

	setmetatable(self, nil)
end

return Maid]],
