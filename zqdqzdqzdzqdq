            SliderStroke.Transparency = 0.9
            SliderStroke.Parent = Wheel.ValueSlider

            Wheel.SliderHandle = Instance.new("Frame")
            Wheel.SliderHandle.Name = "SliderHandle"
            Wheel.SliderHandle.AnchorPoint = Vector2.new(0, 0.5)
            Wheel.SliderHandle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Wheel.SliderHandle.BorderSizePixel = 0
            Wheel.SliderHandle.Position = UDim2.fromScale(0, 0.5)
            Wheel.SliderHandle.Size = UDim2.new(0, 13, 1, 8)
            Wheel.SliderHandle.Parent = Wheel.ValueSlider

            local HandleCorner = Instance.new("UICorner")
            HandleCorner.CornerRadius = UDim.new(1, 0)
            HandleCorner.Parent = Wheel.SliderHandle

            local HandleStroke = Instance.new("UIStroke")
            HandleStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            HandleStroke.Transparency = 0.5
            HandleStroke.Parent = Wheel.SliderHandle

            return Wheel
        end

        local function CreateInputFields(Parent)
            local Inputs = {}

            Inputs.Container = Instance.new("Frame")
            Inputs.Container.Name = "InputsContainer"
            Inputs.Container.AnchorPoint = Vector2.new(1, 0.5)
            Inputs.Container.AutomaticSize = Enum.AutomaticSize.XY
            Inputs.Container.BackgroundTransparency = 1
            Inputs.Container.LayoutOrder = 1
            Inputs.Container.Position = UDim2.fromScale(1, 0.5)
            Inputs.Container.Parent = Parent

            local InputsLayout = Instance.new("UIListLayout")
            InputsLayout.Padding = UI_CONSTANTS.PADDING.Small
            InputsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            InputsLayout.Parent = Inputs.Container

            local inputNames = {"Red", "Green", "Blue"}
            if Settings.Alpha then
                table.insert(inputNames, "Alpha")
            end
            table.insert(inputNames, "Hex")

            for i, name in ipairs(inputNames) do
                Inputs[name] = CreateInputField(name, i, Inputs.Container)
            end

            return Inputs
        end

        function CreateInputField(name, layoutOrder, parent)
            local Field = {}

            Field.Container = Instance.new("Frame")
            Field.Container.Name = name .. "Input"
            Field.Container.AutomaticSize = Enum.AutomaticSize.XY
            Field.Container.BackgroundTransparency = 1
            Field.Container.LayoutOrder = layoutOrder
            Field.Container.Size = UDim2.fromOffset(0, 38)
            Field.Container.Parent = parent

            local FieldLayout = Instance.new("UIListLayout")
            FieldLayout.Padding = UDim.new(0, 15)
            FieldLayout.FillDirection = Enum.FillDirection.Horizontal
            FieldLayout.SortOrder = Enum.SortOrder.LayoutOrder
            FieldLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            FieldLayout.Parent = Field.Container

            Field.Label = Instance.new("TextLabel")
            Field.Label.Name = "Label"
            Field.Label.FontFace = Font.new(Assets.InterFont)
            Field.Label.Text = name
            Field.Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Label.TextSize = 13
            Field.Label.TextTransparency = 0.5
            Field.Label.TextTruncate = Enum.TextTruncate.AtEnd
            Field.Label.TextXAlignment = Enum.TextXAlignment.Left
            Field.Label.TextYAlignment = Enum.TextYAlignment.Top
            Field.Label.AnchorPoint = Vector2.new(0, 0.5)
            Field.Label.AutomaticSize = Enum.AutomaticSize.XY
            Field.Label.BackgroundTransparency = 1
            Field.Label.LayoutOrder = 2
            Field.Label.Position = UDim2.fromScale(0, 0.5)
            Field.Label.Parent = Field.Container

            Field.Input = Instance.new("TextBox")
            Field.Input.Name = "Input"
            Field.Input.ClearTextOnFocus = false
            Field.Input.FontFace = Font.new(Assets.InterFont)
            Field.Input.Text = name == "Hex" and "#FFFFFF" or (name == "Alpha" and "0" or "255")
            Field.Input.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.TextSize = 12
            Field.Input.TextTransparency = 0.1
            Field.Input.TextXAlignment = Enum.TextXAlignment.Left
            Field.Input.AnchorPoint = Vector2.new(1, 0.5)
            Field.Input.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.BackgroundTransparency = 0.95
            Field.Input.BorderSizePixel = 0
            Field.Input.ClipsDescendants = true
            Field.Input.LayoutOrder = 1
            Field.Input.Position = UDim2.fromScale(1, 0.5)
            Field.Input.Size = UDim2.fromOffset(75, 25)
            Field.Input.Parent = Field.Container

            local InputCorner = Instance.new("UICorner")
            InputCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Small
            InputCorner.Parent = Field.Input

            local InputStroke = Instance.new("UIStroke")
            InputStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            InputStroke.Color = Color3.fromRGB(255, 255, 255)
            InputStroke.Transparency = 0.9
            InputStroke.Parent = Field.Input

            local InputPadding = Instance.new("UIPadding")
            InputPadding.PaddingLeft = UDim.new(0, 8)
            InputPadding.PaddingRight = UDim.new(0, 10)
            InputPadding.Parent = Field.Input

            return Field
        end

        local function CreateActionButtons(Parent)
            local Buttons = {}

            Buttons.Container = Instance.new("Frame")
            Buttons.Container.Name = "ActionsContainer"
            Buttons.Container.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Container.BackgroundTransparency = 1
            Buttons.Container.LayoutOrder = 2
            Buttons.Container.Size = UDim2.fromScale(1, 0)
            Buttons.Container.Parent = Parent

            local ButtonsLayout = Instance.new("UIListLayout")
            ButtonsLayout.Padding = UI_CONSTANTS.PADDING.Medium
            ButtonsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            ButtonsLayout.Parent = Buttons.Container

            local ButtonsPadding = Instance.new("UIPadding")
            ButtonsPadding.PaddingTop = UI_CONSTANTS.PADDING.Medium
            ButtonsPadding.Parent = Buttons.Container

            Buttons.Confirm = Instance.new("TextButton")
            Buttons.Confirm.Name = "Confirm"
            Buttons.Confirm.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Confirm.Text = "Confirm"
            Buttons.Confirm.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Confirm.TextSize = 15
            Buttons.Confirm.TextTransparency = 0.5
            Buttons.Confirm.AutoButtonColor = false
            Buttons.Confirm.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Confirm.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Confirm.BorderSizePixel = 0
            Buttons.Confirm.Size = UDim2.fromScale(1, 0)
            Buttons.Confirm.Parent = Buttons.Container

            Buttons.Cancel = Instance.new("TextButton")
            Buttons.Cancel.Name = "Cancel"
            Buttons.Cancel.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Cancel.Text = "Cancel"
            Buttons.Cancel.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Cancel.TextSize = 15
            Buttons.Cancel.TextTransparency = 0.5
            Buttons.Cancel.AutoButtonColor = false
            Buttons.Cancel.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Cancel.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Cancel.BorderSizePixel = 0
            Buttons.Cancel.Size = UDim2.fromScale(1, 0)
            Buttons.Cancel.Parent = Buttons.Container

            for _, button in pairs({Buttons.Confirm, Buttons.Cancel}) do
                local corner = Instance.new("UICorner")
                corner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
                corner.Parent = button

                local padding = Instance.new("UIPadding")
                padding.PaddingBottom = UDim.new(0, 9)
                padding.PaddingLeft = UI_CONSTANTS.PADDING.Medium
                padding.PaddingRight = UI_CONSTANTS.PADDING.Medium
                padding.PaddingTop = UDim.new(0, 9)
                padding.Parent = button
            end

            return Buttons
        end

        local ColorUtils = {}

        function ColorUtils.HSVToRGB(h, s, v)
            return Color3.fromHSV(h, s, v)
        end

        function ColorUtils.RGBToHSV(Color)
            return Color:ToHSV()
        end

        function ColorUtils.RGBToHex(Color)
            return string.format("#%02X%02X%02X",
                math.floor(Color.R * 255 + 0.5),
                math.floor(Color.G * 255 + 0.5),
                math.floor(Color.B * 255 + 0.5)
            )
        end

        function ColorUtils.HexToRGB(hex)
            hex = hex:gsub("#", "")
            if #hex ~= 6 then return Color3.new(1, 1, 1) end
            local r = tonumber(hex:sub(1, 2), 16) or 255
            local g = tonumber(hex:sub(3, 4), 16) or 255
            local b = tonumber(hex:sub(5, 6), 16) or 255
            return Color3.fromRGB(r, g, b)
        end

        function ColorUtils.ClampValue(value, min, max)
            local num = tonumber(value)
            return num and math.clamp(num, min, max) or min
        end

        local function ShowColorDialog()
            if ColorPickerFunctions.IsHidden then return end
            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then return end

            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 1
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Visible = true
            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 0})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 1})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function HideColorDialog()
            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 0
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 1})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 0.95})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Visible = false
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function UpdateColorDisplay()
            local Color = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color
            ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

            if ColorPickerFunctions.Elements.Inputs then
                ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Hex.Input.Text = ColorUtils.RGBToHex(Color)
                if ColorPickerFunctions.Elements.Inputs.Alpha and Settings.Alpha then
                    ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                end
            end
        end

        local function HandleWheelInput(inputPos)
            local wheel = ColorPickerFunctions.Elements.Wheel.ColorWheel
            local wheelRadius = wheel.AbsoluteSize.X / 2
            local mouseOffset = inputPos - (wheel.AbsolutePosition + wheel.AbsoluteSize / 2)

            if mouseOffset:Dot(mouseOffset) > wheelRadius * wheelRadius then
                mouseOffset = mouseOffset.Unit * wheelRadius
            end

            ColorPickerFunctions.Elements.Wheel.Target.Position = UDim2.new(0.5, mouseOffset.X, 0.5, mouseOffset.Y)

            local angle = math.atan2(-mouseOffset.Y, mouseOffset.X)
            local distance = mouseOffset.Magnitude

            ColorPickerFunctions.State.Hue = ((angle + math.pi) / (2 * math.pi)) % 1
            ColorPickerFunctions.State.Saturation = math.clamp(distance / wheelRadius, 0, 1)

            local hueColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, 1)
            ColorPickerFunctions.Elements.Wheel.ValueSlider.BackgroundColor3 = hueColor

            UpdateColorDisplay()
        end

        local function HandleSliderInput(inputX)
            local slider = ColorPickerFunctions.Elements.Wheel.ValueSlider
            local handle = ColorPickerFunctions.Elements.Wheel.SliderHandle
            local relativeX = inputX - slider.AbsolutePosition.X
            local clampedX = math.clamp(relativeX, 0, slider.AbsoluteSize.X - handle.AbsoluteSize.X)

            handle.Position = UDim2.new(0, clampedX, 0.5, 0)
            ColorPickerFunctions.State.Value = 1 - (clampedX / (slider.AbsoluteSize.X - handle.AbsoluteSize.X))

            UpdateColorDisplay()
        end

        local function UpdateFromInputs()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local inputs = ColorPickerFunctions.Elements.Inputs
            local r = ColorUtils.ClampValue(inputs.Red.Input.Text, 0, 255)
            local g = ColorUtils.ClampValue(inputs.Green.Input.Text, 0, 255)
            local b = ColorUtils.ClampValue(inputs.Blue.Input.Text, 0, 255)

            inputs.Red.Input.Text = tostring(r)
            inputs.Green.Input.Text = tostring(g)
            inputs.Blue.Input.Text = tostring(b)

            local Color = Color3.fromRGB(r, g, b)
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            UpdateColorDisplay()
        end

        local function UpdateFromHex()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local hex = ColorPickerFunctions.Elements.Inputs.Hex.Input.Text
            local Color = ColorUtils.HexToRGB(hex)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))

            UpdateColorDisplay()
        end

        local function SetupConnections()
            local function getMousePos()
                local mouse = Utility:GetMouse()
                if mouse then
                    return Vector2.new(mouse.X, mouse.Y)
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return Vector2.new(pos.X, pos.Y)
            end

            local function getMouseX()
                local mouse = Utility:GetMouse()
                if mouse then
                    return mouse.X
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return pos.X
            end

            ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)

            if not Settings.PremiumFeature then
                ColorPickerFunctions.Maid.ColorHover = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = 0})
                end)

                ColorPickerFunctions.Maid.ColorLeave = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = TRANSPARENCY.Default})
                end)
            end

            if ColorPickerFunctions.Elements.Dialog then

                ColorPickerFunctions.Maid.CancelClick = ColorPickerFunctions.Elements.Buttons.Cancel.MouseButton1Click:Connect(HideColorDialog)

                ColorPickerFunctions.Maid.ConfirmClick = ColorPickerFunctions.Elements.Buttons.Confirm.MouseButton1Click:Connect(function()
                    HideColorDialog()

                    local finalColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)
                    ColorPickerFunctions.Color = finalColor

                    if Settings.Alpha then
                        ColorPickerFunctions.Alpha = ColorUtils.ClampValue(ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text, 0, 1)
                    end

                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

                    if Settings.Callback then
                        task.spawn(function()
                            Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                        end)
                    end
                end)

                ColorPickerFunctions.Maid.WheelDown = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = true
                        HandleWheelInput(getMousePos())
                    end
                end)

                ColorPickerFunctions.Maid.WheelUp = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = false
                    end
                end)

                ColorPickerFunctions.Maid.SliderDown = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = true
                        HandleSliderInput(getMouseX())
                    end
                end)

                ColorPickerFunctions.Maid.SliderUp = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = false
                    end
                end)

                ColorPickerFunctions.Maid.MouseMove = Services.UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                        if ColorPickerFunctions.State.WheelDown then
                            HandleWheelInput(getMousePos())
                        elseif ColorPickerFunctions.State.SlideDown then
                            HandleSliderInput(getMouseX())
                        end
                    end
                end)

                if ColorPickerFunctions.Elements.Inputs then
                    local inputs = ColorPickerFunctions.Elements.Inputs

                    ColorPickerFunctions.Maid.RedInput = inputs.Red.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.GreenInput = inputs.Green.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.BlueInput = inputs.Blue.Input.FocusLost:Connect(UpdateFromInputs)

                    ColorPickerFunctions.Maid.HexInput = inputs.Hex.Input.FocusLost:Connect(UpdateFromHex)

                    if inputs.Alpha and Settings.Alpha then
                        ColorPickerFunctions.Maid.AlphaInput = inputs.Alpha.Input.FocusLost:Connect(function()
                            ColorPickerFunctions.Alpha = ColorUtils.ClampValue(inputs.Alpha.Input.Text, 0, 1)
                            inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                            UpdateColorDisplay()
                        end)
                    end
                end
            end
        end

        local function IsPlaceIdAllowed(placeIdSetting)
            if not placeIdSetting then return true end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        local function IsPlaceIdForbidden(placeIdSetting)
            if not placeIdSetting then return false end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        function ColorPickerFunctions:ReturnClass()
            return ColorPickerFunctions.Class
        end

        function ColorPickerFunctions:ReturnTab()
            return ColorPickerFunctions.Tab
        end

        function ColorPickerFunctions:SetColor(Color)
            if not Color then Color = Color3.new(1, 1, 1) end

            ColorPickerFunctions.Color = Color
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color

            if Settings.Callback then
                task.spawn(function()
                    Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                end)
            end
        end

        function ColorPickerFunctions:SetAlpha(Alpha)
            if Settings.Alpha then
                ColorPickerFunctions.Alpha = math.clamp(Alpha or 0, 0, 1)
                ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            end
        end

        function ColorPickerFunctions:GetColor()
            return ColorPickerFunctions.Color
        end

        function ColorPickerFunctions:GetAlpha()
            return Settings.Alpha and ColorPickerFunctions.Alpha or nil
        end

        function ColorPickerFunctions:UpdateName(Name)
            ColorPickerFunctions.Elements.ColorPickerName.Text = Name
            if ColorPickerFunctions.Elements.Controls and ColorPickerFunctions.Elements.Controls.Title then
                ColorPickerFunctions.Elements.Controls.Title.Text = Name
            end
        end

        function ColorPickerFunctions:SetVisibility(State)
            ColorPickerFunctions.Elements.ColorPicker.Visible = State
        end

        function ColorPickerFunctions:GetState()
            return not ColorPickerFunctions.IsHidden
        end

        function ColorPickerFunctions:SetToolTip(tip)
            if ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame") then
                ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not tip or tip == "" then return end

            local toolTipFrame = Instance.new("Frame")
            toolTipFrame.Name = "ToolTipFrame"
            toolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            toolTipFrame.BackgroundTransparency = 1
            toolTipFrame.BorderSizePixel = 0
            toolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            toolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            toolTipFrame.Visible = false
            toolTipFrame.ZIndex = 200
            toolTipFrame.Parent = ColorPickerFunctions.Elements.ColorPicker

            local tooltipCorner = Instance.new("UICorner")
            tooltipCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            tooltipCorner.Parent = toolTipFrame

            local tooltipStroke = Instance.new("UIStroke")
            tooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            tooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            tooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            tooltipStroke.Thickness = 0.1
            tooltipStroke.Transparency = 0.2
            tooltipStroke.Parent = toolTipFrame

            local tooltipText = Instance.new("TextLabel")
            tooltipText.Text = tip
            tooltipText.FontFace = Font.new(Assets.InterFont)
            tooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            tooltipText.TextSize = 12
            tooltipText.TextTransparency = 1
            tooltipText.BackgroundTransparency = 1
            tooltipText.TextXAlignment = Enum.TextXAlignment.Center
            tooltipText.TextYAlignment = Enum.TextYAlignment.Center
            tooltipText.Size = UDim2.new(1, -12, 1, 0)
            tooltipText.Position = UDim2.new(0, 6, 0, 0)
            tooltipText.Parent = toolTipFrame

            local textBounds = Services.TextService:GetTextSize(tip, tooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local tooltipWidth = math.max(80, textBounds.X + 50)
            local tooltipHeight = math.max(28, textBounds.Y + 12)
            toolTipFrame.Size = UDim2.new(0, tooltipWidth, 0, tooltipHeight)

            local function showTooltip()
                toolTipFrame.Visible = true
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function hideTooltip()
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if toolTipFrame and toolTipFrame.Parent then
                        toolTipFrame.Visible = false
                    end
                end)
            end

            ColorPickerFunctions.Maid.TooltipShow = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(showTooltip)
            ColorPickerFunctions.Maid.TooltipHide = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(hideTooltip)
        end

        function ColorPickerFunctions:SetHidden(state, reason)
            if reason then
                self:SetToolTip(reason)
            end

            ColorPickerFunctions.IsHidden = state

            if state then
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ColorPickerFunctions.Maid.ColorClick then
                    ColorPickerFunctions.Maid.ColorClick:Disconnect()
                    ColorPickerFunctions.Maid.ColorClick = nil
                end
            else
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = 0}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)
            end
        end

        function ColorPickerFunctions:Destroy()

            for connectionName, connection in pairs(ColorPickerFunctions.Maid) do
                if type(connection) == "userdata" and connection.Disconnect then
                    connection:Disconnect()
                end
            end
            table.clear(ColorPickerFunctions.Maid)

            if ColorPickerFunctions.Elements.ColorPicker then
                ColorPickerFunctions.Elements.ColorPicker:Destroy()
            end

            if ColorPickerFunctions.Elements.Dialog and ColorPickerFunctions.Elements.Dialog.Overlay then
                ColorPickerFunctions.Elements.Dialog.Overlay:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function Initialize()
            ColorPickerFunctions.Elements = CreateMainElements()

            ColorPickerFunctions.Elements.Dialog = CreateColorDialog()

            ColorPickerFunctions.Elements.Controls = CreateColorControls(ColorPickerFunctions.Elements.Dialog.Prompt)
            ColorPickerFunctions.Elements.Wheel = CreateColorWheel(ColorPickerFunctions.Elements.Controls.ColorOptions)
            ColorPickerFunctions.Elements.Inputs = CreateInputFields(ColorPickerFunctions.Elements.Wheel.Container)

            local ColorWells = Instance.new("Frame")
            ColorWells.Name = "ColorWells"
            ColorWells.AutomaticSize = Enum.AutomaticSize.Y
            ColorWells.BackgroundTransparency = 1
            ColorWells.LayoutOrder = 2
            ColorWells.Size = UDim2.fromScale(1, 0)
            ColorWells.Parent = ColorPickerFunctions.Elements.Controls.ColorOptions

            local WellsGrid = Instance.new("UIGridLayout")
            WellsGrid.CellPadding = UDim2.fromOffset(10, 0)
            WellsGrid.CellSize = UDim2.new(0.5, -5, 0, 30)
            WellsGrid.SortOrder = Enum.SortOrder.LayoutOrder
            WellsGrid.Parent = ColorWells

            local NewColorWell = Instance.new("ImageLabel")
            NewColorWell.Name = "NewColor"
            NewColorWell.Image = Assets.Grid
            NewColorWell.ScaleType = Enum.ScaleType.Tile
            NewColorWell.TileSize = UDim2.fromOffset(500, 500)
            NewColorWell.BackgroundTransparency = 1
            NewColorWell.Parent = ColorWells

            local NewColorDisplay = Instance.new("Frame")
            NewColorDisplay.Name = "Color"
            NewColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            NewColorDisplay.BorderSizePixel = 0
            NewColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            NewColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            NewColorDisplay.Parent = NewColorWell

            local OldColorWell = Instance.new("ImageLabel")
            OldColorWell.Name = "OldColor"
            OldColorWell.Image = Assets.Grid
            OldColorWell.ScaleType = Enum.ScaleType.Tile
            OldColorWell.TileSize = UDim2.fromOffset(500, 500)
            OldColorWell.BackgroundTransparency = 1
            OldColorWell.LayoutOrder = 1
            OldColorWell.Parent = ColorWells

            local OldColorDisplay = Instance.new("Frame")
            OldColorDisplay.Name = "Color"
            OldColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            OldColorDisplay.BorderSizePixel = 0
            OldColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            OldColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            OldColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            OldColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0
            OldColorDisplay.Parent = OldColorWell

            for _, Well in pairs({NewColorWell, OldColorWell}) do
                local corner = Instance.new("UICorner")
                corner.Parent = Well

                local colorCorner = Instance.new("UICorner")
                colorCorner.Parent = Well:FindFirstChild("Color")
            end

            ColorPickerFunctions.Elements.NewColorDisplay = NewColorDisplay
            ColorPickerFunctions.Elements.OldColorDisplay = OldColorDisplay

            ColorPickerFunctions.Elements.Buttons = CreateActionButtons(ColorPickerFunctions.Elements.Dialog.Prompt)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(ColorPickerFunctions.Color)

            SetupConnections()
            UpdateColorDisplay()

            if not Base:FindFirstChild("OverlayFolder") then
                OverlayFolder = Instance.new("Folder")
                OverlayFolder.Name = "OverlayFolder"
                OverlayFolder.Parent = Base
            end

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
        end

        local function ApplyInitialSettings()
            if Settings.Hidden then
                if typeof(Settings.Hidden) == "table" then
                    ColorPickerFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
                else
                    ColorPickerFunctions:SetHidden(Settings.Hidden)
                end
            end

            if Settings.ToolTip then
                ColorPickerFunctions:SetToolTip(Settings.ToolTip)
            end

            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
                ColorPickerFunctions:SetHidden(true, "Premium Feature")
            end

            if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end

            if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end
        end

        Initialize()
        ApplyInitialSettings()

        if Flag then
            XenithLibrary.Options[Flag] = ColorPickerFunctions
        end

        return ColorPickerFunctions
    end
end
]],
  ["Xenith/UserInterface/Components/ColorPicker.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale
    local Base = Requirements.Base
    
    function SectionFunctions:ColorPicker(Settings, Flag)
        local ColorPickerFunctions = {
            Settings = Settings,
            Class = "ColorPicker",
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Color = Settings.Default or Color3.new(1, 1, 1),
            Alpha = Settings.Alpha or 0,
            IsHidden = false,
            Maid = {},
            Elements = {},
            State = {
                Hue = 0,
                Saturation = 0,
                Value = 1,
                WheelDown = false,
                SlideDown = false
            }
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local UI_CONSTANTS = {
            MAIN_HEIGHT = 38,
            COLOR_SIZE = 21,
            CORNER_RADIUS = {
                Small = UDim.new(0, 4),
                Medium = UDim.new(0, 6),
                Large = UDim.new(0, 10)
            },
            PADDING = {
                Small = UDim.new(0, 5),
                Medium = UDim.new(0, 10),
                Large = UDim.new(0, 20)
            }
        }

        local function CreateMainElements()
            local Elements = {}

            Elements.ColorPicker = Instance.new("Frame")
            Elements.ColorPicker.Name = "ColorPicker"
            Elements.ColorPicker.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ColorPicker.BackgroundTransparency = 1
            Elements.ColorPicker.BorderSizePixel = 0
            Elements.ColorPicker.Size = UDim2.new(1, 0, 0, UI_CONSTANTS.MAIN_HEIGHT)
            Elements.ColorPicker.Parent = Section

            Elements.ColorPickerName = Instance.new("TextLabel")
            Elements.ColorPickerName.Name = "ColorPickerName"
            Elements.ColorPickerName.FontFace = Font.new(Assets.InterFont)
            Elements.ColorPickerName.Text = Settings.Name
            Elements.ColorPickerName.RichText = true
            Elements.ColorPickerName.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ColorPickerName.TextSize = 13
            Elements.ColorPickerName.TextTransparency = TRANSPARENCY.Default
            Elements.ColorPickerName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ColorPickerName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ColorPickerName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ColorPickerName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ColorPickerName.AutomaticSize = Enum.AutomaticSize.XY
            Elements.ColorPickerName.BackgroundTransparency = 1
            Elements.ColorPickerName.Position = UDim2.fromScale(0, 0.5)
            Elements.ColorPickerName.Parent = Elements.ColorPicker

            if Settings.PremiumFeature then
                Elements.ColorPickerName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ColorPickerName
            end

            Elements.ColorBackground = Instance.new("ImageLabel")
            Elements.ColorBackground.Name = "ColorBackground"
            Elements.ColorBackground.Image = Assets.Grid
            Elements.ColorBackground.ScaleType = Enum.ScaleType.Tile
            Elements.ColorBackground.TileSize = UDim2.fromOffset(500, 500)
            Elements.ColorBackground.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ColorBackground.BackgroundTransparency = 1
            Elements.ColorBackground.Position = UDim2.fromScale(1, 0.5)
            Elements.ColorBackground.Size = UDim2.fromOffset(UI_CONSTANTS.COLOR_SIZE, UI_CONSTANTS.COLOR_SIZE)
            Elements.ColorBackground.Parent = Elements.ColorPicker

            Elements.ColorDisplay = Instance.new("Frame")
            Elements.ColorDisplay.Name = "ColorDisplay"
            Elements.ColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            Elements.ColorDisplay.BorderSizePixel = 0
            Elements.ColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            Elements.ColorDisplay.Size = UDim2.fromScale(1, 1)
            Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            Elements.ColorDisplay.Parent = Elements.ColorBackground

            Elements.ColorInteract = Instance.new("TextButton")
            Elements.ColorInteract.Name = "ColorInteract"
            Elements.ColorInteract.Text = ""
            Elements.ColorInteract.BackgroundTransparency = 1
            Elements.ColorInteract.Size = UDim2.fromScale(1, 1)
            Elements.ColorInteract.Parent = Elements.ColorDisplay

            local DisplayCorner = Instance.new("UICorner")
            DisplayCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            DisplayCorner.Parent = Elements.ColorDisplay

            local BackgroundCorner = Instance.new("UICorner")
            BackgroundCorner.CornerRadius = UDim.new(0, 8)
            BackgroundCorner.Parent = Elements.ColorBackground

            return Elements
        end

        local function CreateColorDialog()
            local Dialog = {}

            Dialog.Overlay = Instance.new("Frame")
            Dialog.Overlay.Name = "ColorPickerOverlay"
            Dialog.Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Dialog.Overlay.BackgroundTransparency = 0.5
            Dialog.Overlay.BorderSizePixel = 0
            Dialog.Overlay.Size = UDim2.fromScale(1, 1)
            Dialog.Overlay.Visible = false

            local OverlayCorner = Instance.new("UICorner")
            OverlayCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
            OverlayCorner.Parent = Dialog.Overlay

            Dialog.Prompt = Instance.new("Frame")
            Dialog.Prompt.Name = "ColorPrompt"
            Dialog.Prompt.AnchorPoint = Vector2.new(0.5, 0.5)
            Dialog.Prompt.AutomaticSize = Enum.AutomaticSize.Y
            Dialog.Prompt.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
            Dialog.Prompt.BorderSizePixel = 0
            Dialog.Prompt.Position = UDim2.fromScale(0.5, 0.5)
            Dialog.Prompt.Size = UDim2.fromOffset(420, 0)
            Dialog.Prompt.Parent = Dialog.Overlay

            local PromptScale = Instance.new("UIScale")
            PromptScale.Scale = 0.95
            PromptScale.Parent = Dialog.Prompt

            local PromptStroke = Instance.new("UIStroke")
            PromptStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            PromptStroke.Color = Color3.fromRGB(255, 255, 255)
            PromptStroke.Transparency = 0.9
            PromptStroke.Parent = Dialog.Prompt

            local PromptCorner = Instance.new("UICorner")
            PromptCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
            PromptCorner.Parent = Dialog.Prompt

            local PromptLayout = Instance.new("UIListLayout")
            PromptLayout.Padding = UI_CONSTANTS.PADDING.Medium
            PromptLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            PromptLayout.SortOrder = Enum.SortOrder.LayoutOrder
            PromptLayout.Parent = Dialog.Prompt

            local PromptPadding = Instance.new("UIPadding")
            PromptPadding.PaddingBottom = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingLeft = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingRight = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingTop = UI_CONSTANTS.PADDING.Large
            PromptPadding.Parent = Dialog.Prompt

            return Dialog
        end

        local function CreateColorControls(Parent)
            local Controls = {}

            Controls.Header = Instance.new("Frame")
            Controls.Header.Name = "Header"
            Controls.Header.AutomaticSize = Enum.AutomaticSize.Y
            Controls.Header.BackgroundTransparency = 1
            Controls.Header.Size = UDim2.fromScale(1, 0)
            Controls.Header.Parent = Parent

            local HeaderLayout = Instance.new("UIListLayout")
            HeaderLayout.Padding = UDim.new(0, 15)
            HeaderLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            HeaderLayout.SortOrder = Enum.SortOrder.LayoutOrder
            HeaderLayout.Parent = Controls.Header

            local HeaderPadding = Instance.new("UIPadding")
            HeaderPadding.PaddingBottom = UDim.new(0, 15)
            HeaderPadding.Parent = Controls.Header

            Controls.Title = Instance.new("TextLabel")
            Controls.Title.Name = "Title"
            Controls.Title.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.SemiBold)
            Controls.Title.RichText = true
            Controls.Title.Text = Settings.Name
            Controls.Title.TextColor3 = Color3.fromRGB(255, 255, 255)
            Controls.Title.TextSize = 18
            Controls.Title.TextTransparency = 0.4
            Controls.Title.TextWrapped = true
            Controls.Title.TextYAlignment = Enum.TextYAlignment.Top
            Controls.Title.AutomaticSize = Enum.AutomaticSize.XY
            Controls.Title.BackgroundTransparency = 1
            Controls.Title.Size = UDim2.fromScale(1, 0)
            Controls.Title.Parent = Controls.Header

            local TitleLine = Instance.new("Frame")
            TitleLine.Name = "Line"
            TitleLine.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TitleLine.BackgroundTransparency = 0.9
            TitleLine.BorderSizePixel = 0
            TitleLine.LayoutOrder = 1
            TitleLine.Size = UDim2.new(1, 0, 0, 1)
            TitleLine.Parent = Controls.Header

            Controls.ColorOptions = Instance.new("Frame")
            Controls.ColorOptions.Name = "ColorOptions"
            Controls.ColorOptions.AutomaticSize = Enum.AutomaticSize.XY
            Controls.ColorOptions.BackgroundTransparency = 1
            Controls.ColorOptions.LayoutOrder = 1
            Controls.ColorOptions.Size = UDim2.fromScale(1, 0)
            Controls.ColorOptions.Parent = Parent

            local OptionsLayout = Instance.new("UIListLayout")
            OptionsLayout.Padding = UDim.new(0, 25)
            OptionsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            OptionsLayout.Parent = Controls.ColorOptions

            return Controls
        end

        local function CreateColorWheel(Parent)
            local Wheel = {}

            Wheel.Container = Instance.new("Frame")
            Wheel.Container.Name = "WheelContainer"
            Wheel.Container.AutomaticSize = Enum.AutomaticSize.Y
            Wheel.Container.BackgroundTransparency = 1
            Wheel.Container.Size = UDim2.new(1, 0, 0, 100)
            Wheel.Container.Parent = Parent

            Wheel.ColorWheel = Instance.new("ImageButton")
            Wheel.ColorWheel.Name = "ColorWheel"
            Wheel.ColorWheel.Image = Assets.ColorWheel
            Wheel.ColorWheel.AutoButtonColor = false
            Wheel.ColorWheel.BackgroundTransparency = 1
            Wheel.ColorWheel.Size = UDim2.fromOffset(220, 220)
            Wheel.ColorWheel.SizeConstraint = Enum.SizeConstraint.RelativeYY
            Wheel.ColorWheel.Parent = Wheel.Container

            Wheel.Target = Instance.new("ImageLabel")
            Wheel.Target.Name = "Target"
            Wheel.Target.Image = Assets.ColorTarget
            Wheel.Target.ImageColor3 = Color3.fromRGB(0, 0, 0)
            Wheel.Target.AnchorPoint = Vector2.new(0.5, 0.5)
            Wheel.Target.BackgroundTransparency = 1
            Wheel.Target.Position = UDim2.fromScale(0.5, 0.5)
            Wheel.Target.Size = UDim2.fromOffset(22, 22)
            Wheel.Target.SizeConstraint = Enum.SizeConstraint.RelativeYY
            Wheel.Target.Parent = Wheel.ColorWheel

            Wheel.ValueSlider = Instance.new("TextButton")
            Wheel.ValueSlider.Name = "ValueSlider"
            Wheel.ValueSlider.Text = ""
            Wheel.ValueSlider.AutoButtonColor = false
            Wheel.ValueSlider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Wheel.ValueSlider.BorderSizePixel = 0
            Wheel.ValueSlider.LayoutOrder = 1
            Wheel.ValueSlider.Size = UDim2.new(1, 0, 0, 15)
            Wheel.ValueSlider.Position = UDim2.new(0, 0, 0, 15)
            Wheel.ValueSlider.Parent = ColorPickerFunctions.Elements.Controls.ColorOptions

            local SliderGradient = Instance.new("UIGradient")
            SliderGradient.Color = ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 0))
            })
            SliderGradient.Parent = Wheel.ValueSlider

            local SliderCorner = Instance.new("UICorner")
            SliderCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            SliderCorner.Parent = Wheel.ValueSlider

            local SliderStroke = Instance.new("UIStroke")
            SliderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            SliderStroke.Color = Color3.fromRGB(255, 255, 255)
            SliderStroke.Transparency = 0.9
            SliderStroke.Parent = Wheel.ValueSlider

            Wheel.SliderHandle = Instance.new("Frame")
            Wheel.SliderHandle.Name = "SliderHandle"
            Wheel.SliderHandle.AnchorPoint = Vector2.new(0, 0.5)
            Wheel.SliderHandle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Wheel.SliderHandle.BorderSizePixel = 0
            Wheel.SliderHandle.Position = UDim2.fromScale(0, 0.5)
            Wheel.SliderHandle.Size = UDim2.new(0, 13, 1, 8)
            Wheel.SliderHandle.Parent = Wheel.ValueSlider

            local HandleCorner = Instance.new("UICorner")
            HandleCorner.CornerRadius = UDim.new(1, 0)
            HandleCorner.Parent = Wheel.SliderHandle

            local HandleStroke = Instance.new("UIStroke")
            HandleStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            HandleStroke.Transparency = 0.5
            HandleStroke.Parent = Wheel.SliderHandle

            return Wheel
        end

        local function CreateInputFields(Parent)
            local Inputs = {}

            Inputs.Container = Instance.new("Frame")
            Inputs.Container.Name = "InputsContainer"
            Inputs.Container.AnchorPoint = Vector2.new(1, 0.5)
            Inputs.Container.AutomaticSize = Enum.AutomaticSize.XY
            Inputs.Container.BackgroundTransparency = 1
            Inputs.Container.LayoutOrder = 1
            Inputs.Container.Position = UDim2.fromScale(1, 0.5)
            Inputs.Container.Parent = Parent

            local InputsLayout = Instance.new("UIListLayout")
            InputsLayout.Padding = UI_CONSTANTS.PADDING.Small
            InputsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            InputsLayout.Parent = Inputs.Container

            local inputNames = {"Red", "Green", "Blue"}
            if Settings.Alpha then
                table.insert(inputNames, "Alpha")
            end
            table.insert(inputNames, "Hex")

            for i, name in ipairs(inputNames) do
                Inputs[name] = CreateInputField(name, i, Inputs.Container)
            end

            return Inputs
        end

        function CreateInputField(name, layoutOrder, parent)
            local Field = {}

            Field.Container = Instance.new("Frame")
            Field.Container.Name = name .. "Input"
            Field.Container.AutomaticSize = Enum.AutomaticSize.XY
            Field.Container.BackgroundTransparency = 1
            Field.Container.LayoutOrder = layoutOrder
            Field.Container.Size = UDim2.fromOffset(0, 38)
            Field.Container.Parent = parent

            local FieldLayout = Instance.new("UIListLayout")
            FieldLayout.Padding = UDim.new(0, 15)
            FieldLayout.FillDirection = Enum.FillDirection.Horizontal
            FieldLayout.SortOrder = Enum.SortOrder.LayoutOrder
            FieldLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            FieldLayout.Parent = Field.Container

            Field.Label = Instance.new("TextLabel")
            Field.Label.Name = "Label"
            Field.Label.FontFace = Font.new(Assets.InterFont)
            Field.Label.Text = name
            Field.Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Label.TextSize = 13
            Field.Label.TextTransparency = 0.5
            Field.Label.TextTruncate = Enum.TextTruncate.AtEnd
            Field.Label.TextXAlignment = Enum.TextXAlignment.Left
            Field.Label.TextYAlignment = Enum.TextYAlignment.Top
            Field.Label.AnchorPoint = Vector2.new(0, 0.5)
            Field.Label.AutomaticSize = Enum.AutomaticSize.XY
            Field.Label.BackgroundTransparency = 1
            Field.Label.LayoutOrder = 2
            Field.Label.Position = UDim2.fromScale(0, 0.5)
            Field.Label.Parent = Field.Container

            Field.Input = Instance.new("TextBox")
            Field.Input.Name = "Input"
            Field.Input.ClearTextOnFocus = false
            Field.Input.FontFace = Font.new(Assets.InterFont)
            Field.Input.Text = name == "Hex" and "#FFFFFF" or (name == "Alpha" and "0" or "255")
            Field.Input.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.TextSize = 12
            Field.Input.TextTransparency = 0.1
            Field.Input.TextXAlignment = Enum.TextXAlignment.Left
            Field.Input.AnchorPoint = Vector2.new(1, 0.5)
            Field.Input.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.BackgroundTransparency = 0.95
            Field.Input.BorderSizePixel = 0
            Field.Input.ClipsDescendants = true
            Field.Input.LayoutOrder = 1
            Field.Input.Position = UDim2.fromScale(1, 0.5)
            Field.Input.Size = UDim2.fromOffset(75, 25)
            Field.Input.Parent = Field.Container

            local InputCorner = Instance.new("UICorner")
            InputCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Small
            InputCorner.Parent = Field.Input

            local InputStroke = Instance.new("UIStroke")
            InputStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            InputStroke.Color = Color3.fromRGB(255, 255, 255)
            InputStroke.Transparency = 0.9
            InputStroke.Parent = Field.Input

            local InputPadding = Instance.new("UIPadding")
            InputPadding.PaddingLeft = UDim.new(0, 8)
            InputPadding.PaddingRight = UDim.new(0, 10)
            InputPadding.Parent = Field.Input

            return Field
        end

        local function CreateActionButtons(Parent)
            local Buttons = {}

            Buttons.Container = Instance.new("Frame")
            Buttons.Container.Name = "ActionsContainer"
            Buttons.Container.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Container.BackgroundTransparency = 1
            Buttons.Container.LayoutOrder = 2
            Buttons.Container.Size = UDim2.fromScale(1, 0)
            Buttons.Container.Parent = Parent

            local ButtonsLayout = Instance.new("UIListLayout")
            ButtonsLayout.Padding = UI_CONSTANTS.PADDING.Medium
            ButtonsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            ButtonsLayout.Parent = Buttons.Container

            local ButtonsPadding = Instance.new("UIPadding")
            ButtonsPadding.PaddingTop = UI_CONSTANTS.PADDING.Medium
            ButtonsPadding.Parent = Buttons.Container

            Buttons.Confirm = Instance.new("TextButton")
            Buttons.Confirm.Name = "Confirm"
            Buttons.Confirm.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Confirm.Text = "Confirm"
            Buttons.Confirm.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Confirm.TextSize = 15
            Buttons.Confirm.TextTransparency = 0.5
            Buttons.Confirm.AutoButtonColor = false
            Buttons.Confirm.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Confirm.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Confirm.BorderSizePixel = 0
            Buttons.Confirm.Size = UDim2.fromScale(1, 0)
            Buttons.Confirm.Parent = Buttons.Container

            Buttons.Cancel = Instance.new("TextButton")
            Buttons.Cancel.Name = "Cancel"
            Buttons.Cancel.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Cancel.Text = "Cancel"
            Buttons.Cancel.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Cancel.TextSize = 15
            Buttons.Cancel.TextTransparency = 0.5
            Buttons.Cancel.AutoButtonColor = false
            Buttons.Cancel.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Cancel.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Cancel.BorderSizePixel = 0
            Buttons.Cancel.Size = UDim2.fromScale(1, 0)
            Buttons.Cancel.Parent = Buttons.Container

            for _, button in pairs({Buttons.Confirm, Buttons.Cancel}) do
                local corner = Instance.new("UICorner")
                corner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
                corner.Parent = button

                local padding = Instance.new("UIPadding")
                padding.PaddingBottom = UDim.new(0, 9)
                padding.PaddingLeft = UI_CONSTANTS.PADDING.Medium
                padding.PaddingRight = UI_CONSTANTS.PADDING.Medium
                padding.PaddingTop = UDim.new(0, 9)
                padding.Parent = button
            end

            return Buttons
        end

        local ColorUtils = {}

        function ColorUtils.HSVToRGB(h, s, v)
            return Color3.fromHSV(h, s, v)
        end

        function ColorUtils.RGBToHSV(Color)
            return Color:ToHSV()
        end

        function ColorUtils.RGBToHex(Color)
            return string.format("#%02X%02X%02X",
                math.floor(Color.R * 255 + 0.5),
                math.floor(Color.G * 255 + 0.5),
                math.floor(Color.B * 255 + 0.5)
            )
        end

        function ColorUtils.HexToRGB(hex)
            hex = hex:gsub("#", "")
            if #hex ~= 6 then return Color3.new(1, 1, 1) end
            local r = tonumber(hex:sub(1, 2), 16) or 255
            local g = tonumber(hex:sub(3, 4), 16) or 255
            local b = tonumber(hex:sub(5, 6), 16) or 255
            return Color3.fromRGB(r, g, b)
        end

        function ColorUtils.ClampValue(value, min, max)
            local num = tonumber(value)
            return num and math.clamp(num, min, max) or min
        end

        local function ShowColorDialog()
            if ColorPickerFunctions.IsHidden then return end
            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then return end

            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 1
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Visible = true
            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 0})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 1})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function HideColorDialog()
            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 0
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 1})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 0.95})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Visible = false
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function UpdateColorDisplay()
            local Color = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color
            ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

            if ColorPickerFunctions.Elements.Inputs then
                ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Hex.Input.Text = ColorUtils.RGBToHex(Color)
                if ColorPickerFunctions.Elements.Inputs.Alpha and Settings.Alpha then
                    ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                end
            end
        end

        local function HandleWheelInput(inputPos)
            local wheel = ColorPickerFunctions.Elements.Wheel.ColorWheel
            local wheelRadius = wheel.AbsoluteSize.X / 2
            local mouseOffset = inputPos - (wheel.AbsolutePosition + wheel.AbsoluteSize / 2)

            if mouseOffset:Dot(mouseOffset) > wheelRadius * wheelRadius then
                mouseOffset = mouseOffset.Unit * wheelRadius
            end

            ColorPickerFunctions.Elements.Wheel.Target.Position = UDim2.new(0.5, mouseOffset.X, 0.5, mouseOffset.Y)

            local angle = math.atan2(-mouseOffset.Y, mouseOffset.X)
            local distance = mouseOffset.Magnitude

            ColorPickerFunctions.State.Hue = ((angle + math.pi) / (2 * math.pi)) % 1
            ColorPickerFunctions.State.Saturation = math.clamp(distance / wheelRadius, 0, 1)

            local hueColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, 1)
            ColorPickerFunctions.Elements.Wheel.ValueSlider.BackgroundColor3 = hueColor

            UpdateColorDisplay()
        end

        local function HandleSliderInput(inputX)
            local slider = ColorPickerFunctions.Elements.Wheel.ValueSlider
            local handle = ColorPickerFunctions.Elements.Wheel.SliderHandle
            local relativeX = inputX - slider.AbsolutePosition.X
            local clampedX = math.clamp(relativeX, 0, slider.AbsoluteSize.X - handle.AbsoluteSize.X)

            handle.Position = UDim2.new(0, clampedX, 0.5, 0)
            ColorPickerFunctions.State.Value = 1 - (clampedX / (slider.AbsoluteSize.X - handle.AbsoluteSize.X))

            UpdateColorDisplay()
        end

        local function UpdateFromInputs()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local inputs = ColorPickerFunctions.Elements.Inputs
            local r = ColorUtils.ClampValue(inputs.Red.Input.Text, 0, 255)
            local g = ColorUtils.ClampValue(inputs.Green.Input.Text, 0, 255)
            local b = ColorUtils.ClampValue(inputs.Blue.Input.Text, 0, 255)

            inputs.Red.Input.Text = tostring(r)
            inputs.Green.Input.Text = tostring(g)
            inputs.Blue.Input.Text = tostring(b)

            local Color = Color3.fromRGB(r, g, b)
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            UpdateColorDisplay()
        end

        local function UpdateFromHex()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local hex = ColorPickerFunctions.Elements.Inputs.Hex.Input.Text
            local Color = ColorUtils.HexToRGB(hex)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))

            UpdateColorDisplay()
        end

        local function SetupConnections()
            local function getMousePos()
                local mouse = Utility:GetMouse()
                if mouse then
                    return Vector2.new(mouse.X, mouse.Y)
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return Vector2.new(pos.X, pos.Y)
            end

            local function getMouseX()
                local mouse = Utility:GetMouse()
                if mouse then
                    return mouse.X
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return pos.X
            end

            ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)

            if not Settings.PremiumFeature then
                ColorPickerFunctions.Maid.ColorHover = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = 0})
                end)

                ColorPickerFunctions.Maid.ColorLeave = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = TRANSPARENCY.Default})
                end)
            end

            if ColorPickerFunctions.Elements.Dialog then

                ColorPickerFunctions.Maid.CancelClick = ColorPickerFunctions.Elements.Buttons.Cancel.MouseButton1Click:Connect(HideColorDialog)

                ColorPickerFunctions.Maid.ConfirmClick = ColorPickerFunctions.Elements.Buttons.Confirm.MouseButton1Click:Connect(function()
                    HideColorDialog()

                    local finalColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)
                    ColorPickerFunctions.Color = finalColor

                    if Settings.Alpha then
                        ColorPickerFunctions.Alpha = ColorUtils.ClampValue(ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text, 0, 1)
                    end

                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

                    if Settings.Callback then
                        task.spawn(function()
                            Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                        end)
                    end
                end)

                ColorPickerFunctions.Maid.WheelDown = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = true
                        HandleWheelInput(getMousePos())
                    end
                end)

                ColorPickerFunctions.Maid.WheelUp = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = false
                    end
                end)

                ColorPickerFunctions.Maid.SliderDown = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = true
                        HandleSliderInput(getMouseX())
                    end
                end)

                ColorPickerFunctions.Maid.SliderUp = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = false
                    end
                end)

                ColorPickerFunctions.Maid.MouseMove = Services.UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                        if ColorPickerFunctions.State.WheelDown then
                            HandleWheelInput(getMousePos())
                        elseif ColorPickerFunctions.State.SlideDown then
                            HandleSliderInput(getMouseX())
                        end
                    end
                end)

                if ColorPickerFunctions.Elements.Inputs then
                    local inputs = ColorPickerFunctions.Elements.Inputs

                    ColorPickerFunctions.Maid.RedInput = inputs.Red.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.GreenInput = inputs.Green.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.BlueInput = inputs.Blue.Input.FocusLost:Connect(UpdateFromInputs)

                    ColorPickerFunctions.Maid.HexInput = inputs.Hex.Input.FocusLost:Connect(UpdateFromHex)

                    if inputs.Alpha and Settings.Alpha then
                        ColorPickerFunctions.Maid.AlphaInput = inputs.Alpha.Input.FocusLost:Connect(function()
                            ColorPickerFunctions.Alpha = ColorUtils.ClampValue(inputs.Alpha.Input.Text, 0, 1)
                            inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                            UpdateColorDisplay()
                        end)
                    end
                end
            end
        end

        local function IsPlaceIdAllowed(placeIdSetting)
            if not placeIdSetting then return true end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        local function IsPlaceIdForbidden(placeIdSetting)
            if not placeIdSetting then return false end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        function ColorPickerFunctions:ReturnClass()
            return ColorPickerFunctions.Class
        end

        function ColorPickerFunctions:ReturnTab()
            return ColorPickerFunctions.Tab
        end

        function ColorPickerFunctions:SetColor(Color)
            if not Color then Color = Color3.new(1, 1, 1) end

            ColorPickerFunctions.Color = Color
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color

            if Settings.Callback then
                task.spawn(function()
                    Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                end)
            end
        end

        function ColorPickerFunctions:SetAlpha(Alpha)
            if Settings.Alpha then
                ColorPickerFunctions.Alpha = math.clamp(Alpha or 0, 0, 1)
                ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            end
        end

        function ColorPickerFunctions:GetColor()
            return ColorPickerFunctions.Color
        end

        function ColorPickerFunctions:GetAlpha()
            return Settings.Alpha and ColorPickerFunctions.Alpha or nil
        end

        function ColorPickerFunctions:UpdateName(Name)
            ColorPickerFunctions.Elements.ColorPickerName.Text = Name
            if ColorPickerFunctions.Elements.Controls and ColorPickerFunctions.Elements.Controls.Title then
                ColorPickerFunctions.Elements.Controls.Title.Text = Name
            end
        end

        function ColorPickerFunctions:SetVisibility(State)
            ColorPickerFunctions.Elements.ColorPicker.Visible = State
        end

        function ColorPickerFunctions:GetState()
            return not ColorPickerFunctions.IsHidden
        end

        function ColorPickerFunctions:SetToolTip(tip)
            if ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame") then
                ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not tip or tip == "" then return end

            local toolTipFrame = Instance.new("Frame")
            toolTipFrame.Name = "ToolTipFrame"
            toolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            toolTipFrame.BackgroundTransparency = 1
            toolTipFrame.BorderSizePixel = 0
            toolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            toolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            toolTipFrame.Visible = false
            toolTipFrame.ZIndex = 200
            toolTipFrame.Parent = ColorPickerFunctions.Elements.ColorPicker

            local tooltipCorner = Instance.new("UICorner")
            tooltipCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            tooltipCorner.Parent = toolTipFrame

            local tooltipStroke = Instance.new("UIStroke")
            tooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            tooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            tooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            tooltipStroke.Thickness = 0.1
            tooltipStroke.Transparency = 0.2
            tooltipStroke.Parent = toolTipFrame

            local tooltipText = Instance.new("TextLabel")
            tooltipText.Text = tip
            tooltipText.FontFace = Font.new(Assets.InterFont)
            tooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            tooltipText.TextSize = 12
            tooltipText.TextTransparency = 1
            tooltipText.BackgroundTransparency = 1
            tooltipText.TextXAlignment = Enum.TextXAlignment.Center
            tooltipText.TextYAlignment = Enum.TextYAlignment.Center
            tooltipText.Size = UDim2.new(1, -12, 1, 0)
            tooltipText.Position = UDim2.new(0, 6, 0, 0)
            tooltipText.Parent = toolTipFrame

            local textBounds = Services.TextService:GetTextSize(tip, tooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local tooltipWidth = math.max(80, textBounds.X + 50)
            local tooltipHeight = math.max(28, textBounds.Y + 12)
            toolTipFrame.Size = UDim2.new(0, tooltipWidth, 0, tooltipHeight)

            local function showTooltip()
                toolTipFrame.Visible = true
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function hideTooltip()
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if toolTipFrame and toolTipFrame.Parent then
                        toolTipFrame.Visible = false
                    end
                end)
            end

            ColorPickerFunctions.Maid.TooltipShow = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(showTooltip)
            ColorPickerFunctions.Maid.TooltipHide = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(hideTooltip)
        end

        function ColorPickerFunctions:SetHidden(state, reason)
            if reason then
                self:SetToolTip(reason)
            end

            ColorPickerFunctions.IsHidden = state

            if state then
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ColorPickerFunctions.Maid.ColorClick then
                    ColorPickerFunctions.Maid.ColorClick:Disconnect()
                    ColorPickerFunctions.Maid.ColorClick = nil
                end
            else
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = 0}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)
            end
        end

        function ColorPickerFunctions:Destroy()

            for connectionName, connection in pairs(ColorPickerFunctions.Maid) do
                if type(connection) == "userdata" and connection.Disconnect then
                    connection:Disconnect()
                end
            end
            table.clear(ColorPickerFunctions.Maid)

            if ColorPickerFunctions.Elements.ColorPicker then
                ColorPickerFunctions.Elements.ColorPicker:Destroy()
            end

            if ColorPickerFunctions.Elements.Dialog and ColorPickerFunctions.Elements.Dialog.Overlay then
                ColorPickerFunctions.Elements.Dialog.Overlay:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function Initialize()
            ColorPickerFunctions.Elements = CreateMainElements()

            ColorPickerFunctions.Elements.Dialog = CreateColorDialog()

            ColorPickerFunctions.Elements.Controls = CreateColorControls(ColorPickerFunctions.Elements.Dialog.Prompt)
            ColorPickerFunctions.Elements.Wheel = CreateColorWheel(ColorPickerFunctions.Elements.Controls.ColorOptions)
            ColorPickerFunctions.Elements.Inputs = CreateInputFields(ColorPickerFunctions.Elements.Wheel.Container)

            local ColorWells = Instance.new("Frame")
            ColorWells.Name = "ColorWells"
            ColorWells.AutomaticSize = Enum.AutomaticSize.Y
            ColorWells.BackgroundTransparency = 1
            ColorWells.LayoutOrder = 2
            ColorWells.Size = UDim2.fromScale(1, 0)
            ColorWells.Parent = ColorPickerFunctions.Elements.Controls.ColorOptions

            local WellsGrid = Instance.new("UIGridLayout")
            WellsGrid.CellPadding = UDim2.fromOffset(10, 0)
            WellsGrid.CellSize = UDim2.new(0.5, -5, 0, 30)
            WellsGrid.SortOrder = Enum.SortOrder.LayoutOrder
            WellsGrid.Parent = ColorWells

            local NewColorWell = Instance.new("ImageLabel")
            NewColorWell.Name = "NewColor"
            NewColorWell.Image = Assets.Grid
            NewColorWell.ScaleType = Enum.ScaleType.Tile
            NewColorWell.TileSize = UDim2.fromOffset(500, 500)
            NewColorWell.BackgroundTransparency = 1
            NewColorWell.Parent = ColorWells

            local NewColorDisplay = Instance.new("Frame")
            NewColorDisplay.Name = "Color"
            NewColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            NewColorDisplay.BorderSizePixel = 0
            NewColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            NewColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            NewColorDisplay.Parent = NewColorWell

            local OldColorWell = Instance.new("ImageLabel")
            OldColorWell.Name = "OldColor"
            OldColorWell.Image = Assets.Grid
            OldColorWell.ScaleType = Enum.ScaleType.Tile
            OldColorWell.TileSize = UDim2.fromOffset(500, 500)
            OldColorWell.BackgroundTransparency = 1
            OldColorWell.LayoutOrder = 1
            OldColorWell.Parent = ColorWells

            local OldColorDisplay = Instance.new("Frame")
            OldColorDisplay.Name = "Color"
            OldColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            OldColorDisplay.BorderSizePixel = 0
            OldColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            OldColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            OldColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            OldColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0
            OldColorDisplay.Parent = OldColorWell

            for _, Well in pairs({NewColorWell, OldColorWell}) do
                local corner = Instance.new("UICorner")
                corner.Parent = Well

                local colorCorner = Instance.new("UICorner")
                colorCorner.Parent = Well:FindFirstChild("Color")
            end

            ColorPickerFunctions.Elements.NewColorDisplay = NewColorDisplay
            ColorPickerFunctions.Elements.OldColorDisplay = OldColorDisplay

            ColorPickerFunctions.Elements.Buttons = CreateActionButtons(ColorPickerFunctions.Elements.Dialog.Prompt)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(ColorPickerFunctions.Color)

            SetupConnections()
            UpdateColorDisplay()

            if not Base:FindFirstChild("OverlayFolder") then
                OverlayFolder = Instance.new("Folder")
                OverlayFolder.Name = "OverlayFolder"
                OverlayFolder.Parent = Base
            end

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
        end

        local function ApplyInitialSettings()
            if Settings.Hidden then
                if typeof(Settings.Hidden) == "table" then
                    ColorPickerFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
                else
                    ColorPickerFunctions:SetHidden(Settings.Hidden)
                end
            end

            if Settings.ToolTip then
                ColorPickerFunctions:SetToolTip(Settings.ToolTip)
            end

            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
                ColorPickerFunctions:SetHidden(true, "Premium Feature")
            end

            if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end

            if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end
        end

        Initialize()
        ApplyInitialSettings()

        if Flag then
            XenithLibrary.Options[Flag] = ColorPickerFunctions
        end

        return ColorPickerFunctions
    end
end
]],
  ["UserInterface/Components/ColorPicker.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale
    local Base = Requirements.Base
    
    function SectionFunctions:ColorPicker(Settings, Flag)
        local ColorPickerFunctions = {
            Settings = Settings,
            Class = "ColorPicker",
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Color = Settings.Default or Color3.new(1, 1, 1),
            Alpha = Settings.Alpha or 0,
            IsHidden = false,
            Maid = {},
            Elements = {},
            State = {
                Hue = 0,
                Saturation = 0,
                Value = 1,
                WheelDown = false,
                SlideDown = false
            }
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local UI_CONSTANTS = {
            MAIN_HEIGHT = 38,
            COLOR_SIZE = 21,
            CORNER_RADIUS = {
                Small = UDim.new(0, 4),
                Medium = UDim.new(0, 6),
                Large = UDim.new(0, 10)
            },
            PADDING = {
                Small = UDim.new(0, 5),
                Medium = UDim.new(0, 10),
                Large = UDim.new(0, 20)
            }
        }

        local function CreateMainElements()
            local Elements = {}

            Elements.ColorPicker = Instance.new("Frame")
            Elements.ColorPicker.Name = "ColorPicker"
            Elements.ColorPicker.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ColorPicker.BackgroundTransparency = 1
            Elements.ColorPicker.BorderSizePixel = 0
            Elements.ColorPicker.Size = UDim2.new(1, 0, 0, UI_CONSTANTS.MAIN_HEIGHT)
            Elements.ColorPicker.Parent = Section

            Elements.ColorPickerName = Instance.new("TextLabel")
            Elements.ColorPickerName.Name = "ColorPickerName"
            Elements.ColorPickerName.FontFace = Font.new(Assets.InterFont)
            Elements.ColorPickerName.Text = Settings.Name
            Elements.ColorPickerName.RichText = true
            Elements.ColorPickerName.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ColorPickerName.TextSize = 13
            Elements.ColorPickerName.TextTransparency = TRANSPARENCY.Default
            Elements.ColorPickerName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ColorPickerName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ColorPickerName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ColorPickerName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ColorPickerName.AutomaticSize = Enum.AutomaticSize.XY
            Elements.ColorPickerName.BackgroundTransparency = 1
            Elements.ColorPickerName.Position = UDim2.fromScale(0, 0.5)
            Elements.ColorPickerName.Parent = Elements.ColorPicker

            if Settings.PremiumFeature then
                Elements.ColorPickerName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ColorPickerName
            end

            Elements.ColorBackground = Instance.new("ImageLabel")
            Elements.ColorBackground.Name = "ColorBackground"
            Elements.ColorBackground.Image = Assets.Grid
            Elements.ColorBackground.ScaleType = Enum.ScaleType.Tile
            Elements.ColorBackground.TileSize = UDim2.fromOffset(500, 500)
            Elements.ColorBackground.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ColorBackground.BackgroundTransparency = 1
            Elements.ColorBackground.Position = UDim2.fromScale(1, 0.5)
            Elements.ColorBackground.Size = UDim2.fromOffset(UI_CONSTANTS.COLOR_SIZE, UI_CONSTANTS.COLOR_SIZE)
            Elements.ColorBackground.Parent = Elements.ColorPicker

            Elements.ColorDisplay = Instance.new("Frame")
            Elements.ColorDisplay.Name = "ColorDisplay"
            Elements.ColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            Elements.ColorDisplay.BorderSizePixel = 0
            Elements.ColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            Elements.ColorDisplay.Size = UDim2.fromScale(1, 1)
            Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            Elements.ColorDisplay.Parent = Elements.ColorBackground

            Elements.ColorInteract = Instance.new("TextButton")
            Elements.ColorInteract.Name = "ColorInteract"
            Elements.ColorInteract.Text = ""
            Elements.ColorInteract.BackgroundTransparency = 1
            Elements.ColorInteract.Size = UDim2.fromScale(1, 1)
            Elements.ColorInteract.Parent = Elements.ColorDisplay

            local DisplayCorner = Instance.new("UICorner")
            DisplayCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            DisplayCorner.Parent = Elements.ColorDisplay

            local BackgroundCorner = Instance.new("UICorner")
            BackgroundCorner.CornerRadius = UDim.new(0, 8)
            BackgroundCorner.Parent = Elements.ColorBackground

            return Elements
        end

        local function CreateColorDialog()
            local Dialog = {}

            Dialog.Overlay = Instance.new("Frame")
            Dialog.Overlay.Name = "ColorPickerOverlay"
            Dialog.Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Dialog.Overlay.BackgroundTransparency = 0.5
            Dialog.Overlay.BorderSizePixel = 0
            Dialog.Overlay.Size = UDim2.fromScale(1, 1)
            Dialog.Overlay.Visible = false

            local OverlayCorner = Instance.new("UICorner")
            OverlayCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
            OverlayCorner.Parent = Dialog.Overlay

            Dialog.Prompt = Instance.new("Frame")
            Dialog.Prompt.Name = "ColorPrompt"
            Dialog.Prompt.AnchorPoint = Vector2.new(0.5, 0.5)
            Dialog.Prompt.AutomaticSize = Enum.AutomaticSize.Y
            Dialog.Prompt.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
            Dialog.Prompt.BorderSizePixel = 0
            Dialog.Prompt.Position = UDim2.fromScale(0.5, 0.5)
            Dialog.Prompt.Size = UDim2.fromOffset(420, 0)
            Dialog.Prompt.Parent = Dialog.Overlay

            local PromptScale = Instance.new("UIScale")
            PromptScale.Scale = 0.95
            PromptScale.Parent = Dialog.Prompt

            local PromptStroke = Instance.new("UIStroke")
            PromptStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            PromptStroke.Color = Color3.fromRGB(255, 255, 255)
            PromptStroke.Transparency = 0.9
            PromptStroke.Parent = Dialog.Prompt

            local PromptCorner = Instance.new("UICorner")
            PromptCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
            PromptCorner.Parent = Dialog.Prompt

            local PromptLayout = Instance.new("UIListLayout")
            PromptLayout.Padding = UI_CONSTANTS.PADDING.Medium
            PromptLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            PromptLayout.SortOrder = Enum.SortOrder.LayoutOrder
            PromptLayout.Parent = Dialog.Prompt

            local PromptPadding = Instance.new("UIPadding")
            PromptPadding.PaddingBottom = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingLeft = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingRight = UI_CONSTANTS.PADDING.Large
            PromptPadding.PaddingTop = UI_CONSTANTS.PADDING.Large
            PromptPadding.Parent = Dialog.Prompt

            return Dialog
        end

        local function CreateColorControls(Parent)
            local Controls = {}

            Controls.Header = Instance.new("Frame")
            Controls.Header.Name = "Header"
            Controls.Header.AutomaticSize = Enum.AutomaticSize.Y
            Controls.Header.BackgroundTransparency = 1
            Controls.Header.Size = UDim2.fromScale(1, 0)
            Controls.Header.Parent = Parent

            local HeaderLayout = Instance.new("UIListLayout")
            HeaderLayout.Padding = UDim.new(0, 15)
            HeaderLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            HeaderLayout.SortOrder = Enum.SortOrder.LayoutOrder
            HeaderLayout.Parent = Controls.Header

            local HeaderPadding = Instance.new("UIPadding")
            HeaderPadding.PaddingBottom = UDim.new(0, 15)
            HeaderPadding.Parent = Controls.Header

            Controls.Title = Instance.new("TextLabel")
            Controls.Title.Name = "Title"
            Controls.Title.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.SemiBold)
            Controls.Title.RichText = true
            Controls.Title.Text = Settings.Name
            Controls.Title.TextColor3 = Color3.fromRGB(255, 255, 255)
            Controls.Title.TextSize = 18
            Controls.Title.TextTransparency = 0.4
            Controls.Title.TextWrapped = true
            Controls.Title.TextYAlignment = Enum.TextYAlignment.Top
            Controls.Title.AutomaticSize = Enum.AutomaticSize.XY
            Controls.Title.BackgroundTransparency = 1
            Controls.Title.Size = UDim2.fromScale(1, 0)
            Controls.Title.Parent = Controls.Header

            local TitleLine = Instance.new("Frame")
            TitleLine.Name = "Line"
            TitleLine.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TitleLine.BackgroundTransparency = 0.9
            TitleLine.BorderSizePixel = 0
            TitleLine.LayoutOrder = 1
            TitleLine.Size = UDim2.new(1, 0, 0, 1)
            TitleLine.Parent = Controls.Header

            Controls.ColorOptions = Instance.new("Frame")
            Controls.ColorOptions.Name = "ColorOptions"
            Controls.ColorOptions.AutomaticSize = Enum.AutomaticSize.XY
            Controls.ColorOptions.BackgroundTransparency = 1
            Controls.ColorOptions.LayoutOrder = 1
            Controls.ColorOptions.Size = UDim2.fromScale(1, 0)
            Controls.ColorOptions.Parent = Parent

            local OptionsLayout = Instance.new("UIListLayout")
            OptionsLayout.Padding = UDim.new(0, 25)
            OptionsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            OptionsLayout.Parent = Controls.ColorOptions

            return Controls
        end

        local function CreateColorWheel(Parent)
            local Wheel = {}

            Wheel.Container = Instance.new("Frame")
            Wheel.Container.Name = "WheelContainer"
            Wheel.Container.AutomaticSize = Enum.AutomaticSize.Y
            Wheel.Container.BackgroundTransparency = 1
            Wheel.Container.Size = UDim2.new(1, 0, 0, 100)
            Wheel.Container.Parent = Parent

            Wheel.ColorWheel = Instance.new("ImageButton")
            Wheel.ColorWheel.Name = "ColorWheel"
            Wheel.ColorWheel.Image = Assets.ColorWheel
            Wheel.ColorWheel.AutoButtonColor = false
            Wheel.ColorWheel.BackgroundTransparency = 1
            Wheel.ColorWheel.Size = UDim2.fromOffset(220, 220)
            Wheel.ColorWheel.SizeConstraint = Enum.SizeConstraint.RelativeYY
            Wheel.ColorWheel.Parent = Wheel.Container

            Wheel.Target = Instance.new("ImageLabel")
            Wheel.Target.Name = "Target"
            Wheel.Target.Image = Assets.ColorTarget
            Wheel.Target.ImageColor3 = Color3.fromRGB(0, 0, 0)
            Wheel.Target.AnchorPoint = Vector2.new(0.5, 0.5)
            Wheel.Target.BackgroundTransparency = 1
            Wheel.Target.Position = UDim2.fromScale(0.5, 0.5)
            Wheel.Target.Size = UDim2.fromOffset(22, 22)
            Wheel.Target.SizeConstraint = Enum.SizeConstraint.RelativeYY
            Wheel.Target.Parent = Wheel.ColorWheel

            Wheel.ValueSlider = Instance.new("TextButton")
            Wheel.ValueSlider.Name = "ValueSlider"
            Wheel.ValueSlider.Text = ""
            Wheel.ValueSlider.AutoButtonColor = false
            Wheel.ValueSlider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Wheel.ValueSlider.BorderSizePixel = 0
            Wheel.ValueSlider.LayoutOrder = 1
            Wheel.ValueSlider.Size = UDim2.new(1, 0, 0, 15)
            Wheel.ValueSlider.Position = UDim2.new(0, 0, 0, 15)
            Wheel.ValueSlider.Parent = ColorPickerFunctions.Elements.Controls.ColorOptions

            local SliderGradient = Instance.new("UIGradient")
            SliderGradient.Color = ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 0))
            })
            SliderGradient.Parent = Wheel.ValueSlider

            local SliderCorner = Instance.new("UICorner")
            SliderCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            SliderCorner.Parent = Wheel.ValueSlider

            local SliderStroke = Instance.new("UIStroke")
            SliderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            SliderStroke.Color = Color3.fromRGB(255, 255, 255)
            SliderStroke.Transparency = 0.9
            SliderStroke.Parent = Wheel.ValueSlider

            Wheel.SliderHandle = Instance.new("Frame")
            Wheel.SliderHandle.Name = "SliderHandle"
            Wheel.SliderHandle.AnchorPoint = Vector2.new(0, 0.5)
            Wheel.SliderHandle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Wheel.SliderHandle.BorderSizePixel = 0
            Wheel.SliderHandle.Position = UDim2.fromScale(0, 0.5)
            Wheel.SliderHandle.Size = UDim2.new(0, 13, 1, 8)
            Wheel.SliderHandle.Parent = Wheel.ValueSlider

            local HandleCorner = Instance.new("UICorner")
            HandleCorner.CornerRadius = UDim.new(1, 0)
            HandleCorner.Parent = Wheel.SliderHandle

            local HandleStroke = Instance.new("UIStroke")
            HandleStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            HandleStroke.Transparency = 0.5
            HandleStroke.Parent = Wheel.SliderHandle

            return Wheel
        end

        local function CreateInputFields(Parent)
            local Inputs = {}

            Inputs.Container = Instance.new("Frame")
            Inputs.Container.Name = "InputsContainer"
            Inputs.Container.AnchorPoint = Vector2.new(1, 0.5)
            Inputs.Container.AutomaticSize = Enum.AutomaticSize.XY
            Inputs.Container.BackgroundTransparency = 1
            Inputs.Container.LayoutOrder = 1
            Inputs.Container.Position = UDim2.fromScale(1, 0.5)
            Inputs.Container.Parent = Parent

            local InputsLayout = Instance.new("UIListLayout")
            InputsLayout.Padding = UI_CONSTANTS.PADDING.Small
            InputsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            InputsLayout.Parent = Inputs.Container

            local inputNames = {"Red", "Green", "Blue"}
            if Settings.Alpha then
                table.insert(inputNames, "Alpha")
            end
            table.insert(inputNames, "Hex")

            for i, name in ipairs(inputNames) do
                Inputs[name] = CreateInputField(name, i, Inputs.Container)
            end

            return Inputs
        end

        function CreateInputField(name, layoutOrder, parent)
            local Field = {}

            Field.Container = Instance.new("Frame")
            Field.Container.Name = name .. "Input"
            Field.Container.AutomaticSize = Enum.AutomaticSize.XY
            Field.Container.BackgroundTransparency = 1
            Field.Container.LayoutOrder = layoutOrder
            Field.Container.Size = UDim2.fromOffset(0, 38)
            Field.Container.Parent = parent

            local FieldLayout = Instance.new("UIListLayout")
            FieldLayout.Padding = UDim.new(0, 15)
            FieldLayout.FillDirection = Enum.FillDirection.Horizontal
            FieldLayout.SortOrder = Enum.SortOrder.LayoutOrder
            FieldLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            FieldLayout.Parent = Field.Container

            Field.Label = Instance.new("TextLabel")
            Field.Label.Name = "Label"
            Field.Label.FontFace = Font.new(Assets.InterFont)
            Field.Label.Text = name
            Field.Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Label.TextSize = 13
            Field.Label.TextTransparency = 0.5
            Field.Label.TextTruncate = Enum.TextTruncate.AtEnd
            Field.Label.TextXAlignment = Enum.TextXAlignment.Left
            Field.Label.TextYAlignment = Enum.TextYAlignment.Top
            Field.Label.AnchorPoint = Vector2.new(0, 0.5)
            Field.Label.AutomaticSize = Enum.AutomaticSize.XY
            Field.Label.BackgroundTransparency = 1
            Field.Label.LayoutOrder = 2
            Field.Label.Position = UDim2.fromScale(0, 0.5)
            Field.Label.Parent = Field.Container

            Field.Input = Instance.new("TextBox")
            Field.Input.Name = "Input"
            Field.Input.ClearTextOnFocus = false
            Field.Input.FontFace = Font.new(Assets.InterFont)
            Field.Input.Text = name == "Hex" and "#FFFFFF" or (name == "Alpha" and "0" or "255")
            Field.Input.TextColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.TextSize = 12
            Field.Input.TextTransparency = 0.1
            Field.Input.TextXAlignment = Enum.TextXAlignment.Left
            Field.Input.AnchorPoint = Vector2.new(1, 0.5)
            Field.Input.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Field.Input.BackgroundTransparency = 0.95
            Field.Input.BorderSizePixel = 0
            Field.Input.ClipsDescendants = true
            Field.Input.LayoutOrder = 1
            Field.Input.Position = UDim2.fromScale(1, 0.5)
            Field.Input.Size = UDim2.fromOffset(75, 25)
            Field.Input.Parent = Field.Container

            local InputCorner = Instance.new("UICorner")
            InputCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Small
            InputCorner.Parent = Field.Input

            local InputStroke = Instance.new("UIStroke")
            InputStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            InputStroke.Color = Color3.fromRGB(255, 255, 255)
            InputStroke.Transparency = 0.9
            InputStroke.Parent = Field.Input

            local InputPadding = Instance.new("UIPadding")
            InputPadding.PaddingLeft = UDim.new(0, 8)
            InputPadding.PaddingRight = UDim.new(0, 10)
            InputPadding.Parent = Field.Input

            return Field
        end

        local function CreateActionButtons(Parent)
            local Buttons = {}

            Buttons.Container = Instance.new("Frame")
            Buttons.Container.Name = "ActionsContainer"
            Buttons.Container.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Container.BackgroundTransparency = 1
            Buttons.Container.LayoutOrder = 2
            Buttons.Container.Size = UDim2.fromScale(1, 0)
            Buttons.Container.Parent = Parent

            local ButtonsLayout = Instance.new("UIListLayout")
            ButtonsLayout.Padding = UI_CONSTANTS.PADDING.Medium
            ButtonsLayout.SortOrder = Enum.SortOrder.LayoutOrder
            ButtonsLayout.Parent = Buttons.Container

            local ButtonsPadding = Instance.new("UIPadding")
            ButtonsPadding.PaddingTop = UI_CONSTANTS.PADDING.Medium
            ButtonsPadding.Parent = Buttons.Container

            Buttons.Confirm = Instance.new("TextButton")
            Buttons.Confirm.Name = "Confirm"
            Buttons.Confirm.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Confirm.Text = "Confirm"
            Buttons.Confirm.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Confirm.TextSize = 15
            Buttons.Confirm.TextTransparency = 0.5
            Buttons.Confirm.AutoButtonColor = false
            Buttons.Confirm.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Confirm.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Confirm.BorderSizePixel = 0
            Buttons.Confirm.Size = UDim2.fromScale(1, 0)
            Buttons.Confirm.Parent = Buttons.Container

            Buttons.Cancel = Instance.new("TextButton")
            Buttons.Cancel.Name = "Cancel"
            Buttons.Cancel.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium)
            Buttons.Cancel.Text = "Cancel"
            Buttons.Cancel.TextColor3 = Color3.fromRGB(255, 255, 255)
            Buttons.Cancel.TextSize = 15
            Buttons.Cancel.TextTransparency = 0.5
            Buttons.Cancel.AutoButtonColor = false
            Buttons.Cancel.AutomaticSize = Enum.AutomaticSize.Y
            Buttons.Cancel.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Buttons.Cancel.BorderSizePixel = 0
            Buttons.Cancel.Size = UDim2.fromScale(1, 0)
            Buttons.Cancel.Parent = Buttons.Container

            for _, button in pairs({Buttons.Confirm, Buttons.Cancel}) do
                local corner = Instance.new("UICorner")
                corner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Large
                corner.Parent = button

                local padding = Instance.new("UIPadding")
                padding.PaddingBottom = UDim.new(0, 9)
                padding.PaddingLeft = UI_CONSTANTS.PADDING.Medium
                padding.PaddingRight = UI_CONSTANTS.PADDING.Medium
                padding.PaddingTop = UDim.new(0, 9)
                padding.Parent = button
            end

            return Buttons
        end

        local ColorUtils = {}

        function ColorUtils.HSVToRGB(h, s, v)
            return Color3.fromHSV(h, s, v)
        end

        function ColorUtils.RGBToHSV(Color)
            return Color:ToHSV()
        end

        function ColorUtils.RGBToHex(Color)
            return string.format("#%02X%02X%02X",
                math.floor(Color.R * 255 + 0.5),
                math.floor(Color.G * 255 + 0.5),
                math.floor(Color.B * 255 + 0.5)
            )
        end

        function ColorUtils.HexToRGB(hex)
            hex = hex:gsub("#", "")
            if #hex ~= 6 then return Color3.new(1, 1, 1) end
            local r = tonumber(hex:sub(1, 2), 16) or 255
            local g = tonumber(hex:sub(3, 4), 16) or 255
            local b = tonumber(hex:sub(5, 6), 16) or 255
            return Color3.fromRGB(r, g, b)
        end

        function ColorUtils.ClampValue(value, min, max)
            local num = tonumber(value)
            return num and math.clamp(num, min, max) or min
        end

        local function ShowColorDialog()
            if ColorPickerFunctions.IsHidden then return end
            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then return end

            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 1
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Visible = true
            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 0})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 1})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function HideColorDialog()
            local canvas = Instance.new("CanvasGroup")
            canvas.Name = "ColorPickerCanvas"
            canvas.BackgroundTransparency = 1
            canvas.BorderSizePixel = 0
            canvas.Size = UDim2.fromScale(1, 1)
            canvas.ZIndex = 5
            canvas.GroupTransparency = 0
            canvas.Visible = true
            canvas.Parent = Base

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = canvas

            local canvasTween = Utility:PropertyTween(canvas, TWEEN_INFO, {GroupTransparency = 1})
            local scaleTween = Utility:PropertyTween(ColorPickerFunctions.Elements.Dialog.Prompt:FindFirstChild("UIScale"), TWEEN_INFO, {Scale = 0.95})

            canvasTween:Play()
            if scaleTween then scaleTween:Play() end

            canvasTween.Completed:Connect(function()
                ColorPickerFunctions.Elements.Dialog.Overlay.Visible = false
                ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
                canvas:Destroy()
            end)
        end

        local function UpdateColorDisplay()
            local Color = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color
            ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

            if ColorPickerFunctions.Elements.Inputs then
                ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))
                ColorPickerFunctions.Elements.Inputs.Hex.Input.Text = ColorUtils.RGBToHex(Color)
                if ColorPickerFunctions.Elements.Inputs.Alpha and Settings.Alpha then
                    ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                end
            end
        end

        local function HandleWheelInput(inputPos)
            local wheel = ColorPickerFunctions.Elements.Wheel.ColorWheel
            local wheelRadius = wheel.AbsoluteSize.X / 2
            local mouseOffset = inputPos - (wheel.AbsolutePosition + wheel.AbsoluteSize / 2)

            if mouseOffset:Dot(mouseOffset) > wheelRadius * wheelRadius then
                mouseOffset = mouseOffset.Unit * wheelRadius
            end

            ColorPickerFunctions.Elements.Wheel.Target.Position = UDim2.new(0.5, mouseOffset.X, 0.5, mouseOffset.Y)

            local angle = math.atan2(-mouseOffset.Y, mouseOffset.X)
            local distance = mouseOffset.Magnitude

            ColorPickerFunctions.State.Hue = ((angle + math.pi) / (2 * math.pi)) % 1
            ColorPickerFunctions.State.Saturation = math.clamp(distance / wheelRadius, 0, 1)

            local hueColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, 1)
            ColorPickerFunctions.Elements.Wheel.ValueSlider.BackgroundColor3 = hueColor

            UpdateColorDisplay()
        end

        local function HandleSliderInput(inputX)
            local slider = ColorPickerFunctions.Elements.Wheel.ValueSlider
            local handle = ColorPickerFunctions.Elements.Wheel.SliderHandle
            local relativeX = inputX - slider.AbsolutePosition.X
            local clampedX = math.clamp(relativeX, 0, slider.AbsoluteSize.X - handle.AbsoluteSize.X)

            handle.Position = UDim2.new(0, clampedX, 0.5, 0)
            ColorPickerFunctions.State.Value = 1 - (clampedX / (slider.AbsoluteSize.X - handle.AbsoluteSize.X))

            UpdateColorDisplay()
        end

        local function UpdateFromInputs()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local inputs = ColorPickerFunctions.Elements.Inputs
            local r = ColorUtils.ClampValue(inputs.Red.Input.Text, 0, 255)
            local g = ColorUtils.ClampValue(inputs.Green.Input.Text, 0, 255)
            local b = ColorUtils.ClampValue(inputs.Blue.Input.Text, 0, 255)

            inputs.Red.Input.Text = tostring(r)
            inputs.Green.Input.Text = tostring(g)
            inputs.Blue.Input.Text = tostring(b)

            local Color = Color3.fromRGB(r, g, b)
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            UpdateColorDisplay()
        end

        local function UpdateFromHex()
            if not ColorPickerFunctions.Elements.Inputs then return end

            local hex = ColorPickerFunctions.Elements.Inputs.Hex.Input.Text
            local Color = ColorUtils.HexToRGB(hex)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.Inputs.Red.Input.Text = tostring(math.floor(Color.R * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Green.Input.Text = tostring(math.floor(Color.G * 255 + 0.5))
            ColorPickerFunctions.Elements.Inputs.Blue.Input.Text = tostring(math.floor(Color.B * 255 + 0.5))

            UpdateColorDisplay()
        end

        local function SetupConnections()
            local function getMousePos()
                local mouse = Utility:GetMouse()
                if mouse then
                    return Vector2.new(mouse.X, mouse.Y)
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return Vector2.new(pos.X, pos.Y)
            end

            local function getMouseX()
                local mouse = Utility:GetMouse()
                if mouse then
                    return mouse.X
                end
                local pos = Services.UserInputService:GetMouseLocation()
                return pos.X
            end

            ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)

            if not Settings.PremiumFeature then
                ColorPickerFunctions.Maid.ColorHover = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = 0})
                end)

                ColorPickerFunctions.Maid.ColorLeave = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(function()
                    if ColorPickerFunctions.IsHidden then return end
                    Utility:QuickTween(ColorPickerFunctions.Elements.ColorPickerName, {TextTransparency = TRANSPARENCY.Default})
                end)
            end

            if ColorPickerFunctions.Elements.Dialog then

                ColorPickerFunctions.Maid.CancelClick = ColorPickerFunctions.Elements.Buttons.Cancel.MouseButton1Click:Connect(HideColorDialog)

                ColorPickerFunctions.Maid.ConfirmClick = ColorPickerFunctions.Elements.Buttons.Confirm.MouseButton1Click:Connect(function()
                    HideColorDialog()

                    local finalColor = ColorUtils.HSVToRGB(ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value)
                    ColorPickerFunctions.Color = finalColor

                    if Settings.Alpha then
                        ColorPickerFunctions.Alpha = ColorUtils.ClampValue(ColorPickerFunctions.Elements.Inputs.Alpha.Input.Text, 0, 1)
                    end

                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
                    ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0

                    if Settings.Callback then
                        task.spawn(function()
                            Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                        end)
                    end
                end)

                ColorPickerFunctions.Maid.WheelDown = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = true
                        HandleWheelInput(getMousePos())
                    end
                end)

                ColorPickerFunctions.Maid.WheelUp = ColorPickerFunctions.Elements.Wheel.ColorWheel.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.WheelDown = false
                    end
                end)

                ColorPickerFunctions.Maid.SliderDown = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputBegan:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = true
                        HandleSliderInput(getMouseX())
                    end
                end)

                ColorPickerFunctions.Maid.SliderUp = ColorPickerFunctions.Elements.Wheel.ValueSlider.InputEnded:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                        ColorPickerFunctions.State.SlideDown = false
                    end
                end)

                ColorPickerFunctions.Maid.MouseMove = Services.UserInputService.InputChanged:Connect(function(input)
                    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                        if ColorPickerFunctions.State.WheelDown then
                            HandleWheelInput(getMousePos())
                        elseif ColorPickerFunctions.State.SlideDown then
                            HandleSliderInput(getMouseX())
                        end
                    end
                end)

                if ColorPickerFunctions.Elements.Inputs then
                    local inputs = ColorPickerFunctions.Elements.Inputs

                    ColorPickerFunctions.Maid.RedInput = inputs.Red.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.GreenInput = inputs.Green.Input.FocusLost:Connect(UpdateFromInputs)
                    ColorPickerFunctions.Maid.BlueInput = inputs.Blue.Input.FocusLost:Connect(UpdateFromInputs)

                    ColorPickerFunctions.Maid.HexInput = inputs.Hex.Input.FocusLost:Connect(UpdateFromHex)

                    if inputs.Alpha and Settings.Alpha then
                        ColorPickerFunctions.Maid.AlphaInput = inputs.Alpha.Input.FocusLost:Connect(function()
                            ColorPickerFunctions.Alpha = ColorUtils.ClampValue(inputs.Alpha.Input.Text, 0, 1)
                            inputs.Alpha.Input.Text = tostring(ColorPickerFunctions.Alpha)
                            UpdateColorDisplay()
                        end)
                    end
                end
            end
        end

        local function IsPlaceIdAllowed(placeIdSetting)
            if not placeIdSetting then return true end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        local function IsPlaceIdForbidden(placeIdSetting)
            if not placeIdSetting then return false end
            local currentPlaceId = game.PlaceId

            if type(placeIdSetting) == "table" then
                for _, placeId in pairs(placeIdSetting) do
                    if currentPlaceId == placeId then return true end
                end
                return false
            else
                return currentPlaceId == placeIdSetting
            end
        end

        function ColorPickerFunctions:ReturnClass()
            return ColorPickerFunctions.Class
        end

        function ColorPickerFunctions:ReturnTab()
            return ColorPickerFunctions.Tab
        end

        function ColorPickerFunctions:SetColor(Color)
            if not Color then Color = Color3.new(1, 1, 1) end

            ColorPickerFunctions.Color = Color
            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(Color)

            ColorPickerFunctions.Elements.ColorDisplay.BackgroundColor3 = Color

            if Settings.Callback then
                task.spawn(function()
                    Settings.Callback(ColorPickerFunctions.Color, ColorPickerFunctions.Alpha)
                end)
            end
        end

        function ColorPickerFunctions:SetAlpha(Alpha)
            if Settings.Alpha then
                ColorPickerFunctions.Alpha = math.clamp(Alpha or 0, 0, 1)
                ColorPickerFunctions.Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            end
        end

        function ColorPickerFunctions:GetColor()
            return ColorPickerFunctions.Color
        end

        function ColorPickerFunctions:GetAlpha()
            return Settings.Alpha and ColorPickerFunctions.Alpha or nil
        end

        function ColorPickerFunctions:UpdateName(Name)
            ColorPickerFunctions.Elements.ColorPickerName.Text = Name
            if ColorPickerFunctions.Elements.Controls and ColorPickerFunctions.Elements.Controls.Title then
                ColorPickerFunctions.Elements.Controls.Title.Text = Name
            end
        end

        function ColorPickerFunctions:SetVisibility(State)
            ColorPickerFunctions.Elements.ColorPicker.Visible = State
        end

        function ColorPickerFunctions:GetState()
            return not ColorPickerFunctions.IsHidden
        end

        function ColorPickerFunctions:SetToolTip(tip)
            if ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame") then
                ColorPickerFunctions.Elements.ColorPicker:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not tip or tip == "" then return end

            local toolTipFrame = Instance.new("Frame")
            toolTipFrame.Name = "ToolTipFrame"
            toolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            toolTipFrame.BackgroundTransparency = 1
            toolTipFrame.BorderSizePixel = 0
            toolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            toolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            toolTipFrame.Visible = false
            toolTipFrame.ZIndex = 200
            toolTipFrame.Parent = ColorPickerFunctions.Elements.ColorPicker

            local tooltipCorner = Instance.new("UICorner")
            tooltipCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            tooltipCorner.Parent = toolTipFrame

            local tooltipStroke = Instance.new("UIStroke")
            tooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            tooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            tooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            tooltipStroke.Thickness = 0.1
            tooltipStroke.Transparency = 0.2
            tooltipStroke.Parent = toolTipFrame

            local tooltipText = Instance.new("TextLabel")
            tooltipText.Text = tip
            tooltipText.FontFace = Font.new(Assets.InterFont)
            tooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            tooltipText.TextSize = 12
            tooltipText.TextTransparency = 1
            tooltipText.BackgroundTransparency = 1
            tooltipText.TextXAlignment = Enum.TextXAlignment.Center
            tooltipText.TextYAlignment = Enum.TextYAlignment.Center
            tooltipText.Size = UDim2.new(1, -12, 1, 0)
            tooltipText.Position = UDim2.new(0, 6, 0, 0)
            tooltipText.Parent = toolTipFrame

            local textBounds = Services.TextService:GetTextSize(tip, tooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local tooltipWidth = math.max(80, textBounds.X + 50)
            local tooltipHeight = math.max(28, textBounds.Y + 12)
            toolTipFrame.Size = UDim2.new(0, tooltipWidth, 0, tooltipHeight)

            local function showTooltip()
                toolTipFrame.Visible = true
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function hideTooltip()
                Services.TweenService:Create(toolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(tooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(tooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if toolTipFrame and toolTipFrame.Parent then
                        toolTipFrame.Visible = false
                    end
                end)
            end

            ColorPickerFunctions.Maid.TooltipShow = ColorPickerFunctions.Elements.ColorInteract.MouseEnter:Connect(showTooltip)
            ColorPickerFunctions.Maid.TooltipHide = ColorPickerFunctions.Elements.ColorInteract.MouseLeave:Connect(hideTooltip)
        end

        function ColorPickerFunctions:SetHidden(state, reason)
            if reason then
                self:SetToolTip(reason)
            end

            ColorPickerFunctions.IsHidden = state

            if state then
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ColorPickerFunctions.Maid.ColorClick then
                    ColorPickerFunctions.Maid.ColorClick:Disconnect()
                    ColorPickerFunctions.Maid.ColorClick = nil
                end
            else
                Services.TweenService:Create(ColorPickerFunctions.Elements.ColorBackground, TWEEN_INFO, {ImageTransparency = 0}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ColorPickerFunctions.Elements.ColorPickerName, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ColorPickerFunctions.Maid.ColorClick = ColorPickerFunctions.Elements.ColorInteract.MouseButton1Click:Connect(ShowColorDialog)
            end
        end

        function ColorPickerFunctions:Destroy()

            for connectionName, connection in pairs(ColorPickerFunctions.Maid) do
                if type(connection) == "userdata" and connection.Disconnect then
                    connection:Disconnect()
                end
            end
            table.clear(ColorPickerFunctions.Maid)

            if ColorPickerFunctions.Elements.ColorPicker then
                ColorPickerFunctions.Elements.ColorPicker:Destroy()
            end

            if ColorPickerFunctions.Elements.Dialog and ColorPickerFunctions.Elements.Dialog.Overlay then
                ColorPickerFunctions.Elements.Dialog.Overlay:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function Initialize()
            ColorPickerFunctions.Elements = CreateMainElements()

            ColorPickerFunctions.Elements.Dialog = CreateColorDialog()

            ColorPickerFunctions.Elements.Controls = CreateColorControls(ColorPickerFunctions.Elements.Dialog.Prompt)
            ColorPickerFunctions.Elements.Wheel = CreateColorWheel(ColorPickerFunctions.Elements.Controls.ColorOptions)
            ColorPickerFunctions.Elements.Inputs = CreateInputFields(ColorPickerFunctions.Elements.Wheel.Container)

            local ColorWells = Instance.new("Frame")
            ColorWells.Name = "ColorWells"
            ColorWells.AutomaticSize = Enum.AutomaticSize.Y
            ColorWells.BackgroundTransparency = 1
            ColorWells.LayoutOrder = 2
            ColorWells.Size = UDim2.fromScale(1, 0)
            ColorWells.Parent = ColorPickerFunctions.Elements.Controls.ColorOptions

            local WellsGrid = Instance.new("UIGridLayout")
            WellsGrid.CellPadding = UDim2.fromOffset(10, 0)
            WellsGrid.CellSize = UDim2.new(0.5, -5, 0, 30)
            WellsGrid.SortOrder = Enum.SortOrder.LayoutOrder
            WellsGrid.Parent = ColorWells

            local NewColorWell = Instance.new("ImageLabel")
            NewColorWell.Name = "NewColor"
            NewColorWell.Image = Assets.Grid
            NewColorWell.ScaleType = Enum.ScaleType.Tile
            NewColorWell.TileSize = UDim2.fromOffset(500, 500)
            NewColorWell.BackgroundTransparency = 1
            NewColorWell.Parent = ColorWells

            local NewColorDisplay = Instance.new("Frame")
            NewColorDisplay.Name = "Color"
            NewColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            NewColorDisplay.BorderSizePixel = 0
            NewColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            NewColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            NewColorDisplay.Parent = NewColorWell

            local OldColorWell = Instance.new("ImageLabel")
            OldColorWell.Name = "OldColor"
            OldColorWell.Image = Assets.Grid
            OldColorWell.ScaleType = Enum.ScaleType.Tile
            OldColorWell.TileSize = UDim2.fromOffset(500, 500)
            OldColorWell.BackgroundTransparency = 1
            OldColorWell.LayoutOrder = 1
            OldColorWell.Parent = ColorWells

            local OldColorDisplay = Instance.new("Frame")
            OldColorDisplay.Name = "Color"
            OldColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            OldColorDisplay.BorderSizePixel = 0
            OldColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            OldColorDisplay.Size = UDim2.new(1, 0, 1, 0)
            OldColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            OldColorDisplay.BackgroundTransparency = Settings.Alpha and ColorPickerFunctions.Alpha or 0
            OldColorDisplay.Parent = OldColorWell

            for _, Well in pairs({NewColorWell, OldColorWell}) do
                local corner = Instance.new("UICorner")
                corner.Parent = Well

                local colorCorner = Instance.new("UICorner")
                colorCorner.Parent = Well:FindFirstChild("Color")
            end

            ColorPickerFunctions.Elements.NewColorDisplay = NewColorDisplay
            ColorPickerFunctions.Elements.OldColorDisplay = OldColorDisplay

            ColorPickerFunctions.Elements.Buttons = CreateActionButtons(ColorPickerFunctions.Elements.Dialog.Prompt)

            ColorPickerFunctions.State.Hue, ColorPickerFunctions.State.Saturation, ColorPickerFunctions.State.Value = ColorUtils.RGBToHSV(ColorPickerFunctions.Color)

            SetupConnections()
            UpdateColorDisplay()

            if not Base:FindFirstChild("OverlayFolder") then
                OverlayFolder = Instance.new("Folder")
                OverlayFolder.Name = "OverlayFolder"
                OverlayFolder.Parent = Base
            end

            ColorPickerFunctions.Elements.Dialog.Overlay.Parent = OverlayFolder
        end

        local function ApplyInitialSettings()
            if Settings.Hidden then
                if typeof(Settings.Hidden) == "table" then
                    ColorPickerFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
                else
                    ColorPickerFunctions:SetHidden(Settings.Hidden)
                end
            end

            if Settings.ToolTip then
                ColorPickerFunctions:SetToolTip(Settings.ToolTip)
            end

            if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
                ColorPickerFunctions:SetHidden(true, "Premium Feature")
            end

            if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end

            if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
                ColorPickerFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
            end
        end

        Initialize()
        ApplyInitialSettings()

        if Flag then
            XenithLibrary.Options[Flag] = ColorPickerFunctions
        end

        return ColorPickerFunctions
    end
end
]],
  ["Xenith/Xenith/UserInterface/Components/Divider.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Divider()
        local DividerFunctions = {}

        local Divider = Instance.new("Frame")
        Divider.Name = "Divider"
        Divider.AnchorPoint = Vector2.new(0, 1)
        Divider.AutomaticSize = Enum.AutomaticSize.Y
        Divider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Divider.BackgroundTransparency = 1
        Divider.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Divider.BorderSizePixel = 0
        Divider.Position = UDim2.fromScale(0, 1)
        Divider.Size = UDim2.new(1, 0, 0, 1)
        Divider.Parent = Section

        local UIPadding = Instance.new("UIPadding")
        UIPadding.Name = "UIPadding"
        UIPadding.PaddingTop = UDim.new(0, 8)
        UIPadding.PaddingBottom = UDim.new(0, 8)
        UIPadding.Parent = Divider

        local UIListLayout = Instance.new("UIListLayout")
        UIListLayout.Name = "UIListLayout"
        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout.Parent = Divider

        local Line = Instance.new("Frame")
        Line.Name = "Line"
        Line.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Line.BackgroundTransparency = 0.9
        Line.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Line.BorderSizePixel = 0
        Line.Size = UDim2.new(1, 0, 0, 1)
        Line.Parent = Divider

        function DividerFunctions:Remove()
            Divider:Destroy()
        end

        function DividerFunctions:SetVisibility(State)
            Divider.Visible = State
        end

        return DividerFunctions
    end
end]],
  ["Xenith/UserInterface/Components/Divider.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
