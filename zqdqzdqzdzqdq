            DropdownFunctions.Elements.ClearAllButton.BackgroundTransparency = 0.95
            DropdownFunctions.Elements.ClearAllButton.BorderSizePixel = 0
            DropdownFunctions.Elements.ClearAllButton.Size = UDim2.new(0.5, -4, 1, 0)
            DropdownFunctions.Elements.ClearAllButton.Parent = DropdownFunctions.Elements.Editor

            local ClearAllCorner = Instance.new("UICorner")
            ClearAllCorner.CornerRadius = UDim.new(0, 4)
            ClearAllCorner.Parent = DropdownFunctions.Elements.ClearAllButton

            if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                DropdownFunctions.Elements.SelectAllButton.TextTransparency = 0.6
                DropdownFunctions.Elements.ClearAllButton.TextTransparency = 0.6
            end
        end

        local function CreateDropdownEditor()
            if not Settings.DropdownEditor then return end

            DropdownFunctions.Elements.EditorSettings = Instance.new("Frame")
            DropdownFunctions.Elements.EditorSettings.Name = "EditorSettings"
            DropdownFunctions.Elements.EditorSettings.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFunctions.Elements.EditorSettings.BackgroundTransparency = 0.98
            DropdownFunctions.Elements.EditorSettings.BorderSizePixel = 0
            DropdownFunctions.Elements.EditorSettings.LayoutOrder = -2
            DropdownFunctions.Elements.EditorSettings.Size = UDim2.new(1, 0, 0, 150)
            DropdownFunctions.Elements.EditorSettings.Visible = false
            DropdownFunctions.Elements.EditorSettings.Parent = DropdownFunctions.Elements.DropdownFrame

            local EditorCorner = Instance.new("UICorner")
            EditorCorner.CornerRadius = UDim.new(0, 6)
            EditorCorner.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorStroke = Instance.new("UIStroke")
            EditorStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            EditorStroke.Color = Color3.fromRGB(255, 255, 255)
            EditorStroke.Transparency = 0.95
            EditorStroke.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorPadding = Instance.new("UIPadding")
            EditorPadding.PaddingLeft = UDim.new(0, 15)
            EditorPadding.PaddingRight = UDim.new(0, 15)
            EditorPadding.PaddingTop = UDim.new(0, 10)
            EditorPadding.PaddingBottom = UDim.new(0, 10)
            EditorPadding.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorLayout = Instance.new("UIListLayout")
            EditorLayout.Padding = UDim.new(0, 8)
            EditorLayout.SortOrder = Enum.SortOrder.LayoutOrder
            EditorLayout.Parent = DropdownFunctions.Elements.EditorSettings

            local function CreateToggleOption(Name, DefaultValue)
                local ToggleFrame = Instance.new("Frame")
                ToggleFrame.Name = Name .. "Frame"
                ToggleFrame.BackgroundTransparency = 1
                ToggleFrame.Size = UDim2.new(1, 0, 0, 25)
                ToggleFrame.Parent = DropdownFunctions.Elements.EditorSettings

                local ToggleLabel = Instance.new("TextLabel")
                ToggleLabel.Name = Name .. "Label"
                ToggleLabel.FontFace = Font.new(Assets.InterFont)
                ToggleLabel.Text = Name:gsub("Enable", ""):gsub("(%l)(%u)", "%1 %2")
                ToggleLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
                ToggleLabel.TextSize = 12
                ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
                ToggleLabel.BackgroundTransparency = 1
                ToggleLabel.Size = UDim2.new(1, -30, 1, 0)
                ToggleLabel.Parent = ToggleFrame

                local ToggleButton = Instance.new("ImageButton")
                ToggleButton.Name = Name .. "Button"
                ToggleButton.Image = Assets.ToggleBackground
                ToggleButton.ImageColor3 = Color3.fromRGB(87, 86, 86)
                ToggleButton.ImageTransparency = DefaultValue and 0 or 0.5
                ToggleButton.AutoButtonColor = false
                ToggleButton.AnchorPoint = Vector2.new(1, 0.5)
                ToggleButton.BackgroundTransparency = 1
                ToggleButton.Position = UDim2.new(1, 0, 0.5, 0)
                ToggleButton.Size = UDim2.fromOffset(25, 13)
                ToggleButton.Parent = ToggleFrame

                local ToggleHead = Instance.new("ImageLabel")
                ToggleHead.Name = Name .. "Head"
                ToggleHead.Image = Assets.TogglerHead
                ToggleHead.ImageColor3 = Color3.fromRGB(255, 255, 255)
                ToggleHead.ImageTransparency = DefaultValue and 0 or 0.85
                ToggleHead.AnchorPoint = Vector2.new(1, 0.5)
                ToggleHead.BackgroundTransparency = 1
                ToggleHead.Position = DefaultValue and UDim2.new(1, 0, 0.5, 0) or UDim2.new(0.5, 0, 0.5, 0)
                ToggleHead.Size = UDim2.fromOffset(9, 9)
                ToggleHead.ZIndex = 2
                ToggleHead.Parent = ToggleButton

                return ToggleFrame, ToggleButton, ToggleHead
            end

            local EnableSearchFrame, EnableSearchButton, EnableSearchHead = CreateToggleOption("EnableSearch", Settings.Search)
            local EnableClearFrame, EnableClearButton, EnableClearHead = CreateToggleOption("EnableClear", Settings.Editor)
            local EnableSelectFrame, EnableSelectButton, EnableSelectHead = CreateToggleOption("EnableSelect", Settings.Editor)

            local AddOptionFrame = Instance.new("Frame")
            AddOptionFrame.Name = "AddOptionFrame"
            AddOptionFrame.BackgroundTransparency = 1
            AddOptionFrame.Size = UDim2.new(1, 0, 0, 25)
            AddOptionFrame.Parent = DropdownFunctions.Elements.EditorSettings

            local AddOptionBox = Instance.new("TextBox")
            AddOptionBox.Name = "AddOptionBox"
            AddOptionBox.FontFace = Font.new(Assets.InterFont)
            AddOptionBox.PlaceholderText = "Add Option..."
            AddOptionBox.Text = ""
            AddOptionBox.TextColor3 = Color3.fromRGB(200, 200, 200)
            AddOptionBox.TextSize = 12
            AddOptionBox.TextXAlignment = Enum.TextXAlignment.Left
            AddOptionBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            AddOptionBox.BackgroundTransparency = 0.95
            AddOptionBox.BorderSizePixel = 0
            AddOptionBox.Size = UDim2.new(1, -30, 1, 0)
            AddOptionBox.Parent = AddOptionFrame

            local AddOptionCorner = Instance.new("UICorner")
            AddOptionCorner.CornerRadius = UDim.new(0, 4)
            AddOptionCorner.Parent = AddOptionBox

            local AddOptionPadding = Instance.new("UIPadding")
            AddOptionPadding.PaddingLeft = UDim.new(0, 8)
            AddOptionPadding.Parent = AddOptionBox

            local AddOptionButton = Instance.new("ImageButton")
            AddOptionButton.Name = "AddOptionButton"
            AddOptionButton.Image = "rbxassetid://7733964046"
            AddOptionButton.ImageColor3 = Color3.fromRGB(180, 180, 180)
            AddOptionButton.ImageTransparency = 0.3
            AddOptionButton.AnchorPoint = Vector2.new(1, 0.5)
            AddOptionButton.BackgroundTransparency = 1
            AddOptionButton.Position = UDim2.new(1, 0, 0.5, 0)
            AddOptionButton.Size = UDim2.fromOffset(20, 20)
            AddOptionButton.Parent = AddOptionFrame

            DropdownFunctions.Elements.EditorToggles = {
                Search = {Button = EnableSearchButton, Head = EnableSearchHead, State = Settings.Search},
                Clear = {Button = EnableClearButton, Head = EnableClearHead, State = Settings.Editor},
                Select = {Button = EnableSelectButton, Head = EnableSelectHead, State = Settings.Editor},
                AddBox = AddOptionBox,
                AddButton = AddOptionButton
            }
        end

        local function CalculateDropdownSize()
            local TotalHeight = 0
            local VisibleChildrenCount = 0
            local Padding = DropdownFunctions.Elements.DropdownFrame:FindFirstChild("UIPadding")
            local PaddingOffset = Padding and (Padding.PaddingTop.Offset + Padding.PaddingBottom.Offset) or 0

            for _, V in pairs(DropdownFunctions.Elements.DropdownFrame:GetChildren()) do
                if not V:IsA("UIComponent") and V.Visible then
                    TotalHeight += V.AbsoluteSize.Y
                    VisibleChildrenCount += 1
                end
            end

            local Spacing = DropdownFunctions.Elements.DropdownFrameUIListLayout.Padding.Offset * math.max(0, VisibleChildrenCount - 1)
            return TotalHeight + Spacing + PaddingOffset
        end

        local function FindOption()
            if not DropdownFunctions.Elements.SearchBox then return end

            local SearchTerm = DropdownFunctions.Elements.SearchBox.Text:lower()
            for _, V in pairs(DropdownFunctions.OptionObjs) do
                local OptionText = V.NameLabel.Text:lower()
                local IsVisible = string.find(OptionText, SearchTerm) ~= nil
                V.Button.Visible = IsVisible
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function Toggle(OptionName, State)
            local Option = DropdownFunctions.OptionObjs[OptionName]
            if not Option then return end

            local Checkmark = Option.Checkmark
            local OptionNameLabel = Option.NameLabel

            if State then
                if Settings.Multi then
                    if not table.find(DropdownFunctions.Selected, OptionName) then
                        table.insert(DropdownFunctions.Selected, OptionName)
                        DropdownFunctions.Value = DropdownFunctions.Selected
                    end
                else
                    for Name, Opt in pairs(DropdownFunctions.OptionObjs) do
                        if Name ~= OptionName then
                            Utility:PropertyTween(Opt.Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                                Size = UDim2.new(Opt.Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeDecrease, Opt.Checkmark.Size.Y.Scale, Opt.Checkmark.Size.Y.Offset)
                            }):Play()
                            Utility:PropertyTween(Opt.NameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                                TextTransparency = TWEEN_SETTINGS.TransparencyOut
                            }):Play()
                            Opt.Checkmark.TextTransparency = 1
                        end
                    end

                    DropdownFunctions.Selected = {OptionName}
                    DropdownFunctions.Value = DropdownFunctions.Selected[1]
                end

                Utility:PropertyTween(Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    Size = UDim2.new(Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeIncrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
                }):Play()
                Utility:PropertyTween(OptionNameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    TextTransparency = TWEEN_SETTINGS.TransparencyIn
                }):Play()
                Checkmark.TextTransparency = 0
            else
                if Settings.Multi then
                    local Idx = table.find(DropdownFunctions.Selected, OptionName)
                    if Idx then table.remove(DropdownFunctions.Selected, Idx) end
                else
                    DropdownFunctions.Selected = {}
                end

                Utility:PropertyTween(Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    Size = UDim2.new(Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeDecrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
                }):Play()
                Utility:PropertyTween(OptionNameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    TextTransparency = TWEEN_SETTINGS.TransparencyOut
                }):Play()
                Checkmark.TextTransparency = 1
            end

            if Settings.Required and #DropdownFunctions.Selected == 0 and not State then
                return
            end

            if #DropdownFunctions.Selected > 0 then
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "  " .. table.concat(DropdownFunctions.Selected, ", ")
            else
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
            end
        end

        local function ToggleDropdown()
            if DropdownFunctions.IsHidden or DropdownFunctions.Debounce then return end
            if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end

            DropdownFunctions.Debounce = true
            local DefaultDropdownSize = 38
            local IsDropdownOpen = not DropdownFunctions.Dropped
            local TargetSize = IsDropdownOpen and UDim2.new(1, 0, 0, CalculateDropdownSize()) or UDim2.new(1, 0, 0, DefaultDropdownSize)

            local DropTween = Utility:PropertyTween(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
                Size = TargetSize
            })

            Utility:PropertyTween(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.075, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, true), {
                ImageTransparency = 0.8
            }):Play()


            task.delay(0.05, function()
                DropdownFunctions.Elements.DropdownImage.Image = IsDropdownOpen and Assets.PackageOpened or Assets.PackageClosed 
            end)

            DropTween:Play()

            if IsDropdownOpen then
                DropdownFunctions.Elements.DropdownFrame.Visible = true
                DropTween.Completed:Connect(function()
                    DropdownFunctions.Debounce = false
                end)
            else
                DropTween.Completed:Connect(function()
                    DropdownFunctions.Elements.DropdownFrame.Visible = false
                    DropdownFunctions.Debounce = false
                end)
            end

            DropdownFunctions.Dropped = IsDropdownOpen
        end

        local function ToggleEditorSettings()
            if DropdownFunctions.IsHidden or not DropdownFunctions.Elements.EditorSettings then return end

            local IsVisible = DropdownFunctions.Elements.EditorSettings.Visible
            DropdownFunctions.Elements.EditorSettings.Visible = not IsVisible

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function HandleEditorToggle(ToggleName)
            local ToggleData = DropdownFunctions.Elements.EditorToggles[ToggleName]
            if not ToggleData then return end

            ToggleData.State = not ToggleData.State
            local TransparencyValues = ToggleData.State and {0, 0} or {0.5, 0.85}
            local Position = ToggleData.State and UDim2.new(1, 0, 0.5, 0) or UDim2.new(0.5, 0, 0.5, 0)

            Utility:PropertyTween(ToggleData.Button, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
                ImageTransparency = TransparencyValues[1]
            }):Play()

            Utility:PropertyTween(ToggleData.Head, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
                ImageTransparency = TransparencyValues[2],
                Position = Position
            }):Play()

            if ToggleName == "Search" then
                if DropdownFunctions.Elements.Search then
                    DropdownFunctions.Elements.Search.Visible = ToggleData.State
                end
            elseif ToggleName == "Clear" or ToggleName == "Select" then
                if DropdownFunctions.Elements.Editor then
                    local ShouldShow = DropdownFunctions.Elements.EditorToggles.Clear.State or DropdownFunctions.Elements.EditorToggles.Select.State
                    DropdownFunctions.Elements.Editor.Visible = ShouldShow
                    if DropdownFunctions.Elements.SelectAllButton then
                        DropdownFunctions.Elements.SelectAllButton.Visible = DropdownFunctions.Elements.EditorToggles.Select.State
                    end
                    if DropdownFunctions.Elements.ClearAllButton then
                        DropdownFunctions.Elements.ClearAllButton.Visible = DropdownFunctions.Elements.EditorToggles.Clear.State
                    end
                end
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function HandleAddOption()
            local OptionText = DropdownFunctions.Elements.EditorToggles.AddBox.Text:match("^%s*(.-)%s*$")
            if OptionText == "" or DropdownFunctions.OptionObjs[OptionText] then return end

            AddOption(#DropdownFunctions.OptionObjs + 1, OptionText)
            DropdownFunctions.Elements.EditorToggles.AddBox.Text = ""

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function AddOption(I, V, C)
            if typeof(V) == "table" then
                C = V.Color
                V = V.Name
            end

            local Option = Instance.new("TextButton")
            Option.Name = "Option"
            Option.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
            Option.Text = ""
            Option.BackgroundTransparency = 1
            Option.BorderSizePixel = 0
            Option.Size = UDim2.new(1, 0, 0, 30)

            local OptionUIPadding = Instance.new("UIPadding")
            OptionUIPadding.PaddingLeft = UDim.new(0, 15)
            OptionUIPadding.Parent = Option

            local OptionUIListLayout = Instance.new("UIListLayout")
            OptionUIListLayout.Padding = UDim.new(0, 10)
            OptionUIListLayout.FillDirection = Enum.FillDirection.Horizontal
            OptionUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            OptionUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            OptionUIListLayout.Parent = Option

            local Checkmark = Instance.new("TextLabel")
            Checkmark.Name = "Checkmark"
            Checkmark.FontFace = Font.new(Assets.InterFont)
            Checkmark.Text = ""
            Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
            Checkmark.TextSize = 13
            Checkmark.TextTransparency = 1
            Checkmark.TextXAlignment = Enum.TextXAlignment.Left
            Checkmark.TextYAlignment = Enum.TextYAlignment.Top
            Checkmark.AnchorPoint = Vector2.new(0, 0.5)
            Checkmark.AutomaticSize = Enum.AutomaticSize.Y
            Checkmark.BackgroundTransparency = 1
            Checkmark.BorderSizePixel = 0
            Checkmark.LayoutOrder = -1
            Checkmark.Position = UDim2.fromScale(0, 0.5)
            Checkmark.Size = UDim2.fromOffset(-OptionUIListLayout.Padding.Offset, 0)
            Checkmark.Parent = Option

            local OptionName = Instance.new("TextLabel")
            OptionName.Name = "OptionName"
            OptionName.FontFace = Font.new(Assets.InterFont)
            OptionName.Text = V
            OptionName.RichText = true
            OptionName.TextColor3 = C or Color3.fromRGB(255, 255, 255)
            OptionName.TextSize = 13
            OptionName.TextTransparency = 0.5
            OptionName.TextTruncate = Enum.TextTruncate.AtEnd
            OptionName.TextXAlignment = Enum.TextXAlignment.Left
            OptionName.TextYAlignment = Enum.TextYAlignment.Top
            OptionName.AnchorPoint = Vector2.new(0, 0.5)
            OptionName.AutomaticSize = Enum.AutomaticSize.XY
            OptionName.BackgroundTransparency = 1
            OptionName.BorderSizePixel = 0
            OptionName.Position = UDim2.fromScale(0, 0.5)
            OptionName.Parent = Option

            if C then
                OptionName:SetAttribute("NoApplyTheme", true)
            end

            Option.Parent = DropdownFunctions.Elements.DropdownFrame
            DropdownFunctions.OptionObjs[V] = {
                Index = I,
                Button = Option,
                NameLabel = OptionName,
                Checkmark = Checkmark
            }

            if Settings.Default and typeof(Settings.Options) ~= "string" then
                if Settings.Multi then
                    if type(Settings.Default) == "number" then
                        Toggle(Settings.Options[Settings.Default], true)
                    else
                        Toggle(V, table.find(Settings.Default, V) and true or false)
                    end
                elseif type(Settings.Default) == "number" then
                    Toggle(Settings.Options[Settings.Default], true)
                end
            end

            DropdownFunctions.Maid["Option" .. V .. "Click"] = Option.MouseButton1Click:Connect(function()
                if DropdownFunctions.IsHidden then return end

                local IsSelected = table.find(DropdownFunctions.Selected, V) and true or false
                local NewSelected = not IsSelected

                if Settings.Required and not NewSelected and #DropdownFunctions.Selected <= 1 then
                    return
                end

                Toggle(V, NewSelected)

                task.spawn(function()
                    if Settings.Callback then
                        if Settings.Multi then
                            Settings.Callback(DropdownFunctions.Selected)
                        else
                            if NewSelected then
                                Settings.Callback(DropdownFunctions.Selected[1] or nil)
                            end
                        end
                    end
                end)
            end)

            if not Settings.PremiumFeature then
                DropdownFunctions.Maid["Option" .. V .. "Hover"] = Option.MouseEnter:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(OptionName, {TextTransparency = 0})
                end)

                DropdownFunctions.Maid["Option" .. V .. "Leave"] = Option.MouseLeave:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(OptionName, {
                        TextTransparency = table.find(DropdownFunctions.Selected, V) and 0 or 0.5
                    })
                end)
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function SetupConnections()
            DropdownFunctions.Maid.DropdownClick = DropdownFunctions.Elements.Interact.MouseButton1Click:Connect(ToggleDropdown)

            if not Settings.PremiumFeature then
                DropdownFunctions.Maid.DropdownHover = DropdownFunctions.Elements.Interact.MouseEnter:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownName, {TextTransparency = 0})
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownImage, {ImageTransparency = 0})
                end)

                DropdownFunctions.Maid.DropdownLeave = DropdownFunctions.Elements.Interact.MouseLeave:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownName, {TextTransparency = 0.5})
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownImage, {ImageTransparency = 0.5})
                end)
            end

            if DropdownFunctions.Elements.SearchBox then
                DropdownFunctions.Maid.SearchTextChanged = DropdownFunctions.Elements.SearchBox:GetPropertyChangedSignal("Text"):Connect(FindOption)
            end

            if DropdownFunctions.Elements.SelectAllButton then
                if not Settings.PremiumFeature then
                    DropdownFunctions.Maid.SelectAllHover = DropdownFunctions.Elements.SelectAllButton.MouseEnter:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.SelectAllButton, {
                            TextTransparency = 0,
                            BackgroundTransparency = 0.9
                        })
                    end)

                    DropdownFunctions.Maid.SelectAllLeave = DropdownFunctions.Elements.SelectAllButton.MouseLeave:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.SelectAllButton, {
                            TextTransparency = 0.3,
                            BackgroundTransparency = 0.95
                        })
                    end)
                end

                DropdownFunctions.Maid.SelectAllClick = DropdownFunctions.Elements.SelectAllButton.MouseButton1Click:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end

                    local SelectedOptions = {}
                    for OptionName, OptionData in pairs(DropdownFunctions.OptionObjs) do
                        if OptionData.Button.Visible then
                            Toggle(OptionName, true)
                            table.insert(SelectedOptions, OptionName)
                        end
                    end

                    task.spawn(function()
                        if Settings.Callback then
                            if Settings.Multi then
                                Settings.Callback(SelectedOptions)
                            else
                                Settings.Callback(SelectedOptions[1] or nil)
                            end
                        end
                    end)
                end)
            end

            if DropdownFunctions.Elements.ClearAllButton then
                if not Settings.PremiumFeature then
                    DropdownFunctions.Maid.ClearAllHover = DropdownFunctions.Elements.ClearAllButton.MouseEnter:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.ClearAllButton, {
                            TextTransparency = 0,
                            BackgroundTransparency = 0.9
                        })
                    end)

                    DropdownFunctions.Maid.ClearAllLeave = DropdownFunctions.Elements.ClearAllButton.MouseLeave:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.ClearAllButton, {
                            TextTransparency = 0.3,
                            BackgroundTransparency = 0.95
                        })
                    end)
                end

                DropdownFunctions.Maid.ClearAllClick = DropdownFunctions.Elements.ClearAllButton.MouseButton1Click:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end
                    if Settings.Required and #DropdownFunctions.Selected <= 1 then return end

                    for OptionName, _ in pairs(DropdownFunctions.OptionObjs) do
                        Toggle(OptionName, false)
                    end

                    task.spawn(function()
                        if Settings.Callback then
                            if Settings.Multi then
                                Settings.Callback({})
                            else
                                Settings.Callback(nil)
                            end
                        end
                    end)
                end)
            end

            if DropdownFunctions.Elements.EditorButton then
                DropdownFunctions.Maid.EditorClick = DropdownFunctions.Elements.EditorButton.MouseButton1Click:Connect(ToggleEditorSettings)
            end

            if DropdownFunctions.Elements.EditorToggles then
                for ToggleName, ToggleData in pairs(DropdownFunctions.Elements.EditorToggles) do
                    if ToggleName ~= "AddBox" and ToggleName ~= "AddButton" and ToggleData.Button then
                        DropdownFunctions.Maid["Editor" .. ToggleName .. "Click"] = ToggleData.Button.MouseButton1Click:Connect(function()
                            HandleEditorToggle(ToggleName)
                        end)
                    end
                end

                if DropdownFunctions.Elements.EditorToggles.AddButton then
                    DropdownFunctions.Maid.AddOptionClick = DropdownFunctions.Elements.EditorToggles.AddButton.MouseButton1Click:Connect(HandleAddOption)
                end

                if DropdownFunctions.Elements.EditorToggles.AddBox then
                    DropdownFunctions.Maid.AddOptionEnter = DropdownFunctions.Elements.EditorToggles.AddBox.FocusLost:Connect(function(EnterPressed)
                        if EnterPressed then HandleAddOption() end
                    end)
                end
            end
        end

        function DropdownFunctions:SetToolTip(Tip)
            if DropdownFunctions.Maid.TooltipShow then
                DropdownFunctions.Maid.TooltipShow:Disconnect()
                DropdownFunctions.Maid.TooltipShow = nil
            end
            if DropdownFunctions.Maid.TooltipHide then
                DropdownFunctions.Maid.TooltipHide:Disconnect()
                DropdownFunctions.Maid.TooltipHide = nil
            end

            if DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame") then
                DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = DropdownFunctions.Elements.Dropdown

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                DropdownFunctions.Elements.Dropdown.ClipsDescendants = false
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function HideTooltip()
                DropdownFunctions.Elements.Dropdown.ClipsDescendants = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            if not DropdownFunctions.IsHidden then
                DropdownFunctions.Maid.TooltipShow = DropdownFunctions.Elements.Interact.MouseEnter:Connect(ShowTooltip)
                DropdownFunctions.Maid.TooltipHide = DropdownFunctions.Elements.Interact.MouseLeave:Connect(HideTooltip)
            end
        end

        function DropdownFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            DropdownFunctions.IsHidden = State
            local PreviousTransparency = 0.5

            if State then
                if not Settings.PremiumFeature then
                    PreviousTransparency = DropdownFunctions.Elements.DropdownName.TextTransparency
                end

                if DropdownFunctions.Dropped then
                    ToggleDropdown()
                end

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownName, TweenInfo.new(0.2), {
                    TextTransparency = Settings.PremiumFeature and DropdownFunctions.Elements.DropdownName.TextTransparency or 0.9
                }):Play()

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()
                Services.TweenService:Create(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2), {BackgroundTransparency = 0.99}):Play()

                local DropdownUIStroke = DropdownFunctions.Elements.Dropdown:FindFirstChild("UIStroke")
                if DropdownUIStroke then
                    Services.TweenService:Create(DropdownUIStroke, TweenInfo.new(0.2), {Transparency = 0.98}):Play()
                end

                if DropdownFunctions.Elements.SearchIcon then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchIcon, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()
                end

                if DropdownFunctions.Elements.SearchBox then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchBox, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                    DropdownFunctions.Elements.SearchBox.TextEditable = false
                end

                if DropdownFunctions.Elements.SelectAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.SelectAllButton, TweenInfo.new(0.2), {
                        TextTransparency = 0.9,
                        BackgroundTransparency = 0.98
                    }):Play()
                end

                if DropdownFunctions.Elements.ClearAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.ClearAllButton, TweenInfo.new(0.2), {
                        TextTransparency = 0.9,
                        BackgroundTransparency = 0.98
                    }):Play()
                end

                if DropdownFunctions.Maid.TooltipShow then
                    DropdownFunctions.Maid.TooltipShow:Disconnect()
                    DropdownFunctions.Maid.TooltipShow = nil
                end
                if DropdownFunctions.Maid.TooltipHide then
                    DropdownFunctions.Maid.TooltipHide:Disconnect()
                    DropdownFunctions.Maid.TooltipHide = nil
                end
            else
                local OriginalImageTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.9 or 0.5
                local OriginalSearchTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownName, TweenInfo.new(0.2), {
                    TextTransparency = Settings.PremiumFeature and DropdownFunctions.Elements.DropdownName.TextTransparency or PreviousTransparency
                }):Play()

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.2), {ImageTransparency = OriginalImageTransparency}):Play()
                Services.TweenService:Create(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2), {BackgroundTransparency = 0.985}):Play()

                local DropdownUIStroke = DropdownFunctions.Elements.Dropdown:FindFirstChild("UIStroke")
                if DropdownUIStroke then
                    Services.TweenService:Create(DropdownUIStroke, TweenInfo.new(0.2), {Transparency = 0.95}):Play()
                end

                if DropdownFunctions.Elements.SearchIcon then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchIcon, TweenInfo.new(0.2), {ImageTransparency = OriginalSearchTransparency}):Play()
                end

                if DropdownFunctions.Elements.SearchBox then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchBox, TweenInfo.new(0.2), {TextTransparency = 0}):Play()
                    DropdownFunctions.Elements.SearchBox.TextEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)
                end

                if DropdownFunctions.Elements.SelectAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.SelectAllButton, TweenInfo.new(0.2), {
                        TextTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0.3,
                        BackgroundTransparency = 0.95
                    }):Play()
                end

                if DropdownFunctions.Elements.ClearAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.ClearAllButton, TweenInfo.new(0.2), {
                        TextTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0.3,
                        BackgroundTransparency = 0.95
                    }):Play()
                end

                local ExistingTooltip = DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame")
                if ExistingTooltip then
                    local TooltipText = ExistingTooltip:FindFirstChild("TooltipText")
                    if TooltipText and TooltipText.Text ~= "" then
                        self:SetToolTip(TooltipText.Text)
                    end
                end
            end
        end

        function DropdownFunctions:GetState()
            return not DropdownFunctions.IsHidden
        end

        function DropdownFunctions:ReturnClass()
            return DropdownFunctions.Class
        end

        function DropdownFunctions:ReturnTab()
            return DropdownFunctions.Tab
        end

        function DropdownFunctions:UpdateName(New)
            DropdownFunctions.Elements.DropdownName.Text = New
        end

        function DropdownFunctions:SetVisibility(State)
            DropdownFunctions.Elements.Dropdown.Visible = State
        end

        function DropdownFunctions:ToggleOption(DfOption, DfToggle)
            if DropdownFunctions.IsHidden then return end

            local SelectedOptions = {}
            for CurrentOption, CurrentSelected in pairs(self:GetOptions()) do
                if CurrentSelected then
                    table.insert(SelectedOptions, CurrentOption)
                end
            end

            if table.find(SelectedOptions, DfOption) and not DfToggle then
                Toggle(DfOption, false)
                local Index = table.find(SelectedOptions, DfOption)
                if Index then table.remove(SelectedOptions, Index) end
            elseif not table.find(SelectedOptions, DfOption) and DfToggle then
                table.insert(SelectedOptions, DfOption)
                Toggle(DfOption, true)
            end

            if Settings.Callback then
                if Settings.Multi then
                    Settings.Callback(SelectedOptions)
                else
                    Settings.Callback(SelectedOptions[1] or nil)
                end
            end
        end

        function DropdownFunctions:UpdateSelection(NewSelection)
            if not NewSelection or DropdownFunctions.IsHidden then return end

            for Option, _ in pairs(DropdownFunctions.OptionObjs) do
                Toggle(Option, false)
            end

            local SelectedOptions = {}

            if type(NewSelection) == "number" then
                for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = Data.Index == NewSelection
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            elseif type(NewSelection) == "string" then
                for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = Option == NewSelection
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            elseif type(NewSelection) == "table" then
                for Option, _ in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = table.find(NewSelection, Option) ~= nil
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            end

            if Settings.Callback then
                if Settings.Multi then
                    Settings.Callback(SelectedOptions)
                else
                    Settings.Callback(SelectedOptions[1] or nil)
                end
            end
        end

        function DropdownFunctions:InsertOptions(NewOptions)
            if not NewOptions or DropdownFunctions.IsHidden then return end

            Settings.Options = NewOptions
            for I, V in pairs(NewOptions) do
                AddOption(I, V)
            end
        end

        function DropdownFunctions:ClearOptions()
            for OptionName, OptionData in pairs(DropdownFunctions.OptionObjs) do
                OptionData.Button:Destroy()
                if DropdownFunctions.Maid["Option" .. OptionName .. "Click"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Click"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Click"] = nil
                end
                if DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Hover"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] = nil
                end
                if DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Leave"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] = nil
                end
            end

            DropdownFunctions.OptionObjs = {}
            DropdownFunctions.Selected = {}

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end

            DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
        end

        function DropdownFunctions:GetOptions()
            local OptionsStatus = {}
            for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                local IsSelected = table.find(DropdownFunctions.Selected, Option) and true or false
                OptionsStatus[Option] = IsSelected
            end
            return OptionsStatus
        end

        function DropdownFunctions:GetSelected()
            local Options = self:GetOptions()
            return Utility:OptionSelected(Options)
        end

        function DropdownFunctions:IsSelected(Desired)
            local Options = self:GetOptions()
            local Selected = Utility:OptionSelected(Options)

            if typeof(Selected) == "string" then
                if Desired == Selected then
                    return true
                end
            elseif typeof(Selected) == "table" then
                for _, Option in pairs(Selected) do
                    if Desired == Option then
                        return true
                    end
                end
            else
                return Selected
            end

            return false
        end

        function DropdownFunctions:RemoveOptions(Remove)
            if not Remove then return end

            for _, OptionName in ipairs(Remove) do
                local OptionData = DropdownFunctions.OptionObjs[OptionName]
                if OptionData then
                    for I = #DropdownFunctions.Selected, 1, -1 do
                        if DropdownFunctions.Selected[I] == OptionName then
                            table.remove(DropdownFunctions.Selected, I)
                        end
                    end

                    OptionData.Button:Destroy()
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Click"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Click"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Click"] = nil
                    end
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Hover"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] = nil
                    end
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Leave"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] = nil
                    end
                    DropdownFunctions.OptionObjs[OptionName] = nil
                end
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end

            if #DropdownFunctions.Selected > 0 then
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "  " .. table.concat(DropdownFunctions.Selected, ", ")
            else
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
            end
        end

        function DropdownFunctions:ReplaceOptions(NewOptions)
            if not NewOptions then return end
            
            self:ClearOptions()
            self:InsertOptions(NewOptions)
        end

        function DropdownFunctions:IsOption(OptionName)
            if not OptionName then return false end
            return DropdownFunctions.OptionObjs[OptionName] ~= nil
        end

        function DropdownFunctions:Destroy()
            for ConnectionName, Connection in pairs(DropdownFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(DropdownFunctions.Maid)

            for _, OptionData in pairs(DropdownFunctions.OptionObjs) do
                if OptionData.Button then
                    OptionData.Button:Destroy()
                end
            end

            if DropdownFunctions.Elements.Dropdown then
                DropdownFunctions.Elements.Dropdown:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        DropdownFunctions.Elements = CreateElements()
        CreateSearchBox()
        CreateEditor()
        CreateDropdownEditor()
        SetupConnections()

        if Settings.Options then
            for I, V in pairs(Settings.Options) do
                AddOption(I, V)
            end
        end

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                DropdownFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                DropdownFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.ToolTip then
            DropdownFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            DropdownFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            DropdownFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            DropdownFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = DropdownFunctions
        end

        return DropdownFunctions
    end
end
]],
  ["UserInterface/Components/Dropdown.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Dropdown(Settings, Flag)
        local DropdownFunctions = {
            Settings = Settings,
            Class = "Dropdown",
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Value = nil,
            IsHidden = false,
            Dropped = false,
            Debounce = false,
            Selected = {},
            OptionObjs = {},
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_SETTINGS = {
            Duration = 0.2,
            EasingStyle = Enum.EasingStyle.Quint,
            TransparencyIn = 0,
            TransparencyOut = 0.5,
            CheckSizeIncrease = 12,
            CheckSizeDecrease = -13
        }

        local function CreateElements()
            local Elements = {}

            Elements.Dropdown = Instance.new("Frame")
            Elements.Dropdown.Name = "Dropdown"
            Elements.Dropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Elements.Dropdown.BackgroundTransparency = 0.985
            Elements.Dropdown.BorderSizePixel = 0
            Elements.Dropdown.Size = UDim2.new(1, 0, 0, 38)
            Elements.Dropdown.ClipsDescendants = true
            Elements.Dropdown.Parent = Section

            local DropdownUIPadding = Instance.new("UIPadding")
            DropdownUIPadding.PaddingLeft = UDim.new(0, 15)
            DropdownUIPadding.PaddingRight = UDim.new(0, 15)
            DropdownUIPadding.Parent = Elements.Dropdown

            local DropdownUIStroke = Instance.new("UIStroke")
            DropdownUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            DropdownUIStroke.Color = Color3.fromRGB(255, 255, 255)
            DropdownUIStroke.Transparency = 0.95
            DropdownUIStroke.Parent = Elements.Dropdown

            local DropdownUICorner = Instance.new("UICorner")
            DropdownUICorner.CornerRadius = UDim.new(0, 6)
            DropdownUICorner.Parent = Elements.Dropdown

            Elements.Interact = Instance.new("TextButton")
            Elements.Interact.Name = "Interact"
            Elements.Interact.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
            Elements.Interact.Text = ""
            Elements.Interact.BackgroundTransparency = 1
            Elements.Interact.BorderSizePixel = 0
            Elements.Interact.Size = UDim2.new(1, 0, 0, 38)
            Elements.Interact.Parent = Elements.Dropdown

            Elements.DropdownName = Instance.new("TextLabel")
            Elements.DropdownName.Name = "DropdownName"
            Elements.DropdownName.FontFace = Font.new(Assets.InterFont)
            Elements.DropdownName.Text = Settings.Default and (Settings.Name .. "  " .. table.concat(DropdownFunctions.Selected, ", ")) or (Settings.Name .. "...")
            Elements.DropdownName.RichText = true
            Elements.DropdownName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.DropdownName.TextSize = 13
            Elements.DropdownName.TextTransparency = 0.5
            Elements.DropdownName.TextTruncate = Enum.TextTruncate.SplitWord
            Elements.DropdownName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.DropdownName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.DropdownName.BackgroundTransparency = 1
            Elements.DropdownName.BorderSizePixel = 0
            Elements.DropdownName.Size = UDim2.new(1, -20, 0, 38)
            Elements.DropdownName.Parent = Elements.Dropdown

            if Settings.PremiumFeature then
                Elements.DropdownName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.DropdownName
            end

            Elements.DropdownImage = Instance.new("ImageLabel")
            Elements.DropdownImage.Name = "DropdownImage"
            Elements.DropdownImage.Image = Assets.PackageClosed
            Elements.DropdownImage.ImageTransparency = 0.5
            Elements.DropdownImage.AnchorPoint = Vector2.new(1, 0)
            Elements.DropdownImage.BackgroundTransparency = 1
            Elements.DropdownImage.BorderSizePixel = 0
            Elements.DropdownImage.Position = UDim2.new(1, 2, 0, 10)
            Elements.DropdownImage.Size = UDim2.fromOffset(18, 18)
            Elements.DropdownImage.Parent = Elements.Dropdown

            if Settings.DropdownEditor then
                Elements.EditorButton = Instance.new("ImageButton")
                Elements.EditorButton.Name = "EditorButton"
                Elements.EditorButton.Image = Assets.Dropdown
                Elements.EditorButton.ImageTransparency = 0.5
                Elements.EditorButton.AnchorPoint = Vector2.new(1, 0)
                Elements.EditorButton.BackgroundTransparency = 1
                Elements.EditorButton.BorderSizePixel = 0
                Elements.EditorButton.Position = UDim2.new(1, -20, 0, 12)
                Elements.EditorButton.Size = UDim2.fromOffset(14, 14)
                Elements.EditorButton.Parent = Elements.Dropdown
            end

            Elements.DropdownFrame = Instance.new("Frame")
            Elements.DropdownFrame.Name = "DropdownFrame"
            Elements.DropdownFrame.BackgroundTransparency = 1
            Elements.DropdownFrame.BorderSizePixel = 0
            Elements.DropdownFrame.ClipsDescendants = true
            Elements.DropdownFrame.Size = UDim2.fromScale(1, 1)
            Elements.DropdownFrame.Visible = false
            Elements.DropdownFrame.AutomaticSize = Enum.AutomaticSize.Y
            Elements.DropdownFrame.Parent = Elements.Dropdown

            local DropdownFrameUIPadding = Instance.new("UIPadding")
            DropdownFrameUIPadding.PaddingTop = UDim.new(0, 38)
            DropdownFrameUIPadding.PaddingBottom = UDim.new(0, 10)
            DropdownFrameUIPadding.Parent = Elements.DropdownFrame

            Elements.DropdownFrameUIListLayout = Instance.new("UIListLayout")
            Elements.DropdownFrameUIListLayout.Padding = UDim.new(0, 5)
            Elements.DropdownFrameUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            Elements.DropdownFrameUIListLayout.Parent = Elements.DropdownFrame

            return Elements
        end

        local function CreateSearchBox()
            if not Settings.Search or Settings.Editor then return end

            DropdownFunctions.Elements.Search = Instance.new("Frame")
            DropdownFunctions.Elements.Search.Name = "Search"
            DropdownFunctions.Elements.Search.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFunctions.Elements.Search.BackgroundTransparency = 0.95
            DropdownFunctions.Elements.Search.BorderSizePixel = 0
            DropdownFunctions.Elements.Search.LayoutOrder = -1
            DropdownFunctions.Elements.Search.Size = UDim2.new(1, 0, 0, 30)
            DropdownFunctions.Elements.Search.Visible = true
            DropdownFunctions.Elements.Search.Parent = DropdownFunctions.Elements.DropdownFrame

            local SectionUICorner = Instance.new("UICorner")
            SectionUICorner.Parent = DropdownFunctions.Elements.Search

            DropdownFunctions.Elements.SearchIcon = Instance.new("ImageLabel")
            DropdownFunctions.Elements.SearchIcon.Name = "SearchIcon"
            DropdownFunctions.Elements.SearchIcon.Image = Assets.SearchIcon
            DropdownFunctions.Elements.SearchIcon.ImageColor3 = Color3.fromRGB(180, 180, 180)
            DropdownFunctions.Elements.SearchIcon.AnchorPoint = Vector2.new(0, 0.5)
            DropdownFunctions.Elements.SearchIcon.BackgroundTransparency = 1
            DropdownFunctions.Elements.SearchIcon.BorderSizePixel = 0
            DropdownFunctions.Elements.SearchIcon.Position = UDim2.fromScale(0, 0.5)
            DropdownFunctions.Elements.SearchIcon.Size = UDim2.fromOffset(12, 12)
            DropdownFunctions.Elements.SearchIcon.Parent = DropdownFunctions.Elements.Search

            if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                DropdownFunctions.Elements.SearchIcon.ImageTransparency = 0.6
            end

            local UIPadding = Instance.new("UIPadding")
            UIPadding.PaddingLeft = UDim.new(0, 15)
            UIPadding.Parent = DropdownFunctions.Elements.Search

            DropdownFunctions.Elements.SearchBox = Instance.new("TextBox")
            DropdownFunctions.Elements.SearchBox.Name = "SearchBox"
            DropdownFunctions.Elements.SearchBox.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium, Enum.FontStyle.Normal)
            DropdownFunctions.Elements.SearchBox.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
            DropdownFunctions.Elements.SearchBox.PlaceholderText = "Search..."
            DropdownFunctions.Elements.SearchBox.Text = ""
            DropdownFunctions.Elements.SearchBox.TextColor3 = Color3.fromRGB(200, 200, 200)
            DropdownFunctions.Elements.SearchBox.TextSize = 14
            DropdownFunctions.Elements.SearchBox.TextXAlignment = Enum.TextXAlignment.Left
            DropdownFunctions.Elements.SearchBox.BackgroundTransparency = 1
            DropdownFunctions.Elements.SearchBox.BorderSizePixel = 0
            DropdownFunctions.Elements.SearchBox.Size = UDim2.fromScale(1, 1)
            DropdownFunctions.Elements.SearchBox.Parent = DropdownFunctions.Elements.Search

            local UIPadding1 = Instance.new("UIPadding")
            UIPadding1.PaddingLeft = UDim.new(0, 23)
            UIPadding1.Parent = DropdownFunctions.Elements.SearchBox
        end

        local function CreateEditor()
            if not Settings.Editor then return end

            DropdownFunctions.Elements.Editor = Instance.new("Frame")
            DropdownFunctions.Elements.Editor.Name = "Editor"
            DropdownFunctions.Elements.Editor.BackgroundTransparency = 1
            DropdownFunctions.Elements.Editor.BorderSizePixel = 0
            DropdownFunctions.Elements.Editor.LayoutOrder = -1
            DropdownFunctions.Elements.Editor.Size = UDim2.new(1, 0, 0, 30)
            DropdownFunctions.Elements.Editor.Visible = true
            DropdownFunctions.Elements.Editor.Parent = DropdownFunctions.Elements.DropdownFrame

            local EditorUIListLayout = Instance.new("UIListLayout")
            EditorUIListLayout.Padding = UDim.new(0, 8)
            EditorUIListLayout.FillDirection = Enum.FillDirection.Horizontal
            EditorUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            EditorUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            EditorUIListLayout.Parent = DropdownFunctions.Elements.Editor

            DropdownFunctions.Elements.SelectAllButton = Instance.new("TextButton")
            DropdownFunctions.Elements.SelectAllButton.Name = "SelectAllButton"
            DropdownFunctions.Elements.SelectAllButton.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium, Enum.FontStyle.Normal)
            DropdownFunctions.Elements.SelectAllButton.Text = "Select All"
            DropdownFunctions.Elements.SelectAllButton.TextColor3 = Color3.fromRGB(180, 180, 180)
            DropdownFunctions.Elements.SelectAllButton.TextSize = 12
            DropdownFunctions.Elements.SelectAllButton.TextXAlignment = Enum.TextXAlignment.Center
            DropdownFunctions.Elements.SelectAllButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFunctions.Elements.SelectAllButton.BackgroundTransparency = 0.95
            DropdownFunctions.Elements.SelectAllButton.BorderSizePixel = 0
            DropdownFunctions.Elements.SelectAllButton.Size = UDim2.new(0.5, -4, 1, 0)
            DropdownFunctions.Elements.SelectAllButton.Parent = DropdownFunctions.Elements.Editor

            local SelectAllCorner = Instance.new("UICorner")
            SelectAllCorner.CornerRadius = UDim.new(0, 4)
            SelectAllCorner.Parent = DropdownFunctions.Elements.SelectAllButton

            DropdownFunctions.Elements.ClearAllButton = Instance.new("TextButton")
            DropdownFunctions.Elements.ClearAllButton.Name = "ClearAllButton"
            DropdownFunctions.Elements.ClearAllButton.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium, Enum.FontStyle.Normal)
            DropdownFunctions.Elements.ClearAllButton.Text = "Clear All"
            DropdownFunctions.Elements.ClearAllButton.TextColor3 = Color3.fromRGB(180, 180, 180)
            DropdownFunctions.Elements.ClearAllButton.TextSize = 12
            DropdownFunctions.Elements.ClearAllButton.TextXAlignment = Enum.TextXAlignment.Center
            DropdownFunctions.Elements.ClearAllButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFunctions.Elements.ClearAllButton.BackgroundTransparency = 0.95
            DropdownFunctions.Elements.ClearAllButton.BorderSizePixel = 0
            DropdownFunctions.Elements.ClearAllButton.Size = UDim2.new(0.5, -4, 1, 0)
            DropdownFunctions.Elements.ClearAllButton.Parent = DropdownFunctions.Elements.Editor

            local ClearAllCorner = Instance.new("UICorner")
            ClearAllCorner.CornerRadius = UDim.new(0, 4)
            ClearAllCorner.Parent = DropdownFunctions.Elements.ClearAllButton

            if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                DropdownFunctions.Elements.SelectAllButton.TextTransparency = 0.6
                DropdownFunctions.Elements.ClearAllButton.TextTransparency = 0.6
            end
        end

        local function CreateDropdownEditor()
            if not Settings.DropdownEditor then return end

            DropdownFunctions.Elements.EditorSettings = Instance.new("Frame")
            DropdownFunctions.Elements.EditorSettings.Name = "EditorSettings"
            DropdownFunctions.Elements.EditorSettings.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFunctions.Elements.EditorSettings.BackgroundTransparency = 0.98
            DropdownFunctions.Elements.EditorSettings.BorderSizePixel = 0
            DropdownFunctions.Elements.EditorSettings.LayoutOrder = -2
            DropdownFunctions.Elements.EditorSettings.Size = UDim2.new(1, 0, 0, 150)
            DropdownFunctions.Elements.EditorSettings.Visible = false
            DropdownFunctions.Elements.EditorSettings.Parent = DropdownFunctions.Elements.DropdownFrame

            local EditorCorner = Instance.new("UICorner")
            EditorCorner.CornerRadius = UDim.new(0, 6)
            EditorCorner.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorStroke = Instance.new("UIStroke")
            EditorStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            EditorStroke.Color = Color3.fromRGB(255, 255, 255)
            EditorStroke.Transparency = 0.95
            EditorStroke.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorPadding = Instance.new("UIPadding")
            EditorPadding.PaddingLeft = UDim.new(0, 15)
            EditorPadding.PaddingRight = UDim.new(0, 15)
            EditorPadding.PaddingTop = UDim.new(0, 10)
            EditorPadding.PaddingBottom = UDim.new(0, 10)
            EditorPadding.Parent = DropdownFunctions.Elements.EditorSettings

            local EditorLayout = Instance.new("UIListLayout")
            EditorLayout.Padding = UDim.new(0, 8)
            EditorLayout.SortOrder = Enum.SortOrder.LayoutOrder
            EditorLayout.Parent = DropdownFunctions.Elements.EditorSettings

            local function CreateToggleOption(Name, DefaultValue)
                local ToggleFrame = Instance.new("Frame")
                ToggleFrame.Name = Name .. "Frame"
                ToggleFrame.BackgroundTransparency = 1
                ToggleFrame.Size = UDim2.new(1, 0, 0, 25)
                ToggleFrame.Parent = DropdownFunctions.Elements.EditorSettings

                local ToggleLabel = Instance.new("TextLabel")
                ToggleLabel.Name = Name .. "Label"
                ToggleLabel.FontFace = Font.new(Assets.InterFont)
                ToggleLabel.Text = Name:gsub("Enable", ""):gsub("(%l)(%u)", "%1 %2")
                ToggleLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
                ToggleLabel.TextSize = 12
                ToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
                ToggleLabel.BackgroundTransparency = 1
                ToggleLabel.Size = UDim2.new(1, -30, 1, 0)
                ToggleLabel.Parent = ToggleFrame

                local ToggleButton = Instance.new("ImageButton")
                ToggleButton.Name = Name .. "Button"
                ToggleButton.Image = Assets.ToggleBackground
                ToggleButton.ImageColor3 = Color3.fromRGB(87, 86, 86)
                ToggleButton.ImageTransparency = DefaultValue and 0 or 0.5
                ToggleButton.AutoButtonColor = false
                ToggleButton.AnchorPoint = Vector2.new(1, 0.5)
                ToggleButton.BackgroundTransparency = 1
                ToggleButton.Position = UDim2.new(1, 0, 0.5, 0)
                ToggleButton.Size = UDim2.fromOffset(25, 13)
                ToggleButton.Parent = ToggleFrame

                local ToggleHead = Instance.new("ImageLabel")
                ToggleHead.Name = Name .. "Head"
                ToggleHead.Image = Assets.TogglerHead
                ToggleHead.ImageColor3 = Color3.fromRGB(255, 255, 255)
                ToggleHead.ImageTransparency = DefaultValue and 0 or 0.85
                ToggleHead.AnchorPoint = Vector2.new(1, 0.5)
                ToggleHead.BackgroundTransparency = 1
                ToggleHead.Position = DefaultValue and UDim2.new(1, 0, 0.5, 0) or UDim2.new(0.5, 0, 0.5, 0)
                ToggleHead.Size = UDim2.fromOffset(9, 9)
                ToggleHead.ZIndex = 2
                ToggleHead.Parent = ToggleButton

                return ToggleFrame, ToggleButton, ToggleHead
            end

            local EnableSearchFrame, EnableSearchButton, EnableSearchHead = CreateToggleOption("EnableSearch", Settings.Search)
            local EnableClearFrame, EnableClearButton, EnableClearHead = CreateToggleOption("EnableClear", Settings.Editor)
            local EnableSelectFrame, EnableSelectButton, EnableSelectHead = CreateToggleOption("EnableSelect", Settings.Editor)

            local AddOptionFrame = Instance.new("Frame")
            AddOptionFrame.Name = "AddOptionFrame"
            AddOptionFrame.BackgroundTransparency = 1
            AddOptionFrame.Size = UDim2.new(1, 0, 0, 25)
            AddOptionFrame.Parent = DropdownFunctions.Elements.EditorSettings

            local AddOptionBox = Instance.new("TextBox")
            AddOptionBox.Name = "AddOptionBox"
            AddOptionBox.FontFace = Font.new(Assets.InterFont)
            AddOptionBox.PlaceholderText = "Add Option..."
            AddOptionBox.Text = ""
            AddOptionBox.TextColor3 = Color3.fromRGB(200, 200, 200)
            AddOptionBox.TextSize = 12
            AddOptionBox.TextXAlignment = Enum.TextXAlignment.Left
            AddOptionBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            AddOptionBox.BackgroundTransparency = 0.95
            AddOptionBox.BorderSizePixel = 0
            AddOptionBox.Size = UDim2.new(1, -30, 1, 0)
            AddOptionBox.Parent = AddOptionFrame

            local AddOptionCorner = Instance.new("UICorner")
            AddOptionCorner.CornerRadius = UDim.new(0, 4)
            AddOptionCorner.Parent = AddOptionBox

            local AddOptionPadding = Instance.new("UIPadding")
            AddOptionPadding.PaddingLeft = UDim.new(0, 8)
            AddOptionPadding.Parent = AddOptionBox

            local AddOptionButton = Instance.new("ImageButton")
            AddOptionButton.Name = "AddOptionButton"
            AddOptionButton.Image = "rbxassetid://7733964046"
            AddOptionButton.ImageColor3 = Color3.fromRGB(180, 180, 180)
            AddOptionButton.ImageTransparency = 0.3
            AddOptionButton.AnchorPoint = Vector2.new(1, 0.5)
            AddOptionButton.BackgroundTransparency = 1
            AddOptionButton.Position = UDim2.new(1, 0, 0.5, 0)
            AddOptionButton.Size = UDim2.fromOffset(20, 20)
            AddOptionButton.Parent = AddOptionFrame

            DropdownFunctions.Elements.EditorToggles = {
                Search = {Button = EnableSearchButton, Head = EnableSearchHead, State = Settings.Search},
                Clear = {Button = EnableClearButton, Head = EnableClearHead, State = Settings.Editor},
                Select = {Button = EnableSelectButton, Head = EnableSelectHead, State = Settings.Editor},
                AddBox = AddOptionBox,
                AddButton = AddOptionButton
            }
        end

        local function CalculateDropdownSize()
            local TotalHeight = 0
            local VisibleChildrenCount = 0
            local Padding = DropdownFunctions.Elements.DropdownFrame:FindFirstChild("UIPadding")
            local PaddingOffset = Padding and (Padding.PaddingTop.Offset + Padding.PaddingBottom.Offset) or 0

            for _, V in pairs(DropdownFunctions.Elements.DropdownFrame:GetChildren()) do
                if not V:IsA("UIComponent") and V.Visible then
                    TotalHeight += V.AbsoluteSize.Y
                    VisibleChildrenCount += 1
                end
            end

            local Spacing = DropdownFunctions.Elements.DropdownFrameUIListLayout.Padding.Offset * math.max(0, VisibleChildrenCount - 1)
            return TotalHeight + Spacing + PaddingOffset
        end

        local function FindOption()
            if not DropdownFunctions.Elements.SearchBox then return end

            local SearchTerm = DropdownFunctions.Elements.SearchBox.Text:lower()
            for _, V in pairs(DropdownFunctions.OptionObjs) do
                local OptionText = V.NameLabel.Text:lower()
                local IsVisible = string.find(OptionText, SearchTerm) ~= nil
                V.Button.Visible = IsVisible
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function Toggle(OptionName, State)
            local Option = DropdownFunctions.OptionObjs[OptionName]
            if not Option then return end

            local Checkmark = Option.Checkmark
            local OptionNameLabel = Option.NameLabel

            if State then
                if Settings.Multi then
                    if not table.find(DropdownFunctions.Selected, OptionName) then
                        table.insert(DropdownFunctions.Selected, OptionName)
                        DropdownFunctions.Value = DropdownFunctions.Selected
                    end
                else
                    for Name, Opt in pairs(DropdownFunctions.OptionObjs) do
                        if Name ~= OptionName then
                            Utility:PropertyTween(Opt.Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                                Size = UDim2.new(Opt.Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeDecrease, Opt.Checkmark.Size.Y.Scale, Opt.Checkmark.Size.Y.Offset)
                            }):Play()
                            Utility:PropertyTween(Opt.NameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                                TextTransparency = TWEEN_SETTINGS.TransparencyOut
                            }):Play()
                            Opt.Checkmark.TextTransparency = 1
                        end
                    end

                    DropdownFunctions.Selected = {OptionName}
                    DropdownFunctions.Value = DropdownFunctions.Selected[1]
                end

                Utility:PropertyTween(Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    Size = UDim2.new(Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeIncrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
                }):Play()
                Utility:PropertyTween(OptionNameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    TextTransparency = TWEEN_SETTINGS.TransparencyIn
                }):Play()
                Checkmark.TextTransparency = 0
            else
                if Settings.Multi then
                    local Idx = table.find(DropdownFunctions.Selected, OptionName)
                    if Idx then table.remove(DropdownFunctions.Selected, Idx) end
                else
                    DropdownFunctions.Selected = {}
                end

                Utility:PropertyTween(Checkmark, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    Size = UDim2.new(Checkmark.Size.X.Scale, TWEEN_SETTINGS.CheckSizeDecrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
                }):Play()
                Utility:PropertyTween(OptionNameLabel, TweenInfo.new(TWEEN_SETTINGS.Duration, TWEEN_SETTINGS.EasingStyle), {
                    TextTransparency = TWEEN_SETTINGS.TransparencyOut
                }):Play()
                Checkmark.TextTransparency = 1
            end

            if Settings.Required and #DropdownFunctions.Selected == 0 and not State then
                return
            end

            if #DropdownFunctions.Selected > 0 then
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "  " .. table.concat(DropdownFunctions.Selected, ", ")
            else
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
            end
        end

        local function ToggleDropdown()
            if DropdownFunctions.IsHidden or DropdownFunctions.Debounce then return end
            if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end

            DropdownFunctions.Debounce = true
            local DefaultDropdownSize = 38
            local IsDropdownOpen = not DropdownFunctions.Dropped
            local TargetSize = IsDropdownOpen and UDim2.new(1, 0, 0, CalculateDropdownSize()) or UDim2.new(1, 0, 0, DefaultDropdownSize)

            local DropTween = Utility:PropertyTween(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
                Size = TargetSize
            })

            Utility:PropertyTween(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.075, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, true), {
                ImageTransparency = 0.8
            }):Play()


            task.delay(0.05, function()
                DropdownFunctions.Elements.DropdownImage.Image = IsDropdownOpen and Assets.PackageOpened or Assets.PackageClosed 
            end)

            DropTween:Play()

            if IsDropdownOpen then
                DropdownFunctions.Elements.DropdownFrame.Visible = true
                DropTween.Completed:Connect(function()
                    DropdownFunctions.Debounce = false
                end)
            else
                DropTween.Completed:Connect(function()
                    DropdownFunctions.Elements.DropdownFrame.Visible = false
                    DropdownFunctions.Debounce = false
                end)
            end

            DropdownFunctions.Dropped = IsDropdownOpen
        end

        local function ToggleEditorSettings()
            if DropdownFunctions.IsHidden or not DropdownFunctions.Elements.EditorSettings then return end

            local IsVisible = DropdownFunctions.Elements.EditorSettings.Visible
            DropdownFunctions.Elements.EditorSettings.Visible = not IsVisible

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function HandleEditorToggle(ToggleName)
            local ToggleData = DropdownFunctions.Elements.EditorToggles[ToggleName]
            if not ToggleData then return end

            ToggleData.State = not ToggleData.State
            local TransparencyValues = ToggleData.State and {0, 0} or {0.5, 0.85}
            local Position = ToggleData.State and UDim2.new(1, 0, 0.5, 0) or UDim2.new(0.5, 0, 0.5, 0)

            Utility:PropertyTween(ToggleData.Button, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
                ImageTransparency = TransparencyValues[1]
            }):Play()

            Utility:PropertyTween(ToggleData.Head, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
                ImageTransparency = TransparencyValues[2],
                Position = Position
            }):Play()

            if ToggleName == "Search" then
                if DropdownFunctions.Elements.Search then
                    DropdownFunctions.Elements.Search.Visible = ToggleData.State
                end
            elseif ToggleName == "Clear" or ToggleName == "Select" then
                if DropdownFunctions.Elements.Editor then
                    local ShouldShow = DropdownFunctions.Elements.EditorToggles.Clear.State or DropdownFunctions.Elements.EditorToggles.Select.State
                    DropdownFunctions.Elements.Editor.Visible = ShouldShow
                    if DropdownFunctions.Elements.SelectAllButton then
                        DropdownFunctions.Elements.SelectAllButton.Visible = DropdownFunctions.Elements.EditorToggles.Select.State
                    end
                    if DropdownFunctions.Elements.ClearAllButton then
                        DropdownFunctions.Elements.ClearAllButton.Visible = DropdownFunctions.Elements.EditorToggles.Clear.State
                    end
                end
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function HandleAddOption()
            local OptionText = DropdownFunctions.Elements.EditorToggles.AddBox.Text:match("^%s*(.-)%s*$")
            if OptionText == "" or DropdownFunctions.OptionObjs[OptionText] then return end

            AddOption(#DropdownFunctions.OptionObjs + 1, OptionText)
            DropdownFunctions.Elements.EditorToggles.AddBox.Text = ""

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function AddOption(I, V, C)
            if typeof(V) == "table" then
                C = V.Color
                V = V.Name
            end

            local Option = Instance.new("TextButton")
            Option.Name = "Option"
            Option.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
            Option.Text = ""
            Option.BackgroundTransparency = 1
            Option.BorderSizePixel = 0
            Option.Size = UDim2.new(1, 0, 0, 30)

            local OptionUIPadding = Instance.new("UIPadding")
            OptionUIPadding.PaddingLeft = UDim.new(0, 15)
            OptionUIPadding.Parent = Option

            local OptionUIListLayout = Instance.new("UIListLayout")
            OptionUIListLayout.Padding = UDim.new(0, 10)
            OptionUIListLayout.FillDirection = Enum.FillDirection.Horizontal
            OptionUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            OptionUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
            OptionUIListLayout.Parent = Option

            local Checkmark = Instance.new("TextLabel")
            Checkmark.Name = "Checkmark"
            Checkmark.FontFace = Font.new(Assets.InterFont)
            Checkmark.Text = ""
            Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
            Checkmark.TextSize = 13
            Checkmark.TextTransparency = 1
            Checkmark.TextXAlignment = Enum.TextXAlignment.Left
            Checkmark.TextYAlignment = Enum.TextYAlignment.Top
            Checkmark.AnchorPoint = Vector2.new(0, 0.5)
            Checkmark.AutomaticSize = Enum.AutomaticSize.Y
            Checkmark.BackgroundTransparency = 1
            Checkmark.BorderSizePixel = 0
            Checkmark.LayoutOrder = -1
            Checkmark.Position = UDim2.fromScale(0, 0.5)
            Checkmark.Size = UDim2.fromOffset(-OptionUIListLayout.Padding.Offset, 0)
            Checkmark.Parent = Option

            local OptionName = Instance.new("TextLabel")
            OptionName.Name = "OptionName"
            OptionName.FontFace = Font.new(Assets.InterFont)
            OptionName.Text = V
            OptionName.RichText = true
            OptionName.TextColor3 = C or Color3.fromRGB(255, 255, 255)
            OptionName.TextSize = 13
            OptionName.TextTransparency = 0.5
            OptionName.TextTruncate = Enum.TextTruncate.AtEnd
            OptionName.TextXAlignment = Enum.TextXAlignment.Left
            OptionName.TextYAlignment = Enum.TextYAlignment.Top
            OptionName.AnchorPoint = Vector2.new(0, 0.5)
            OptionName.AutomaticSize = Enum.AutomaticSize.XY
            OptionName.BackgroundTransparency = 1
            OptionName.BorderSizePixel = 0
            OptionName.Position = UDim2.fromScale(0, 0.5)
            OptionName.Parent = Option

            if C then
                OptionName:SetAttribute("NoApplyTheme", true)
            end

            Option.Parent = DropdownFunctions.Elements.DropdownFrame
            DropdownFunctions.OptionObjs[V] = {
                Index = I,
                Button = Option,
                NameLabel = OptionName,
                Checkmark = Checkmark
            }

            if Settings.Default and typeof(Settings.Options) ~= "string" then
                if Settings.Multi then
                    if type(Settings.Default) == "number" then
                        Toggle(Settings.Options[Settings.Default], true)
                    else
                        Toggle(V, table.find(Settings.Default, V) and true or false)
                    end
                elseif type(Settings.Default) == "number" then
                    Toggle(Settings.Options[Settings.Default], true)
                end
            end

            DropdownFunctions.Maid["Option" .. V .. "Click"] = Option.MouseButton1Click:Connect(function()
                if DropdownFunctions.IsHidden then return end

                local IsSelected = table.find(DropdownFunctions.Selected, V) and true or false
                local NewSelected = not IsSelected

                if Settings.Required and not NewSelected and #DropdownFunctions.Selected <= 1 then
                    return
                end

                Toggle(V, NewSelected)

                task.spawn(function()
                    if Settings.Callback then
                        if Settings.Multi then
                            Settings.Callback(DropdownFunctions.Selected)
                        else
                            if NewSelected then
                                Settings.Callback(DropdownFunctions.Selected[1] or nil)
                            end
                        end
                    end
                end)
            end)

            if not Settings.PremiumFeature then
                DropdownFunctions.Maid["Option" .. V .. "Hover"] = Option.MouseEnter:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(OptionName, {TextTransparency = 0})
                end)

                DropdownFunctions.Maid["Option" .. V .. "Leave"] = Option.MouseLeave:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(OptionName, {
                        TextTransparency = table.find(DropdownFunctions.Selected, V) and 0 or 0.5
                    })
                end)
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end
        end

        local function SetupConnections()
            DropdownFunctions.Maid.DropdownClick = DropdownFunctions.Elements.Interact.MouseButton1Click:Connect(ToggleDropdown)

            if not Settings.PremiumFeature then
                DropdownFunctions.Maid.DropdownHover = DropdownFunctions.Elements.Interact.MouseEnter:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownName, {TextTransparency = 0})
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownImage, {ImageTransparency = 0})
                end)

                DropdownFunctions.Maid.DropdownLeave = DropdownFunctions.Elements.Interact.MouseLeave:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownName, {TextTransparency = 0.5})
                    Utility:QuickTween(DropdownFunctions.Elements.DropdownImage, {ImageTransparency = 0.5})
                end)
            end

            if DropdownFunctions.Elements.SearchBox then
                DropdownFunctions.Maid.SearchTextChanged = DropdownFunctions.Elements.SearchBox:GetPropertyChangedSignal("Text"):Connect(FindOption)
            end

            if DropdownFunctions.Elements.SelectAllButton then
                if not Settings.PremiumFeature then
                    DropdownFunctions.Maid.SelectAllHover = DropdownFunctions.Elements.SelectAllButton.MouseEnter:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.SelectAllButton, {
                            TextTransparency = 0,
                            BackgroundTransparency = 0.9
                        })
                    end)

                    DropdownFunctions.Maid.SelectAllLeave = DropdownFunctions.Elements.SelectAllButton.MouseLeave:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.SelectAllButton, {
                            TextTransparency = 0.3,
                            BackgroundTransparency = 0.95
                        })
                    end)
                end

                DropdownFunctions.Maid.SelectAllClick = DropdownFunctions.Elements.SelectAllButton.MouseButton1Click:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end

                    local SelectedOptions = {}
                    for OptionName, OptionData in pairs(DropdownFunctions.OptionObjs) do
                        if OptionData.Button.Visible then
                            Toggle(OptionName, true)
                            table.insert(SelectedOptions, OptionName)
                        end
                    end

                    task.spawn(function()
                        if Settings.Callback then
                            if Settings.Multi then
                                Settings.Callback(SelectedOptions)
                            else
                                Settings.Callback(SelectedOptions[1] or nil)
                            end
                        end
                    end)
                end)
            end

            if DropdownFunctions.Elements.ClearAllButton then
                if not Settings.PremiumFeature then
                    DropdownFunctions.Maid.ClearAllHover = DropdownFunctions.Elements.ClearAllButton.MouseEnter:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.ClearAllButton, {
                            TextTransparency = 0,
                            BackgroundTransparency = 0.9
                        })
                    end)

                    DropdownFunctions.Maid.ClearAllLeave = DropdownFunctions.Elements.ClearAllButton.MouseLeave:Connect(function()
                        if DropdownFunctions.IsHidden then return end
                        Utility:QuickTween(DropdownFunctions.Elements.ClearAllButton, {
                            TextTransparency = 0.3,
                            BackgroundTransparency = 0.95
                        })
                    end)
                end

                DropdownFunctions.Maid.ClearAllClick = DropdownFunctions.Elements.ClearAllButton.MouseButton1Click:Connect(function()
                    if DropdownFunctions.IsHidden then return end
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then return end
                    if Settings.Required and #DropdownFunctions.Selected <= 1 then return end

                    for OptionName, _ in pairs(DropdownFunctions.OptionObjs) do
                        Toggle(OptionName, false)
                    end

                    task.spawn(function()
                        if Settings.Callback then
                            if Settings.Multi then
                                Settings.Callback({})
                            else
                                Settings.Callback(nil)
                            end
                        end
                    end)
                end)
            end

            if DropdownFunctions.Elements.EditorButton then
                DropdownFunctions.Maid.EditorClick = DropdownFunctions.Elements.EditorButton.MouseButton1Click:Connect(ToggleEditorSettings)
            end

            if DropdownFunctions.Elements.EditorToggles then
                for ToggleName, ToggleData in pairs(DropdownFunctions.Elements.EditorToggles) do
                    if ToggleName ~= "AddBox" and ToggleName ~= "AddButton" and ToggleData.Button then
                        DropdownFunctions.Maid["Editor" .. ToggleName .. "Click"] = ToggleData.Button.MouseButton1Click:Connect(function()
                            HandleEditorToggle(ToggleName)
                        end)
                    end
                end

                if DropdownFunctions.Elements.EditorToggles.AddButton then
                    DropdownFunctions.Maid.AddOptionClick = DropdownFunctions.Elements.EditorToggles.AddButton.MouseButton1Click:Connect(HandleAddOption)
                end

                if DropdownFunctions.Elements.EditorToggles.AddBox then
                    DropdownFunctions.Maid.AddOptionEnter = DropdownFunctions.Elements.EditorToggles.AddBox.FocusLost:Connect(function(EnterPressed)
                        if EnterPressed then HandleAddOption() end
                    end)
                end
            end
        end

        function DropdownFunctions:SetToolTip(Tip)
            if DropdownFunctions.Maid.TooltipShow then
                DropdownFunctions.Maid.TooltipShow:Disconnect()
                DropdownFunctions.Maid.TooltipShow = nil
            end
            if DropdownFunctions.Maid.TooltipHide then
                DropdownFunctions.Maid.TooltipHide:Disconnect()
                DropdownFunctions.Maid.TooltipHide = nil
            end

            if DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame") then
                DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = DropdownFunctions.Elements.Dropdown

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                DropdownFunctions.Elements.Dropdown.ClipsDescendants = false
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function HideTooltip()
                DropdownFunctions.Elements.Dropdown.ClipsDescendants = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            if not DropdownFunctions.IsHidden then
                DropdownFunctions.Maid.TooltipShow = DropdownFunctions.Elements.Interact.MouseEnter:Connect(ShowTooltip)
                DropdownFunctions.Maid.TooltipHide = DropdownFunctions.Elements.Interact.MouseLeave:Connect(HideTooltip)
            end
        end

        function DropdownFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            DropdownFunctions.IsHidden = State
            local PreviousTransparency = 0.5

            if State then
                if not Settings.PremiumFeature then
                    PreviousTransparency = DropdownFunctions.Elements.DropdownName.TextTransparency
                end

                if DropdownFunctions.Dropped then
                    ToggleDropdown()
                end

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownName, TweenInfo.new(0.2), {
                    TextTransparency = Settings.PremiumFeature and DropdownFunctions.Elements.DropdownName.TextTransparency or 0.9
                }):Play()

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()
                Services.TweenService:Create(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2), {BackgroundTransparency = 0.99}):Play()

                local DropdownUIStroke = DropdownFunctions.Elements.Dropdown:FindFirstChild("UIStroke")
                if DropdownUIStroke then
                    Services.TweenService:Create(DropdownUIStroke, TweenInfo.new(0.2), {Transparency = 0.98}):Play()
                end

                if DropdownFunctions.Elements.SearchIcon then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchIcon, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()
                end

                if DropdownFunctions.Elements.SearchBox then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchBox, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                    DropdownFunctions.Elements.SearchBox.TextEditable = false
                end

                if DropdownFunctions.Elements.SelectAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.SelectAllButton, TweenInfo.new(0.2), {
                        TextTransparency = 0.9,
                        BackgroundTransparency = 0.98
                    }):Play()
                end

                if DropdownFunctions.Elements.ClearAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.ClearAllButton, TweenInfo.new(0.2), {
                        TextTransparency = 0.9,
                        BackgroundTransparency = 0.98
                    }):Play()
                end

                if DropdownFunctions.Maid.TooltipShow then
                    DropdownFunctions.Maid.TooltipShow:Disconnect()
                    DropdownFunctions.Maid.TooltipShow = nil
                end
                if DropdownFunctions.Maid.TooltipHide then
                    DropdownFunctions.Maid.TooltipHide:Disconnect()
                    DropdownFunctions.Maid.TooltipHide = nil
                end
            else
                local OriginalImageTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.9 or 0.5
                local OriginalSearchTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownName, TweenInfo.new(0.2), {
                    TextTransparency = Settings.PremiumFeature and DropdownFunctions.Elements.DropdownName.TextTransparency or PreviousTransparency
                }):Play()

                Services.TweenService:Create(DropdownFunctions.Elements.DropdownImage, TweenInfo.new(0.2), {ImageTransparency = OriginalImageTransparency}):Play()
                Services.TweenService:Create(DropdownFunctions.Elements.Dropdown, TweenInfo.new(0.2), {BackgroundTransparency = 0.985}):Play()

                local DropdownUIStroke = DropdownFunctions.Elements.Dropdown:FindFirstChild("UIStroke")
                if DropdownUIStroke then
                    Services.TweenService:Create(DropdownUIStroke, TweenInfo.new(0.2), {Transparency = 0.95}):Play()
                end

                if DropdownFunctions.Elements.SearchIcon then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchIcon, TweenInfo.new(0.2), {ImageTransparency = OriginalSearchTransparency}):Play()
                end

                if DropdownFunctions.Elements.SearchBox then
                    Services.TweenService:Create(DropdownFunctions.Elements.SearchBox, TweenInfo.new(0.2), {TextTransparency = 0}):Play()
                    DropdownFunctions.Elements.SearchBox.TextEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)
                end

                if DropdownFunctions.Elements.SelectAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.SelectAllButton, TweenInfo.new(0.2), {
                        TextTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0.3,
                        BackgroundTransparency = 0.95
                    }):Play()
                end

                if DropdownFunctions.Elements.ClearAllButton then
                    Services.TweenService:Create(DropdownFunctions.Elements.ClearAllButton, TweenInfo.new(0.2), {
                        TextTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0.3,
                        BackgroundTransparency = 0.95
                    }):Play()
                end

                local ExistingTooltip = DropdownFunctions.Elements.Dropdown:FindFirstChild("ToolTipFrame")
                if ExistingTooltip then
                    local TooltipText = ExistingTooltip:FindFirstChild("TooltipText")
                    if TooltipText and TooltipText.Text ~= "" then
                        self:SetToolTip(TooltipText.Text)
                    end
                end
            end
        end

        function DropdownFunctions:GetState()
            return not DropdownFunctions.IsHidden
        end

        function DropdownFunctions:ReturnClass()
            return DropdownFunctions.Class
        end

        function DropdownFunctions:ReturnTab()
            return DropdownFunctions.Tab
        end

        function DropdownFunctions:UpdateName(New)
            DropdownFunctions.Elements.DropdownName.Text = New
        end

        function DropdownFunctions:SetVisibility(State)
            DropdownFunctions.Elements.Dropdown.Visible = State
        end

        function DropdownFunctions:ToggleOption(DfOption, DfToggle)
            if DropdownFunctions.IsHidden then return end

            local SelectedOptions = {}
            for CurrentOption, CurrentSelected in pairs(self:GetOptions()) do
                if CurrentSelected then
                    table.insert(SelectedOptions, CurrentOption)
                end
            end

            if table.find(SelectedOptions, DfOption) and not DfToggle then
                Toggle(DfOption, false)
                local Index = table.find(SelectedOptions, DfOption)
                if Index then table.remove(SelectedOptions, Index) end
            elseif not table.find(SelectedOptions, DfOption) and DfToggle then
                table.insert(SelectedOptions, DfOption)
                Toggle(DfOption, true)
            end

            if Settings.Callback then
                if Settings.Multi then
                    Settings.Callback(SelectedOptions)
                else
                    Settings.Callback(SelectedOptions[1] or nil)
                end
            end
        end

        function DropdownFunctions:UpdateSelection(NewSelection)
            if not NewSelection or DropdownFunctions.IsHidden then return end

            for Option, _ in pairs(DropdownFunctions.OptionObjs) do
                Toggle(Option, false)
            end

            local SelectedOptions = {}

            if type(NewSelection) == "number" then
                for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = Data.Index == NewSelection
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            elseif type(NewSelection) == "string" then
                for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = Option == NewSelection
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            elseif type(NewSelection) == "table" then
                for Option, _ in pairs(DropdownFunctions.OptionObjs) do
                    local IsSelected = table.find(NewSelection, Option) ~= nil
                    if IsSelected then
                        Toggle(Option, true)
                        table.insert(SelectedOptions, Option)
                    end
                end
            end

            if Settings.Callback then
                if Settings.Multi then
                    Settings.Callback(SelectedOptions)
                else
                    Settings.Callback(SelectedOptions[1] or nil)
                end
            end
        end

        function DropdownFunctions:InsertOptions(NewOptions)
            if not NewOptions or DropdownFunctions.IsHidden then return end

            Settings.Options = NewOptions
            for I, V in pairs(NewOptions) do
                AddOption(I, V)
            end
        end

        function DropdownFunctions:ClearOptions()
            for OptionName, OptionData in pairs(DropdownFunctions.OptionObjs) do
                OptionData.Button:Destroy()
                if DropdownFunctions.Maid["Option" .. OptionName .. "Click"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Click"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Click"] = nil
                end
                if DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Hover"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] = nil
                end
                if DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] then
                    DropdownFunctions.Maid["Option" .. OptionName .. "Leave"]:Disconnect()
                    DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] = nil
                end
            end

            DropdownFunctions.OptionObjs = {}
            DropdownFunctions.Selected = {}

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end

            DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
        end

        function DropdownFunctions:GetOptions()
            local OptionsStatus = {}
            for Option, Data in pairs(DropdownFunctions.OptionObjs) do
                local IsSelected = table.find(DropdownFunctions.Selected, Option) and true or false
                OptionsStatus[Option] = IsSelected
            end
            return OptionsStatus
        end

        function DropdownFunctions:GetSelected()
            local Options = self:GetOptions()
            return Utility:OptionSelected(Options)
        end

        function DropdownFunctions:IsSelected(Desired)
            local Options = self:GetOptions()
            local Selected = Utility:OptionSelected(Options)

            if typeof(Selected) == "string" then
                if Desired == Selected then
                    return true
                end
            elseif typeof(Selected) == "table" then
                for _, Option in pairs(Selected) do
                    if Desired == Option then
                        return true
                    end
                end
            else
                return Selected
            end

            return false
        end

        function DropdownFunctions:RemoveOptions(Remove)
            if not Remove then return end

            for _, OptionName in ipairs(Remove) do
                local OptionData = DropdownFunctions.OptionObjs[OptionName]
                if OptionData then
                    for I = #DropdownFunctions.Selected, 1, -1 do
                        if DropdownFunctions.Selected[I] == OptionName then
                            table.remove(DropdownFunctions.Selected, I)
                        end
                    end

                    OptionData.Button:Destroy()
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Click"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Click"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Click"] = nil
                    end
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Hover"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Hover"] = nil
                    end
                    if DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] then
                        DropdownFunctions.Maid["Option" .. OptionName .. "Leave"]:Disconnect()
                        DropdownFunctions.Maid["Option" .. OptionName .. "Leave"] = nil
                    end
                    DropdownFunctions.OptionObjs[OptionName] = nil
                end
            end

            if DropdownFunctions.Dropped then
                DropdownFunctions.Elements.Dropdown.Size = UDim2.new(1, 0, 0, CalculateDropdownSize())
            end

            if #DropdownFunctions.Selected > 0 then
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "  " .. table.concat(DropdownFunctions.Selected, ", ")
            else
                DropdownFunctions.Elements.DropdownName.Text = Settings.Name .. "..."
            end
        end

        function DropdownFunctions:ReplaceOptions(NewOptions)
            if not NewOptions then return end
            
            self:ClearOptions()
            self:InsertOptions(NewOptions)
        end

        function DropdownFunctions:IsOption(OptionName)
            if not OptionName then return false end
            return DropdownFunctions.OptionObjs[OptionName] ~= nil
        end

        function DropdownFunctions:Destroy()
            for ConnectionName, Connection in pairs(DropdownFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(DropdownFunctions.Maid)

            for _, OptionData in pairs(DropdownFunctions.OptionObjs) do
                if OptionData.Button then
                    OptionData.Button:Destroy()
                end
            end

            if DropdownFunctions.Elements.Dropdown then
                DropdownFunctions.Elements.Dropdown:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        DropdownFunctions.Elements = CreateElements()
        CreateSearchBox()
        CreateEditor()
        CreateDropdownEditor()
        SetupConnections()

        if Settings.Options then
            for I, V in pairs(Settings.Options) do
                AddOption(I, V)
            end
        end

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                DropdownFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                DropdownFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.ToolTip then
            DropdownFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            DropdownFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            DropdownFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            DropdownFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = DropdownFunctions
        end

        return DropdownFunctions
    end
end
]],
  ["Xenith/Xenith/UserInterface/Components/Header.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Header(Settings, Flag)
        local HeaderFunctions = { Settings = Settings }

        local Header = Instance.new("Frame")
        Header.Name = "Header"
        Header.AutomaticSize = Enum.AutomaticSize.Y
        Header.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Header.BackgroundTransparency = 1
        Header.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Header.BorderSizePixel = 0
        Header.LayoutOrder = 0
        Header.Size = UDim2.fromScale(1, 0)
        Header.Parent = Section

        local UIPadding = Instance.new("UIPadding")
        UIPadding.Name = "UIPadding"
        UIPadding.PaddingBottom = UDim.new(0, 5)
        UIPadding.Parent = Header

        local HeaderText = Instance.new("TextLabel")
        HeaderText.Name = "HeaderText"
        HeaderText.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium, Enum.FontStyle.Normal)
        HeaderText.RichText = true
        HeaderText.Text = HeaderFunctions.Settings.Text or HeaderFunctions.Settings.Name
        HeaderText.TextColor3 = Color3.fromRGB(255, 255, 255)
        HeaderText.TextSize = 16
        HeaderText.TextTransparency = 0.3
        HeaderText.TextWrapped = true
        HeaderText.TextXAlignment = Enum.TextXAlignment.Left
        HeaderText.AutomaticSize = Enum.AutomaticSize.Y
        HeaderText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        HeaderText.BackgroundTransparency = 1
        HeaderText.BorderColor3 = Color3.fromRGB(0, 0, 0)
        HeaderText.BorderSizePixel = 0
        HeaderText.Size = UDim2.fromScale(1, 0)
        HeaderText.Parent = Header

        function HeaderFunctions:UpdateName(New)
            HeaderText.Text = New
        end

        function HeaderFunctions:SetVisibility(State)
            Header.Visible = State
        end

        if Flag then
            XenithLibrary.Options[Flag] = HeaderFunctions
        end

        return HeaderFunctions
    end
end]],
