  ["Xenith/Xenith/UserInterface/Components/Slider.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Slider(Settings, Flag)
        local SliderFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Class = "Slider",
            Tab = Requirements.TabName,
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(SliderFunctions.Settings.Name, " ", "_")
        end

        local Slider = Instance.new("Frame")
        Slider.Name = "Slider"
        Slider.AutomaticSize = Enum.AutomaticSize.Y
        Slider.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BackgroundTransparency = 1
        Slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BorderSizePixel = 0
        Slider.Size = UDim2.new(1, 0, 0, 38)
        Slider.Parent = Section

        local SliderName = Instance.new("TextLabel")
        SliderName.Name = "SliderName"
        SliderName.FontFace = Font.new(Assets.InterFont)
        SliderName.Text = SliderFunctions.Settings.Name
        SliderName.RichText = true
        SliderName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
        SliderName.TextSize = 13
        SliderName.TextTransparency = 0.5
        SliderName.TextTruncate = Enum.TextTruncate.AtEnd
        SliderName.TextXAlignment = Enum.TextXAlignment.Left
        SliderName.TextYAlignment = Enum.TextYAlignment.Top
        SliderName.AnchorPoint = Vector2.new(0, 0.5)
        SliderName.AutomaticSize = Enum.AutomaticSize.XY
        SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderName.BackgroundTransparency = 1
        SliderName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderName.BorderSizePixel = 0
        SliderName.Position = UDim2.fromScale(1.3e-07, 0.5)
        SliderName.Parent = Slider

        if Settings.PremiumFeature then
            SliderName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = SliderName
        end

        local SliderElements = Instance.new("Frame")
        SliderElements.Name = "SliderElements"
        SliderElements.AnchorPoint = Vector2.new(1, 0)
        SliderElements.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderElements.BackgroundTransparency = 1
        SliderElements.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderElements.BorderSizePixel = 0
        SliderElements.Position = UDim2.fromScale(1, 0)
        SliderElements.Size = UDim2.fromScale(1, 1)

        local SliderValue = Instance.new("TextBox")
        SliderValue.Name = "SliderValue"
        SliderValue.FontFace = Font.new(Assets.InterFont)
        SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.TextSize = 12
        SliderValue.TextTransparency = 0.1

        SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.BackgroundTransparency = 0.95
        SliderValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderValue.BorderSizePixel = 0
        SliderValue.LayoutOrder = 1
        SliderValue.Position = UDim2.fromScale(-0.0789, 0.171)
        SliderValue.Size = UDim2.fromOffset(41, 21)
        SliderValue.ClipsDescendants = true

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderValue.TextEditable = false
            SliderValue.ClearTextOnFocus = false
            SliderValue.TextTransparency = 0.8
        end

        local SliderValueUICorner = Instance.new("UICorner")
        SliderValueUICorner.Name = "SliderValueUICorner"
        SliderValueUICorner.CornerRadius = UDim.new(0, 4)
        SliderValueUICorner.Parent = SliderValue

        local SliderValueUIStroke = Instance.new("UIStroke")
        SliderValueUIStroke.Name = "SliderValueUIStroke"
        SliderValueUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        SliderValueUIStroke.Color = Color3.fromRGB(255, 255, 255)
        SliderValueUIStroke.Transparency = 0.9
        SliderValueUIStroke.Parent = SliderValue

        local SliderValueUIPadding = Instance.new("UIPadding")
        SliderValueUIPadding.Name = "SliderValueUIPadding"
        SliderValueUIPadding.PaddingLeft = UDim.new(0, 2)
        SliderValueUIPadding.PaddingRight = UDim.new(0, 2)
        SliderValueUIPadding.Parent = SliderValue

        SliderValue.Parent = SliderElements

        local SliderElementsUIListLayout = Instance.new("UIListLayout")
        SliderElementsUIListLayout.Name = "SliderElementsUIListLayout"
        SliderElementsUIListLayout.Padding = UDim.new(0, 20)
        SliderElementsUIListLayout.FillDirection = Enum.FillDirection.Horizontal
        SliderElementsUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
        SliderElementsUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        SliderElementsUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
        SliderElementsUIListLayout.Parent = SliderElements

        local SliderBar = Instance.new("ImageLabel")
        SliderBar.Name = "SliderBar"
        SliderBar.Image = Assets.SliderBar
        SliderBar.ImageColor3 = Color3.fromRGB(87, 86, 86)
        SliderBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderBar.BackgroundTransparency = 1
        SliderBar.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderBar.BorderSizePixel = 0
        SliderBar.Position = UDim2.fromScale(0.219, 0.457)
        SliderBar.Size = UDim2.fromOffset(123, 3)

        local SliderHead = Instance.new("ImageButton")
        SliderHead.Name = "SliderHead"
        SliderHead.Image = Assets.SliderHead
        SliderHead.AnchorPoint = Vector2.new(0.5, 0.5)
        SliderHead.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderHead.BackgroundTransparency = 1
        SliderHead.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderHead.BorderSizePixel = 0
        SliderHead.Position = UDim2.fromScale(1, 0.5)
        SliderHead.Size = UDim2.fromOffset(12, 12)
        SliderHead.Parent = SliderBar

        local IsHidden = false
        local SliderConnection = nil
        local PreviousNameTransparency = 0.5
        local PreviousValueTransparency = 0.1
        local Dragging = false

        if not Settings.PremiumFeature then
            SliderHead.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderHead.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)

            SliderValue.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderValue.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderBar.ImageTransparency = 0.6
            SliderHead.ImageTransparency = 0.3
        end

        SliderBar.Parent = SliderElements

        local SliderElementsUIPadding = Instance.new("UIPadding")
        SliderElementsUIPadding.Name = "SliderElementsUIPadding"
        SliderElementsUIPadding.PaddingTop = UDim.new(0, 3)
        SliderElementsUIPadding.Parent = SliderElements

        SliderElements.Parent = Slider

        local DisplayMethods = {
            Thousandths = function(SliderDisplayValue)
                return string.format("%.3f", SliderDisplayValue)
            end,

            Hundredths = function(SliderDisplayValue)
                return string.format("%.2f", SliderDisplayValue)
            end,

            Tenths = function(SliderDisplayValue)
                return string.format("%.1f", SliderDisplayValue)
            end,

            Round = function(SliderDisplayValue, Precision)
                local NumericValue = tonumber(SliderDisplayValue) or 0
                if Precision then
                    return string.format("%." .. Precision .. "f", NumericValue)
                else
                    return tostring(math.round(NumericValue))
                end
            end,

            Degrees = function(SliderDisplayValue, Precision)
                local FormattedValue = Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
                return FormattedValue .. ""
            end,

            Percent = function(SliderDisplayValue, Precision)
                local Percentage = (SliderDisplayValue - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) * 100
                return Precision and string.format("%." .. Precision .. "f", Percentage) .. "%" or tostring(math.round(Percentage)) .. "%"
            end,

            Value = function(SliderDisplayValue, Precision)
                return Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
            end,

            Time24Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                return string.format("%02d:%02d:%02d", Hours, Minutes, Seconds)
            end,

            Time12Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours24 = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                
                local Hours12 = Hours24 % 12
                if Hours12 == 0 then Hours12 = 12 end
                local Period = Hours24 >= 12 and "PM" or "AM"
                
                return string.format("%02d:%02d:%02d %s", Hours12, Minutes, Seconds, Period)
            end
        }

        local ValueDisplayMethod = DisplayMethods[SliderFunctions.Settings.DisplayMethod] or DisplayMethods.Value
        local FinalValue

        local function SetValue(Val, IgnoreCallback)
            local PosXScale

            if typeof(Val) == "Instance" then
                local Input = Val
                PosXScale = math.clamp((Input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
            else
                local Value = Val
                PosXScale = (Value - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - Settings.Minimum)
            end

            local Pos = UDim2.new(PosXScale, 0, 0.5, 0)
            SliderHead.Position = Pos

            FinalValue = PosXScale * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) + Settings.Minimum

            SliderValue.Text = (Settings.Prefix or "") .. ValueDisplayMethod(FinalValue, SliderFunctions.Settings.Precision) .. (Settings.Suffix or "")

            if not IgnoreCallback then
                task.spawn(function()
                    if SliderFunctions.Settings.Callback then
                        SliderFunctions.Settings.Callback(FinalValue)
                    end
                end)
            end

            SliderFunctions.Value = FinalValue
        end

        SetValue(SliderFunctions.Settings.Default, true)

        local function ConnectSliderEvents()
            SliderHead.InputBegan:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end
                    Dragging = true
                    SetValue(Input)
                end
            end)

            SliderHead.InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end

                    Dragging = false
                    if SliderFunctions.Settings.OnInputComplete then
                        SliderFunctions.Settings.OnInputComplete(FinalValue)
                    end
                end
            end)

            SliderValue.FocusLost:Connect(function(EnterPressed)
                if IsHidden then return end

                local InputText = SliderValue.Text
                local Value, IsPercent = InputText:match("^(%-?%d+%.?%d*)(%%?)$")

                if Value then
                    Value = tonumber(Value)
                    IsPercent = IsPercent == "%"

                    if IsPercent then
                        Value = SliderFunctions.Settings.Minimum + (Value / 100) * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum)
                    end

                    local NewValue = math.clamp(Value, SliderFunctions.Settings.Minimum, SliderFunctions.Settings.Maximum)
                    SetValue(NewValue)
                else
                    SliderValue.Text = ValueDisplayMethod(SliderValue)
                end

                if SliderFunctions.Settings.OnInputComplete then
                    SliderFunctions.Settings.OnInputComplete(FinalValue)
                end
            end)

            SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                    SetValue(Input)
                end
            end)
        end

        ConnectSliderEvents()

        local function UpdateSliderBarSize()
            local Padding = SliderElementsUIListLayout.Padding.Offset
            local SliderValueWidth = SliderValue.AbsoluteSize.X
            local SliderNameWidth = SliderName.AbsoluteSize.X
            local TotalWidth = SliderElements.AbsoluteSize.X

            local NewBarWidth = (TotalWidth - (Padding + SliderValueWidth + SliderNameWidth + 20)) / BaseUIScale.Scale
            SliderBar.Size = UDim2.new(SliderBar.Size.X.Scale, NewBarWidth, SliderBar.Size.Y.Scale, SliderBar.Size.Y.Offset)
        end

        UpdateSliderBarSize()

        SliderName:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)
        Section:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)

        function SliderFunctions:SetToolTip(Tip)
            if Slider:FindFirstChild("ToolTipFrame") then
                Slider:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Slider

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            SliderHead.MouseEnter:Connect(ShowTooltip)
            SliderHead.MouseLeave:Connect(HideTooltip)
            SliderValue.MouseEnter:Connect(ShowTooltip)
            SliderValue.MouseLeave:Connect(HideTooltip)
        end

        function SliderFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then

                if not Settings.PremiumFeature then
                    PreviousNameTransparency = SliderName.TextTransparency
                end
                PreviousValueTransparency = SliderValue.TextTransparency

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                SliderValue.TextEditable = false
                SliderHead.AutoButtonColor = false

                if SliderConnection then
                    SliderConnection:Disconnect()
                    SliderConnection = nil
                end

            else

                local originalBarTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0
                local originalHeadTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.3 or 0

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalBarTransparency }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalHeadTransparency }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                local valueTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local valueEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = valueTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                SliderValue.TextEditable = valueEditable
                SliderHead.AutoButtonColor = false

                if not SliderConnection then
                    SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                        if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                            SetValue(Input)
                        end
                    end)
                end
            end
        end

        function SliderFunctions:UpdateName(Name)
            SliderName.Text = Name
        end

        function SliderFunctions:SetVisibility(State)
            Slider.Visible = State
        end

        function SliderFunctions:UpdateValue(Value, IgnoreCallback)
            SetValue(tonumber(Value), IgnoreCallback or false)
        end

        function SliderFunctions:GetValue()
            return FinalValue
        end

        function SliderFunctions:ReturnClass()
            return SliderFunctions.Class
        end

        function SliderFunctions:ReturnTab()
            return SliderFunctions.Tab
        end

        function SliderFunctions:GetState()
            return not IsHidden
        end

        function SliderFunctions:ReturnSettings()
            return Settings
        end

        function SliderFunctions:Destroy()
            if SliderConnection then
                SliderConnection:Disconnect()
            end

            if Slider then
                Slider:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        if SliderFunctions.Settings.Hidden then
            if typeof(SliderFunctions.Settings.Hidden) == "table" then
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden[1], SliderFunctions.Settings.Hidden[2])
            else
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden)
            end
        end

        if SliderFunctions.Settings.ToolTip then
            SliderFunctions:SetToolTip(Settings.ToolTip)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            SliderFunctions:SetHidden(true, "Premium Feature")
        end

        if Flag then
            XenithLibrary.Options[Flag] = SliderFunctions
        end

        return SliderFunctions
    end
end]],
  ["Xenith/UserInterface/Components/Slider.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Slider(Settings, Flag)
        local SliderFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Class = "Slider",
            Tab = Requirements.TabName,
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(SliderFunctions.Settings.Name, " ", "_")
        end

        local Slider = Instance.new("Frame")
        Slider.Name = "Slider"
        Slider.AutomaticSize = Enum.AutomaticSize.Y
        Slider.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BackgroundTransparency = 1
        Slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BorderSizePixel = 0
        Slider.Size = UDim2.new(1, 0, 0, 38)
        Slider.Parent = Section

        local SliderName = Instance.new("TextLabel")
        SliderName.Name = "SliderName"
        SliderName.FontFace = Font.new(Assets.InterFont)
        SliderName.Text = SliderFunctions.Settings.Name
        SliderName.RichText = true
        SliderName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
        SliderName.TextSize = 13
        SliderName.TextTransparency = 0.5
        SliderName.TextTruncate = Enum.TextTruncate.AtEnd
        SliderName.TextXAlignment = Enum.TextXAlignment.Left
        SliderName.TextYAlignment = Enum.TextYAlignment.Top
        SliderName.AnchorPoint = Vector2.new(0, 0.5)
        SliderName.AutomaticSize = Enum.AutomaticSize.XY
        SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderName.BackgroundTransparency = 1
        SliderName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderName.BorderSizePixel = 0
        SliderName.Position = UDim2.fromScale(1.3e-07, 0.5)
        SliderName.Parent = Slider

        if Settings.PremiumFeature then
            SliderName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = SliderName
        end

        local SliderElements = Instance.new("Frame")
        SliderElements.Name = "SliderElements"
        SliderElements.AnchorPoint = Vector2.new(1, 0)
        SliderElements.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderElements.BackgroundTransparency = 1
        SliderElements.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderElements.BorderSizePixel = 0
        SliderElements.Position = UDim2.fromScale(1, 0)
        SliderElements.Size = UDim2.fromScale(1, 1)

        local SliderValue = Instance.new("TextBox")
        SliderValue.Name = "SliderValue"
        SliderValue.FontFace = Font.new(Assets.InterFont)
        SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.TextSize = 12
        SliderValue.TextTransparency = 0.1

        SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.BackgroundTransparency = 0.95
        SliderValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderValue.BorderSizePixel = 0
        SliderValue.LayoutOrder = 1
        SliderValue.Position = UDim2.fromScale(-0.0789, 0.171)
        SliderValue.Size = UDim2.fromOffset(41, 21)
        SliderValue.ClipsDescendants = true

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderValue.TextEditable = false
            SliderValue.ClearTextOnFocus = false
            SliderValue.TextTransparency = 0.8
        end

        local SliderValueUICorner = Instance.new("UICorner")
        SliderValueUICorner.Name = "SliderValueUICorner"
        SliderValueUICorner.CornerRadius = UDim.new(0, 4)
        SliderValueUICorner.Parent = SliderValue

        local SliderValueUIStroke = Instance.new("UIStroke")
        SliderValueUIStroke.Name = "SliderValueUIStroke"
        SliderValueUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        SliderValueUIStroke.Color = Color3.fromRGB(255, 255, 255)
        SliderValueUIStroke.Transparency = 0.9
        SliderValueUIStroke.Parent = SliderValue

        local SliderValueUIPadding = Instance.new("UIPadding")
        SliderValueUIPadding.Name = "SliderValueUIPadding"
        SliderValueUIPadding.PaddingLeft = UDim.new(0, 2)
        SliderValueUIPadding.PaddingRight = UDim.new(0, 2)
        SliderValueUIPadding.Parent = SliderValue

        SliderValue.Parent = SliderElements

        local SliderElementsUIListLayout = Instance.new("UIListLayout")
        SliderElementsUIListLayout.Name = "SliderElementsUIListLayout"
        SliderElementsUIListLayout.Padding = UDim.new(0, 20)
        SliderElementsUIListLayout.FillDirection = Enum.FillDirection.Horizontal
        SliderElementsUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
        SliderElementsUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        SliderElementsUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
        SliderElementsUIListLayout.Parent = SliderElements

        local SliderBar = Instance.new("ImageLabel")
        SliderBar.Name = "SliderBar"
        SliderBar.Image = Assets.SliderBar
        SliderBar.ImageColor3 = Color3.fromRGB(87, 86, 86)
        SliderBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderBar.BackgroundTransparency = 1
        SliderBar.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderBar.BorderSizePixel = 0
        SliderBar.Position = UDim2.fromScale(0.219, 0.457)
        SliderBar.Size = UDim2.fromOffset(123, 3)

        local SliderHead = Instance.new("ImageButton")
        SliderHead.Name = "SliderHead"
        SliderHead.Image = Assets.SliderHead
        SliderHead.AnchorPoint = Vector2.new(0.5, 0.5)
        SliderHead.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderHead.BackgroundTransparency = 1
        SliderHead.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderHead.BorderSizePixel = 0
        SliderHead.Position = UDim2.fromScale(1, 0.5)
        SliderHead.Size = UDim2.fromOffset(12, 12)
        SliderHead.Parent = SliderBar

        local IsHidden = false
        local SliderConnection = nil
        local PreviousNameTransparency = 0.5
        local PreviousValueTransparency = 0.1
        local Dragging = false

        if not Settings.PremiumFeature then
            SliderHead.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderHead.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)

            SliderValue.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderValue.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderBar.ImageTransparency = 0.6
            SliderHead.ImageTransparency = 0.3
        end

        SliderBar.Parent = SliderElements

        local SliderElementsUIPadding = Instance.new("UIPadding")
        SliderElementsUIPadding.Name = "SliderElementsUIPadding"
        SliderElementsUIPadding.PaddingTop = UDim.new(0, 3)
        SliderElementsUIPadding.Parent = SliderElements

        SliderElements.Parent = Slider

        local DisplayMethods = {
            Thousandths = function(SliderDisplayValue)
                return string.format("%.3f", SliderDisplayValue)
            end,

            Hundredths = function(SliderDisplayValue)
                return string.format("%.2f", SliderDisplayValue)
            end,

            Tenths = function(SliderDisplayValue)
                return string.format("%.1f", SliderDisplayValue)
            end,

            Round = function(SliderDisplayValue, Precision)
                local NumericValue = tonumber(SliderDisplayValue) or 0
                if Precision then
                    return string.format("%." .. Precision .. "f", NumericValue)
                else
                    return tostring(math.round(NumericValue))
                end
            end,

            Degrees = function(SliderDisplayValue, Precision)
                local FormattedValue = Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
                return FormattedValue .. ""
            end,

            Percent = function(SliderDisplayValue, Precision)
                local Percentage = (SliderDisplayValue - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) * 100
                return Precision and string.format("%." .. Precision .. "f", Percentage) .. "%" or tostring(math.round(Percentage)) .. "%"
            end,

            Value = function(SliderDisplayValue, Precision)
                return Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
            end,

            Time24Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                return string.format("%02d:%02d:%02d", Hours, Minutes, Seconds)
            end,

            Time12Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours24 = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                
                local Hours12 = Hours24 % 12
                if Hours12 == 0 then Hours12 = 12 end
                local Period = Hours24 >= 12 and "PM" or "AM"
                
                return string.format("%02d:%02d:%02d %s", Hours12, Minutes, Seconds, Period)
            end
        }

        local ValueDisplayMethod = DisplayMethods[SliderFunctions.Settings.DisplayMethod] or DisplayMethods.Value
        local FinalValue

        local function SetValue(Val, IgnoreCallback)
            local PosXScale

            if typeof(Val) == "Instance" then
                local Input = Val
                PosXScale = math.clamp((Input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
            else
                local Value = Val
                PosXScale = (Value - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - Settings.Minimum)
            end

            local Pos = UDim2.new(PosXScale, 0, 0.5, 0)
            SliderHead.Position = Pos

            FinalValue = PosXScale * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) + Settings.Minimum

            SliderValue.Text = (Settings.Prefix or "") .. ValueDisplayMethod(FinalValue, SliderFunctions.Settings.Precision) .. (Settings.Suffix or "")

            if not IgnoreCallback then
                task.spawn(function()
                    if SliderFunctions.Settings.Callback then
                        SliderFunctions.Settings.Callback(FinalValue)
                    end
                end)
            end

            SliderFunctions.Value = FinalValue
        end

        SetValue(SliderFunctions.Settings.Default, true)

        local function ConnectSliderEvents()
            SliderHead.InputBegan:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end
                    Dragging = true
                    SetValue(Input)
                end
            end)

            SliderHead.InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end

                    Dragging = false
                    if SliderFunctions.Settings.OnInputComplete then
                        SliderFunctions.Settings.OnInputComplete(FinalValue)
                    end
                end
            end)

            SliderValue.FocusLost:Connect(function(EnterPressed)
                if IsHidden then return end

                local InputText = SliderValue.Text
                local Value, IsPercent = InputText:match("^(%-?%d+%.?%d*)(%%?)$")

                if Value then
                    Value = tonumber(Value)
                    IsPercent = IsPercent == "%"

                    if IsPercent then
                        Value = SliderFunctions.Settings.Minimum + (Value / 100) * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum)
                    end

                    local NewValue = math.clamp(Value, SliderFunctions.Settings.Minimum, SliderFunctions.Settings.Maximum)
                    SetValue(NewValue)
                else
                    SliderValue.Text = ValueDisplayMethod(SliderValue)
                end

                if SliderFunctions.Settings.OnInputComplete then
                    SliderFunctions.Settings.OnInputComplete(FinalValue)
                end
            end)

            SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                    SetValue(Input)
                end
            end)
        end

        ConnectSliderEvents()

        local function UpdateSliderBarSize()
            local Padding = SliderElementsUIListLayout.Padding.Offset
            local SliderValueWidth = SliderValue.AbsoluteSize.X
            local SliderNameWidth = SliderName.AbsoluteSize.X
            local TotalWidth = SliderElements.AbsoluteSize.X

            local NewBarWidth = (TotalWidth - (Padding + SliderValueWidth + SliderNameWidth + 20)) / BaseUIScale.Scale
            SliderBar.Size = UDim2.new(SliderBar.Size.X.Scale, NewBarWidth, SliderBar.Size.Y.Scale, SliderBar.Size.Y.Offset)
        end

        UpdateSliderBarSize()

        SliderName:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)
        Section:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)

        function SliderFunctions:SetToolTip(Tip)
            if Slider:FindFirstChild("ToolTipFrame") then
                Slider:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Slider

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            SliderHead.MouseEnter:Connect(ShowTooltip)
            SliderHead.MouseLeave:Connect(HideTooltip)
            SliderValue.MouseEnter:Connect(ShowTooltip)
            SliderValue.MouseLeave:Connect(HideTooltip)
        end

        function SliderFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then

                if not Settings.PremiumFeature then
                    PreviousNameTransparency = SliderName.TextTransparency
                end
                PreviousValueTransparency = SliderValue.TextTransparency

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                SliderValue.TextEditable = false
                SliderHead.AutoButtonColor = false

                if SliderConnection then
                    SliderConnection:Disconnect()
                    SliderConnection = nil
                end

            else

                local originalBarTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0
                local originalHeadTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.3 or 0

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalBarTransparency }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalHeadTransparency }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                local valueTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local valueEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = valueTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                SliderValue.TextEditable = valueEditable
                SliderHead.AutoButtonColor = false

                if not SliderConnection then
                    SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                        if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                            SetValue(Input)
                        end
                    end)
                end
            end
        end

        function SliderFunctions:UpdateName(Name)
            SliderName.Text = Name
        end

        function SliderFunctions:SetVisibility(State)
            Slider.Visible = State
        end

        function SliderFunctions:UpdateValue(Value, IgnoreCallback)
            SetValue(tonumber(Value), IgnoreCallback or false)
        end

        function SliderFunctions:GetValue()
            return FinalValue
        end

        function SliderFunctions:ReturnClass()
            return SliderFunctions.Class
        end

        function SliderFunctions:ReturnTab()
            return SliderFunctions.Tab
        end

        function SliderFunctions:GetState()
            return not IsHidden
        end

        function SliderFunctions:ReturnSettings()
            return Settings
        end

        function SliderFunctions:Destroy()
            if SliderConnection then
                SliderConnection:Disconnect()
            end

            if Slider then
                Slider:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        if SliderFunctions.Settings.Hidden then
            if typeof(SliderFunctions.Settings.Hidden) == "table" then
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden[1], SliderFunctions.Settings.Hidden[2])
            else
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden)
            end
        end

        if SliderFunctions.Settings.ToolTip then
            SliderFunctions:SetToolTip(Settings.ToolTip)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            SliderFunctions:SetHidden(true, "Premium Feature")
        end

        if Flag then
            XenithLibrary.Options[Flag] = SliderFunctions
        end

        return SliderFunctions
    end
end]],
  ["UserInterface/Components/Slider.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Slider(Settings, Flag)
        local SliderFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Class = "Slider",
            Tab = Requirements.TabName,
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(SliderFunctions.Settings.Name, " ", "_")
        end

        local Slider = Instance.new("Frame")
        Slider.Name = "Slider"
        Slider.AutomaticSize = Enum.AutomaticSize.Y
        Slider.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BackgroundTransparency = 1
        Slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Slider.BorderSizePixel = 0
        Slider.Size = UDim2.new(1, 0, 0, 38)
        Slider.Parent = Section

        local SliderName = Instance.new("TextLabel")
        SliderName.Name = "SliderName"
        SliderName.FontFace = Font.new(Assets.InterFont)
        SliderName.Text = SliderFunctions.Settings.Name
        SliderName.RichText = true
        SliderName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
        SliderName.TextSize = 13
        SliderName.TextTransparency = 0.5
        SliderName.TextTruncate = Enum.TextTruncate.AtEnd
        SliderName.TextXAlignment = Enum.TextXAlignment.Left
        SliderName.TextYAlignment = Enum.TextYAlignment.Top
        SliderName.AnchorPoint = Vector2.new(0, 0.5)
        SliderName.AutomaticSize = Enum.AutomaticSize.XY
        SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderName.BackgroundTransparency = 1
        SliderName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderName.BorderSizePixel = 0
        SliderName.Position = UDim2.fromScale(1.3e-07, 0.5)
        SliderName.Parent = Slider

        if Settings.PremiumFeature then
            SliderName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = SliderName
        end

        local SliderElements = Instance.new("Frame")
        SliderElements.Name = "SliderElements"
        SliderElements.AnchorPoint = Vector2.new(1, 0)
        SliderElements.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderElements.BackgroundTransparency = 1
        SliderElements.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderElements.BorderSizePixel = 0
        SliderElements.Position = UDim2.fromScale(1, 0)
        SliderElements.Size = UDim2.fromScale(1, 1)

        local SliderValue = Instance.new("TextBox")
        SliderValue.Name = "SliderValue"
        SliderValue.FontFace = Font.new(Assets.InterFont)
        SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.TextSize = 12
        SliderValue.TextTransparency = 0.1

        SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderValue.BackgroundTransparency = 0.95
        SliderValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderValue.BorderSizePixel = 0
        SliderValue.LayoutOrder = 1
        SliderValue.Position = UDim2.fromScale(-0.0789, 0.171)
        SliderValue.Size = UDim2.fromOffset(41, 21)
        SliderValue.ClipsDescendants = true

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderValue.TextEditable = false
            SliderValue.ClearTextOnFocus = false
            SliderValue.TextTransparency = 0.8
        end

        local SliderValueUICorner = Instance.new("UICorner")
        SliderValueUICorner.Name = "SliderValueUICorner"
        SliderValueUICorner.CornerRadius = UDim.new(0, 4)
        SliderValueUICorner.Parent = SliderValue

        local SliderValueUIStroke = Instance.new("UIStroke")
        SliderValueUIStroke.Name = "SliderValueUIStroke"
        SliderValueUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        SliderValueUIStroke.Color = Color3.fromRGB(255, 255, 255)
        SliderValueUIStroke.Transparency = 0.9
        SliderValueUIStroke.Parent = SliderValue

        local SliderValueUIPadding = Instance.new("UIPadding")
        SliderValueUIPadding.Name = "SliderValueUIPadding"
        SliderValueUIPadding.PaddingLeft = UDim.new(0, 2)
        SliderValueUIPadding.PaddingRight = UDim.new(0, 2)
        SliderValueUIPadding.Parent = SliderValue

        SliderValue.Parent = SliderElements

        local SliderElementsUIListLayout = Instance.new("UIListLayout")
        SliderElementsUIListLayout.Name = "SliderElementsUIListLayout"
        SliderElementsUIListLayout.Padding = UDim.new(0, 20)
        SliderElementsUIListLayout.FillDirection = Enum.FillDirection.Horizontal
        SliderElementsUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
        SliderElementsUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
        SliderElementsUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
        SliderElementsUIListLayout.Parent = SliderElements

        local SliderBar = Instance.new("ImageLabel")
        SliderBar.Name = "SliderBar"
        SliderBar.Image = Assets.SliderBar
        SliderBar.ImageColor3 = Color3.fromRGB(87, 86, 86)
        SliderBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderBar.BackgroundTransparency = 1
        SliderBar.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderBar.BorderSizePixel = 0
        SliderBar.Position = UDim2.fromScale(0.219, 0.457)
        SliderBar.Size = UDim2.fromOffset(123, 3)

        local SliderHead = Instance.new("ImageButton")
        SliderHead.Name = "SliderHead"
        SliderHead.Image = Assets.SliderHead
        SliderHead.AnchorPoint = Vector2.new(0.5, 0.5)
        SliderHead.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SliderHead.BackgroundTransparency = 1
        SliderHead.BorderColor3 = Color3.fromRGB(0, 0, 0)
        SliderHead.BorderSizePixel = 0
        SliderHead.Position = UDim2.fromScale(1, 0.5)
        SliderHead.Size = UDim2.fromOffset(12, 12)
        SliderHead.Parent = SliderBar

        local IsHidden = false
        local SliderConnection = nil
        local PreviousNameTransparency = 0.5
        local PreviousValueTransparency = 0.1
        local Dragging = false

        if not Settings.PremiumFeature then
            SliderHead.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderHead.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)

            SliderValue.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = 0
                })
            end)

            SliderValue.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(SliderName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            SliderBar.ImageTransparency = 0.6
            SliderHead.ImageTransparency = 0.3
        end

        SliderBar.Parent = SliderElements

        local SliderElementsUIPadding = Instance.new("UIPadding")
        SliderElementsUIPadding.Name = "SliderElementsUIPadding"
        SliderElementsUIPadding.PaddingTop = UDim.new(0, 3)
        SliderElementsUIPadding.Parent = SliderElements

        SliderElements.Parent = Slider

        local DisplayMethods = {
            Thousandths = function(SliderDisplayValue)
                return string.format("%.3f", SliderDisplayValue)
            end,

            Hundredths = function(SliderDisplayValue)
                return string.format("%.2f", SliderDisplayValue)
            end,

            Tenths = function(SliderDisplayValue)
                return string.format("%.1f", SliderDisplayValue)
            end,

            Round = function(SliderDisplayValue, Precision)
                local NumericValue = tonumber(SliderDisplayValue) or 0
                if Precision then
                    return string.format("%." .. Precision .. "f", NumericValue)
                else
                    return tostring(math.round(NumericValue))
                end
            end,

            Degrees = function(SliderDisplayValue, Precision)
                local FormattedValue = Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
                return FormattedValue .. ""
            end,

            Percent = function(SliderDisplayValue, Precision)
                local Percentage = (SliderDisplayValue - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) * 100
                return Precision and string.format("%." .. Precision .. "f", Percentage) .. "%" or tostring(math.round(Percentage)) .. "%"
            end,

            Value = function(SliderDisplayValue, Precision)
                return Precision and string.format("%." .. Precision .. "f", SliderDisplayValue) or tostring(SliderDisplayValue)
            end,

            Time24Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                return string.format("%02d:%02d:%02d", Hours, Minutes, Seconds)
            end,

            Time12Hour = function(SliderDisplayValue)
                local TotalSeconds = math.floor(SliderDisplayValue)
                local Hours24 = math.floor(TotalSeconds / 3600) % 24
                local Minutes = math.floor((TotalSeconds % 3600) / 60)
                local Seconds = TotalSeconds % 60
                
                local Hours12 = Hours24 % 12
                if Hours12 == 0 then Hours12 = 12 end
                local Period = Hours24 >= 12 and "PM" or "AM"
                
                return string.format("%02d:%02d:%02d %s", Hours12, Minutes, Seconds, Period)
            end
        }

        local ValueDisplayMethod = DisplayMethods[SliderFunctions.Settings.DisplayMethod] or DisplayMethods.Value
        local FinalValue

        local function SetValue(Val, IgnoreCallback)
            local PosXScale

            if typeof(Val) == "Instance" then
                local Input = Val
                PosXScale = math.clamp((Input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
            else
                local Value = Val
                PosXScale = (Value - SliderFunctions.Settings.Minimum) / (SliderFunctions.Settings.Maximum - Settings.Minimum)
            end

            local Pos = UDim2.new(PosXScale, 0, 0.5, 0)
            SliderHead.Position = Pos

            FinalValue = PosXScale * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum) + Settings.Minimum

            SliderValue.Text = (Settings.Prefix or "") .. ValueDisplayMethod(FinalValue, SliderFunctions.Settings.Precision) .. (Settings.Suffix or "")

            if not IgnoreCallback then
                task.spawn(function()
                    if SliderFunctions.Settings.Callback then
                        SliderFunctions.Settings.Callback(FinalValue)
                    end
                end)
            end

            SliderFunctions.Value = FinalValue
        end

        SetValue(SliderFunctions.Settings.Default, true)

        local function ConnectSliderEvents()
            SliderHead.InputBegan:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end
                    Dragging = true
                    SetValue(Input)
                end
            end)

            SliderHead.InputEnded:Connect(function(Input)
                if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
                    if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
                        return
                    end
                    if IsHidden then
                        return
                    end

                    Dragging = false
                    if SliderFunctions.Settings.OnInputComplete then
                        SliderFunctions.Settings.OnInputComplete(FinalValue)
                    end
                end
            end)

            SliderValue.FocusLost:Connect(function(EnterPressed)
                if IsHidden then return end

                local InputText = SliderValue.Text
                local Value, IsPercent = InputText:match("^(%-?%d+%.?%d*)(%%?)$")

                if Value then
                    Value = tonumber(Value)
                    IsPercent = IsPercent == "%"

                    if IsPercent then
                        Value = SliderFunctions.Settings.Minimum + (Value / 100) * (SliderFunctions.Settings.Maximum - SliderFunctions.Settings.Minimum)
                    end

                    local NewValue = math.clamp(Value, SliderFunctions.Settings.Minimum, SliderFunctions.Settings.Maximum)
                    SetValue(NewValue)
                else
                    SliderValue.Text = ValueDisplayMethod(SliderValue)
                end

                if SliderFunctions.Settings.OnInputComplete then
                    SliderFunctions.Settings.OnInputComplete(FinalValue)
                end
            end)

            SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                    SetValue(Input)
                end
            end)
        end

        ConnectSliderEvents()

        local function UpdateSliderBarSize()
            local Padding = SliderElementsUIListLayout.Padding.Offset
            local SliderValueWidth = SliderValue.AbsoluteSize.X
            local SliderNameWidth = SliderName.AbsoluteSize.X
            local TotalWidth = SliderElements.AbsoluteSize.X

            local NewBarWidth = (TotalWidth - (Padding + SliderValueWidth + SliderNameWidth + 20)) / BaseUIScale.Scale
            SliderBar.Size = UDim2.new(SliderBar.Size.X.Scale, NewBarWidth, SliderBar.Size.Y.Scale, SliderBar.Size.Y.Offset)
        end

        UpdateSliderBarSize()

        SliderName:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)
        Section:GetPropertyChangedSignal("AbsoluteSize"):Connect(UpdateSliderBarSize)

        function SliderFunctions:SetToolTip(Tip)
            if Slider:FindFirstChild("ToolTipFrame") then
                Slider:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Slider

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            SliderHead.MouseEnter:Connect(ShowTooltip)
            SliderHead.MouseLeave:Connect(HideTooltip)
            SliderValue.MouseEnter:Connect(ShowTooltip)
            SliderValue.MouseLeave:Connect(HideTooltip)
        end

        function SliderFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then

                if not Settings.PremiumFeature then
                    PreviousNameTransparency = SliderName.TextTransparency
                end
                PreviousValueTransparency = SliderValue.TextTransparency

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                SliderValue.TextEditable = false
                SliderHead.AutoButtonColor = false

                if SliderConnection then
                    SliderConnection:Disconnect()
                    SliderConnection = nil
                end

            else

                local originalBarTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.6 or 0
                local originalHeadTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.3 or 0

                Services.TweenService:Create(
                    SliderBar,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalBarTransparency }
                ):Play()

                Services.TweenService:Create(
                    SliderHead,
                    TweenInfo.new(0.2),
                    { ImageTransparency = originalHeadTransparency }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        SliderName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                local valueTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local valueEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    SliderValue,
                    TweenInfo.new(0.2),
                    { TextTransparency = valueTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    SliderValueUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                SliderValue.TextEditable = valueEditable
                SliderHead.AutoButtonColor = false

                if not SliderConnection then
                    SliderConnection = Services.UserInputService.InputChanged:Connect(function(Input)
                        if Dragging and not IsHidden and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
                            SetValue(Input)
                        end
                    end)
                end
            end
        end

        function SliderFunctions:UpdateName(Name)
            SliderName.Text = Name
        end

        function SliderFunctions:SetVisibility(State)
            Slider.Visible = State
        end

        function SliderFunctions:UpdateValue(Value, IgnoreCallback)
            SetValue(tonumber(Value), IgnoreCallback or false)
        end

        function SliderFunctions:GetValue()
            return FinalValue
        end

        function SliderFunctions:ReturnClass()
            return SliderFunctions.Class
        end

        function SliderFunctions:ReturnTab()
            return SliderFunctions.Tab
        end

        function SliderFunctions:GetState()
            return not IsHidden
        end

        function SliderFunctions:ReturnSettings()
            return Settings
        end

        function SliderFunctions:Destroy()
            if SliderConnection then
                SliderConnection:Disconnect()
            end

            if Slider then
                Slider:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        if SliderFunctions.Settings.Hidden then
            if typeof(SliderFunctions.Settings.Hidden) == "table" then
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden[1], SliderFunctions.Settings.Hidden[2])
            else
                SliderFunctions:SetHidden(SliderFunctions.Settings.Hidden)
            end
        end

        if SliderFunctions.Settings.ToolTip then
            SliderFunctions:SetToolTip(Settings.ToolTip)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            SliderFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            SliderFunctions:SetHidden(true, "Premium Feature")
        end

        if Flag then
            XenithLibrary.Options[Flag] = SliderFunctions
        end

        return SliderFunctions
    end
end]],
  ["Xenith/Xenith/UserInterface/Components/Toggle.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Toggle(Settings, Flag)
        local ToggleFunctions = {
            Settings = Settings,
            Tab = Requirements.TabName,
            IgnoreConfig = Settings.IgnoreConfig,
            Class = "Toggle",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Enabled = {Toggle = 0.5, Head = 0},
            Disabled = {Toggle = 0.8, Head = 0.85}
        }

        local POSITIONS = {
            Enabled = UDim2.new(1, 0, 0.5, 0),
            Disabled = UDim2.new(0.5, 0, 0.5, 0)
        }

        local function CreateElements()
            local Elements = {}

            Elements.Toggle = Instance.new("Frame")
            Elements.Toggle.Name = "Toggle"
            Elements.Toggle.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Toggle.BackgroundTransparency = 1
            Elements.Toggle.BorderSizePixel = 0
            Elements.Toggle.Size = UDim2.new(1, 0, 0, 38)
            Elements.Toggle.Parent = Section

            Elements.ToggleName = Instance.new("TextLabel")
            Elements.ToggleName.Name = "ToggleName"
            Elements.ToggleName.FontFace = Font.new(Assets.InterFont)
            Elements.ToggleName.Text = Settings.Name
            Elements.ToggleName.RichText = true
            Elements.ToggleName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.ToggleName.TextSize = 13
            Elements.ToggleName.TextTransparency = 0.5
            Elements.ToggleName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ToggleName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ToggleName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ToggleName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ToggleName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ToggleName.BackgroundTransparency = 1
            Elements.ToggleName.Position = UDim2.fromScale(0, 0.5)
            Elements.ToggleName.Size = UDim2.new(1, -50, 0, 0)
            Elements.ToggleName.Parent = Elements.Toggle

            if Settings.PremiumFeature then
                Elements.ToggleName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ToggleName
            end

            Elements.ToggleButton = Instance.new("ImageButton")
            Elements.ToggleButton.Name = "ToggleButton"
            Elements.ToggleButton.Image = Assets.ToggleBackground
            Elements.ToggleButton.ImageColor3 = Color3.fromRGB(87, 86, 86)
            Elements.ToggleButton.AutoButtonColor = false
            Elements.ToggleButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ToggleButton.BackgroundTransparency = 1
            Elements.ToggleButton.BorderSizePixel = 0
            Elements.ToggleButton.Position = UDim2.fromScale(1, 0.5)
            Elements.ToggleButton.Size = UDim2.fromOffset(41, 21)
            Elements.ToggleButton.ImageTransparency = 0.8
            Elements.ToggleButton.Parent = Elements.Toggle

            local ToggleUIPadding = Instance.new("UIPadding")
            ToggleUIPadding.PaddingBottom = UDim.new(0, 1)
            ToggleUIPadding.PaddingLeft = UDim.new(0, -2)
            ToggleUIPadding.PaddingRight = UDim.new(0, 3)
            ToggleUIPadding.PaddingTop = UDim.new(0, 1)
            ToggleUIPadding.Parent = Elements.ToggleButton

            Elements.TogglerHead = Instance.new("ImageLabel")
            Elements.TogglerHead.Name = "TogglerHead"
            Elements.TogglerHead.Image = Assets.TogglerHead
            Elements.TogglerHead.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Elements.TogglerHead.AnchorPoint = Vector2.new(1, 0.5)
            Elements.TogglerHead.BackgroundTransparency = 1
            Elements.TogglerHead.BorderSizePixel = 0
            Elements.TogglerHead.Position = UDim2.fromScale(0.5, 0.5)
            Elements.TogglerHead.Size = UDim2.fromOffset(15, 15)
            Elements.TogglerHead.ZIndex = 2
            Elements.TogglerHead.ImageTransparency = 0.8
            Elements.TogglerHead.Parent = Elements.ToggleButton

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ToggleFunctions.Elements.Toggle,
                Class = ToggleFunctions.Class,
                Maid = ToggleFunctions.Maid,
                Functions = ToggleFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ToggleFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ToggleFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function UpdateToggleState(State)
            local TransparencyValues = State and TRANSPARENCY.Enabled or TRANSPARENCY.Disabled
            local Position = State and POSITIONS.Enabled or POSITIONS.Disabled

            Utility:PropertyTween(ToggleFunctions.Elements.ToggleButton, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Toggle
            }):Play()

            Utility:PropertyTween(ToggleFunctions.Elements.TogglerHead, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Head,
                Position = Position
            }):Play()

            ToggleFunctions.State = State
        end

        local function ToggleTrigger()
            if Settings.Fixed or ToggleFunctions.IsHidden then return end

            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)

            if Settings.Callback then
                if Settings.ToggleOffActive then
                    for _, SignalToDisable in pairs(Settings.ToggleOffActive) do
                    end
                end
                Settings.Callback(ToggleFunctions.State)
            end
        end

        local function SetupConnections()
            ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency

                ToggleFunctions.Maid.ToggleHover = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = 0})
                end)

                ToggleFunctions.Maid.ToggleLeave = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ToggleFunctions:Toggle()
            ToggleTrigger()
        end

        function ToggleFunctions:ServerToggle()
            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)
            if Settings.Callback then
                Settings.Callback(ToggleFunctions.State)
            end
        end

        function ToggleFunctions:UpdateState(State, Callback)
            ToggleFunctions.State = State
            UpdateToggleState(ToggleFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(ToggleFunctions.State)
                end
            end
        end

        function ToggleFunctions:GetState()
            return ToggleFunctions.State
        end

        function ToggleFunctions:ReturnHidden()
            return ToggleFunctions.IsHidden
        end

        function ToggleFunctions:ReturnSettings()
            return Settings
        end

        function ToggleFunctions:ReturnClass()
            return ToggleFunctions.Class
        end

        function ToggleFunctions:ReturnTab()
            return ToggleFunctions.Tab
        end

        function ToggleFunctions:UpdateName(Name)
            ToggleFunctions.Elements.ToggleName.Text = Name
        end

        function ToggleFunctions:SetToolTip(Tip)
            if ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame") then
                ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ToggleFunctions.Elements.Toggle

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            ToggleFunctions.Maid.TooltipShow = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            ToggleFunctions.Maid.TooltipHide = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function ToggleFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ToggleFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ToggleFunctions.Elements.KeybindButton,
                KeybindLabel = ToggleFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if ToggleFunctions.Maid.ToggleClick then
                    ToggleFunctions.Maid.ToggleClick:Disconnect()
                    ToggleFunctions.Maid.ToggleClick = nil
                end
            else
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.5}):Play()
                
                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)
            end
        end

        function ToggleFunctions:SetVisibility(State)
            ToggleFunctions.Elements.Toggle.Visible = State
        end

        function ToggleFunctions:Destroy()
            for ConnectionName, Connection in pairs(ToggleFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            
            table.clear(ToggleFunctions.Maid)

            if ToggleFunctions.Elements.Toggle then
                ToggleFunctions.Elements.Toggle:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ToggleFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()
        UpdateToggleState(ToggleFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ToggleFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ToggleFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and not Settings.IgnoreDefault and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            ToggleFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ToggleFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ToggleFunctions
        end

        return ToggleFunctions
    end
end]],
  ["Xenith/UserInterface/Components/Toggle.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Toggle(Settings, Flag)
        local ToggleFunctions = {
            Settings = Settings,
            Tab = Requirements.TabName,
            IgnoreConfig = Settings.IgnoreConfig,
            Class = "Toggle",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Enabled = {Toggle = 0.5, Head = 0},
            Disabled = {Toggle = 0.8, Head = 0.85}
        }

        local POSITIONS = {
            Enabled = UDim2.new(1, 0, 0.5, 0),
            Disabled = UDim2.new(0.5, 0, 0.5, 0)
        }

        local function CreateElements()
            local Elements = {}

            Elements.Toggle = Instance.new("Frame")
            Elements.Toggle.Name = "Toggle"
            Elements.Toggle.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Toggle.BackgroundTransparency = 1
            Elements.Toggle.BorderSizePixel = 0
            Elements.Toggle.Size = UDim2.new(1, 0, 0, 38)
            Elements.Toggle.Parent = Section

            Elements.ToggleName = Instance.new("TextLabel")
            Elements.ToggleName.Name = "ToggleName"
            Elements.ToggleName.FontFace = Font.new(Assets.InterFont)
            Elements.ToggleName.Text = Settings.Name
            Elements.ToggleName.RichText = true
            Elements.ToggleName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.ToggleName.TextSize = 13
            Elements.ToggleName.TextTransparency = 0.5
            Elements.ToggleName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ToggleName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ToggleName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ToggleName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ToggleName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ToggleName.BackgroundTransparency = 1
            Elements.ToggleName.Position = UDim2.fromScale(0, 0.5)
            Elements.ToggleName.Size = UDim2.new(1, -50, 0, 0)
            Elements.ToggleName.Parent = Elements.Toggle

            if Settings.PremiumFeature then
                Elements.ToggleName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ToggleName
            end

            Elements.ToggleButton = Instance.new("ImageButton")
            Elements.ToggleButton.Name = "ToggleButton"
            Elements.ToggleButton.Image = Assets.ToggleBackground
            Elements.ToggleButton.ImageColor3 = Color3.fromRGB(87, 86, 86)
            Elements.ToggleButton.AutoButtonColor = false
            Elements.ToggleButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ToggleButton.BackgroundTransparency = 1
            Elements.ToggleButton.BorderSizePixel = 0
            Elements.ToggleButton.Position = UDim2.fromScale(1, 0.5)
            Elements.ToggleButton.Size = UDim2.fromOffset(41, 21)
            Elements.ToggleButton.ImageTransparency = 0.8
            Elements.ToggleButton.Parent = Elements.Toggle

            local ToggleUIPadding = Instance.new("UIPadding")
            ToggleUIPadding.PaddingBottom = UDim.new(0, 1)
            ToggleUIPadding.PaddingLeft = UDim.new(0, -2)
            ToggleUIPadding.PaddingRight = UDim.new(0, 3)
            ToggleUIPadding.PaddingTop = UDim.new(0, 1)
            ToggleUIPadding.Parent = Elements.ToggleButton

            Elements.TogglerHead = Instance.new("ImageLabel")
            Elements.TogglerHead.Name = "TogglerHead"
            Elements.TogglerHead.Image = Assets.TogglerHead
            Elements.TogglerHead.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Elements.TogglerHead.AnchorPoint = Vector2.new(1, 0.5)
            Elements.TogglerHead.BackgroundTransparency = 1
            Elements.TogglerHead.BorderSizePixel = 0
            Elements.TogglerHead.Position = UDim2.fromScale(0.5, 0.5)
            Elements.TogglerHead.Size = UDim2.fromOffset(15, 15)
            Elements.TogglerHead.ZIndex = 2
            Elements.TogglerHead.ImageTransparency = 0.8
            Elements.TogglerHead.Parent = Elements.ToggleButton

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ToggleFunctions.Elements.Toggle,
                Class = ToggleFunctions.Class,
                Maid = ToggleFunctions.Maid,
                Functions = ToggleFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ToggleFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ToggleFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function UpdateToggleState(State)
            local TransparencyValues = State and TRANSPARENCY.Enabled or TRANSPARENCY.Disabled
            local Position = State and POSITIONS.Enabled or POSITIONS.Disabled

            Utility:PropertyTween(ToggleFunctions.Elements.ToggleButton, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Toggle
            }):Play()

            Utility:PropertyTween(ToggleFunctions.Elements.TogglerHead, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Head,
                Position = Position
            }):Play()

            ToggleFunctions.State = State
        end

        local function ToggleTrigger()
            if Settings.Fixed or ToggleFunctions.IsHidden then return end

            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)

            if Settings.Callback then
                if Settings.ToggleOffActive then
                    for _, SignalToDisable in pairs(Settings.ToggleOffActive) do
                    end
                end
                Settings.Callback(ToggleFunctions.State)
            end
        end

        local function SetupConnections()
            ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency

                ToggleFunctions.Maid.ToggleHover = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = 0})
                end)

                ToggleFunctions.Maid.ToggleLeave = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ToggleFunctions:Toggle()
            ToggleTrigger()
        end

        function ToggleFunctions:ServerToggle()
            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)
            if Settings.Callback then
                Settings.Callback(ToggleFunctions.State)
            end
        end

        function ToggleFunctions:UpdateState(State, Callback)
            ToggleFunctions.State = State
            UpdateToggleState(ToggleFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(ToggleFunctions.State)
                end
            end
        end

        function ToggleFunctions:GetState()
            return ToggleFunctions.State
        end

        function ToggleFunctions:ReturnHidden()
            return ToggleFunctions.IsHidden
        end

        function ToggleFunctions:ReturnSettings()
            return Settings
        end

        function ToggleFunctions:ReturnClass()
            return ToggleFunctions.Class
        end

        function ToggleFunctions:ReturnTab()
            return ToggleFunctions.Tab
        end

        function ToggleFunctions:UpdateName(Name)
            ToggleFunctions.Elements.ToggleName.Text = Name
        end

        function ToggleFunctions:SetToolTip(Tip)
            if ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame") then
                ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ToggleFunctions.Elements.Toggle

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            ToggleFunctions.Maid.TooltipShow = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            ToggleFunctions.Maid.TooltipHide = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function ToggleFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ToggleFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ToggleFunctions.Elements.KeybindButton,
                KeybindLabel = ToggleFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if ToggleFunctions.Maid.ToggleClick then
                    ToggleFunctions.Maid.ToggleClick:Disconnect()
                    ToggleFunctions.Maid.ToggleClick = nil
                end
            else
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.5}):Play()
                
                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)
            end
        end

        function ToggleFunctions:SetVisibility(State)
            ToggleFunctions.Elements.Toggle.Visible = State
        end

        function ToggleFunctions:Destroy()
            for ConnectionName, Connection in pairs(ToggleFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            
            table.clear(ToggleFunctions.Maid)

            if ToggleFunctions.Elements.Toggle then
                ToggleFunctions.Elements.Toggle:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ToggleFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()
        UpdateToggleState(ToggleFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ToggleFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ToggleFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and not Settings.IgnoreDefault and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            ToggleFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ToggleFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ToggleFunctions
        end

        return ToggleFunctions
    end
end]],
  ["UserInterface/Components/Toggle.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Toggle(Settings, Flag)
        local ToggleFunctions = {
            Settings = Settings,
            Tab = Requirements.TabName,
            IgnoreConfig = Settings.IgnoreConfig,
            Class = "Toggle",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Enabled = {Toggle = 0.5, Head = 0},
            Disabled = {Toggle = 0.8, Head = 0.85}
        }

        local POSITIONS = {
            Enabled = UDim2.new(1, 0, 0.5, 0),
            Disabled = UDim2.new(0.5, 0, 0.5, 0)
        }

        local function CreateElements()
            local Elements = {}

            Elements.Toggle = Instance.new("Frame")
            Elements.Toggle.Name = "Toggle"
            Elements.Toggle.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Toggle.BackgroundTransparency = 1
            Elements.Toggle.BorderSizePixel = 0
            Elements.Toggle.Size = UDim2.new(1, 0, 0, 38)
            Elements.Toggle.Parent = Section

            Elements.ToggleName = Instance.new("TextLabel")
            Elements.ToggleName.Name = "ToggleName"
            Elements.ToggleName.FontFace = Font.new(Assets.InterFont)
            Elements.ToggleName.Text = Settings.Name
            Elements.ToggleName.RichText = true
            Elements.ToggleName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.ToggleName.TextSize = 13
            Elements.ToggleName.TextTransparency = 0.5
            Elements.ToggleName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ToggleName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ToggleName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ToggleName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ToggleName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ToggleName.BackgroundTransparency = 1
            Elements.ToggleName.Position = UDim2.fromScale(0, 0.5)
            Elements.ToggleName.Size = UDim2.new(1, -50, 0, 0)
            Elements.ToggleName.Parent = Elements.Toggle

            if Settings.PremiumFeature then
                Elements.ToggleName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ToggleName
            end

            Elements.ToggleButton = Instance.new("ImageButton")
            Elements.ToggleButton.Name = "ToggleButton"
            Elements.ToggleButton.Image = Assets.ToggleBackground
            Elements.ToggleButton.ImageColor3 = Color3.fromRGB(87, 86, 86)
            Elements.ToggleButton.AutoButtonColor = false
            Elements.ToggleButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ToggleButton.BackgroundTransparency = 1
            Elements.ToggleButton.BorderSizePixel = 0
            Elements.ToggleButton.Position = UDim2.fromScale(1, 0.5)
            Elements.ToggleButton.Size = UDim2.fromOffset(41, 21)
            Elements.ToggleButton.ImageTransparency = 0.8
            Elements.ToggleButton.Parent = Elements.Toggle

            local ToggleUIPadding = Instance.new("UIPadding")
            ToggleUIPadding.PaddingBottom = UDim.new(0, 1)
            ToggleUIPadding.PaddingLeft = UDim.new(0, -2)
            ToggleUIPadding.PaddingRight = UDim.new(0, 3)
            ToggleUIPadding.PaddingTop = UDim.new(0, 1)
            ToggleUIPadding.Parent = Elements.ToggleButton

            Elements.TogglerHead = Instance.new("ImageLabel")
            Elements.TogglerHead.Name = "TogglerHead"
            Elements.TogglerHead.Image = Assets.TogglerHead
            Elements.TogglerHead.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Elements.TogglerHead.AnchorPoint = Vector2.new(1, 0.5)
            Elements.TogglerHead.BackgroundTransparency = 1
            Elements.TogglerHead.BorderSizePixel = 0
            Elements.TogglerHead.Position = UDim2.fromScale(0.5, 0.5)
            Elements.TogglerHead.Size = UDim2.fromOffset(15, 15)
            Elements.TogglerHead.ZIndex = 2
            Elements.TogglerHead.ImageTransparency = 0.8
            Elements.TogglerHead.Parent = Elements.ToggleButton

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ToggleFunctions.Elements.Toggle,
                Class = ToggleFunctions.Class,
                Maid = ToggleFunctions.Maid,
                Functions = ToggleFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ToggleFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ToggleFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function UpdateToggleState(State)
            local TransparencyValues = State and TRANSPARENCY.Enabled or TRANSPARENCY.Disabled
            local Position = State and POSITIONS.Enabled or POSITIONS.Disabled

            Utility:PropertyTween(ToggleFunctions.Elements.ToggleButton, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Toggle
            }):Play()

            Utility:PropertyTween(ToggleFunctions.Elements.TogglerHead, TWEEN_INFO, {
                ImageTransparency = TransparencyValues.Head,
                Position = Position
            }):Play()

            ToggleFunctions.State = State
        end

        local function ToggleTrigger()
            if Settings.Fixed or ToggleFunctions.IsHidden then return end

            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)

            if Settings.Callback then
                if Settings.ToggleOffActive then
                    for _, SignalToDisable in pairs(Settings.ToggleOffActive) do
                    end
                end
                Settings.Callback(ToggleFunctions.State)
            end
        end

        local function SetupConnections()
            ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency

                ToggleFunctions.Maid.ToggleHover = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    PreviousTransparency = ToggleFunctions.Elements.ToggleName.TextTransparency
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = 0})
                end)

                ToggleFunctions.Maid.ToggleLeave = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                    if ToggleFunctions.IsHidden then return end
                    Utility:QuickTween(ToggleFunctions.Elements.ToggleName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ToggleFunctions:Toggle()
            ToggleTrigger()
        end

        function ToggleFunctions:ServerToggle()
            ToggleFunctions.State = not ToggleFunctions.State
            UpdateToggleState(ToggleFunctions.State)
            if Settings.Callback then
                Settings.Callback(ToggleFunctions.State)
            end
        end

        function ToggleFunctions:UpdateState(State, Callback)
            ToggleFunctions.State = State
            UpdateToggleState(ToggleFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(ToggleFunctions.State)
                end
            end
        end

        function ToggleFunctions:GetState()
            return ToggleFunctions.State
        end

        function ToggleFunctions:ReturnHidden()
            return ToggleFunctions.IsHidden
        end

        function ToggleFunctions:ReturnSettings()
            return Settings
        end

        function ToggleFunctions:ReturnClass()
            return ToggleFunctions.Class
        end

        function ToggleFunctions:ReturnTab()
            return ToggleFunctions.Tab
        end

        function ToggleFunctions:UpdateName(Name)
            ToggleFunctions.Elements.ToggleName.Text = Name
        end

        function ToggleFunctions:SetToolTip(Tip)
            if ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame") then
                ToggleFunctions.Elements.Toggle:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ToggleFunctions.Elements.Toggle

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            ToggleFunctions.Maid.TooltipShow = ToggleFunctions.Elements.ToggleButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            ToggleFunctions.Maid.TooltipHide = ToggleFunctions.Elements.ToggleButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function ToggleFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ToggleFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ToggleFunctions.Elements.KeybindButton,
                KeybindLabel = ToggleFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.9}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if ToggleFunctions.Maid.ToggleClick then
                    ToggleFunctions.Maid.ToggleClick:Disconnect()
                    ToggleFunctions.Maid.ToggleClick = nil
                end
            else
                Services.TweenService:Create(ToggleFunctions.Elements.ToggleButton, TweenInfo.new(0.2), {ImageTransparency = 0.5}):Play()
                
                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ToggleFunctions.Elements.ToggleName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                ToggleFunctions.Maid.ToggleClick = ToggleFunctions.Elements.ToggleButton.MouseButton1Click:Connect(ToggleTrigger)
            end
        end

        function ToggleFunctions:SetVisibility(State)
            ToggleFunctions.Elements.Toggle.Visible = State
        end

        function ToggleFunctions:Destroy()
            for ConnectionName, Connection in pairs(ToggleFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            
            table.clear(ToggleFunctions.Maid)

            if ToggleFunctions.Elements.Toggle then
                ToggleFunctions.Elements.Toggle:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ToggleFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()
        UpdateToggleState(ToggleFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ToggleFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ToggleFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and not Settings.IgnoreDefault and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            ToggleFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ToggleFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ToggleFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ToggleFunctions
        end

        return ToggleFunctions
    end
end]],
  ["Xenith/Xenith/UserInterface/Imports/Settings/InsertConfigSection.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions

	function TabFunctions:InsertConfigSection(Side)
		local ConfigSection = TabFunctions:Section({Side = Side})
		
		ConfigSection:Header({Name = "Configuration Settings"})

		local InputPath = nil
		local SelectedConfig = nil
		local AutoloadLabel = nil

		local function GetAutoloadPath()
			return XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/autoload.txt"
		end

		local function ShowNotification(Description)
			WindowFunctions:Notify({
				Title = "Interface",
				Description = Description
			})
		end

		local function ShowErrorNotification(Message, ErrorDetails)
			ShowNotification(Message .. ", return error: " .. ErrorDetails)
		end

		local function RefreshConfigDropdown(ConfigSelection)
			ConfigSelection:ClearOptions()
			ConfigSelection:InsertOptions(XenithLibrary:RefreshConfigList())
		end

		local function UpdateAutoloadLabel(ConfigName)
			AutoloadLabel:UpdateName("Autoload config: " .. ConfigName)
		end

		local function InitializeAutoloadLabel()
			local AutoloadPath = GetAutoloadPath()
			if isfile(AutoloadPath) then
				local ConfigName = readfile(AutoloadPath)
				UpdateAutoloadLabel(ConfigName)
			end
		end

		ConfigSection:Input({
			Name = "Config Name",
			Placeholder = "Name",
			AcceptedCharacters = "All",
			Callback = function(Input)
				InputPath = Input
			end
		})

		local ConfigSelection = ConfigSection:Dropdown({
			Name = "Select Config",
			Multi = false,
			Required = false,
			Options = XenithLibrary:RefreshConfigList(),
			Callback = function(Value)
				SelectedConfig = Value
			end
		})

		ConfigSection:Button({
			Name = "Create Config",
			Callback = function()
				if not InputPath or string.gsub(InputPath, " ", "") == "" then
					ShowNotification("Config name cannot be empty.")
					return
				end

				local Success, ErrorMessage = XenithLibrary:SaveConfig(InputPath)
				if not Success then
					ShowErrorNotification("Unable to save config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Created config %q", InputPath))
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Load Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:LoadConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to load config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Loaded config %q", ConfigSelection.Value))
			end
		})

		ConfigSection:Button({
			Name = "Overwrite Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:SaveConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to overwrite config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Overwrote config %q", ConfigSelection.Value))
			end
		})

		ConfigSection:Button({
			Name = "Delete Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:DeleteConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to delete config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Deleted config %q", ConfigSelection.Value))
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Delete All Configs",
			Callback = function()
				local Success, Message = XenithLibrary:DeleteAllConfigs()
				if not Success then
					ShowErrorNotification("Unable to delete all configs", Message)
					return
				end

				ShowNotification(Message)
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Refresh Config List",
			Callback = function()
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Set as autoload",
			Callback = function()
				local ConfigName = ConfigSelection.Value
				writefile(GetAutoloadPath(), ConfigName)
				UpdateAutoloadLabel(ConfigName)
				ShowNotification(string.format("Set %q as autoload", ConfigName))
			end
		})

		AutoloadLabel = ConfigSection:Label({Text = "Autoload config: None"})
		InitializeAutoloadLabel()
	end
end]],
  ["Xenith/UserInterface/Imports/Settings/InsertConfigSection.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions

	function TabFunctions:InsertConfigSection(Side)
		local ConfigSection = TabFunctions:Section({Side = Side})
		
		ConfigSection:Header({Name = "Configuration Settings"})

		local InputPath = nil
		local SelectedConfig = nil
		local AutoloadLabel = nil

		local function GetAutoloadPath()
			return XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/autoload.txt"
		end

		local function ShowNotification(Description)
			WindowFunctions:Notify({
				Title = "Interface",
				Description = Description
			})
		end

		local function ShowErrorNotification(Message, ErrorDetails)
			ShowNotification(Message .. ", return error: " .. ErrorDetails)
		end

		local function RefreshConfigDropdown(ConfigSelection)
			ConfigSelection:ClearOptions()
			ConfigSelection:InsertOptions(XenithLibrary:RefreshConfigList())
		end

		local function UpdateAutoloadLabel(ConfigName)
			AutoloadLabel:UpdateName("Autoload config: " .. ConfigName)
		end

		local function InitializeAutoloadLabel()
			local AutoloadPath = GetAutoloadPath()
			if isfile(AutoloadPath) then
				local ConfigName = readfile(AutoloadPath)
				UpdateAutoloadLabel(ConfigName)
			end
		end

		ConfigSection:Input({
			Name = "Config Name",
			Placeholder = "Name",
			AcceptedCharacters = "All",
			Callback = function(Input)
				InputPath = Input
			end
		})

		local ConfigSelection = ConfigSection:Dropdown({
			Name = "Select Config",
			Multi = false,
			Required = false,
			Options = XenithLibrary:RefreshConfigList(),
			Callback = function(Value)
				SelectedConfig = Value
			end
		})

		ConfigSection:Button({
			Name = "Create Config",
			Callback = function()
				if not InputPath or string.gsub(InputPath, " ", "") == "" then
					ShowNotification("Config name cannot be empty.")
					return
				end

				local Success, ErrorMessage = XenithLibrary:SaveConfig(InputPath)
				if not Success then
					ShowErrorNotification("Unable to save config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Created config %q", InputPath))
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Load Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:LoadConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to load config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Loaded config %q", ConfigSelection.Value))
			end
		})

		ConfigSection:Button({
			Name = "Overwrite Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:SaveConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to overwrite config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Overwrote config %q", ConfigSelection.Value))
			end
		})

		ConfigSection:Button({
			Name = "Delete Config",
			Callback = function()
				local Success, ErrorMessage = XenithLibrary:DeleteConfig(ConfigSelection.Value)
				if not Success then
					ShowErrorNotification("Unable to delete config", ErrorMessage)
					return
				end

				ShowNotification(string.format("Deleted config %q", ConfigSelection.Value))
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Delete All Configs",
			Callback = function()
				local Success, Message = XenithLibrary:DeleteAllConfigs()
				if not Success then
					ShowErrorNotification("Unable to delete all configs", Message)
					return
				end

				ShowNotification(Message)
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Refresh Config List",
			Callback = function()
				RefreshConfigDropdown(ConfigSelection)
			end
		})

		ConfigSection:Button({
			Name = "Set as autoload",
			Callback = function()
				local ConfigName = ConfigSelection.Value
				writefile(GetAutoloadPath(), ConfigName)
				UpdateAutoloadLabel(ConfigName)
				ShowNotification(string.format("Set %q as autoload", ConfigName))
			end
		})

		AutoloadLabel = ConfigSection:Label({Text = "Autoload config: None"})
		InitializeAutoloadLabel()
	end
end]],
  ["UserInterface/Imports/Settings/InsertConfigSection.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions

	function TabFunctions:InsertConfigSection(Side)
		local ConfigSection = TabFunctions:Section({Side = Side})
		
		ConfigSection:Header({Name = "Configuration Settings"})

		local InputPath = nil
		local SelectedConfig = nil
