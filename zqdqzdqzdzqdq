  ["Xenith/Xenith/UserInterface/Components/Input.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Input(Settings, Flag)
        local InputFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Tab = Requirements.TabName,
            Class = "Input" 
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(InputFunctions.Settings.Name, " ", "_")
        end

        local Input = Instance.new("Frame")
        Input.Name = "Input"
        Input.AutomaticSize = Enum.AutomaticSize.Y
        Input.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Input.BackgroundTransparency = 1
        Input.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Input.BorderSizePixel = 0
        Input.Size = UDim2.new(1, 0, 0, 38)
        Input.Parent = Section

        local InputName = Instance.new("TextLabel")
        InputName.Name = "InputName"
        InputName.FontFace = Font.new(Assets.InterFont)
        InputName.Text = InputFunctions.Settings.Name
        InputName.RichText = true
        InputName.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputName.TextSize = 13
        InputName.TextTransparency = 0.5
        InputName.TextTruncate = Enum.TextTruncate.AtEnd
        InputName.TextXAlignment = Enum.TextXAlignment.Left
        InputName.TextYAlignment = Enum.TextYAlignment.Top
        InputName.AnchorPoint = Vector2.new(0, 0.5)
        InputName.AutomaticSize = Enum.AutomaticSize.XY
        InputName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputName.BackgroundTransparency = 1
        InputName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputName.BorderSizePixel = 0
        InputName.Position = UDim2.fromScale(0, 0.5)
        InputName.Parent = Input

        if Settings.PremiumFeature then
            InputName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = InputName
        end

        local InputBox = Instance.new("TextBox")
        InputBox.Name = "InputBox"
        InputBox.FontFace = Font.new(Assets.InterFont)
        InputBox.Text = "Hello world!"
        InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.TextSize = 12
        InputBox.TextTransparency = 0.1
        InputBox.AnchorPoint = Vector2.new(1, 0.5)
        InputBox.AutomaticSize = Enum.AutomaticSize.X
        InputBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.BackgroundTransparency = 0.95
        InputBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputBox.BorderSizePixel = 0
        InputBox.ClipsDescendants = true
        InputBox.LayoutOrder = 1
        InputBox.Position = UDim2.fromScale(1, 0.5)
        InputBox.Size = UDim2.fromOffset(21, 21)
        InputBox.TextXAlignment = Enum.TextXAlignment.Right

        local IsHidden = false
        local PreviousNameTransparency = 0.5
        local PreviousBoxTransparency = 0.1

        if not Settings.PremiumFeature then
            InputBox.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = 0
                })
            end)

            InputBox.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            InputBox.TextEditable = false
            InputBox.ClearTextOnFocus = false
            InputBox.TextTransparency = 0.8
        end

        local InputBoxUICorner = Instance.new("UICorner")
        InputBoxUICorner.Name = "InputBoxUICorner"
        InputBoxUICorner.CornerRadius = UDim.new(0, 4)
        InputBoxUICorner.Parent = InputBox

        local InputBoxUIStroke = Instance.new("UIStroke")
        InputBoxUIStroke.Name = "InputBoxUIStroke"
        InputBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        InputBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
        InputBoxUIStroke.Transparency = 0.9
        InputBoxUIStroke.Parent = InputBox

        local InputBoxUIPadding = Instance.new("UIPadding")
        InputBoxUIPadding.Name = "InputBoxUIPadding"
        InputBoxUIPadding.PaddingLeft = UDim.new(0, 5)
        InputBoxUIPadding.PaddingRight = UDim.new(0, 5)
        InputBoxUIPadding.Parent = InputBox

        local InputBoxUISizeConstraint = Instance.new("UISizeConstraint")
        InputBoxUISizeConstraint.Name = "InputBoxUISizeConstraint"
        InputBoxUISizeConstraint.Parent = InputBox

        InputBox.Parent = Input

        local InputElement = Input
        local InputBoxElement = InputBox
        local InputNameElement = InputName
        local Constraint = InputBoxUISizeConstraint

        local function ApplyCharacterLimit(Value)
            if InputFunctions.Settings.CharacterLimit then
                return Value:sub(1, InputFunctions.Settings.CharacterLimit)
            end
            return Value
        end

        local CharacterSubs = {
            All = function(Value)
                return ApplyCharacterLimit(Value)
            end,
            Numeric = function(Value)
                local Result = Value:match("^%-?%d*$") and Value or Value:gsub("[^%d-]", ""):gsub("(%-)", function(Match, Pos)
                    return Pos == 1 and Match or ""
                end)
                return ApplyCharacterLimit(Result)
            end,
            Alphabetic = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z ]", ""))
            end,
            AlphaNumeric = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z0-9]", ""))
            end,
        }

        local AcceptedCharacters

        if type(InputFunctions.Settings.AcceptedCharacters) == "function" then
            AcceptedCharacters = InputFunctions.Settings.AcceptedCharacters
        else
            AcceptedCharacters = CharacterSubs[InputFunctions.Settings.AcceptedCharacters] or CharacterSubs.All
        end

        InputBoxElement.AutomaticSize = Enum.AutomaticSize.X

        local function CheckSize()
            local NameWidth = InputNameElement.AbsoluteSize.X
            local TotalWidth = InputElement.AbsoluteSize.X

            local MaxWidth = (TotalWidth - NameWidth - 20) / BaseUIScale.Scale
            Constraint.MaxSize = Vector2.new(MaxWidth, 9e9)
        end

        CheckSize()
        InputNameElement:GetPropertyChangedSignal("AbsoluteSize"):Connect(CheckSize)

        InputBoxElement.FocusLost:Connect(function()
            if IsHidden then return end
            local InputText = InputBoxElement.Text
            local FilteredText = AcceptedCharacters(InputText)
            InputBoxElement.Text = FilteredText
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end)
        InputBoxElement.Text = InputFunctions.Settings.Default or ""
        InputBoxElement.PlaceholderText = InputFunctions.Settings.Placeholder or ""

        InputBoxElement:GetPropertyChangedSignal("Text"):Connect(function()
            if IsHidden then return end
            InputBoxElement.Text = AcceptedCharacters(InputBoxElement.Text)
            if InputFunctions.Settings.OnChanged then
                InputFunctions.Settings.OnChanged(InputBoxElement.Text)
            end
            InputFunctions.Text = InputBoxElement.Text
        end)

        function InputFunctions:SetToolTip(Tip)
            if Input:FindFirstChild("ToolTipFrame") then
                Input:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Input

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            InputBox.MouseEnter:Connect(ShowTooltip)
            InputBox.MouseLeave:Connect(HideTooltip)
        end

        function InputFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then
                if not Settings.PremiumFeature then
                    PreviousNameTransparency = InputName.TextTransparency
                end
                PreviousBoxTransparency = InputBox.TextTransparency

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                InputBox.TextEditable = false
            else
                local originalBoxTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local boxEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = originalBoxTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                InputBox.TextEditable = boxEditable
            end
        end

        function InputFunctions:GetState()
            return not IsHidden
        end

        function InputFunctions:ReturnClass()
            return InputFunctions.Class
        end

        function InputFunctions:ReturnTab()
            return InputFunctions.Tab
        end

        function InputFunctions:ReturnSettings()
            return Settings
        end

        function InputFunctions:UpdateName(Name)
            InputName.Text = Name
        end

        function InputFunctions:SetVisibility(State)
            Input.Visible = State
        end

        function InputFunctions:GetInput()
            return InputBox.Text
        end

        function InputFunctions:UpdatePlaceholder(Placeholder)
            InputBox.PlaceholderText = Placeholder
        end

        function InputFunctions:UpdateText(Text)
            local FilteredText = AcceptedCharacters(Text)
            InputBox.Text = FilteredText
            InputFunctions.Text = FilteredText
            
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end

        function InputFunctions:Destroy()
            if Input then
                Input:Destroy()
            end
            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if InputFunctions.Settings.Hidden then
            if typeof(InputFunctions.Settings.Hidden) == "table" then
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden[1], InputFunctions.Settings.Hidden[2])
            else
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden)
            end
        end

        if InputFunctions.Settings.ToolTip then
            InputFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            InputFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = InputFunctions
        end
        return InputFunctions
    end
end]],
  ["Xenith/UserInterface/Components/Input.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Input(Settings, Flag)
        local InputFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Tab = Requirements.TabName,
            Class = "Input" 
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(InputFunctions.Settings.Name, " ", "_")
        end

        local Input = Instance.new("Frame")
        Input.Name = "Input"
        Input.AutomaticSize = Enum.AutomaticSize.Y
        Input.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Input.BackgroundTransparency = 1
        Input.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Input.BorderSizePixel = 0
        Input.Size = UDim2.new(1, 0, 0, 38)
        Input.Parent = Section

        local InputName = Instance.new("TextLabel")
        InputName.Name = "InputName"
        InputName.FontFace = Font.new(Assets.InterFont)
        InputName.Text = InputFunctions.Settings.Name
        InputName.RichText = true
        InputName.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputName.TextSize = 13
        InputName.TextTransparency = 0.5
        InputName.TextTruncate = Enum.TextTruncate.AtEnd
        InputName.TextXAlignment = Enum.TextXAlignment.Left
        InputName.TextYAlignment = Enum.TextYAlignment.Top
        InputName.AnchorPoint = Vector2.new(0, 0.5)
        InputName.AutomaticSize = Enum.AutomaticSize.XY
        InputName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputName.BackgroundTransparency = 1
        InputName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputName.BorderSizePixel = 0
        InputName.Position = UDim2.fromScale(0, 0.5)
        InputName.Parent = Input

        if Settings.PremiumFeature then
            InputName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = InputName
        end

        local InputBox = Instance.new("TextBox")
        InputBox.Name = "InputBox"
        InputBox.FontFace = Font.new(Assets.InterFont)
        InputBox.Text = "Hello world!"
        InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.TextSize = 12
        InputBox.TextTransparency = 0.1
        InputBox.AnchorPoint = Vector2.new(1, 0.5)
        InputBox.AutomaticSize = Enum.AutomaticSize.X
        InputBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.BackgroundTransparency = 0.95
        InputBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputBox.BorderSizePixel = 0
        InputBox.ClipsDescendants = true
        InputBox.LayoutOrder = 1
        InputBox.Position = UDim2.fromScale(1, 0.5)
        InputBox.Size = UDim2.fromOffset(21, 21)
        InputBox.TextXAlignment = Enum.TextXAlignment.Right

        local IsHidden = false
        local PreviousNameTransparency = 0.5
        local PreviousBoxTransparency = 0.1

        if not Settings.PremiumFeature then
            InputBox.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = 0
                })
            end)

            InputBox.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            InputBox.TextEditable = false
            InputBox.ClearTextOnFocus = false
            InputBox.TextTransparency = 0.8
        end

        local InputBoxUICorner = Instance.new("UICorner")
        InputBoxUICorner.Name = "InputBoxUICorner"
        InputBoxUICorner.CornerRadius = UDim.new(0, 4)
        InputBoxUICorner.Parent = InputBox

        local InputBoxUIStroke = Instance.new("UIStroke")
        InputBoxUIStroke.Name = "InputBoxUIStroke"
        InputBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        InputBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
        InputBoxUIStroke.Transparency = 0.9
        InputBoxUIStroke.Parent = InputBox

        local InputBoxUIPadding = Instance.new("UIPadding")
        InputBoxUIPadding.Name = "InputBoxUIPadding"
        InputBoxUIPadding.PaddingLeft = UDim.new(0, 5)
        InputBoxUIPadding.PaddingRight = UDim.new(0, 5)
        InputBoxUIPadding.Parent = InputBox

        local InputBoxUISizeConstraint = Instance.new("UISizeConstraint")
        InputBoxUISizeConstraint.Name = "InputBoxUISizeConstraint"
        InputBoxUISizeConstraint.Parent = InputBox

        InputBox.Parent = Input

        local InputElement = Input
        local InputBoxElement = InputBox
        local InputNameElement = InputName
        local Constraint = InputBoxUISizeConstraint

        local function ApplyCharacterLimit(Value)
            if InputFunctions.Settings.CharacterLimit then
                return Value:sub(1, InputFunctions.Settings.CharacterLimit)
            end
            return Value
        end

        local CharacterSubs = {
            All = function(Value)
                return ApplyCharacterLimit(Value)
            end,
            Numeric = function(Value)
                local Result = Value:match("^%-?%d*$") and Value or Value:gsub("[^%d-]", ""):gsub("(%-)", function(Match, Pos)
                    return Pos == 1 and Match or ""
                end)
                return ApplyCharacterLimit(Result)
            end,
            Alphabetic = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z ]", ""))
            end,
            AlphaNumeric = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z0-9]", ""))
            end,
        }

        local AcceptedCharacters

        if type(InputFunctions.Settings.AcceptedCharacters) == "function" then
            AcceptedCharacters = InputFunctions.Settings.AcceptedCharacters
        else
            AcceptedCharacters = CharacterSubs[InputFunctions.Settings.AcceptedCharacters] or CharacterSubs.All
        end

        InputBoxElement.AutomaticSize = Enum.AutomaticSize.X

        local function CheckSize()
            local NameWidth = InputNameElement.AbsoluteSize.X
            local TotalWidth = InputElement.AbsoluteSize.X

            local MaxWidth = (TotalWidth - NameWidth - 20) / BaseUIScale.Scale
            Constraint.MaxSize = Vector2.new(MaxWidth, 9e9)
        end

        CheckSize()
        InputNameElement:GetPropertyChangedSignal("AbsoluteSize"):Connect(CheckSize)

        InputBoxElement.FocusLost:Connect(function()
            if IsHidden then return end
            local InputText = InputBoxElement.Text
            local FilteredText = AcceptedCharacters(InputText)
            InputBoxElement.Text = FilteredText
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end)
        InputBoxElement.Text = InputFunctions.Settings.Default or ""
        InputBoxElement.PlaceholderText = InputFunctions.Settings.Placeholder or ""

        InputBoxElement:GetPropertyChangedSignal("Text"):Connect(function()
            if IsHidden then return end
            InputBoxElement.Text = AcceptedCharacters(InputBoxElement.Text)
            if InputFunctions.Settings.OnChanged then
                InputFunctions.Settings.OnChanged(InputBoxElement.Text)
            end
            InputFunctions.Text = InputBoxElement.Text
        end)

        function InputFunctions:SetToolTip(Tip)
            if Input:FindFirstChild("ToolTipFrame") then
                Input:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Input

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            InputBox.MouseEnter:Connect(ShowTooltip)
            InputBox.MouseLeave:Connect(HideTooltip)
        end

        function InputFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then
                if not Settings.PremiumFeature then
                    PreviousNameTransparency = InputName.TextTransparency
                end
                PreviousBoxTransparency = InputBox.TextTransparency

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                InputBox.TextEditable = false
            else
                local originalBoxTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local boxEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = originalBoxTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                InputBox.TextEditable = boxEditable
            end
        end

        function InputFunctions:GetState()
            return not IsHidden
        end

        function InputFunctions:ReturnClass()
            return InputFunctions.Class
        end

        function InputFunctions:ReturnTab()
            return InputFunctions.Tab
        end

        function InputFunctions:ReturnSettings()
            return Settings
        end

        function InputFunctions:UpdateName(Name)
            InputName.Text = Name
        end

        function InputFunctions:SetVisibility(State)
            Input.Visible = State
        end

        function InputFunctions:GetInput()
            return InputBox.Text
        end

        function InputFunctions:UpdatePlaceholder(Placeholder)
            InputBox.PlaceholderText = Placeholder
        end

        function InputFunctions:UpdateText(Text)
            local FilteredText = AcceptedCharacters(Text)
            InputBox.Text = FilteredText
            InputFunctions.Text = FilteredText
            
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end

        function InputFunctions:Destroy()
            if Input then
                Input:Destroy()
            end
            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if InputFunctions.Settings.Hidden then
            if typeof(InputFunctions.Settings.Hidden) == "table" then
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden[1], InputFunctions.Settings.Hidden[2])
            else
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden)
            end
        end

        if InputFunctions.Settings.ToolTip then
            InputFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            InputFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = InputFunctions
        end
        return InputFunctions
    end
end]],
  ["UserInterface/Components/Input.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Input(Settings, Flag)
        local InputFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Tab = Requirements.TabName,
            Class = "Input" 
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(InputFunctions.Settings.Name, " ", "_")
        end

        local Input = Instance.new("Frame")
        Input.Name = "Input"
        Input.AutomaticSize = Enum.AutomaticSize.Y
        Input.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Input.BackgroundTransparency = 1
        Input.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Input.BorderSizePixel = 0
        Input.Size = UDim2.new(1, 0, 0, 38)
        Input.Parent = Section

        local InputName = Instance.new("TextLabel")
        InputName.Name = "InputName"
        InputName.FontFace = Font.new(Assets.InterFont)
        InputName.Text = InputFunctions.Settings.Name
        InputName.RichText = true
        InputName.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputName.TextSize = 13
        InputName.TextTransparency = 0.5
        InputName.TextTruncate = Enum.TextTruncate.AtEnd
        InputName.TextXAlignment = Enum.TextXAlignment.Left
        InputName.TextYAlignment = Enum.TextYAlignment.Top
        InputName.AnchorPoint = Vector2.new(0, 0.5)
        InputName.AutomaticSize = Enum.AutomaticSize.XY
        InputName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputName.BackgroundTransparency = 1
        InputName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputName.BorderSizePixel = 0
        InputName.Position = UDim2.fromScale(0, 0.5)
        InputName.Parent = Input

        if Settings.PremiumFeature then
            InputName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = InputName
        end

        local InputBox = Instance.new("TextBox")
        InputBox.Name = "InputBox"
        InputBox.FontFace = Font.new(Assets.InterFont)
        InputBox.Text = "Hello world!"
        InputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.TextSize = 12
        InputBox.TextTransparency = 0.1
        InputBox.AnchorPoint = Vector2.new(1, 0.5)
        InputBox.AutomaticSize = Enum.AutomaticSize.X
        InputBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        InputBox.BackgroundTransparency = 0.95
        InputBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
        InputBox.BorderSizePixel = 0
        InputBox.ClipsDescendants = true
        InputBox.LayoutOrder = 1
        InputBox.Position = UDim2.fromScale(1, 0.5)
        InputBox.Size = UDim2.fromOffset(21, 21)
        InputBox.TextXAlignment = Enum.TextXAlignment.Right

        local IsHidden = false
        local PreviousNameTransparency = 0.5
        local PreviousBoxTransparency = 0.1

        if not Settings.PremiumFeature then
            InputBox.MouseEnter:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = 0
                })
            end)

            InputBox.MouseLeave:Connect(function()
                if IsHidden then return end
                Utility:QuickTween(InputName, {
                    TextTransparency = PreviousNameTransparency
                })
            end)
        end

        if not Utility:IsPremiumHubUser() and Settings.PremiumFeature then
            InputBox.TextEditable = false
            InputBox.ClearTextOnFocus = false
            InputBox.TextTransparency = 0.8
        end

        local InputBoxUICorner = Instance.new("UICorner")
        InputBoxUICorner.Name = "InputBoxUICorner"
        InputBoxUICorner.CornerRadius = UDim.new(0, 4)
        InputBoxUICorner.Parent = InputBox

        local InputBoxUIStroke = Instance.new("UIStroke")
        InputBoxUIStroke.Name = "InputBoxUIStroke"
        InputBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        InputBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
        InputBoxUIStroke.Transparency = 0.9
        InputBoxUIStroke.Parent = InputBox

        local InputBoxUIPadding = Instance.new("UIPadding")
        InputBoxUIPadding.Name = "InputBoxUIPadding"
        InputBoxUIPadding.PaddingLeft = UDim.new(0, 5)
        InputBoxUIPadding.PaddingRight = UDim.new(0, 5)
        InputBoxUIPadding.Parent = InputBox

        local InputBoxUISizeConstraint = Instance.new("UISizeConstraint")
        InputBoxUISizeConstraint.Name = "InputBoxUISizeConstraint"
        InputBoxUISizeConstraint.Parent = InputBox

        InputBox.Parent = Input

        local InputElement = Input
        local InputBoxElement = InputBox
        local InputNameElement = InputName
        local Constraint = InputBoxUISizeConstraint

        local function ApplyCharacterLimit(Value)
            if InputFunctions.Settings.CharacterLimit then
                return Value:sub(1, InputFunctions.Settings.CharacterLimit)
            end
            return Value
        end

        local CharacterSubs = {
            All = function(Value)
                return ApplyCharacterLimit(Value)
            end,
            Numeric = function(Value)
                local Result = Value:match("^%-?%d*$") and Value or Value:gsub("[^%d-]", ""):gsub("(%-)", function(Match, Pos)
                    return Pos == 1 and Match or ""
                end)
                return ApplyCharacterLimit(Result)
            end,
            Alphabetic = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z ]", ""))
            end,
            AlphaNumeric = function(Value)
                return ApplyCharacterLimit(Value:gsub("[^a-zA-Z0-9]", ""))
            end,
        }

        local AcceptedCharacters

        if type(InputFunctions.Settings.AcceptedCharacters) == "function" then
            AcceptedCharacters = InputFunctions.Settings.AcceptedCharacters
        else
            AcceptedCharacters = CharacterSubs[InputFunctions.Settings.AcceptedCharacters] or CharacterSubs.All
        end

        InputBoxElement.AutomaticSize = Enum.AutomaticSize.X

        local function CheckSize()
            local NameWidth = InputNameElement.AbsoluteSize.X
            local TotalWidth = InputElement.AbsoluteSize.X

            local MaxWidth = (TotalWidth - NameWidth - 20) / BaseUIScale.Scale
            Constraint.MaxSize = Vector2.new(MaxWidth, 9e9)
        end

        CheckSize()
        InputNameElement:GetPropertyChangedSignal("AbsoluteSize"):Connect(CheckSize)

        InputBoxElement.FocusLost:Connect(function()
            if IsHidden then return end
            local InputText = InputBoxElement.Text
            local FilteredText = AcceptedCharacters(InputText)
            InputBoxElement.Text = FilteredText
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end)
        InputBoxElement.Text = InputFunctions.Settings.Default or ""
        InputBoxElement.PlaceholderText = InputFunctions.Settings.Placeholder or ""

        InputBoxElement:GetPropertyChangedSignal("Text"):Connect(function()
            if IsHidden then return end
            InputBoxElement.Text = AcceptedCharacters(InputBoxElement.Text)
            if InputFunctions.Settings.OnChanged then
                InputFunctions.Settings.OnChanged(InputBoxElement.Text)
            end
            InputFunctions.Text = InputBoxElement.Text
        end)

        function InputFunctions:SetToolTip(Tip)
            if Input:FindFirstChild("ToolTipFrame") then
                Input:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then
                return
            end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = Input

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Name = "TooltipText"
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.TextWrapped = false
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(
                Tip,
                TooltipText.TextSize,
                Enum.Font.SourceSansBold,
                Vector2.new(math.huge, math.huge)
            )

            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                ToolTipFrame.BackgroundTransparency = 1
                TooltipText.TextTransparency = 1

                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 0.1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 0}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 0.8}
                ):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(
                    ToolTipFrame,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipText,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {TextTransparency = 1}
                ):Play()

                Services.TweenService:Create(
                    TooltipStroke,
                    TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
                    {Transparency = 1}
                ):Play()

                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            InputBox.MouseEnter:Connect(ShowTooltip)
            InputBox.MouseLeave:Connect(HideTooltip)
        end

        function InputFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            IsHidden = State

            if State then
                if not Settings.PremiumFeature then
                    PreviousNameTransparency = InputName.TextTransparency
                end
                PreviousBoxTransparency = InputBox.TextTransparency

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = 0.9, BackgroundTransparency = 0.99 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.95 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = 0.9 }
                    ):Play()
                end

                InputBox.TextEditable = false
            else
                local originalBoxTransparency = (not Utility:IsPremiumHubUser() and Settings.PremiumFeature) and 0.8 or 0.1
                local boxEditable = not (not Utility:IsPremiumHubUser() and Settings.PremiumFeature)

                Services.TweenService:Create(
                    InputBox,
                    TweenInfo.new(0.2),
                    { TextTransparency = originalBoxTransparency, BackgroundTransparency = 0.95 }
                ):Play()

                Services.TweenService:Create(
                    InputBoxUIStroke,
                    TweenInfo.new(0.2),
                    { Transparency = 0.9 }
                ):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(
                        InputName,
                        TweenInfo.new(0.2),
                        { TextTransparency = PreviousNameTransparency }
                    ):Play()
                end

                InputBox.TextEditable = boxEditable
            end
        end

        function InputFunctions:GetState()
            return not IsHidden
        end

        function InputFunctions:ReturnClass()
            return InputFunctions.Class
        end

        function InputFunctions:ReturnTab()
            return InputFunctions.Tab
        end

        function InputFunctions:ReturnSettings()
            return Settings
        end

        function InputFunctions:UpdateName(Name)
            InputName.Text = Name
        end

        function InputFunctions:SetVisibility(State)
            Input.Visible = State
        end

        function InputFunctions:GetInput()
            return InputBox.Text
        end

        function InputFunctions:UpdatePlaceholder(Placeholder)
            InputBox.PlaceholderText = Placeholder
        end

        function InputFunctions:UpdateText(Text)
            local FilteredText = AcceptedCharacters(Text)
            InputBox.Text = FilteredText
            InputFunctions.Text = FilteredText
            
            task.spawn(function()
                if InputFunctions.Settings.Callback then
                    InputFunctions.Settings.Callback(FilteredText)
                end
            end)
        end

        function InputFunctions:Destroy()
            if Input then
                Input:Destroy()
            end
            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end

            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then
                        return true
                    end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        if InputFunctions.Settings.Hidden then
            if typeof(InputFunctions.Settings.Hidden) == "table" then
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden[1], InputFunctions.Settings.Hidden[2])
            else
                InputFunctions:SetHidden(InputFunctions.Settings.Hidden)
            end
        end

        if InputFunctions.Settings.ToolTip then
            InputFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            InputFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            InputFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = InputFunctions
        end
        return InputFunctions
    end
end]],
  ["Xenith/Xenith/UserInterface/Components/Keybind.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Keybind(Settings, Flag)
        local KeybindFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Tab = Requirements.TabName,
            Class = "Keybind" 
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(KeybindFunctions.Settings.Name, " ", "_")
        end

        local Keybind = Instance.new("Frame")
        Keybind.Name = "Keybind"
        Keybind.AutomaticSize = Enum.AutomaticSize.Y
        Keybind.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Keybind.BackgroundTransparency = 1
        Keybind.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Keybind.BorderSizePixel = 0
        Keybind.Size = UDim2.new(1, 0, 0, 38)
        Keybind.Parent = Section

        local KeybindName = Instance.new("TextLabel")
        KeybindName.Name = "KeybindName"
        KeybindName.FontFace = Font.new(Assets.InterFont)
        KeybindName.Text = KeybindFunctions.Settings.Name
        KeybindName.RichText = true
        KeybindName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
        KeybindName.TextSize = 13
        KeybindName.TextTransparency = 0.5
        KeybindName.TextTruncate = Enum.TextTruncate.AtEnd
        KeybindName.TextXAlignment = Enum.TextXAlignment.Left
        KeybindName.TextYAlignment = Enum.TextYAlignment.Top
        KeybindName.AnchorPoint = Vector2.new(0, 0.5)
        KeybindName.AutomaticSize = Enum.AutomaticSize.XY
        KeybindName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        KeybindName.BackgroundTransparency = 1
        KeybindName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        KeybindName.BorderSizePixel = 0
        KeybindName.Position = UDim2.fromScale(0, 0.5)
        KeybindName.Parent = Keybind

        if Settings.PremiumFeature then
            KeybindName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(0.4, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = KeybindName
        end

        local BinderBox = Instance.new("TextBox")
        BinderBox.Name = "BinderBox"
        BinderBox.CursorPosition = -1
        BinderBox.FontFace = Font.new(Assets.InterFont)
        BinderBox.PlaceholderText = "..."
        BinderBox.Text = ""
        BinderBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        BinderBox.TextSize = 12
        BinderBox.TextTransparency = 0.1
        BinderBox.AnchorPoint = Vector2.new(1, 0.5)
        BinderBox.AutomaticSize = Enum.AutomaticSize.X
        BinderBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        BinderBox.BackgroundTransparency = 0.95
        BinderBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
        BinderBox.BorderSizePixel = 0
        BinderBox.ClipsDescendants = true
        BinderBox.LayoutOrder = 1
        BinderBox.Position = UDim2.fromScale(1, 0.5)
        BinderBox.Size = UDim2.fromOffset(21, 21)

        if not Settings.PremiumFeature then
            BinderBox.MouseEnter:Connect(function()
                Utility:QuickTween(KeybindName, {
                    TextTransparency = 0
                })
            end)

            BinderBox.MouseLeave:Connect(function()
                Utility:QuickTween(KeybindName, {
                    TextTransparency = 0.5
                })
            end)
        end

        local BinderBoxUICorner = Instance.new("UICorner")
        BinderBoxUICorner.Name = "BinderBoxUICorner"
        BinderBoxUICorner.CornerRadius = UDim.new(0, 4)
        BinderBoxUICorner.Parent = BinderBox

        local BinderBoxUIStroke = Instance.new("UIStroke")
        BinderBoxUIStroke.Name = "BinderBoxUIStroke"
        BinderBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        BinderBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
        BinderBoxUIStroke.Transparency = 0.9
        BinderBoxUIStroke.Parent = BinderBox

        local BinderBoxUIPadding = Instance.new("UIPadding")
        BinderBoxUIPadding.Name = "BinderBoxUIPadding"
        BinderBoxUIPadding.PaddingLeft = UDim.new(0, 5)
        BinderBoxUIPadding.PaddingRight = UDim.new(0, 5)
        BinderBoxUIPadding.Parent = BinderBox

        local BinderBoxUISizeConstraint = Instance.new("UISizeConstraint")
        BinderBoxUISizeConstraint.Name = "BinderBoxUISizeConstraint"
        BinderBoxUISizeConstraint.Parent = BinderBox

        BinderBox.Parent = Keybind

        local Focused
        local IsBinding = false
        local Reset = false
        local Binded = KeybindFunctions.Settings.Default
        local BindMode = KeybindFunctions.Settings.Mode or KeybindFunctions.Settings.mode or "Toggle"
        local Toggled = BindMode == "Always"

        local function ResetFocusState()
            Focused = false
            IsBinding = false
            BinderBox:ReleaseFocus()
        end

        if Binded then
            BinderBox.Text = Binded.Name
        end

        BinderBox.Focused:Connect(function()
            Focused = true
        end)

        BinderBox.FocusLost:Connect(function()
            Focused = false
        end)

        Services.UserInputService.InputBegan:Connect(function(Inp)
            if Focused and not IsBinding then
                IsBinding = true

                local Event
                Event = Services.UserInputService.InputBegan:Connect(function(Input)
                    if KeybindFunctions.Settings.Blacklist and (table.find(KeybindFunctions.KeybindFunctions.Settings.Blacklist, Input.KeyCode) or table.find(KeybindFunctions.Settings.Blacklist, Input.UserInputType)) then
                        BinderBox:ReleaseFocus()
                        ResetFocusState()
                        Event:Disconnect()
                        return
                    end

                    if Input.UserInputType == Enum.UserInputType.Keyboard then
                        Binded = Input.KeyCode
                        BinderBox.Text = Input.KeyCode.Name
                    elseif Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.MouseButton2 then
                        Binded = Input.UserInputType
                        BinderBox.Text = Input.UserInputType.Name
                    end

                    if KeybindFunctions.Settings.OnBinded then
                        KeybindFunctions.Settings.OnBinded(Binded)
                    end
                    if KeybindFunctions.Settings.Callback then
                        KeybindFunctions.Settings.Callback(Toggled)
                    end
                    Reset = true
                    ResetFocusState()
                    Event:Disconnect()
                end)
            else
                if not Reset and (Inp.KeyCode == Binded or Inp.UserInputType == Binded) then
                    if KeybindFunctions.Settings.Callback then
                        if BindMode == "Toggle" then
                            Toggled = not Toggled
                        elseif BindMode == "Hold" then
                            Toggled = true
                        else
                            Toggled = true
                        end
                        KeybindFunctions.Settings.Callback(Toggled)
                    end
                    if KeybindFunctions.Settings.OnBindHeld then
                        KeybindFunctions.Settings.OnBindHeld(true, Binded)
                    end
                else
                    Reset = false
                end
            end
        end)

        Services.UserInputService.InputEnded:Connect(function(Inp)
            if not Focused and not IsBinding then
                if Inp.KeyCode == Binded or Inp.UserInputType == Binded then
                    if BindMode == "Hold" then
                        Toggled = false
                        if Settings.Callback then
                            Settings.Callback(Toggled)
                        end
                    end
                    if Settings.OnBindHeld then
                        Settings.OnBindHeld(false, Binded)
                    end
                end
            end
        end)

        function KeybindFunctions:Bind(Key)
            Binded = Key
            BinderBox.Text = Key.Name

            if KeybindFunctions.Settings.Callback then
                KeybindFunctions.Settings.Callback(Toggled)
            end
        end

        function KeybindFunctions:Unbind()
            Binded = nil
            BinderBox.Text = ""
        end

        function KeybindFunctions:ReturnClass()
            return KeybindFunctions.Class
        end

        function KeybindFunctions:ReturnTab()
            return KeybindFunctions.Tab
        end

        function KeybindFunctions:GetBind()
            return Binded
        end

        function KeybindFunctions:UpdateName(Name)
            KeybindName.Text = Name
        end

        function KeybindFunctions:SetVisibility(State)
            Keybind.Visible = State
        end

        function KeybindFunctions:Destroy()
            if Keybind then
                Keybind:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        if Flag then
            XenithLibrary.Options[Flag] = KeybindFunctions
        end

        return KeybindFunctions
    end
end
]],
  ["Xenith/UserInterface/Components/Keybind.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:Keybind(Settings, Flag)
        local KeybindFunctions = { 
            Settings = Settings, 
            IgnoreConfig = Settings.IgnoreConfig, 
            Tab = Requirements.TabName,
            Class = "Keybind" 
        }

        if not Flag then
            Flag = "WriteHere"
        end

        if Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(KeybindFunctions.Settings.Name, " ", "_")
        end

        local Keybind = Instance.new("Frame")
        Keybind.Name = "Keybind"
        Keybind.AutomaticSize = Enum.AutomaticSize.Y
        Keybind.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        Keybind.BackgroundTransparency = 1
        Keybind.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Keybind.BorderSizePixel = 0
        Keybind.Size = UDim2.new(1, 0, 0, 38)
        Keybind.Parent = Section

        local KeybindName = Instance.new("TextLabel")
        KeybindName.Name = "KeybindName"
        KeybindName.FontFace = Font.new(Assets.InterFont)
        KeybindName.Text = KeybindFunctions.Settings.Name
        KeybindName.RichText = true
        KeybindName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
        KeybindName.TextSize = 13
        KeybindName.TextTransparency = 0.5
        KeybindName.TextTruncate = Enum.TextTruncate.AtEnd
        KeybindName.TextXAlignment = Enum.TextXAlignment.Left
        KeybindName.TextYAlignment = Enum.TextYAlignment.Top
        KeybindName.AnchorPoint = Vector2.new(0, 0.5)
        KeybindName.AutomaticSize = Enum.AutomaticSize.XY
        KeybindName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        KeybindName.BackgroundTransparency = 1
        KeybindName.BorderColor3 = Color3.fromRGB(0, 0, 0)
        KeybindName.BorderSizePixel = 0
        KeybindName.Position = UDim2.fromScale(0, 0.5)
        KeybindName.Parent = Keybind

        if Settings.PremiumFeature then
            KeybindName.TextTransparency = 0
            local PremiumGradient = Instance.new("UIGradient")
            PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(0.4, Color3.fromRGB(163, 201, 255)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
            })

            PremiumGradient.Rotation = 0
            PremiumGradient.Parent = KeybindName
        end

        local BinderBox = Instance.new("TextBox")
        BinderBox.Name = "BinderBox"
        BinderBox.CursorPosition = -1
        BinderBox.FontFace = Font.new(Assets.InterFont)
        BinderBox.PlaceholderText = "..."
        BinderBox.Text = ""
        BinderBox.TextColor3 = Color3.fromRGB(255, 255, 255)
        BinderBox.TextSize = 12
        BinderBox.TextTransparency = 0.1
        BinderBox.AnchorPoint = Vector2.new(1, 0.5)
        BinderBox.AutomaticSize = Enum.AutomaticSize.X
        BinderBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        BinderBox.BackgroundTransparency = 0.95
        BinderBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
        BinderBox.BorderSizePixel = 0
        BinderBox.ClipsDescendants = true
        BinderBox.LayoutOrder = 1
        BinderBox.Position = UDim2.fromScale(1, 0.5)
        BinderBox.Size = UDim2.fromOffset(21, 21)

        if not Settings.PremiumFeature then
            BinderBox.MouseEnter:Connect(function()
                Utility:QuickTween(KeybindName, {
                    TextTransparency = 0
                })
            end)

            BinderBox.MouseLeave:Connect(function()
                Utility:QuickTween(KeybindName, {
                    TextTransparency = 0.5
                })
            end)
        end

        local BinderBoxUICorner = Instance.new("UICorner")
        BinderBoxUICorner.Name = "BinderBoxUICorner"
        BinderBoxUICorner.CornerRadius = UDim.new(0, 4)
        BinderBoxUICorner.Parent = BinderBox

        local BinderBoxUIStroke = Instance.new("UIStroke")
        BinderBoxUIStroke.Name = "BinderBoxUIStroke"
        BinderBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        BinderBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
        BinderBoxUIStroke.Transparency = 0.9
        BinderBoxUIStroke.Parent = BinderBox

        local BinderBoxUIPadding = Instance.new("UIPadding")
        BinderBoxUIPadding.Name = "BinderBoxUIPadding"
        BinderBoxUIPadding.PaddingLeft = UDim.new(0, 5)
        BinderBoxUIPadding.PaddingRight = UDim.new(0, 5)
        BinderBoxUIPadding.Parent = BinderBox

        local BinderBoxUISizeConstraint = Instance.new("UISizeConstraint")
        BinderBoxUISizeConstraint.Name = "BinderBoxUISizeConstraint"
        BinderBoxUISizeConstraint.Parent = BinderBox

        BinderBox.Parent = Keybind

        local Focused
        local IsBinding = false
        local Reset = false
        local Binded = KeybindFunctions.Settings.Default
        local BindMode = KeybindFunctions.Settings.Mode or KeybindFunctions.Settings.mode or "Toggle"
        local Toggled = BindMode == "Always"

        local function ResetFocusState()
            Focused = false
            IsBinding = false
            BinderBox:ReleaseFocus()
        end

        if Binded then
            BinderBox.Text = Binded.Name
        end

        BinderBox.Focused:Connect(function()
            Focused = true
        end)

        BinderBox.FocusLost:Connect(function()
            Focused = false
        end)

        Services.UserInputService.InputBegan:Connect(function(Inp)
            if Focused and not IsBinding then
                IsBinding = true

                local Event
                Event = Services.UserInputService.InputBegan:Connect(function(Input)
                    if KeybindFunctions.Settings.Blacklist and (table.find(KeybindFunctions.KeybindFunctions.Settings.Blacklist, Input.KeyCode) or table.find(KeybindFunctions.Settings.Blacklist, Input.UserInputType)) then
                        BinderBox:ReleaseFocus()
                        ResetFocusState()
                        Event:Disconnect()
                        return
                    end

                    if Input.UserInputType == Enum.UserInputType.Keyboard then
                        Binded = Input.KeyCode
                        BinderBox.Text = Input.KeyCode.Name
                    elseif Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.MouseButton2 then
                        Binded = Input.UserInputType
                        BinderBox.Text = Input.UserInputType.Name
                    end

                    if KeybindFunctions.Settings.OnBinded then
                        KeybindFunctions.Settings.OnBinded(Binded)
                    end
                    if KeybindFunctions.Settings.Callback then
                        KeybindFunctions.Settings.Callback(Toggled)
                    end
                    Reset = true
                    ResetFocusState()
                    Event:Disconnect()
                end)
            else
                if not Reset and (Inp.KeyCode == Binded or Inp.UserInputType == Binded) then
                    if KeybindFunctions.Settings.Callback then
                        if BindMode == "Toggle" then
                            Toggled = not Toggled
                        elseif BindMode == "Hold" then
                            Toggled = true
                        else
                            Toggled = true
                        end
                        KeybindFunctions.Settings.Callback(Toggled)
                    end
                    if KeybindFunctions.Settings.OnBindHeld then
                        KeybindFunctions.Settings.OnBindHeld(true, Binded)
                    end
                else
                    Reset = false
                end
            end
        end)

        Services.UserInputService.InputEnded:Connect(function(Inp)
            if not Focused and not IsBinding then
                if Inp.KeyCode == Binded or Inp.UserInputType == Binded then
                    if BindMode == "Hold" then
                        Toggled = false
                        if Settings.Callback then
                            Settings.Callback(Toggled)
                        end
                    end
                    if Settings.OnBindHeld then
                        Settings.OnBindHeld(false, Binded)
                    end
                end
            end
        end)

        function KeybindFunctions:Bind(Key)
            Binded = Key
            BinderBox.Text = Key.Name

            if KeybindFunctions.Settings.Callback then
                KeybindFunctions.Settings.Callback(Toggled)
            end
        end

        function KeybindFunctions:Unbind()
            Binded = nil
            BinderBox.Text = ""
        end

        function KeybindFunctions:ReturnClass()
            return KeybindFunctions.Class
        end

        function KeybindFunctions:ReturnTab()
            return KeybindFunctions.Tab
        end

        function KeybindFunctions:GetBind()
            return Binded
        end

        function KeybindFunctions:UpdateName(Name)
            KeybindName.Text = Name
        end

        function KeybindFunctions:SetVisibility(State)
            Keybind.Visible = State
        end

        function KeybindFunctions:Destroy()
            if Keybind then
                Keybind:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        if Flag then
            XenithLibrary.Options[Flag] = KeybindFunctions
        end

        return KeybindFunctions
    end
end
]],
