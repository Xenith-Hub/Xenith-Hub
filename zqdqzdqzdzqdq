	local TopbarUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "TopbarUIListLayout", FillDirection = Enum.FillDirection.Horizontal, SortOrder = Enum.SortOrder.LayoutOrder, HorizontalAlignment = Enum.HorizontalAlignment.Right, VerticalAlignment = Enum.VerticalAlignment.Center, Padding = UDim.new(0, 10), Parent = TopbarFolder})

	local MoveIcon = UILibraryUtility:CreateImageButton({Name = "MoveIcon", Image = Assets.Transform, ImageTransparency = 0.7, AnchorPoint = Vector2.new(1, 0.5), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(1, 0.5), Size = UDim2.fromOffset(15, 15), Parent = TopbarFolder})
	MoveIcon.Visible = not Settings.DragStyle or Settings.DragStyle == 1

	local Interact = UILibraryUtility:CreateTextButton({Name = "Interact", FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json"), Text = "", TextColor3 = Color3.fromRGB(0, 0, 0), TextSize = 14, AnchorPoint = Vector2.new(0.5, 0.5), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0.5, 0.5), Size = UDim2.fromOffset(40, 40), Parent = MoveIcon})

	local function ChangeMoveIconState(State)
		if State == "Default" then
			Utility:PropertyTween(MoveIcon, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
				ImageTransparency = 0.7
			}):Play()
		elseif State == "Hover" then
			Utility:PropertyTween(MoveIcon, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
				ImageTransparency = 0.4
			}):Play()
		end
	end

	Interact.MouseEnter:Connect(function()
		ChangeMoveIconState("Hover")
	end)

	Interact.MouseLeave:Connect(function()
		ChangeMoveIconState("Default")
	end)

	local Dragging = false
	local DragInput
	local DragStart
	local StartPos

	local function Update(Input)
		local Delta = Input.Position - DragStart
		Base.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + Delta.X, StartPos.Y.Scale, StartPos.Y.Offset + Delta.Y)
	end

	local function OnDragStart(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPos = Base.Position

			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end

	local function OnDragUpdate(Input)
		if Dragging and (Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch) then
			DragInput = Input
		end
	end

	if not Settings.DragStyle or Settings.DragStyle == 1 then
		Interact.InputBegan:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
				OnDragStart(Input)
			end
		end)

		Interact.InputChanged:Connect(OnDragUpdate)

		Services.UserInputService.InputChanged:Connect(function(Input)
			if Input == DragInput and Dragging then
				Update(Input)
			end
		end)

		Interact.InputEnded:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
				Dragging = false
			end
		end)
	elseif Settings.DragStyle == 2 then
		Base.InputBegan:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
				OnDragStart(Input)
			end
		end)

		Base.InputChanged:Connect(OnDragUpdate)

		Services.UserInputService.InputChanged:Connect(function(Input)
			if Input == DragInput and Dragging then
				Update(Input)
			end
		end)

		Base.InputEnded:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
				Dragging = false
			end
		end)
	end

	local CurrentTabDivider = UILibraryUtility:CreateFrame({Name = "CurrentTabDivider", AnchorPoint = Vector2.new(0, 1), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.9, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Visible = false, Position = UDim2.new(0.5, 0, 0, 42.5), AnchorPoint = Vector2.new(0.5, 0), Size = UDim2.new(0.05, 0, 0, 1), Parent = Elements})

	local CurrentTab = UILibraryUtility:CreateTextLabel({Name = "CurrentTab", FontFace = Font.new(Assets.InterFont, Enum.FontWeight.SemiBold), RichText = true, Text = "Loading", RichText = true, TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 18, TextTransparency = 0.5, TextTruncate = Enum.TextTruncate.SplitWord, TextXAlignment = Enum.TextXAlignment.Center, TextYAlignment = Enum.TextYAlignment.Top, AnchorPoint = Vector2.new(0, 0.5), AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0, 0.5), Size = UDim2.fromScale(1, 0), Parent = Elements})

	Elements.Parent = Topbar
	Topbar.Parent = Content
	Content.Parent = Base

	local GlobalSettings = Instance.new("Frame")
	GlobalSettings.Name = "Settings"
	GlobalSettings.AutomaticSize = Enum.AutomaticSize.XY
	GlobalSettings.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	GlobalSettings.BorderColor3 = Color3.fromRGB(0, 0, 0)
	GlobalSettings.BorderSizePixel = 0
	GlobalSettings.Position = UDim2.fromScale(0.07, 0.025)

	UILibraryUtility:CreateUIStroke({Name = "GlobalSettingsUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.9}, GlobalSettings)

	UILibraryUtility:CreateUICorner(10, GlobalSettings)

	local GlobalSettingsUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "GlobalSettingsUIPadding", PaddingBottom = UDim.new(0, 10), PaddingTop = UDim.new(0, 10), Parent = GlobalSettings})

	local GlobalSettingsUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "GlobalSettingsUIListLayout", Padding = UDim.new(0, 5), SortOrder = Enum.SortOrder.LayoutOrder, Parent = GlobalSettings})

	local GlobalSettingsUIScale = UILibraryUtility:CreateInstance("UIScale", {Name = "GlobalSettingsUIScale", Scale = 1e-07, Parent = GlobalSettings})
	GlobalSettings.Parent = Base

	Base.Parent = XenithLibraryUI

	local Terminal = TerminalModule.new(Base, TopbarFolder, XenithLibrary, XenithLibraryUI, WindowFunctions)

	local Hovering
	local Toggled = GlobalSettingsUIScale.Scale == 1 and true or false

	local function Toggle()
		if not Toggled then
			local InTween = Utility:PropertyTween(GlobalSettingsUIScale, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
				Scale = 1
			})
			InTween:Play()
			InTween.Completed:Wait()
			Toggled = true
		elseif Toggled then
			local OutTween = Utility:PropertyTween(GlobalSettingsUIScale, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
				Scale = 0
			})
			OutTween:Play()
			OutTween.Completed:Wait()
			Toggled = false
		end
	end

	Services.UserInputService.InputEnded:Connect(function(Inp)
		if Inp.UserInputType == Enum.UserInputType.MouseButton1 and Toggled and not Hovering then
			Toggle()
		end
	end)

	local BlurTargets = {Base, Base.Terminal}
	local DepthOfFieldTag = "."
	local BlurSize = 0.2
	local FrameSize = UDim2.new(0.97, 0, 0.97, 0)
	local FramePosition = UDim2.new(0.5, 0, 0.5, 0)
	local FrameAnchor = Vector2.new(0.5, 0.5)
	local BlurTransparency = 0.98
	local BlurColor = BrickColor.new('Institutional white')
	local ZIndexOffset = 0.05

	local NewInstance = Instance.new
	local Sqrt = math.sqrt
	local Acos = math.acos
	local Max = math.max
	local Pi = math.pi
	local Sin = math.sin
	local Cos = math.cos
	local Rad = math.rad

	local Camera = Utility:GetCamera()
	local WedgeGuid = Services.HttpService:GenerateGUID(true)
	local Binds = {}
	local DepthOfField

	local function IsNotNaN(Value)
		return Value == Value
	end

	local function SafeDisconnect(Connection)
		if Connection and typeof(Connection) == "RBXScriptConnection" then
			pcall(function()
				Connection:Disconnect()
			end)
		end
	end

	local function SafeDestroy(Instance)
		if Instance then
			pcall(function()
				Instance.Parent = nil
				Instance:Destroy()
			end)
		end
	end

	local function GetOrCreateDepthOfField()
		for _, LightInstance in pairs(Services.Lighting:GetChildren()) do
			if LightInstance:IsA("DepthOfFieldEffect") and LightInstance:HasTag(DepthOfFieldTag) then
				return LightInstance
			end
		end
		
		local DepthOfFieldEffect = NewInstance('DepthOfFieldEffect')
		DepthOfFieldEffect.FarIntensity = 0
		DepthOfFieldEffect.FocusDistance = 51.6
		DepthOfFieldEffect.InFocusRadius = 50
		DepthOfFieldEffect.NearIntensity = 1
		DepthOfFieldEffect.Name = Services.HttpService:GenerateGUID(true)
		DepthOfFieldEffect:AddTag(DepthOfFieldTag)
		
		return DepthOfFieldEffect
	end

	local function WaitForCameraReady(CameraInstance, Timeout)
		local StartTime = tick()
		Timeout = Timeout or 5
		
		while tick() - StartTime < Timeout do
			local Success, Ray = pcall(function()
				return CameraInstance:ScreenPointToRay(0, 0)
			end)
			
			if Success and Ray and IsNotNaN(Ray.Origin.x) then
				return true
			end
			
			Services.RunService.RenderStepped:Wait()
		end
		
		return false
	end

	local PartPool = {}
	PartPool.Available = {}
	PartPool.Active = {}

	function PartPool.GetPart()
		local Part = table.remove(PartPool.Available)
		
		if not Part then
			Part = NewInstance('Part')
			Part.FormFactor = 'Custom'
			Part.TopSurface = 0
			Part.BottomSurface = 0
			Part.Anchored = true
			Part.CanCollide = false
			Part.CastShadow = false
			Part.Material = "Glass"
			Part.Size = Vector3.new(BlurSize, BlurSize, BlurSize)
			Part.Name = Services.HttpService:GenerateGUID(true)
			Part.Transparency = BlurTransparency
			Part.BrickColor = BlurColor
			
			local Mesh = NewInstance('SpecialMesh', Part)
			Mesh.MeshType = 2
			Mesh.Name = WedgeGuid
		end
		
		table.insert(PartPool.Active, Part)
		return Part
	end

	function PartPool.ReturnPart(Part)
		if not Part then return end
		
		for Index, ActivePart in ipairs(PartPool.Active) do
			if ActivePart == Part then
				table.remove(PartPool.Active, Index)
				break
			end
		end
		
		Part.Parent = nil
		table.insert(PartPool.Available, Part)
	end

	function PartPool.Clear()
		for _, Part in ipairs(PartPool.Active) do
			SafeDestroy(Part)
		end
		
		for _, Part in ipairs(PartPool.Available) do
			SafeDestroy(Part)
		end
		
		PartPool.Active = {}
		PartPool.Available = {}
	end

	local function DrawTriangle(Vertex1, Vertex2, Vertex3, Part0, Part1)
		local Side1 = (Vertex1 - Vertex2).Magnitude
		local Side2 = (Vertex2 - Vertex3).Magnitude
		local Side3 = (Vertex3 - Vertex1).Magnitude
		local SideMaximum = Max(Side1, Side2, Side3)
		
		local VertexA, VertexB, VertexC
		if Side1 == SideMaximum then
			VertexA, VertexB, VertexC = Vertex1, Vertex2, Vertex3
		elseif Side2 == SideMaximum then
			VertexA, VertexB, VertexC = Vertex2, Vertex3, Vertex1
		else
			VertexA, VertexB, VertexC = Vertex3, Vertex1, Vertex2
		end
		
		local EdgeLength = (VertexA - VertexB).Magnitude
		local Parallel = ((VertexB - VertexA):Dot(VertexC - VertexA)) / EdgeLength
		local Perpendicular = Sqrt(Max(0, (VertexC - VertexA).Magnitude^2 - Parallel * Parallel))
		local DifferenceParallel = EdgeLength - Parallel
		
		local Start = CFrame.new(VertexB, VertexA)
		local ZAngle = CFrame.Angles(Pi / 2, 0, 0)
		local CoordinateFrame0 = Start
		local TopLookVector = (CoordinateFrame0 * ZAngle).LookVector
		local MidPoint = VertexA + CFrame.new(VertexA, VertexB).LookVector * Parallel
		local NeededLookVector = CFrame.new(MidPoint, VertexC).LookVector
		local DotProduct = TopLookVector:Dot(NeededLookVector)
		local AngleCFrame = CFrame.Angles(0, 0, Acos(DotProduct))
		
		CoordinateFrame0 = CoordinateFrame0 * AngleCFrame
		if ((CoordinateFrame0 * ZAngle).LookVector - NeededLookVector).Magnitude > 0.01 then
			CoordinateFrame0 = CoordinateFrame0 * CFrame.Angles(0, 0, -2 * Acos(DotProduct))
		end
		CoordinateFrame0 = CoordinateFrame0 * CFrame.new(0, Perpendicular / 2, -(DifferenceParallel + Parallel / 2))
		
		local CoordinateFrame1 = Start * AngleCFrame * CFrame.Angles(0, Pi, 0)
		if ((CoordinateFrame1 * ZAngle).LookVector - NeededLookVector).Magnitude > 0.01 then
			CoordinateFrame1 = CoordinateFrame1 * CFrame.Angles(0, 0, 2 * Acos(DotProduct))
		end
		CoordinateFrame1 = CoordinateFrame1 * CFrame.new(0, Perpendicular / 2, DifferenceParallel / 2)
		
		if not Part0 or not Part0.Parent then
			Part0 = PartPool.GetPart()
		end
		
		local Mesh0 = Part0:FindFirstChild(WedgeGuid)
		if Mesh0 then
			Mesh0.Scale = Vector3.new(0, Perpendicular / BlurSize, Parallel / BlurSize)
			Part0.CFrame = CoordinateFrame0
		end
		
		if not Part1 or not Part1.Parent then
			Part1 = PartPool.GetPart()
		end
		
		local Mesh1 = Part1:FindFirstChild(WedgeGuid)
		if Mesh1 then
			Mesh1.Scale = Vector3.new(0, Perpendicular / BlurSize, DifferenceParallel / BlurSize)
			Part1.CFrame = CoordinateFrame1
		end
		
		return Part0, Part1
	end

	local function DrawQuad(Vertex1, Vertex2, Vertex3, Vertex4, Parts)
		Parts[1], Parts[2] = DrawTriangle(Vertex1, Vertex2, Vertex3, Parts[1], Parts[2])
		Parts[3], Parts[4] = DrawTriangle(Vertex3, Vertex2, Vertex4, Parts[3], Parts[4])
	end

	local function IsVisible(Frame)
		local Current = Frame
		while Current do
			if Current:IsA("GuiObject") then
				if not Current.Visible then
					return false
				end
			elseif Current:IsA("ScreenGui") then
				if not Current.Enabled then
					return false
				end
				break
			end
			Current = Current.Parent
		end
		return true
	end

	local function GetRotationSum(Parents)
		local Rotation = 0
		for _, Parent in ipairs(Parents) do
			Rotation = Rotation + Parent.Rotation
		end
		return Rotation
	end

	local function RotatePoint(Point, Middle, Rotation)
		local SinValue = Sin(Rad(Rotation))
		local CosValue = Cos(Rad(Rotation))
		local Offset = Point - Middle
		
		return Vector2.new(
			CosValue * Offset.X - SinValue * Offset.Y,
			SinValue * Offset.X + CosValue * Offset.Y
		) + Middle
	end

	local function CleanupBlur()
		for Frame, Data in pairs(Binds) do
			SafeDisconnect(Data.Connection)
			
			if Data.Parts then
				for _, Part in pairs(Data.Parts) do
					PartPool.ReturnPart(Part)
				end
				Data.Parts = {}
			end
			
			SafeDestroy(Frame)
		end
		
		Binds = {}
		PartPool.Clear()
		
		if DepthOfField then
			DepthOfField.Enabled = false
			DepthOfField.Parent = nil
		end
		
		SafeDisconnect(XenithLibrary.UpdateOrientation)
		XenithLibrary.UpdateOrientation = nil
	end

	DepthOfField = GetOrCreateDepthOfField()

	if XenithLibrary.Unloaded or not WaitForCameraReady(Camera) then
		CleanupBlur()
		return
	end

	for _, BlurTarget in ipairs(BlurTargets) do
		if not BlurTarget then continue end
		
		local Frame = NewInstance('Frame')
		Frame.Parent = BlurTarget
		Frame.Size = FrameSize
		Frame.Position = FramePosition
		Frame.AnchorPoint = FrameAnchor
		Frame.BackgroundTransparency = 1
		Frame.Name = Services.HttpService:GenerateGUID(true)
		
		if Binds[Frame] then continue end
		
		local Parts = {}
		local Parents = {}
		
		local function AddParent(Child)
			if Child:IsA('GuiObject') then
				table.insert(Parents, Child)
				if Child.Parent then
					AddParent(Child.Parent)
				end
			end
		end
		AddParent(Frame)
		
		local function UpdateOrientation()
			if not IsVisible(Frame) or not XenithLibrary.AcrylicBlur or XenithLibrary.Unloaded then
				for Index, Part in pairs(Parts) do
					PartPool.ReturnPart(Part)
					Parts[Index] = nil
				end
				
				if DepthOfField then
					DepthOfField.Enabled = false
					DepthOfField.Parent = nil
				end
				return
			end
			
			if DepthOfField and not DepthOfField.Parent then
				DepthOfField.Parent = Services.Lighting
				DepthOfField.Enabled = true
			end
			
			local ZIndex = 1 - ZIndexOffset * Frame.ZIndex
			local TopLeft = Frame.AbsolutePosition
			local BottomRight = Frame.AbsolutePosition + Frame.AbsoluteSize
			local TopRight = Vector2.new(BottomRight.X, TopLeft.Y)
			local BottomLeft = Vector2.new(TopLeft.X, BottomRight.Y)
			
			local Rotation = GetRotationSum(Parents)
			
			if Rotation ~= 0 and Rotation % 180 ~= 0 then
				local Middle = TopLeft:Lerp(BottomRight, 0.5)
				TopLeft = RotatePoint(TopLeft, Middle, Rotation)
				TopRight = RotatePoint(TopRight, Middle, Rotation)
				BottomLeft = RotatePoint(BottomLeft, Middle, Rotation)
				BottomRight = RotatePoint(BottomRight, Middle, Rotation)
			end
			
			local Success, Error = pcall(function()
				DrawQuad(
					Camera:ScreenPointToRay(TopLeft.X, TopLeft.Y, ZIndex).Origin,
					Camera:ScreenPointToRay(TopRight.X, TopRight.Y, ZIndex).Origin,
					Camera:ScreenPointToRay(BottomLeft.X, BottomLeft.Y, ZIndex).Origin,
					Camera:ScreenPointToRay(BottomRight.X, BottomRight.Y, ZIndex).Origin,
					Parts
				)
			end)
			
			if not Success then return end
			
			for _, Part in pairs(Parts) do
				if Part and Part:IsA("Part") then
					Part.Parent = Camera
				end
			end
		end
		
		local Connection = Services.RunService.RenderStepped:Connect(UpdateOrientation)
		XenithLibrary.UpdateOrientation = Connection
		Binds[Frame] = {Parts = Parts, Connection = Connection}
	end

	task.spawn(function()
		while not XenithLibrary.Unloaded do
			task.wait(0.1)
		end
		CleanupBlur()
	end)

	function WindowFunctions:AdjustSidebars(ValueOne, ValueTwo)
		Sidebar.Size = UDim2.new(0, ValueOne, 1, 0)
		Content.Size = UDim2.new(0, ValueTwo, 1, 0)
	end

	function WindowFunctions:GlobalSetting(Settings)
		XenithLibrary.InitializedGlobalSettings = true

		local GlobalSettingFunctions = {}
		local GlobalSetting = Instance.new("TextButton")
		GlobalSetting.Name = "GlobalSetting"
		GlobalSetting.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
		GlobalSetting.Text = ""
		GlobalSetting.TextColor3 = Color3.fromRGB(0, 0, 0)
		GlobalSetting.TextSize = 14
		GlobalSetting.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		GlobalSetting.BackgroundTransparency = 1
		GlobalSetting.BorderColor3 = Color3.fromRGB(0, 0, 0)
		GlobalSetting.BorderSizePixel = 0
		GlobalSetting.Size = UDim2.fromOffset(200, 30)

		local GlobalSettingToggleUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "GlobalSettingToggleUIPadding", PaddingLeft = UDim.new(0, 15), Parent = GlobalSetting})

		local SettingName = UILibraryUtility:CreateTextLabel({Name = "SettingName", FontFace = Font.new(Assets.InterFont), Text = Settings.Name, RichText = true, TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 13, TextTransparency = 0.5, TextTruncate = Enum.TextTruncate.SplitWord, TextXAlignment = Enum.TextXAlignment.Left, TextYAlignment = Enum.TextYAlignment.Top, AnchorPoint = Vector2.new(0, 0.5), AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(1.3e-07, 0.5), Size = UDim2.new(1,-40,0,0), Parent = GlobalSetting})

		local GlobalSettingToggleUIListLayout = UILibraryUtility:CreateUIListLayout({Name = "GlobalSettingToggleUIListLayout", Padding = UDim.new(0, 10), FillDirection = Enum.FillDirection.Horizontal, VerticalAlignment = Enum.VerticalAlignment.Center}, GlobalSetting)

		local Checkmark = Instance.new("TextLabel")
		Checkmark.Name = "Checkmark"
		Checkmark.FontFace = Font.new(
			Assets.InterFont,
			Enum.FontWeight.Medium,
			Enum.FontStyle.Normal
		)
		Checkmark.Text = ""
		Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
		Checkmark.TextSize = 13
		Checkmark.TextTransparency = 1
		Checkmark.TextXAlignment = Enum.TextXAlignment.Left
		Checkmark.TextYAlignment = Enum.TextYAlignment.Top
		Checkmark.AnchorPoint = Vector2.new(0, 0.5)
		Checkmark.AutomaticSize = Enum.AutomaticSize.Y
		Checkmark.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Checkmark.BackgroundTransparency = 1
		Checkmark.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Checkmark.BorderSizePixel = 0
		Checkmark.LayoutOrder = -1
		Checkmark.Position = UDim2.fromScale(1.3e-07, 0.5)
		Checkmark.Size = UDim2.fromOffset(-10, 0)
		Checkmark.Parent = GlobalSetting

		GlobalSetting.Parent = GlobalSettings

		local TweenSettings = {
			Duration = 0.2,
			EasingStyle = Enum.EasingStyle.Quint,
			TransparencyIn = 0.2,
			TransparencyOut = 0.5,
			CheckSizeIncrease = 12,
			CheckSizeDecrease = -GlobalSettingToggleUIListLayout.Padding.Offset,
			WaitTime = 1
		}

		local Tweens = {
			CheckIn = Utility:PropertyTween(Checkmark, TweenInfo.new(TweenSettings.Duration, TweenSettings.EasingStyle), {
				Size = UDim2.new(Checkmark.Size.X.Scale, TweenSettings.CheckSizeIncrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
			}),
			CheckOut = Utility:PropertyTween(Checkmark, TweenInfo.new(TweenSettings.Duration, TweenSettings.EasingStyle),{
				Size = UDim2.new(Checkmark.Size.X.Scale, TweenSettings.CheckSizeDecrease, Checkmark.Size.Y.Scale, Checkmark.Size.Y.Offset)
			}),
			NameIn = Utility:PropertyTween(SettingName, TweenInfo.new(TweenSettings.Duration, TweenSettings.EasingStyle),{
				TextTransparency = TweenSettings.TransparencyIn
			}),
			NameOut = Utility:PropertyTween(SettingName, TweenInfo.new(TweenSettings.Duration, TweenSettings.EasingStyle),{
				TextTransparency = TweenSettings.TransparencyOut
			})
		}

		local function Toggle(State)
			if not State then
				Tweens.CheckOut:Play()
				Tweens.NameOut:Play()
				Checkmark:GetPropertyChangedSignal("AbsoluteSize"):Connect(function()
					if Checkmark.AbsoluteSize.X <= 0 then
						Checkmark.TextTransparency = 1
					end
				end)
			else
				Tweens.CheckIn:Play()
				Tweens.NameIn:Play()
				Checkmark:GetPropertyChangedSignal("AbsoluteSize"):Connect(function()
					if Checkmark.AbsoluteSize.X > 0 then
						Checkmark.TextTransparency = 0
					end
				end)
			end
		end

		local IsToggled = Settings.Default
		Toggle(IsToggled)

		GlobalSetting.MouseButton1Click:Connect(function()
			IsToggled = not IsToggled
			Toggle(IsToggled)

			task.spawn(function()
				if Settings.Callback then
					Settings.Callback(IsToggled)
				end
			end)
		end)

		function GlobalSettingFunctions:UpdateName(NewName)
			SettingName.Text = NewName
		end

		function GlobalSettingFunctions:UpdateState(NewState)
			Toggle(NewState)
			IsToggled = NewState
		end

		return GlobalSettingFunctions
	end

	function WindowFunctions:TabGroup()
		local SectionFunctions = {}

		local TabGroup = Instance.new("Frame")
		TabGroup.Name = "Section"
		TabGroup.AutomaticSize = Enum.AutomaticSize.Y
		TabGroup.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabGroup.BackgroundTransparency = 1
		TabGroup.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TabGroup.BorderSizePixel = 0
		TabGroup.Size = UDim2.fromScale(1, 0)

		local Divider3 = UILibraryUtility:CreateFrame({Name = "Divider", AnchorPoint = Vector2.new(0.5, 1), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.9, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0.5, 1), Size = UDim2.new(1, -21, 0, 1), Parent = TabGroup})

		local SectionTabSwitchers = Instance.new("Frame")
		SectionTabSwitchers.Name = "SectionTabSwitchers"
		SectionTabSwitchers.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SectionTabSwitchers.BackgroundTransparency = 1
		SectionTabSwitchers.BorderColor3 = Color3.fromRGB(0, 0, 0)
		SectionTabSwitchers.BorderSizePixel = 0
		SectionTabSwitchers.Size = UDim2.fromScale(1, 1)

		local UIListLayout1 = UILibraryUtility:CreateInstance("UIListLayout", {Name = "UIListLayout", Padding = UDim.new(0, 15), HorizontalAlignment = Enum.HorizontalAlignment.Center, SortOrder = Enum.SortOrder.LayoutOrder, Parent = SectionTabSwitchers})

		local UIPadding1 = UILibraryUtility:CreateInstance("UIPadding", {Name = "UIPadding", PaddingBottom = UDim.new(0, 15), Parent = SectionTabSwitchers})

		SectionTabSwitchers.Parent = TabGroup
		TabGroup.Parent = TabSwitchersScrollingFrame

		function SectionFunctions:Tab(Settings)
			local TabFunctions = {Settings = Settings}

			if not XenithLibrary.TabSettings.InitializeTab or XenithLibrary.TabSettings.InitializeTab == nil then
				XenithLibrary.TabSettings.InitializeTab = Settings.Name
			end

			local TabSwitcher = Instance.new("TextButton")
			TabSwitcher.Name = "TabSwitcher"
			TabSwitcher.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
			TabSwitcher.Text = ""
			TabSwitcher.Visible = not TabFunctions.Settings.Hidden
			TabSwitcher.TextColor3 = Color3.fromRGB(0, 0, 0)
			TabSwitcher.TextSize = 14
			TabSwitcher.AutoButtonColor = false
			TabSwitcher.AnchorPoint = Vector2.new(0.5, 0)
			TabSwitcher.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TabSwitcher.BackgroundTransparency = 1
			TabSwitcher.BorderColor3 = Color3.fromRGB(0, 0, 0)
			TabSwitcher.BorderSizePixel = 0
			TabSwitcher.Position = UDim2.fromScale(0.5, 0)
			TabSwitcher.Size = UDim2.new(1, -21, 0, 40)

			Maid.TabIndex += 1
			TabSwitcher.LayoutOrder = Maid.TabIndex

			local TabSwitcherUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "TabSwitcherUICorner", Parent = TabSwitcher})

			UILibraryUtility:CreateUIStroke({Name = "TabSwitcherUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 1}, TabSwitcher)

			UILibraryUtility:CreateUIListLayout({Name = "TabSwitcherUIListLayout", Padding = UDim.new(0, 9), FillDirection = Enum.FillDirection.Horizontal, VerticalAlignment = Enum.VerticalAlignment.Center}, TabSwitcher)

			local TabSwitcherIndicator = UILibraryUtility:CreateFrame({Name = "TabSwitcherIndicator", Size = UDim2.new(0, 4, 0.5, 0), Position = UDim2.new(0.08, 0, 0, 0), BackgroundTransparency = 0.4, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Parent = TabSwitcher})

			local TabSwitcherIndicatorUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "TabSwitcherIndicatorUICorner", Parent = TabSwitcherIndicator})

			local TabImage

			if TabFunctions.Settings.Image then
				TabImage = Instance.new("ImageLabel")
				TabImage.Name = "TabImage"
				TabImage.Image = Settings.Image
				TabImage.ImageTransparency = 0.5
				TabImage.ImageTransparency = 0.5
				TabImage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TabImage.BackgroundTransparency = 1
				TabImage.BorderColor3 = Color3.fromRGB(0, 0, 0)
				TabImage.BorderSizePixel = 0
				TabImage.Size = UDim2.fromOffset(18, 18)
				TabImage.Parent = TabSwitcher
			end

			local TabSwitcherName = Instance.new("TextLabel")
			TabSwitcherName.Name = "TabSwitcherName"
			TabSwitcherName.FontFace = Font.new(
				Assets.InterFont,
				Enum.FontWeight.Medium,
				Enum.FontStyle.Normal
			)
			TabSwitcherName.Text = TabFunctions.Settings.Name
			TabSwitcherName.RichText = true
			TabSwitcherName.TextColor3 = Color3.fromRGB(255, 255, 255)
			TabSwitcherName.TextSize = 16
			TabSwitcherName.TextTransparency = 0.5
			TabSwitcherName.TextTruncate = Enum.TextTruncate.SplitWord
			TabSwitcherName.TextXAlignment = Enum.TextXAlignment.Left
			TabSwitcherName.TextYAlignment = Enum.TextYAlignment.Top
			TabSwitcherName.AutomaticSize = Enum.AutomaticSize.Y
			TabSwitcherName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TabSwitcherName.BackgroundTransparency = 1
			TabSwitcherName.BorderColor3 = Color3.fromRGB(0, 0, 0)
			TabSwitcherName.BorderSizePixel = 0
			TabSwitcherName.Size = UDim2.fromScale(1, 0)
			TabSwitcherName.Parent = TabSwitcher
			TabSwitcherName.LayoutOrder = 1

			local TabSwitcherUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "TabSwitcherUIPadding", PaddingLeft = UDim.new(0, 24), PaddingRight = UDim.new(0, 35), PaddingTop = UDim.new(0, 1), Parent = TabSwitcher})

			TabSwitcher.Parent = SectionTabSwitchers

			local Elements1 = Instance.new("Frame")
			Elements1.Name = "Elements"
			Elements1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Elements1.BackgroundTransparency = 1
			Elements1.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Elements1.BorderSizePixel = 0
			Elements1.Position = UDim2.fromOffset(0, 63)
			Elements1.Size = UDim2.new(1, 0, 1, -63)
			Elements1.ClipsDescendants = true

			local ElementsUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "ElementsUIPadding", PaddingRight = UDim.new(0, 5), PaddingTop = UDim.new(0, 10), PaddingBottom = UDim.new(0, 10), Parent = Elements1})

			local ElementsScrolling = Instance.new("ScrollingFrame")
			ElementsScrolling.Name = "ElementsScrolling"
			ElementsScrolling.AutomaticCanvasSize = TabFunctions.Settings.MiddleFrame and Enum.AutomaticSize.None or Enum.AutomaticSize.Y
			ElementsScrolling.BottomImage = ""
			ElementsScrolling.CanvasSize = TabFunctions.Settings.MiddleFrame and UDim2.new() or UDim2.new(0, 0, 1, 100)
			ElementsScrolling.ScrollBarImageTransparency = 0.5
			ElementsScrolling.ScrollBarThickness = 1
			ElementsScrolling.TopImage = ""
			ElementsScrolling.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ElementsScrolling.BackgroundTransparency = 1
			ElementsScrolling.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ElementsScrolling.BorderSizePixel = 0
			ElementsScrolling.Size = UDim2.fromScale(1, 1)
			ElementsScrolling.ClipsDescendants = false

			UILibraryUtility:CreateUIPadding(11, 3, 5, 5, ElementsScrolling)

			UILibraryUtility:CreateUIListLayout({Name = "ElementsScrollingUIListLayout", Padding = UDim.new(0, 15), FillDirection = Enum.FillDirection.Horizontal}, ElementsScrolling)

			local Left = Instance.new("Frame")
			Left.Name = "Left"
			Left.AutomaticSize = Enum.AutomaticSize.Y
			Left.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			Left.BackgroundTransparency = 1
			Left.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Left.BorderSizePixel = 0
			Left.Position = UDim2.fromScale(0.512, 0)
			Left.Size = UDim2.new(0.5, -10, 0, 0)

			local LeftUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "LeftUIListLayout", Padding = UDim.new(0, 15), SortOrder = Enum.SortOrder.LayoutOrder, Parent = Left})

			local Right = Instance.new("Frame")
			Right.Name = "Right"
			Right.AutomaticSize = Enum.AutomaticSize.Y
			Right.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			Right.BackgroundTransparency = 1
			Right.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Right.BorderSizePixel = 0
			Right.LayoutOrder = 1
			Right.Position = UDim2.fromScale(0.512, 0)
			Right.Size = UDim2.new(0.5, -10, 0, 0)

			local RightUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "RightUIListLayout", Padding = UDim.new(0, 15), SortOrder = Enum.SortOrder.LayoutOrder, Parent = Right})

			local Middle = Instance.new("ScrollingFrame")
			Middle.Name = "Middle"
			Middle.AutomaticSize = Enum.AutomaticSize.Y
			Middle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Middle.BackgroundTransparency = 1
			Middle.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Middle.BorderSizePixel = 0
			Middle.Position = UDim2.fromScale(0.512, 0)
			Middle.Size = UDim2.new(1, -10, 1, 0)
			Middle.ScrollBarImageTransparency = 0.8
			Middle.ScrollBarThickness = 1
			Middle.CanvasSize = UDim2.new(0, 0, 15, 0)

			if TabFunctions.Settings.MiddleFrame then
				Middle.Parent = ElementsScrolling

				local MiddleFrameUIList = Instance.new("UIListLayout")
				for Property, Value in pairs(TabFunctions.Settings.FrameSettings) do
					if MiddleFrameUIList[Property] ~= nil then
						MiddleFrameUIList[Property] = Value
					end
				end

				MiddleFrameUIList.Parent = Middle
			else
				Left.Parent = ElementsScrolling
				Right.Parent = ElementsScrolling
			end

			ElementsScrolling.Parent = Elements1

			function TabFunctions:Card(Item)
				Item.Parent = Middle
			end

			function TabFunctions:HideContents()
				Middle.Visible = false
			end

			function TabFunctions:ShowContents()
				Middle.Visible = true
			end

			function TabFunctions:CreateLoading()
				local LoadingContainer = UILibraryUtility:CreateFrame({Name = "LoadingContainer", BackgroundTransparency = 1, Size = UDim2.fromScale(1, 1), Position = UDim2.fromScale(0, 0), Parent = ElementsScrolling})

				local LoadingFrame = UILibraryUtility:CreateFrame({Name = "LoadingFrame", BackgroundTransparency = 1, Size = UDim2.fromOffset(120, 40), Position = UDim2.fromScale(0.5, 0.5), AnchorPoint = Vector2.new(0.5, 0.5), Parent = LoadingContainer})

				local Dots = {}
				local DotCount = 5
				local DotSize = 8
				local Spacing = 20

				for I = 1, DotCount do
					local Dot = UILibraryUtility:CreateFrame({Name = "LoadingDot" .. I, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.3, BorderSizePixel = 0, Size = UDim2.fromOffset(DotSize, DotSize), Position = UDim2.fromOffset((I - 1) * Spacing + (120 - (DotCount - 1) * Spacing) / 2, 16), Parent = LoadingFrame})

					local Corner = UILibraryUtility:CreateInstance("UICorner", {CornerRadius = UDim.new(1, 0), Parent = Dot})

					table.insert(Dots, Dot)
				end

				local function AnimateDots()
					local AnimationDelay = 0.15
					local AnimationDuration = 0.4
					local ScaleFactor = 1.5

					spawn(function()
						while LoadingContainer.Parent do
							for I, Dot in ipairs(Dots) do
								spawn(function()
									local ScaleUpTween = Services.TweenService:Create(
										Dot,
										TweenInfo.new(AnimationDuration / 2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
										{
											Size = UDim2.fromOffset(DotSize * ScaleFactor, DotSize * ScaleFactor),
											BackgroundTransparency = 0.1
										}
									)

									local ScaleDownTween = Services.TweenService:Create(
										Dot,
										TweenInfo.new(AnimationDuration / 2, Enum.EasingStyle.Quart, Enum.EasingDirection.In),
										{
											Size = UDim2.fromOffset(DotSize, DotSize),
											BackgroundTransparency = 0.3
										}
									)

									ScaleUpTween:Play()
									ScaleUpTween.Completed:Connect(function()
										ScaleDownTween:Play()
									end)
								end)

								task.wait(AnimationDelay)
							end

							task.wait(0.3)
						end
					end)
				end

				AnimateDots()

				return LoadingContainer
			end

			function TabFunctions:RemoveLoading()
				local LoadingContainer = ElementsScrolling:FindFirstChild("LoadingContainer")
				if LoadingContainer then
					LoadingContainer:Destroy()
				end
			end

			function TabFunctions:CreateCustomElementPanel()
				local CustomPanel = UILibraryUtility:CreateFrame({Name = "CustomElementPanel", BackgroundColor3 = Color3.fromRGB(15, 15, 15), BackgroundTransparency = 0.25, BorderSizePixel = 0, Size = UDim2.fromScale(1, 1), Position = UDim2.fromScale(0, 0), Parent = ElementsScrolling})

				UILibraryUtility:CreateUICorner(12, CustomPanel)

				local PanelStroke = UILibraryUtility:CreateInstance("UIStroke", {ApplyStrokeMode = Enum.ApplyStrokeMode.Border, LineJoinMode = Enum.LineJoinMode.Round, Color = Color3.fromRGB(60, 60, 70), Thickness = 1, Transparency = 0.5, Parent = CustomPanel})

				local PanelPadding = UILibraryUtility:CreateInstance("UIPadding", {PaddingTop = UDim.new(0, 20), PaddingBottom = UDim.new(0, 20), PaddingLeft = UDim.new(0, 20), PaddingRight = UDim.new(0, 20), Parent = CustomPanel})

				return CustomPanel
			end

			function TabFunctions:Section(Settings)
				local SectionFunctions = {}
				local Section = UILibraryUtility:CreateFrame({Name = "Section", AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.98, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0, 6.78e-08), Size = UDim2.fromScale(1, 0), ClipsDescendants = true, Parent = Settings.Side == "Left" and Left or Right})

				local SectionPropertiesFolder = UILibraryUtility:CreateInstance("Folder", {Name = "SectionPropertiesFolder", Parent = Section})

				local SectionToggleVisible = UILibraryUtility:CreateImageButton({Name = "SectionToggleVisible", Image = Assets.ButtonImage, ImageColor3 = Color3.fromRGB(87, 86, 86), AutoButtonColor = false, AnchorPoint = Vector2.new(1, 0.5), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Size = UDim2.fromOffset(20, 20), ImageTransparency = 0, ZIndex = 100, Rotation = 90, Position = UDim2.new(1, 0, 0, 12.5), Parent = SectionPropertiesFolder})

				local SavedElements = false
				SectionToggleVisible.MouseButton1Click:Connect(function()
					if SavedElements then
						for _, Element in pairs(SavedElements) do
							if Element:IsA("Frame") then
								Element.Visible = true
							end
						end

						SavedElements = false
						Utility:QuickTween(SectionToggleVisible, {Rotation = 90})
					else
						SavedElements = {}
						local DetectedHeaders = 0
						for _, Element in pairs(Section:GetChildren()) do
							if Element:IsA("Frame") and Element.Visible and Element.Name ~= "Header" then
								SavedElements[Element] = Element
								Element.Visible = false
							elseif Element:IsA("Frame") and Element.Visible and Element.Name == "Header" then
								DetectedHeaders += 1
								if DetectedHeaders == 2 then
									SavedElements[Element] = Element
									Element.Visible = false
								end
							end
						end

						Utility:QuickTween(SectionToggleVisible, {Rotation = 0})
					end
				end)

				local SectionUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "SectionUICorner", Parent = Section})

				UILibraryUtility:CreateUIStroke({Name = "SectionUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.95}, Section)

				local SectionUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "SectionUIListLayout", Padding = UDim.new(0, 10), SortOrder = Enum.SortOrder.LayoutOrder, Parent = Section})

				UILibraryUtility:CreateUIPadding(20, 18, 22, 20, Section)

				local ComponentDirectory = "UserInterface/Components"
				for Name, Component in pairs(Utility:ListManager(ComponentDirectory)) do
					Utility:LoadFile(Component)({
						Section = Section,
						WindowFunctions = WindowFunctions,
						XenithLibraryUI = XenithLibraryUI,
						XenithLibrary = XenithLibrary,
						SectionFunctions = SectionFunctions,
						BaseUIScale = BaseUIScale,
						Base = Base,

						ComponentManager = ComponentManager,
						TabName = TabFunctions.Settings.Name
					})
				end

				function SectionFunctions:Replication()
					if Settings.Replication then
						local ReplicationGame, ReplicationSection = Settings.Replication.GameDirectory, Settings.Replication.SectionName
						Utility:LoadFile("Games/Game Features/" .. ReplicationGame .. "/" .. ReplicationSection .. ".lua")

						return true
					end

					return false
				end

				function SectionFunctions:DisplayTitle()
					if Settings.DisplayTitle then
						SectionFunctions:Header({Name = Settings.DisplayTitle})
					end

					return Settings.DisplayTitle
				end

				return SectionFunctions
			end

			local function TabSelection()
				local EaseTime = 0.25
				local EaseStyle = Enum.EasingStyle.Quad
				local EaseDirection = Enum.EasingDirection.Out
				local VerticalOffset = 80

				if XenithLibrary.TabSettings.CurrentTab then
					XenithLibrary.TabSettings.CurrentTab.Parent = nil
				end

				if Settings.OnTouchCallback then
					Settings.OnTouchCallback()
				end

				for Switcher, TabInfo in pairs(XenithLibrary.Tabs) do
					Utility:PropertyTween(Switcher, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
						BackgroundTransparency = (Switcher == TabSwitcher and 0.98 or 1)
					}):Play()

					if TabInfo.TabIndicator then
						TabInfo.TabIndicator.BackgroundColor3 = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
						Utility:PropertyTween(TabInfo.TabIndicator, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
							BackgroundTransparency = (Switcher == TabSwitcher and 0.5 or 1)
						}):Play()
					end

					if TabInfo.TabStroke then
						Utility:PropertyTween(TabInfo.TabStroke, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
							Transparency = (Switcher == TabSwitcher and 0.95 or 1)
						}):Play()
					end

					if TabInfo.SwitcherImage then
						Utility:PropertyTween(TabInfo.SwitcherImage, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
							ImageTransparency = (Switcher == TabSwitcher and 0.1 or 0.5)
						}):Play()
					end

					if TabInfo.SwitcherName then
						Utility:PropertyTween(TabInfo.SwitcherName, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
							TextTransparency = (Switcher == TabSwitcher and 0.1 or 0.5)
						}):Play()
					end
				end

				local TabContent = XenithLibrary.Tabs[TabSwitcher].TabContent

				local function StoreCurrentState(Element)
					if Element:IsA("GuiObject") then
						Element:SetAttribute("CurrentBackgroundTransparency", Element.BackgroundTransparency)

						if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
							Element:SetAttribute("CurrentTextTransparency", Element.TextTransparency)
						end

						if Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
							Element:SetAttribute("CurrentImageTransparency", Element.ImageTransparency)
						end

						local Stroke = Element:FindFirstChildOfClass("UIStroke")
						if Stroke then
							Element:SetAttribute("CurrentStrokeTransparency", Stroke.Transparency)
						end

						for _, Child in pairs(Element:GetChildren()) do
							StoreCurrentState(Child)
						end
					end
				end

				if not TabContent:GetAttribute("OriginalStateStored") then
					local function StoreOriginalTransparencies(Element)
						if Element:IsA("GuiObject") then
							Element:SetAttribute("OriginalBackgroundTransparency", Element.BackgroundTransparency)

							if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
								Element:SetAttribute("OriginalTextTransparency", Element.TextTransparency)
							end

							if Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
								Element:SetAttribute("OriginalImageTransparency", Element.ImageTransparency)
							end

							local Stroke = Element:FindFirstChildOfClass("UIStroke")
							if Stroke then
								Element:SetAttribute("OriginalStrokeTransparency", Stroke.Transparency)
							end

							for _, Child in pairs(Element:GetChildren()) do
								StoreOriginalTransparencies(Child)
							end
						end
					end

					StoreOriginalTransparencies(TabContent)
					TabContent:SetAttribute("OriginalStateStored", true)
				else
					StoreCurrentState(TabContent)
				end

				local function SetFullyTransparent(Element)
					if Element:IsA("GuiObject") then
						Element.BackgroundTransparency = 1

						if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
							Element.TextTransparency = 1
						end

						if Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
							Element.ImageTransparency = 1
						end

						local Stroke = Element:FindFirstChildOfClass("UIStroke")
						if Stroke and not Element:FindFirstAncestor("CheckBoxButton") and not Element:FindFirstAncestor("CheckBox") then
							Stroke.Transparency = 1
						end

						for _, Child in pairs(Element:GetChildren()) do
							SetFullyTransparent(Child)
						end
					end
				end

				TabContent.Position = UDim2.new(0, 0, 0, VerticalOffset)
				SetFullyTransparent(TabContent)

				TabContent.Parent = Content

				if WindowFunctions:GetCurrentTheme() then
					WindowFunctions:ApplyCurrentTheme()
				end

				local function AnimateToCurrentTransparency(Element)
					if Element:IsA("GuiObject") then
						local CurrentTheme = WindowFunctions:GetCurrentTheme()
						local ThemeData = CurrentTheme and WindowFunctions:GetTheme(CurrentTheme)

						local function GetThemeValue(ElementType, PropertyType)
							if not ThemeData then return nil end

							local ThemeMapping = {
								Base = {
									BackgroundTransparency = ThemeData.BackgroundTransparency,
									BackgroundColor3 = ThemeData.Background
								},
								Terminal = {
									BackgroundTransparency = ThemeData.BackgroundTransparency,
									BackgroundColor3 = ThemeData.Background
								},
								Sidebar = {
									BackgroundTransparency = ThemeData.SidebarTransparency,
									BackgroundColor3 = ThemeData.SidebarColor
								},
								Section = {
									BackgroundColor3 = ThemeData.Foreground
								},
								Surface = {
									BackgroundColor3 = ThemeData.Surface,
									BackgroundTransparency = ThemeData.SurfaceTransparency
								},
								Title = {
									TextColor3 = ThemeData.HeaderColor,
									TextTransparency = ThemeData.HeaderTransparency
								},
								ParagraphHeader = {
									TextColor3 = ThemeData.HeaderColor,
									TextTransparency = ThemeData.HeaderTransparency
								},
								HeaderText = {
									TextColor3 = ThemeData.HeaderColor,
									TextTransparency = ThemeData.HeaderTransparency
								},
								Wallpaper = {
									ImageTransparency = ThemeData.WallpaperEnabled and ThemeData.WallpaperTransparency or 1
								},
								OutputWallpaper = {
									ImageTransparency = ThemeData.WallpaperEnabled and ThemeData.WallpaperTransparency or 1
								}
							}

							local ElementTheme = ThemeMapping[Element.Name]
							return ElementTheme and ElementTheme[PropertyType]
						end

						local TargetBackgroundTransparency = GetThemeValue(Element.Name, "BackgroundTransparency")
						if TargetBackgroundTransparency == nil then
							local CurrentBackgroundTransparency = Element:GetAttribute("CurrentBackgroundTransparency")
							local OriginalBackgroundTransparency = Element:GetAttribute("OriginalBackgroundTransparency")
							TargetBackgroundTransparency = CurrentBackgroundTransparency or OriginalBackgroundTransparency
						end

						if TargetBackgroundTransparency ~= nil then
							local Properties = {
								BackgroundTransparency = TargetBackgroundTransparency
							}

							if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
								local TargetTextTransparency = nil

								if Element.Name == "Title" or Element.Name == "ParagraphHeader" or Element.Name == "HeaderText" then
									TargetTextTransparency = ThemeData and ThemeData.HeaderTransparency
								elseif Element:IsA("TextBox") then
									TargetTextTransparency = ThemeData and ThemeData.InputFieldTransparency
								end

								if TargetTextTransparency == nil then
									local CurrentTextTransparency = Element:GetAttribute("CurrentTextTransparency")
									local OriginalTextTransparency = Element:GetAttribute("OriginalTextTransparency")
									TargetTextTransparency = CurrentTextTransparency or OriginalTextTransparency
								end

								if TargetTextTransparency ~= nil then
									Properties.TextTransparency = TargetTextTransparency
								end

								local ThemeTextColor = GetThemeValue(Element.Name, "TextColor3")
								if ThemeTextColor then
									Properties.TextColor3 = ThemeTextColor
								end
							end

							if Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
								local TargetImageTransparency = GetThemeValue(Element.Name, "ImageTransparency")

								if TargetImageTransparency == nil then
									local CurrentImageTransparency = Element:GetAttribute("CurrentImageTransparency")
									local OriginalImageTransparency = Element:GetAttribute("OriginalImageTransparency")
									TargetImageTransparency = CurrentImageTransparency or OriginalImageTransparency
								end

								if TargetImageTransparency ~= nil then
									Properties.ImageTransparency = TargetImageTransparency
								end
							end

							Utility:PropertyTween(Element, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), Properties):Play()

							local Stroke = Element:FindFirstChildOfClass("UIStroke")
							if Stroke and not Stroke:FindFirstAncestor("CheckBoxButton") then
								local TargetStrokeTransparency = ThemeData and ThemeData.StrokeTransparency

								if TargetStrokeTransparency == nil then
									local CurrentStrokeTransparency = Element:GetAttribute("CurrentStrokeTransparency")
									local OriginalStrokeTransparency = Element:GetAttribute("OriginalStrokeTransparency")
									TargetStrokeTransparency = CurrentStrokeTransparency or OriginalStrokeTransparency
								end

								if TargetStrokeTransparency ~= nil then
									local StrokeProperties = {
										Transparency = TargetStrokeTransparency
									}

									if ThemeData and ThemeData.BorderColor then
										StrokeProperties.Color = ThemeData.BorderColor
									end

									Utility:PropertyTween(Stroke, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), StrokeProperties):Play()
								end
							end
						end

						for _, Child in pairs(Element:GetChildren()) do
							AnimateToCurrentTransparency(Child)
						end
					end
				end

				Utility:PropertyTween(TabContent, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
					Position = UDim2.new(0, 0, 0, 63)
				}):Play()

				AnimateToCurrentTransparency(TabContent)

				XenithLibrary.TabSettings.CurrentTab = TabContent
				CurrentTab.Text = Settings.Name

				local Widith = CurrentTab.TextBounds.X
				local Margin = 8

				CurrentTabDivider.Size = UDim2.new(0, math.max(0, Widith - Margin), 0, 1)
				CurrentTabDivider.Visible = CurrentTab.Text ~= "" and true or false
			end

			TabSwitcher.MouseButton1Click:Connect(function()
				if TabSwitcher:GetAttribute("Debounce") or XenithLibrary.TabSettings.CurrentTab == nil then 
					return 
				end

				TabSwitcher:SetAttribute("Debounce", true)

				TabSelection()

				Maid:GiveTask(task.delay(0.5, function()
					TabSwitcher:SetAttribute("Debounce", false)
				end))
			end)

			function TabFunctions:Select()
				TabSelection()
			end

			function TabFunctions:RetrieveFirstTab()
				return Settings.Name == XenithLibrary.TabSettings.InitializeTab
			end

			local CurrentTheme = "Default"
			local LivePreviewEnabled = true

			function WindowFunctions:Initialize()
				Themes.Custom = table.clone(Themes.Default)
				self:LoadStartupTheme()
			end

			local SettingsDirectory = "UserInterface/Imports/Settings"
			for Name, Component in pairs(Utility:ListManager(SettingsDirectory)) do
				Utility:LoadFile(Component)({
					WindowFunctions = WindowFunctions,
					XenithLibraryUI = XenithLibraryUI,
					XenithLibrary = XenithLibrary,
					TabFunctions = TabFunctions,
					ThemeColorSchemes = ThemeColorSchemes,
					ThemeTemplates = ThemeTemplates,
					PerformanceModes = PerformanceModes,
					ThemeUtility = ThemeUtility,
					Themes = Themes,

					Subtitle = Subtitle,
					Exit = Exit,
					Minimize = Minimize
				})
			end

			XenithLibrary.Tabs[TabSwitcher] = {
				TabContent = Elements1,
				TabStroke = TabSwitcherUIStroke,
				SwitcherImage = TabImage,
				SwitcherName = TabSwitcherName,
				TabIndicator = TabSwitcherIndicator
			}

			return TabFunctions
		end

		return SectionFunctions
	end

	function WindowFunctions:SelectTab(TabName)
		for TabSwitcher, TabInfo in pairs(XenithLibrary.Tabs) do
			if TabInfo.SwitcherName and TabInfo.SwitcherName.Text == TabName then
				if not TabSwitcher:GetAttribute("Debounce") and XenithLibrary.TabSettings.CurrentTab ~= nil then
					TabSwitcher:SetAttribute("Debounce", true)

					local EaseTime = 0.25
					local EaseStyle = Enum.EasingStyle.Quad
					local EaseDirection = Enum.EasingDirection.Out

					if XenithLibrary.TabSettings.CurrentTab then
						XenithLibrary.TabSettings.CurrentTab.Parent = nil
					end

					for Switcher, Info in pairs(XenithLibrary.Tabs) do
						Utility:PropertyTween(Switcher, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
							BackgroundTransparency = (Switcher == TabSwitcher and 0.98 or 1)
						}):Play()

						if Info.TabIndicator then
							Info.TabIndicator.BackgroundColor3 = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
							Utility:PropertyTween(Info.TabIndicator, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
								BackgroundTransparency = (Switcher == TabSwitcher and 0.5 or 1)
							}):Play()
						end

						if Info.TabStroke then
							Utility:PropertyTween(Info.TabStroke, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
								Transparency = (Switcher == TabSwitcher and 0.95 or 1)
							}):Play()
						end

						if Info.SwitcherImage then
							Utility:PropertyTween(Info.SwitcherImage, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
								ImageTransparency = (Switcher == TabSwitcher and 0.1 or 0.5)
							}):Play()
						end

						if Info.SwitcherName then
							Utility:PropertyTween(Info.SwitcherName, TweenInfo.new(EaseTime, EaseStyle, EaseDirection), {
								TextTransparency = (Switcher == TabSwitcher and 0.1 or 0.5)
							}):Play()
						end
					end

					local TabContent = TabInfo.TabContent
					TabContent.Parent = Content
					XenithLibrary.TabSettings.CurrentTab = TabContent

					Maid:GiveTask(task.delay(0.5, function()
						TabSwitcher:SetAttribute("Debounce", false)
					end))

					return true
				end
				return false, "Tab is debounced or no current tab exists"
			end
		end

		return false, "Tab not found: " .. tostring(TabName)
	end

	function WindowFunctions:BuildTree()
		local PathTree = {
			XenithLibrary.Folder,
			XenithLibrary.Folder .. "/settings",
			XenithLibrary.Folder .. "/settings/startup",
		}

		for Increment = 1, #PathTree do
			local String = PathTree[Increment]
			if not isfolder(String) then
				makefolder(String)
			end
		end
	end

	function WindowFunctions:CreateOption(Requirements, Option)
		local OptionInstance = UILibraryUtility:CreateFrame({AutomaticSize = Enum.AutomaticSize.Y, Name = Option.Settings.Name, BackgroundColor3 = Color3.fromRGB(0, 0, 0), Visible = false, BackgroundTransparency = 1, BorderSizePixel = 0, Size = UDim2.new(1, 0, 0, 20), Parent = Requirements.HeaderSearchOptions})

		local OptionIcon = UILibraryUtility:CreateImageLabel({BackgroundColor3 = Color3.fromRGB(0, 0, 0), Position = UDim2.new(0, 5, 0.5, 0), AnchorPoint = Vector2.new(0, 0.25), Name = "OptionIcon", BackgroundTransparency = 1, BorderSizePixel = 0, Size = UDim2.new(0, 17.5, 0, 17.5), Image = Assets["Search" .. Option.Class], Parent = OptionInstance})

		local OptionDetails = UILibraryUtility:CreateTextLabel({FontFace = Font.new(Assets.InterFont), Name = "OptionDetails", BackgroundColor3 = Color3.fromRGB(0, 0, 0), AnchorPoint = Vector2.new(0, 0.25), Position = UDim2.new(0, 30, 0.5, 0), BackgroundTransparency = 1, BorderSizePixel = 0, Size = UDim2.new(1, -30, 0, 20), TextTruncate = Enum.TextTruncate.SplitWord, TextSize = 10, RichText = true, Text = "<b>" .. Option.Settings.Name .. "</b>  |  <font transparency=\"0.5\">" .. Option:ReturnClass() .. " - " .. Option:ReturnTab() .. "</font>", TextXAlignment = Enum.TextXAlignment.Left, Parent = OptionInstance})

		local OptionButton = UILibraryUtility:CreateTextButton({Name = "OptionButton", BackgroundTransparency = 1, BorderSizePixel = 0, Size = UDim2.new(1, 0, 1, 0), Position = UDim2.new(0, 0, 0, 0), Text = "", ZIndex = 5, Parent = OptionInstance})

		local function CreateHoverAnimation()
			local IconTween = Services.TweenService:Create(
				OptionIcon,
				TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
				{Size = UDim2.new(0, 22, 0, 22)}
			)

			local TextTween = Services.TweenService:Create(
				OptionDetails,
				TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{Position = UDim2.new(0, 35, 0.5, 0)}
			)

			return {IconTween, TextTween}
		end

		local function CreateUnhoverAnimation()
			local IconTween = Services.TweenService:Create(
				OptionIcon,
				TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{Size = UDim2.new(0, 20, 0, 20)}
			)

			local TextTween = Services.TweenService:Create(
				OptionDetails,
				TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{Position = UDim2.new(0, 30, 0.5, 0)}
			)

			return {IconTween, TextTween}
		end

		local function CreateClickAnimation()
			local ShrinkTween = Services.TweenService:Create(
				OptionInstance,
				TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{Size = UDim2.new(0.98, 0, 0, 18)}
			)

			local IconShrinkTween = Services.TweenService:Create(
				OptionIcon,
				TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{Size = UDim2.new(0, 18, 0, 18)}
			)

			local FlashTween = Services.TweenService:Create(
				OptionInstance,
				TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundTransparency = 0.85}
			)

			ShrinkTween:Play()
			IconShrinkTween:Play()
			FlashTween:Play()

			ShrinkTween.Completed:Connect(function()
				local BounceTween = Services.TweenService:Create(
					OptionInstance,
					TweenInfo.new(0.15, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
					{Size = UDim2.new(1, 0, 0, 20)}
				)

				local IconBounceTween = Services.TweenService:Create(
					OptionIcon,
					TweenInfo.new(0.15, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
					{Size = UDim2.new(0, 22, 0, 22)}
				)

				BounceTween:Play()
				IconBounceTween:Play()
			end)
		end

		OptionButton.MouseEnter:Connect(function()
			local Animations = CreateHoverAnimation()
			for _, Animation in ipairs(Animations) do
				Animation:Play()
			end
		end)

		OptionButton.MouseLeave:Connect(function()
			local Animations = CreateUnhoverAnimation()
			for _, Animation in ipairs(Animations) do
				Animation:Play()
			end
		end)

		OptionButton.MouseButton1Down:Connect(function()
			CreateClickAnimation()
		end)

		OptionButton.MouseButton1Click:Connect(function()
			self:SelectTab(Option:ReturnTab())
		end)

		return OptionInstance
	end

	function WindowFunctions:EnableStatusHub()
		StatusHub.Visible = true
	end

	function WindowFunctions:DisableStatusHub()
		StatusHub.Visible = false
	end

	function WindowFunctions:SetStatusHubTitle(Name)
		StatusTitle.Text = Name or "No Title"
	end

	function WindowFunctions:AddStatusInfo(StatusName, Value)
		local Section = UILibraryUtility:CreateTextLabel({Name = "StatusSection", AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.98, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0, 0), ClipsDescendants = true, Parent = StatusHandle})

		local SectionUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "SectionUICorner", Parent = Section})

		UILibraryUtility:CreateUIStroke({Name = "SectionUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.95}, Section)

		Section.FontFace = Font.new(
			Assets.InterFont,
			Enum.FontWeight.Medium,
			Enum.FontStyle.Normal
		)
		Section.RichText = true
		Section.Text = StatusName .. "      " .. tostring(Value)
		Section.TextColor3 = Color3.fromRGB(255, 255, 255)
		Section.TextSize = 12
		Section.TextTransparency = 0

		task.defer(function()
			Section.Size = UDim2.new(0, Section.TextBounds.X + 15, 0, Section.TextBounds.Y + 5)
		end)
	end

	function WindowFunctions:EditStatusInfo(Flag, Value)
		for _, Child in ipairs(StatusHandle:GetChildren()) do
			if Child:IsA("TextLabel") and Child.Text:match("^" .. Flag) then
				Child.Text = Flag .. "      " .. tostring(Value)

				task.defer(function()
					Child.Size = UDim2.new(0, Child.TextBounds.X + 15, 0, Child.TextBounds.Y + 5)
				end)

				return
			end
		end
	end

	function WindowFunctions:Notify(Settings)
		local NotificationFunctions = {}

		local Notification = Instance.new("Frame")
		Notification.Name = "Notification"
		Notification.AnchorPoint = Vector2.new(0.5, 0.5)
		Notification.AutomaticSize = Enum.AutomaticSize.Y
		Notification.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		Notification.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Notification.BorderSizePixel = 0
		Notification.Position = UDim2.fromScale(0.5, 0.5)
		Notification.Size = UDim2.fromOffset(Settings.SizeX or 250, 0)

		Notification.Parent = Notifications

		UILibraryUtility:CreateUIStroke({Name = "NotificationUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.9}, Notification)

		UILibraryUtility:CreateUICorner(10, Notification)

		local NotificationUIScale = UILibraryUtility:CreateInstance("UIScale", {Name = "NotificationUIScale", Parent = Notification})
		NotificationUIScale.Scale = 0

		local NotificationInformation = Instance.new("Frame")
		NotificationInformation.Name = "NotificationInformation"
		NotificationInformation.AutomaticSize = Enum.AutomaticSize.Y
		NotificationInformation.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NotificationInformation.BackgroundTransparency = 1
		NotificationInformation.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NotificationInformation.BorderSizePixel = 0
		NotificationInformation.Size = UDim2.fromScale(1, 1)

		local NotificationTitle = Instance.new("TextLabel")
		NotificationTitle.Name = "NotificationTitle"
		NotificationTitle.FontFace = Font.new(
			Assets.InterFont,
			Enum.FontWeight.SemiBold,
			Enum.FontStyle.Normal
		)
		NotificationTitle.RichText = true
		NotificationTitle.Text = Settings.Title
		NotificationTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
		NotificationTitle.TextSize = 13
		NotificationTitle.TextTransparency = 0.2
		NotificationTitle.TextTruncate = Enum.TextTruncate.SplitWord
		NotificationTitle.TextXAlignment = Enum.TextXAlignment.Left
		NotificationTitle.TextYAlignment = Enum.TextYAlignment.Top
		NotificationTitle.AutomaticSize = Enum.AutomaticSize.XY
		NotificationTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NotificationTitle.BackgroundTransparency = 1
		NotificationTitle.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NotificationTitle.BorderSizePixel = 0
		NotificationTitle.Size = UDim2.new(1, -12, 0, 0)

		local NotificationTitleUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "NotificationTitleUIPadding", PaddingRight = UDim.new(0, 25), Parent = NotificationTitle})

		NotificationTitle.Parent = NotificationInformation

		local NotificationDescription = Instance.new("TextLabel")
		NotificationDescription.Name = "NotificationDescription"
		NotificationDescription.FontFace = Font.new(
			Assets.InterFont,
			Enum.FontWeight.Medium,
			Enum.FontStyle.Normal
		)
		NotificationDescription.Text = Settings.Description
		NotificationDescription.TextColor3 = Color3.fromRGB(255, 255, 255)
		NotificationDescription.TextSize = 11
		NotificationDescription.TextTransparency = 0.5
		NotificationDescription.TextWrapped = true
		NotificationDescription.RichText = true
		NotificationDescription.TextXAlignment = Enum.TextXAlignment.Left
		NotificationDescription.TextYAlignment = Enum.TextYAlignment.Top
		NotificationDescription.AutomaticSize = Enum.AutomaticSize.XY
		NotificationDescription.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NotificationDescription.BackgroundTransparency = 1
		NotificationDescription.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NotificationDescription.BorderSizePixel = 0
		NotificationDescription.Size = UDim2.new(1, -12, 0, 0)

		local NotificationDescriptionUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "NotificationDescriptionUIPadding", PaddingRight = UDim.new(0, 25), PaddingTop = UDim.new(0, 17), Parent = NotificationDescription})

		NotificationDescription.Parent = NotificationInformation

		UILibraryUtility:CreateUIPadding(10, 10, 10, 12, NotificationInformation)

		NotificationInformation.Parent = Notification

		local NotificationControls = Instance.new("Frame")
		NotificationControls.Name = "NotificationControls"
		NotificationControls.AutomaticSize = Enum.AutomaticSize.Y
		NotificationControls.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		NotificationControls.BackgroundTransparency = 1
		NotificationControls.BorderColor3 = Color3.fromRGB(0, 0, 0)
		NotificationControls.BorderSizePixel = 0
		NotificationControls.Size = UDim2.fromScale(1, 1)

		local Interactable = UILibraryUtility:CreateTextButton({Name = "Interactable", FontFace = Font.new(Assets.InterFont), Text = "", TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 17, TextTransparency = 0.2, AnchorPoint = Vector2.new(1, 0.5), AutomaticSize = Enum.AutomaticSize.XY, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, LayoutOrder = 1, Position = UDim2.fromScale(1, 0.5), Parent = NotificationControls})

		local UIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "UIPadding", PaddingBottom = UDim.new(0, 6), PaddingRight = UDim.new(0, 13), PaddingTop = UDim.new(0, 6), Parent = NotificationControls})

		NotificationControls.Parent = Notification

		local Tweens = {
			In = Utility:PropertyTween(NotificationUIScale, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
				Scale = Settings.Scale or 1
			}),
			Out = Utility:PropertyTween(NotificationUIScale, TweenInfo.new(0.2, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out), {
				Scale = 0
			}),
		}

		local Styles = {
			None = function() Interactable:Destroy() end,
			Confirm = function() Interactable.Text = "" end,
			Cancel = function() Interactable.Text = "" end
		}

		local Style = Styles[Settings.Style] or function() Interactable:Destroy() end
		Style()

		if Interactable then
			Interactable.MouseButton1Click:Connect(function()
				NotificationFunctions:Cancel()
				if Settings.Callback then
					task.spawn(Settings.Callback)
				end
			end)
		end

		local AnimateNotification = task.spawn(function()
			Tweens.In:Play()

			Settings.Lifetime = Settings.Lifetime or 3

			if Settings.Lifetime ~= 0 then
				task.wait(Settings.Lifetime)

				local OutTween = Tweens.Out
				OutTween:Play()
				OutTween.Completed:Wait()
				Notification:Destroy()
			end
		end)

		function NotificationFunctions:UpdateTitle(New)
			NotificationTitle.Text = New
		end

		function NotificationFunctions:UpdateDescription(New)
			NotificationDescription.Text = New
		end

		function NotificationFunctions:Resize(X)
			local Target = X or 250
			Notification.Size = UDim2.fromOffset(Target, 0)
		end

		function NotificationFunctions:Cancel()
			task.cancel(AnimateNotification)

			local OutTween = Tweens.Out
			OutTween:Play()
			OutTween.Completed:Wait()
			Notification:Destroy()
		end

		return NotificationFunctions
	end

	function WindowFunctions:Dialog(Settings)
		local DialogFunctions = {}

		local DialogCanvas = UILibraryUtility:CreateInstance("CanvasGroup", {Name = "DialogCanvas", BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Size = UDim2.fromScale(1, 1), GroupTransparency = 1, Parent = Base})

		local Dialog = Instance.new("Frame")
		Dialog.Name = "Dialog"
		Dialog.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Dialog.BackgroundTransparency = 0.5
		Dialog.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Dialog.BorderSizePixel = 0
		Dialog.Size = UDim2.fromScale(1, 1)

		UILibraryUtility:CreateUICorner(10, Dialog)

		local Prompt = Instance.new("Frame")
		Prompt.Name = "Prompt"
		Prompt.AnchorPoint = Vector2.new(0.5, 0.5)
		Prompt.AutomaticSize = Enum.AutomaticSize.Y
		Prompt.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
		Prompt.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Prompt.BorderSizePixel = 0
		Prompt.Position = UDim2.fromScale(0.5, 0.5)
		Prompt.Size = UDim2.fromOffset(280, 0)

		local PromptUIScale = UILibraryUtility:CreateInstance("UIScale", {Name = "PromptUIScale", Parent = Prompt})
		PromptUIScale.Scale = 0.95

		UILibraryUtility:CreateUIStroke({Name = "GlobalUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.9}, Prompt)

		UILibraryUtility:CreateUICorner(10, Prompt)

		UILibraryUtility:CreateUIPadding(20, 20, 20, 20, Prompt)

		local Paragraph = Instance.new("Frame")
		Paragraph.Name = "Paragraph"
		Paragraph.AutomaticSize = Enum.AutomaticSize.Y
		Paragraph.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Paragraph.BackgroundTransparency = 1
		Paragraph.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Paragraph.BorderSizePixel = 0
		Paragraph.Size = UDim2.new(1, 0, 0, 38)

		local ParagraphHeader = UILibraryUtility:CreateTextLabel({Name = "ParagraphHeader", FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Medium, Enum.FontStyle.Normal), RichText = true, Text = Settings.Title, TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 18, TextTransparency = 0.4, TextWrapped = true, AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Size = UDim2.fromScale(1, 0), Parent = Paragraph})

		local ParagraphLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "ParagraphLayout", Padding = UDim.new(0, 15), SortOrder = Enum.SortOrder.LayoutOrder, Parent = Paragraph})

		local ParagraphBody = UILibraryUtility:CreateTextLabel({Name = "ParagraphBody", FontFace = Font.new(Assets.InterFont), RichText = true, Text = Settings.Description, TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 14, TextTransparency = 0.5, TextWrapped = true, AutomaticSize = Enum.AutomaticSize.Y, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, LayoutOrder = 1, Size = UDim2.fromScale(1, 0), Parent = Paragraph})

		Paragraph.Parent = Prompt

		local Interactions = Instance.new("Frame")
		Interactions.Name = "Interactions"
		Interactions.AutomaticSize = Enum.AutomaticSize.Y
		Interactions.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Interactions.BackgroundTransparency = 1
		Interactions.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Interactions.BorderSizePixel = 0
		Interactions.LayoutOrder = 1
		Interactions.Size = UDim2.fromScale(1, 0)

		local InteractionLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "InteractionLayout", Padding = UDim.new(0, 10), SortOrder = Enum.SortOrder.LayoutOrder, Parent = Interactions})

		local InteractionPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "InteractionPadding", PaddingTop = UDim.new(0, 20), Parent = Interactions})

		Interactions.Parent = Prompt

		local PromptLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "PromptLayout", SortOrder = Enum.SortOrder.LayoutOrder, Parent = Prompt})

		Prompt.Parent = Dialog
		Dialog.Parent = DialogCanvas

		local CanvasIn = Utility:PropertyTween(DialogCanvas, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { GroupTransparency = 0 })
		local CanvasOut = Utility:PropertyTween(DialogCanvas, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { GroupTransparency = 1 })

		local ScaleIn = Utility:PropertyTween(PromptUIScale, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { Scale = 1 })
		local ScaleOut = Utility:PropertyTween(PromptUIScale, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { Scale = 0.95 })

		local function DialogIn()
			CanvasIn:Play()
			ScaleIn:Play()
			CanvasIn.Completed:Wait()
			Dialog.Parent = Base
		end

		local function DialogOut()
			Dialog.Parent = DialogCanvas
			CanvasOut:Play()
			ScaleOut:Play()
			CanvasOut.Completed:Wait()
			DialogCanvas:Destroy()
		end

		for _, InstanceButton in pairs(Settings.Buttons) do
			local Button = Instance.new("TextButton")
			Button.Name = "Button"
			Button.FontFace = Font.new(Assets.InterFont)
			Button.Text = InstanceButton.Name
			Button.TextColor3 = Color3.fromRGB(255, 255, 255)
			Button.TextSize = 15
			Button.TextTransparency = 0.5
			Button.TextTruncate = Enum.TextTruncate.AtEnd
			Button.AutoButtonColor = false
			Button.AutomaticSize = Enum.AutomaticSize.Y
			Button.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
			Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Button.BorderSizePixel = 0
			Button.Size = UDim2.fromScale(1, 0)

			UILibraryUtility:CreateUIPadding(10, 10, 9, 9, Button)

			UILibraryUtility:CreateUICorner(10, Button)

			Button.Parent = Interactions

			local TweenSettings = {
				DefaultTransparency = 0,
				DefaultTransparency2 = 0.5,
				HoverTransparency = 0.3,
				HoverTransparency2 = 0.6,
				EasingStyle = Enum.EasingStyle.Sine
			}

			local function ChangeState(State)
				if State == "Idle" then
					Utility:PropertyTween(Button, TweenInfo.new(0.2, TweenSettings.EasingStyle), {
						BackgroundTransparency = TweenSettings.DefaultTransparency,
						TextTransparency = TweenSettings.DefaultTransparency2
					}):Play()
				elseif State == "Hover" then
					Utility:PropertyTween(Button, TweenInfo.new(0.2, TweenSettings.EasingStyle), {
						BackgroundTransparency = TweenSettings.HoverTransparency,
						TextTransparency = TweenSettings.HoverTransparency2
					}):Play()
				end
			end

			Button.MouseButton1Click:Connect(function()
				if DialogCanvas.GroupTransparency ~= 0 then return end
				if InstanceButton.Callback then
					InstanceButton.Callback()
				end

				if XenithLibrary.Unloaded then
					return
				end

				DialogOut()
			end)

			Button.MouseEnter:Connect(function()
				ChangeState("Hover")
			end)

			Button.MouseLeave:Connect(function()
				ChangeState("Idle")
			end)
		end

		DialogIn()

		function DialogFunctions:UpdateTitle(New)
			ParagraphHeader.Text = New
		end

		function DialogFunctions:UpdateDescription(New)
			ParagraphBody.Text = New
		end

		function DialogFunctions:Cancel()
			DialogOut()
		end

		WindowFunctions:ApplyCurrentTheme()

		return DialogFunctions
	end

	function WindowFunctions:Encounter(Settings)
		local EncounterFunctions = {}
		Settings = Settings or {}

		local EncounterCanvas = UILibraryUtility:CreateInstance("CanvasGroup", {Name = "EncounterCanvas", BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Size = UDim2.fromScale(1, 1), GroupTransparency = 1, Parent = Base})

		local Encounter = Instance.new("Frame")
		Encounter.Name = "Encounter"
		Encounter.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Encounter.BackgroundTransparency = 0.5
		Encounter.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Encounter.BorderSizePixel = 0
		Encounter.Size = UDim2.fromScale(1, 1)
		Encounter.Active = true

		UILibraryUtility:CreateUICorner(10, Encounter)

		local ContentContainer = Instance.new("ScrollingFrame")
		ContentContainer.Name = "ContentContainer"
		ContentContainer.AutomaticCanvasSize = Enum.AutomaticSize.XY
		ContentContainer.BackgroundTransparency = 1
		ContentContainer.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ContentContainer.BorderSizePixel = 0
		ContentContainer.Size = UDim2.fromScale(1, 1)
		ContentContainer.ScrollBarThickness = 4
		ContentContainer.ScrollBarImageColor3 = Color3.fromRGB(255, 255, 255)
		ContentContainer.ScrollBarImageTransparency = 0.8
		ContentContainer.CanvasSize = UDim2.new(0, 0, 0, 0)
		ContentContainer.Active = true

		local ContentUIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "ContentUIPadding", PaddingBottom = UDim.new(0, Settings.ContainerPaddingBottom or 20), PaddingLeft = UDim.new(0, Settings.ContainerPaddingLeft or 20), PaddingRight = UDim.new(0, Settings.ContainerPaddingRight or 20), PaddingTop = UDim.new(0, Settings.ContainerPaddingTop or 20), Parent = ContentContainer})

		local CardsContainer = UILibraryUtility:CreateFrame({Name = "CardsContainer", AnchorPoint = Vector2.new(0.5, 0.5), Position = UDim2.fromScale(0.5, 0.5), AutomaticSize = Enum.AutomaticSize.XY, BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Parent = ContentContainer})

		local UIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "UIListLayout", Padding = UDim.new(0, Settings.CardSpacing or 15), SortOrder = Enum.SortOrder.LayoutOrder, FillDirection = Settings.FillDirection or Enum.FillDirection.Horizontal, HorizontalAlignment = Settings.HorizontalAlignment or Enum.HorizontalAlignment.Center, VerticalAlignment = Settings.VerticalAlignment or Enum.VerticalAlignment.Top, Wraps = true, Parent = CardsContainer})

		local CardsContainerPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "CardsContainerPadding", PaddingBottom = UDim.new(0, Settings.CardsPaddingBottom or 0), PaddingLeft = UDim.new(0, Settings.CardsPaddingLeft or 0), PaddingRight = UDim.new(0, Settings.CardsPaddingRight or 0), PaddingTop = UDim.new(0, Settings.CardsPaddingTop or 0), Parent = CardsContainer})

		ContentContainer.Parent = Encounter
		Encounter.Parent = EncounterCanvas

		Encounter.InputBegan:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 then
				EncounterFunctions:Delete()
			end
		end)

		local CanvasIn = Utility:PropertyTween(EncounterCanvas, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { GroupTransparency = 0 })
		local CanvasOut = Utility:PropertyTween(EncounterCanvas, TweenInfo.new(0.1, Enum.EasingStyle.Sine), { GroupTransparency = 1 })

		local function EncounterIn()
			CanvasIn:Play()
			CanvasIn.Completed:Wait()
			Encounter.Parent = Base
		end

		local function EncounterOut()
			Encounter.Parent = EncounterCanvas
			CanvasOut:Play()
			CanvasOut.Completed:Wait()
			EncounterCanvas:Destroy()
		end EncounterIn()

		function EncounterFunctions:AddCard(Card)
			if typeof(Card) ~= "Instance" then
				error("AddCard expects an Instance (GuiObject) as parameter")
			end

			Card.Parent = CardsContainer

			return Card
		end

		function EncounterFunctions:CreateDefaultCard(CardSettings)
			CardSettings = CardSettings or {}

			local Card = Instance.new("Frame")
			Card.Name = CardSettings.Name or "Card"
			Card.Size = CardSettings.Size or UDim2.fromOffset(170, 220)
			Card.BackgroundColor3 = CardSettings.BackgroundColor or Color3.fromRGB(25, 25, 25)
			Card.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Card.BorderSizePixel = 0

			UILibraryUtility:CreateUICorner(8, Card)

			UILibraryUtility:CreateUIStroke({ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.95}, Card)

			UILibraryUtility:CreateUIPadding(10, 10, 10, 10, Card)

			local CardLayout = UILibraryUtility:CreateInstance("UIListLayout", {Padding = UDim.new(0, 8), SortOrder = Enum.SortOrder.LayoutOrder, HorizontalAlignment = Enum.HorizontalAlignment.Center, Parent = Card})

			if CardSettings.Image then
				local CardImage = UILibraryUtility:CreateImageLabel({Name = "CardImage", Image = CardSettings.Image, ImageColor3 = CardSettings.ImageColor or Color3.fromRGB(255, 255, 255), ImageTransparency = CardSettings.ImageTransparency or 0, BackgroundTransparency = 1, Size = UDim2.new(1, -20, 0, 80), ScaleType = Enum.ScaleType.Fit, Parent = Card})
			end

			if CardSettings.Title then
				local CardTitle = Instance.new("TextLabel")
				CardTitle.Name = "CardTitle"
				CardTitle.FontFace = Font.new(
					Assets.InterFont,
					Enum.FontWeight.Medium,
					Enum.FontStyle.Normal
				)
				CardTitle.Text = CardSettings.Title
				CardTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
				CardTitle.TextSize = 16
				CardTitle.TextTransparency = 0.3
				CardTitle.TextWrapped = true
				CardTitle.BackgroundTransparency = 1
				CardTitle.Size = UDim2.new(1, 0, 0, 20)
				CardTitle.LayoutOrder = 1
				CardTitle.Parent = Card
			end

			if CardSettings.Description then
				local CardDescription = UILibraryUtility:CreateTextLabel({Name = "CardDescription", FontFace = Font.new(Assets.InterFont), Text = CardSettings.Description, TextColor3 = Color3.fromRGB(255, 255, 255), TextSize = 13, TextTransparency = 0.5, TextWrapped = true, BackgroundTransparency = 1, AutomaticSize = Enum.AutomaticSize.Y, Size = UDim2.fromScale(1, 0), LayoutOrder = 2, Parent = Card})
			end

			if CardSettings.ButtonText then
				local CardButton = Instance.new("TextButton")
				CardButton.Name = "CardButton"
				CardButton.FontFace = Font.new(Assets.InterFont)
				CardButton.Text = CardSettings.ButtonText
				CardButton.TextColor3 = Color3.fromRGB(255, 255, 255)
				CardButton.TextSize = 14
				CardButton.TextTransparency = 0.4
				CardButton.AutoButtonColor = false
				CardButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
				CardButton.BorderSizePixel = 0
				CardButton.Size = UDim2.new(1, 0, 0, 30)
				CardButton.LayoutOrder = 3

				UILibraryUtility:CreateUICorner(6, CardButton)

				CardButton.Parent = Card

				CardButton.MouseButton1Click:Connect(function()
					if CardSettings.Callback then
						CardSettings.Callback()
					end
				end)

				CardButton.MouseEnter:Connect(function()
					Utility:PropertyTween(CardButton, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
						BackgroundColor3 = Color3.fromRGB(45, 45, 45)
					}):Play()
				end)

				CardButton.MouseLeave:Connect(function()
					Utility:PropertyTween(CardButton, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
						BackgroundColor3 = Color3.fromRGB(35, 35, 35)
					}):Play()
				end)
			end

			Card.MouseEnter:Connect(function()
				Utility:PropertyTween(Card, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
					BackgroundTransparency = 0.1
				}):Play()
				Utility:PropertyTween(CardUIStroke, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
					Transparency = 0.9
				}):Play()
			end)

			Card.MouseLeave:Connect(function()
				Utility:PropertyTween(Card, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
					BackgroundTransparency = 0
				}):Play()
				Utility:PropertyTween(CardUIStroke, TweenInfo.new(0.2, Enum.EasingStyle.Sine), {
					Transparency = 0.95
				}):Play()
			end)

			return Card
		end

		function EncounterFunctions:Delete()
			EncounterOut()
		end

		return EncounterFunctions
	end

	function WindowFunctions:SetNotificationsState(State)
		Notifications.Visible = State
	end

	function WindowFunctions:GetNotificationsState(State)
		return Notifications.Visible
	end

	function WindowFunctions:SetWindowState(State)
		XenithLibrary.WindowState = State
	end

	function WindowFunctions:SetState(State)
		XenithLibrary.WindowState = State
		Base.Visible = State
	end

	function WindowFunctions:GetState()
		return XenithLibrary.WindowState
	end

	local InformationPanel = UILibraryUtility:CreateTextButton({Name = "InformationResult", AnchorPoint = Vector2.new(0.5, 0.5), BackgroundColor3 = Color3.fromRGB(15, 15, 15), BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0.5, -0.25), Size = UDim2.fromOffset(460, 55), Parent = XenithLibraryUI})
	InformationPanel.FontFace = Font.new(Assets.InterFont)
	InformationPanel.RichText = true
	InformationPanel.TextSize = 18
	InformationPanel.Text = ""
	InformationPanel.TextTruncate = Enum.TextTruncate.SplitWord
	InformationPanel.TextColor3 = Color3.fromRGB(255, 255, 255)
	InformationPanel.Visible = false

	UILibraryUtility:CreateUIStroke({Name = "InformationUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.9}, InformationPanel)

	UILibraryUtility:CreateUICorner(50, InformationPanel)

	local InformationWallpaper = UILibraryUtility:CreateImageLabel({Name = "InformationWallpaper", Image = Assets.StyleOneWallpaper, Size = UDim2.new(1, 0, 1, 0), Position = UDim2.new(0, 0, 0, 0), BackgroundTransparency = 1, ImageTransparency = 0.65, ImageColor3 = Color3.fromRGB(32, 32, 32), ScaleType = Enum.ScaleType.Crop, Parent = InformationPanel})

	local InformationLabel = UILibraryUtility:CreateTextLabel({Name = "InformationWallpaper", BackgroundColor3 = Color3.fromRGB(15, 15, 15), BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, FontFace = Font.new(Assets.InterFont), RichText = true, TextSize = 18, Text = "", TextTruncate = Enum.TextTruncate.SplitWord, TextColor3 = Color3.fromRGB(255, 255, 255), Visible = true, Size = UDim2.new(1, 0, 1, 0), Position = UDim2.new(0, 0, 0, 0), BackgroundTransparency = 1, ZIndex = 5, Parent = InformationPanel})

	UILibraryUtility:CreateUICorner(50, InformationWallpaper)

	function WindowFunctions:NotifyInformation(Message, Duration)
		InformationLabel.Text = Message or "No Message Shown."
		InformationPanel.Visible = true

		Utility:PropertyTween(InformationPanel, TweenInfo.new(0.75, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
			Position = UDim2.fromScale(0.5, 0.05)
		}):Play()

		if Duration then
			task.delay(Duration, function()
				Utility:PropertyTween(InformationPanel, TweenInfo.new(1, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
					Position = UDim2.fromScale(0.5, -0.25)
				}):Play()
			end)

			task.delay(Duration + 1, function()
				InformationPanel.Visible = false
			end)
		end
	end;

	function WindowFunctions:SubWindowsUnload()
		if not SubWindows then
			return
		end

		for _, SubWindow in pairs(SubWindows) do
			SubWindow:Unload()
		end
	end

	function WindowFunctions:Unload()
		task.spawn(function()
			getgenv().QueuedTeleport = nil
			clearteleportqueue()

			self:SubWindowsUnload()

			for _, Item in pairs(XenithLibrary.Options) do
				if Item.Class == "Toggle" and Item:GetState() and not Item.Settings.Default and not Item.Settings.GlobalSetting then
					Item.Settings.Callback(false)
				end
			end

			ComponentManager:Unload()

			if OnUnloadCallback then
				OnUnloadCallback()
			end

			for NameOfConnection, Item in pairs(Maid) do
				if typeof(Item) == "RBXScriptConnection" then
					Item:Disconnect()
				elseif typeof(Item) == "thread" then
					task.cancel(Item)
				elseif typeof(Item) == "function" then
					Item()
				end

				Item = nil
			end

			Maid:DoCleaning()
			XenithLibraryUI:Destroy()

			XenithLibrary.Unloaded = true
		end)
	end

	function WindowFunctions.OnUnloaded(Callback)
		OnUnloadCallback = Callback
	end

	local MenuKeybind = Settings.Keybind or Enum.KeyCode.RightControl
	local KeybindFile = XenithLibrary.Folder .. "/settings/startup/KeybindSaved.Xenith"
	if isfile(KeybindFile) then
		local KeybindDetected = readfile(KeybindFile)
		MenuKeybind = Enum.KeyCode[KeybindDetected]
	end

	local function ToggleMenu()
		local WindowState = not WindowFunctions:GetState()
		WindowFunctions:SetState(WindowState)

		if getgenv().LockBehaviorBasedOnState then
			if WindowState then
				Services.UserInputService.MouseBehavior = Enum.MouseBehavior.Default
			else
				Services.UserInputService.MouseBehavior = Enum.MouseBehavior.LockCenter
			end
		end
	end

	Maid.MenuKeybind = Services.UserInputService.InputEnded:Connect(function(Input, Typing)
		if Typing then
			return
		end

		if Input.KeyCode == MenuKeybind then
			ToggleMenu()
		end
	end)

	Minimize.MouseButton1Click:Connect(ToggleMenu)

	Exit.MouseButton1Click:Connect(function()
		WindowFunctions:Dialog({
			Title = Settings.Title,
			Description = "Are you sure you want to exit the menu? You will lose any unsaved configurations.",
			Buttons = {
				{
					Name = "Confirm",
					Callback = function()
						WindowFunctions:Unload()
						if Services.CoreGui:FindFirstChild("StartupScreen") then
							Services.CoreGui:FindFirstChild("StartupScreen"):Destroy()
						end
					end,
				},
				{
					Name = "Cancel"
				}
			}
		})
	end)

	function WindowFunctions:SetKeybind(Keycode)
		local KeybindFile = XenithLibrary.Folder .. "/settings/startup/KeybindSaved.Xenith"
		writefile(KeybindFile, Keycode.Name)
		MenuKeybind = Keycode
	end

	function WindowFunctions:SetAcrylicBlurState(State)
		XenithLibrary.AcrylicBlur = State
		Base.BackgroundTransparency = WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency")
	end

	function WindowFunctions:GetAcrylicBlurState()
		return XenithLibrary.AcrylicBlur
	end

	local function _SetUserInfoState(State)
		ShowUserInfo = State

		task.spawn(function()
			if State then
				Headshot.Image = (IsReady and HeadshotImage) or Assets.Null
				if Utility:GetPlayer() then
					Username.Text = "@" .. Utility:GetPlayer().Name
					DisplayName.Text = Utility:GetPlayer().DisplayName
				else
					Username.Text = "@" .. "Unknown"
					DisplayName.Text = "Unknown"
				end
			else
				Headshot.Image = Assets.UserInfoBlurred
				local nameLength = #Utility:GetPlayer().Name
				local displayNameLength = #Utility:GetPlayer().DisplayName
				Username.Text = "@" .. string.rep(".", nameLength)
				DisplayName.Text = string.rep(".", displayNameLength)
			end
		end)
	end

	function WindowFunctions:SetUserInfoState(State)
		_SetUserInfoState(State)
	end _SetUserInfoState(Settings.ShowUserInfo ~= nil and Settings.ShowUserInfo or true)

	function WindowFunctions:GetUserInfoState(State)
		return ShowUserInfo
	end

	function WindowFunctions:SetSize(Size)
		Base.Size = Size
	end

	function WindowFunctions:GetSize(Size)
		return Base.Size
	end

	function WindowFunctions:SetScale(Scale)
		BaseUIScale.Scale = Scale
	end

	function WindowFunctions:GetScale()
		return BaseUIScale.Scale
	end

	function WindowFunctions:ReturnFeatures()
		return XenithLibrary.Options
	end

	function WindowFunctions:RetrieveFeature(Feature)
		return XenithLibrary.Options[Feature]
	end

	function WindowFunctions:GetKeybindSet(Flag)
		for BoundFlag, BoundKey in pairs(Maid.ToggleKeybinds) do
			if BoundFlag == Flag then
				return BoundKey
			end
		end

		return nil
	end

	local function BuildFolderTree()
		local Paths = {
			XenithLibrary.Folder,
			XenithLibrary.Folder .. "/settings",
			XenithLibrary.Folder .. "/settings/" .. game.GameId,
		}

		for Index = 1, #Paths do
			local PathString = Paths[Index]
			if not isfolder(PathString) then
				makefolder(PathString)
			end
		end
	end

	function XenithLibrary:LoadAutoLoadConfig()
		if isfile(XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/" .. "autoload.txt") then
			local Name = readfile(XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/" .. "autoload.txt")

			local Success, ErrorMessage = XenithLibrary:LoadConfig(Name)
			if not Success then
				WindowFunctions:Notify({
					Title = "Interface",
					Description = "Error loading autoload config: " .. ErrorMessage
				})
			end

			WindowFunctions:Notify({
				Title = "Interface",
				Description = string.format("Autoloaded config: %q", Name),
			})
		end
	end

	function XenithLibrary:SetFolder(Folder)
		XenithLibrary.Folder = Folder or "XenithHubSettings"
		BuildFolderTree()
	end

	function XenithLibrary:SaveConfig(Path)
		if (not Path) then
			return false, "Please select a config file."
		end

		local FullPath = XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/" .. Path .. ".json"

		local Data = {
			Objects = {}
		}

		for Flag, Option in next, XenithLibrary.Options do
			if not ClassParser[Option.Class] then 
				continue 
			end

			if Option.IgnoreConfig then
				continue 
			end

			table.insert(Data.Objects, ClassParser[Option.Class].Save(Flag, Option))
		end

		local Success, Encoded = pcall(Services.HttpService.JSONEncode, Services.HttpService, Data)
		if not Success then
			return false, "Unable to encode into JSON data"
		end

		writefile(FullPath, Encoded)
		return true
	end

	function XenithLibrary:LoadConfig(Path)
		if (not Path) then
			return false, "Please select a config file."
		end

		local File = XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/" .. Path .. ".json"

		if not isfile(File) then 
			return false, "Invalid file" 
		end

		local Success, Decoded = pcall(Services.HttpService.JSONDecode, Services.HttpService, readfile(File))
		if not Success then 
			return false, "Unable to decode JSON data." 
		end

		for _, Option in next, Decoded.Objects do
			if ClassParser[Option.ClassType] then
				ClassParser[Option.ClassType].Load(Option.Flag, Option)
			end
		end

		return true
	end

	function XenithLibrary:DeleteConfig(Path)
		if (not Path) then
			return false, "Please select a config file."
		end

		local FullPath = XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/" .. Path .. ".json"

		if not isfile(FullPath) then 
			return false, "Config file does not exist." 
		end

		local Success, Error = pcall(delfile, FullPath)
		if not Success then
			return false, "Unable to delete config file: " .. tostring(Error)
		end

		return true
	end

	function XenithLibrary:DeleteAllConfigs()
		local ConfigFolder = XenithLibrary.Folder .. "/settings/" .. game.GameId

		if not isfolder(ConfigFolder) then 
			return false, "No configs folder found."
		end

		local Files = listfiles(ConfigFolder)
		local DeletedCount = 0
		local Errors = {}

		for _, FilePath in next, Files do
			if FilePath:match("%.json$") then
				local Success, Error = pcall(delfile, FilePath)
				if Success then
					DeletedCount = DeletedCount + 1
				else
					table.insert(Errors, FilePath .. ": " .. tostring(Error))
				end
			end
		end

		if #Errors > 0 then
			return false, "Deleted " .. DeletedCount .. " configs, but encountered errors: " .. table.concat(Errors, ", ")
		end

		return true, "Successfully deleted " .. DeletedCount .. " config(s)."
	end

	function XenithLibrary:RefreshConfigList()
		if not isfolder(XenithLibrary.Folder) or not isfolder(XenithLibrary.Folder .. "/settings") or not isfolder(XenithLibrary.Folder .. "/settings/" .. game.GameId) then
			XenithLibrary:SetFolder("XenithHubSettings")
		end

		local List = listfiles(XenithLibrary.Folder .. "/settings/" .. game.GameId .. "/")

		local Out = {}
		for Index = 1, #List do
			local File = List[Index]
			if File:sub(-5) == ".json" then
				local Pos = File:find(".json", 1, true)
				local Start = Pos

				local Char = File:sub(Pos, Pos)
				while Char ~= "/" and Char ~= "\\" and Char ~= "" do
					Pos = Pos - 1
					Char = File:sub(Pos, Pos)
				end

				if Char == "/" or Char == "\\" then
					local Name = File:sub(Pos + 1, Start - 1)
					if Name ~= "options" then
						table.insert(Out, Name)
					end
				end
			end
		end

		return Out
	end

	local _ = HiddenMode and WindowFunctions:SetWindowState(false)

	XenithLibrary.WindowState = true
	Base.Visible = false

	Base.Position = UDim2.fromScale(0.5, 0.5)
	Base.AnchorPoint = Vector2.new(0.5, 0.5)
	BaseUIScale.Scale = 0.95
	Base.BackgroundTransparency = 1

	for _, Child in ipairs(Base:GetChildren()) do
		if Child:IsA("GuiObject") and Child.Name ~= "Terminal" then
			ThemeUtility:MakeInvisible(Child)
		end
	end

	if Base:FindFirstChild("Terminal") then
		local Terminal = Base.Terminal

		Terminal:SetAttribute("OriginalBackgroundTransparency", Terminal.BackgroundTransparency)

		Terminal.BackgroundTransparency = 1

		for _, Element in pairs(Terminal:GetDescendants()) do
			if Element:IsA("GuiObject") then
				Element:SetAttribute("OriginalBackgroundTransparency", Element.BackgroundTransparency)
				Element.BackgroundTransparency = 1

				if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
					Element:SetAttribute("OriginalTextTransparency", Element.TextTransparency)
					Element.TextTransparency = 1
				elseif Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
					Element:SetAttribute("OriginalImageTransparency", Element.ImageTransparency)
					Element.ImageTransparency = 1
				end

				local Stroke = Element:FindFirstChildOfClass("UIStroke")
				if Stroke then
					Element:SetAttribute("OriginalStrokeTransparency", Stroke.Transparency)
					Stroke.Transparency = 1
				end
			end
		end

		if Terminal:FindFirstChild("OutputUIStroke") then
			Terminal.OutputUIStroke:SetAttribute("OriginalTransparency", Terminal.OutputUIStroke.Transparency)
			Terminal.OutputUIStroke.Transparency = 1
		end

		Terminal.Visible = Utility:SaveOpened()
	end

	local function AnimateOutputScreen(DelayTime, Duration)
		if not Base:FindFirstChild("Terminal") then return end

		local Terminal = Base.Terminal
		local ShouldBeVisible = Terminal:GetAttribute("OriginalVisibility")

		if ShouldBeVisible == false then
			return
		end

		Utility:PropertyTween(Terminal, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
			BackgroundTransparency = WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency")
		}):Play()

		if Terminal:FindFirstChild("OutputUIStroke") then
			Utility:PropertyTween(Terminal.OutputUIStroke, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Transparency = Terminal:GetAttribute("StrokeTransparency")
			}):Play()
		end

		local DescendantDelay = 0
		for _, Element in pairs(Terminal:GetDescendants()) do
			if Element:IsA("GuiObject") and Element.Name ~= "OutputWallpaper" then
				task.delay(DelayTime + DescendantDelay, function()
					local Properties = {}

					local BackgroundTransparency = Element:GetAttribute("OriginalBackgroundTransparency")
					if BackgroundTransparency ~= nil then
						Properties.BackgroundTransparency = BackgroundTransparency
					end

					if Element:IsA("TextLabel") or Element:IsA("TextButton") or Element:IsA("TextBox") then
						local TextTransparency = Element:GetAttribute("OriginalTextTransparency")
						if TextTransparency ~= nil then
							Properties.TextTransparency = TextTransparency
						end
					elseif Element:IsA("ImageLabel") or Element:IsA("ImageButton") then
						local ImageTransparency = Element:GetAttribute("OriginalImageTransparency")
						if ImageTransparency ~= nil then
							Properties.ImageTransparency = ImageTransparency
						end
					end

					if next(Properties) ~= nil then
						Utility:PropertyTween(Element, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), Properties):Play()
					end

					local Stroke = Element:FindFirstChildOfClass("UIStroke")
					if Stroke then
						local StrokeTransparency = Element:GetAttribute("OriginalStrokeTransparency")
						if StrokeTransparency ~= nil then
							Utility:PropertyTween(Stroke, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
								Transparency = StrokeTransparency
							}):Play()
						end
					end
				end)

				DescendantDelay = DescendantDelay + 0.01
			end
		end

		if Terminal:FindFirstChild("OutputWallpaper") then
			Utility:PropertyTween(Terminal.OutputWallpaper, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				ImageTransparency = WindowFunctions:GetCurrentThemeAttribute("WallpaperTransparency")
			}):Play()
		end
	end

	task.delay(0.1, function()		
		if not HiddenMode then
			Base.Visible = true
		end

		Utility:PropertyTween(BaseUIScale, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
			Scale = 1
		}):Play()

		Utility:PropertyTween(Base, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
			BackgroundTransparency = WindowFunctions:GetCurrentThemeAttribute("BackgroundTransparency")
		}):Play()

		if Wallpaper then
			Utility:PropertyTween(Wallpaper, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				ImageTransparency = WindowFunctions:GetCurrentThemeAttribute("WallpaperTransparency")
			}):Play()
		end

		if BaseUIStroke then
			Utility:PropertyTween(BaseUIStroke, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Transparency = WindowFunctions:GetCurrentThemeAttribute("StrokeTransparency")
			}):Play()
		end

		task.delay(0.1, function()
			if Sidebar then
				Utility:PropertyTween(Sidebar, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
					BackgroundTransparency = WindowFunctions:GetCurrentThemeAttribute("SidebarTransparency")
				}):Play()

				if SidebarDivider then
					Utility:PropertyTween(SidebarDivider, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
						Transparency = WindowFunctions:GetCurrentThemeAttribute("StrokeTransparency")
					}):Play()
				end

				ThemeUtility:AnimateIn(Sidebar, 0.1, 0.3)
			end

			if Content then
				ThemeUtility:AnimateIn(Content, 0.2, 0.3)

				task.delay(0.3, function()
					if TabSwitcher and TabSelection then
						TabSelection()
					end
				end)
			end

			task.delay(0.1, function()
				AnimateOutputScreen(0.1, 0.4)
			end)
		end)

		task.spawn(function()
			while XenithLibraryUI and task.wait(0.1) and not WindowFunctions.Settings.IgnoreThemeUpdate and not Maid.DelayThemeActivation do
				WindowFunctions:ApplyCurrentTheme()
			end
		end)
	end)

	return WindowFunctions
end

return XenithLibrary
]],
  ["Xenith/XenithLibrary.lua"] = [[-- To do; Padding config, theme replacement colors, resizing windows is automatic.
local XenithLibrary = { 
    Options = {}, 
    Folder = "XenithHubSettings", 
    TabSettings = {}, 
    Unloaded = false, 
    WindowState = nil, 
    AcrylicBlur = false, 
    InitializedGlobalSettings = false, 
    Tabs = {} 
}

local UILibraryUtility = {}

function UILibraryUtility:CreateInstance(ClassName, Properties)
	local Instance = Instance.new(ClassName)
	if Properties then
		for Property, Value in pairs(Properties) do
			if Property ~= "Parent" then
				Instance[Property] = Value
			end
		end
		if Properties.Parent then
			Instance.Parent = Properties.Parent
		end
	end
	return Instance
end

function UILibraryUtility:CreateFrame(Properties)
	return self:CreateInstance("Frame", Properties)
end

function UILibraryUtility:CreateTextLabel(Properties)
	return self:CreateInstance("TextLabel", Properties)
end

function UILibraryUtility:CreateTextButton(Properties)
	return self:CreateInstance("TextButton", Properties)
end

function UILibraryUtility:CreateImageLabel(Properties)
	return self:CreateInstance("ImageLabel", Properties)
end

function UILibraryUtility:CreateImageButton(Properties)
	return self:CreateInstance("ImageButton", Properties)
end

function UILibraryUtility:CreateScrollingFrame(Properties)
	return self:CreateInstance("ScrollingFrame", Properties)
end

function UILibraryUtility:CreateUICorner(CornerRadius, Parent)
	return self:CreateInstance("UICorner", {
		CornerRadius = UDim.new(0, CornerRadius),
		Parent = Parent
	})
end

function UILibraryUtility:CreateUIStroke(Properties, Parent)
	Properties = Properties or {}
	Properties.Parent = Parent
	return self:CreateInstance("UIStroke", Properties)
end

function UILibraryUtility:CreateUIPadding(PaddingLeft, PaddingRight, PaddingTop, PaddingBottom, Parent)
	return self:CreateInstance("UIPadding", {
		PaddingLeft = UDim.new(0, PaddingLeft),
		PaddingRight = UDim.new(0, PaddingRight),
		PaddingTop = UDim.new(0, PaddingTop),
		PaddingBottom = UDim.new(0, PaddingBottom),
		Parent = Parent
	})
end

function UILibraryUtility:CreateUIListLayout(Properties, Parent)
	Properties = Properties or {}
	Properties.SortOrder = Properties.SortOrder or Enum.SortOrder.LayoutOrder
	Properties.Parent = Parent
	return self:CreateInstance("UIListLayout", Properties)
end

function UILibraryUtility:CreateUIGradient(Properties, Parent)
	Properties = Properties or {}
	Properties.Parent = Parent
	return self:CreateInstance("UIGradient", Properties)
end

function UILibraryUtility:ApplyProperties(Instance, Properties)
	for Property, Value in pairs(Properties) do
		Instance[Property] = Value
	end
end

function XenithLibrary:CreateShadow(Options)
	local ShadowContainer = Instance.new("ImageLabel")
	ShadowContainer.Name = "Shadow" .. math.random(0, 10000)

	ShadowContainer.Size = UDim2.new(1, 100, 1, 96)
	ShadowContainer.Position = UDim2.new(0, -50, 0, -28)
	ShadowContainer.AnchorPoint = Vector2.new(0, 0)
	ShadowContainer.BackgroundTransparency = 1
	ShadowContainer.BackgroundColor3 = Color3.new(0.639216, 0.635294, 0.647059)

	ShadowContainer.Image = Assets.ShadowImage
	ShadowContainer.ImageTransparency = 0.3
	ShadowContainer.ImageColor3 = Color3.new(1, 1, 1)
	ShadowContainer.ImageRectOffset = Vector2.new(0, 0)
	ShadowContainer.ImageRectSize = Vector2.new(0, 0)
	ShadowContainer.ScaleType = Enum.ScaleType.Slice
	ShadowContainer.SliceCenter = Rect.new(99, 99, 99, 99)
	ShadowContainer.SliceScale = 1
	ShadowContainer.TileSize = UDim2.new(1, 0, 1, 0)

	ShadowContainer.BorderSizePixel = 0
	ShadowContainer.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
	ShadowContainer.BorderMode = Enum.BorderMode.Outline

	ShadowContainer.ZIndex = -1

	ShadowContainer.Visible = true
	ShadowContainer.Active = false
	ShadowContainer.ClipsDescendants = false
	ShadowContainer.Selectable = false
	ShadowContainer.AutoLocalize = true
	ShadowContainer.SizeConstraint = Enum.SizeConstraint.RelativeXY

	ShadowContainer.NextSelectionUp = nil
	ShadowContainer.NextSelectionDown = nil
	ShadowContainer.NextSelectionLeft = nil
	ShadowContainer.NextSelectionRight = nil
	ShadowContainer.SelectionImageObject = nil

	if Options.Gradient then
		local UIGradient = Instance.new("UIGradient")

		if Options.Gradient.Color then
			UIGradient.Color = Options.Gradient.Color
		end

		if Options.Gradient.Transparency then
			UIGradient.Transparency = Options.Gradient.Transparency
		end

		if Options.Gradient.Rotation then
			UIGradient.Rotation = Options.Gradient.Rotation
		end

		if Options.Gradient.Offset then
			UIGradient.Offset = Options.Gradient.Offset
		end

		UIGradient.Parent = ShadowContainer
	end

	if Options.Color and not Options.Gradient then
		ShadowContainer.ImageColor3 = Options.Color
	end

	table.insert(Maid.Shadows, ShadowContainer)

	local ReplicationConnection
	if Options.ObjectReplication then
		local function UpdateShadowProperties()
			local TargetObject = Options.ObjectReplication

			local AbsoluteSize = TargetObject.AbsoluteSize
			ShadowContainer.Size = UDim2.new(0, AbsoluteSize.X + 100, 0, AbsoluteSize.Y + 106)

			local AbsolutePosition = TargetObject.AbsolutePosition
			ShadowContainer.Position = UDim2.new(0, AbsolutePosition.X - 50, 0, AbsolutePosition.Y + 10)

			if Options.ReplicateVisible then
				if Options.ReplicateVisible[2] then
					if not Options.ReplicateVisible[1].Visible then
						ShadowContainer.Visible = Options.ReplicateVisible[1].Visible
					elseif Options.ReplicateVisible[1].Visible and not Options.ReplicateVisible[2].Visible then
						ShadowContainer.Visible = Options.ReplicateVisible[2].Visible
					else
						ShadowContainer.Visible = Options.ReplicateVisible[1].Visible
					end
				else
					ShadowContainer.Visible = Options.ReplicateVisible[1].Visible
				end
			else
				ShadowContainer.Visible = TargetObject.Visible
			end

			ShadowContainer.AnchorPoint = Vector2.new(0, 0)
			ShadowContainer.ZIndex = TargetObject.ZIndex - 1

			if TargetObject.Rotation then
				ShadowContainer.Rotation = TargetObject.Rotation
			end

			if TargetObject.LayoutOrder then
				ShadowContainer.LayoutOrder = TargetObject.LayoutOrder - 1
			end
		end

		UpdateShadowProperties()

		ReplicationConnection = Services.RunService.Heartbeat:Connect(UpdateShadowProperties)
	end

	if Options.Object:FindFirstChildOfClass("UIListLayout") then
		ShadowContainer.LayoutOrder = -9999
	else
		ShadowContainer.Parent = Options.Object
	end

	if ReplicationConnection then
		ShadowContainer.AncestryChanged:Connect(function()
			if not ShadowContainer.Parent then
				ReplicationConnection:Disconnect()
			end
		end)
	end

	return ShadowContainer
end

function XenithLibrary:SetupSettings(Settings)
	if XenithLibrary.TabSettings then
		XenithLibrary.TabSettings = nil
	end

	XenithLibrary.TabSettings = {}

	XenithLibrary.TabSettings.InitializeTab = nil
	XenithLibrary.TabSettings.CurrentTab = nil

	if Maid.Shadows ~= {} and Maid.Shadows ~= nil then
		for _, Container in pairs(Maid.Shadows) do
			Container:Destroy()
		end

		Maid.Shadows = nil
	end

	Maid.Shadows = {}

	if Settings.AcrylicBlur ~= nil then
		XenithLibrary.AcrylicBlur = Settings.AcrylicBlur
	end

	if Maid.MenuKeybind ~= nil then
		Maid.MenuKeybind:Disconnect()
	end

	Maid.MenuKeybind = nil
	Maid.TabIndex = 0

	Maid.RegisteredKeybinds = {}
	Maid.HiddenKeybinds = {}

	XenithLibrary.Unloaded = false
	XenithLibrary.InitializedGlobalSettings = false

	XenithLibrary.WindowState = nil

	XenithLibrary.Tabs = {}
	XenithLibrary.Options = {}
end

function XenithLibrary:Window(Settings)
	local WindowFunctions = {Settings = Settings}
	self:SetupSettings(Settings)

	local ComponentManager = Utility:LoadFile("UserInterface/Managers/ComponentManager.lua")

	local TerminalModule = Utility:LoadFile("UserInterface/Imports/Terminal/TerminalModule.lua")

	local ThemeColorSchemes = Utility:LoadFile("UserInterface/Imports/Themes/ThemeColorSchemes.lua")
	local ThemeTemplates = Utility:LoadFile("UserInterface/Imports/Themes/ThemeTemplates.lua")

	local PerformanceModes = Utility:LoadFile("UserInterface/Imports/Themes/PerformanceModes.lua")
	local ThemeUtility = Utility:LoadFile("UserInterface/Imports/Themes/ThemeUtility.lua")

	local Themes = ThemeUtility:RetrieveThemes(XenithLibrary)

	local ClassParser = {
		["Toggle"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Toggle",
					Flag = Flag,
					State = Data.State or false,
					Keybind = ComponentManager:CheckFlagKeybind(Flag)
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] then
					if Data.State then
						XenithLibrary.Options[Flag]:UpdateState(Data.State)
					end

					if Data.Keybind then
						ComponentManager.AddFlagKeybind(Flag, Data.Keybind)
					end
				end
			end
		},

		["Button"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Button",
					Flag = Flag,
					Keybind = ComponentManager:CheckFlagKeybind(Flag)
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] then
					if Data.Keybind then
						ComponentManager.AddFlagKeybind(Flag, Data.Keybind)
					end
				end
			end
		},

		["CheckBox"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "CheckBox",
					Flag = Flag,
					State = Data.State or false
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.State  then
					XenithLibrary.Options[Flag]:UpdateState(Data.State)
				end
			end
		},

		["Slider"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Slider",
					Flag = Flag,
					Value = (Data.Value and tostring(Data.Value)) or false
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Value then
					XenithLibrary.Options[Flag]:UpdateValue(Data.Value)
				end
			end
		},

		["RangeSlider"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "RangeSlider",
					Flag = Flag,
					Minimum = Data.MinValue,
					Maximum = Data.MaxValue
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Minimum and Data.Maximum then
					XenithLibrary.Options[Flag]:UpdateRange(Data.Minimum, Data.Maximum)
				end
			end
		},

		["Input"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Input",
					Flag = Flag,
					Input = Data.Text
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Input and type(Data.Input) == "string" then
					XenithLibrary.Options[Flag]:UpdateText(Data.Input)
				end
			end
		},

		["Keybind"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Keybind",
					Flag = Flag,
					Bind = Data:GetBind().Name
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Bind then
					XenithLibrary.Options[Flag]:Bind(Enum.KeyCode[Data.Bind])
				end
			end
		},

		["Dropdown"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "Dropdown",
					Flag = Flag,
					Selections = Data.Value
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Selections then
					XenithLibrary.Options[Flag]:UpdateSelection(Data.Selections)
				end
			end
		},

		["ColorPicker"] = {
			Save = function(Flag, Data)
				return {
					ClassType = "ColorPicker",
					Flag = Flag,
					Color = Utility:Color3ToHex(Data.Color) or nil,
					Alpha = Data.Alpha
				}
			end,

			Load = function(Flag, Data)
				if XenithLibrary.Options[Flag] and Data.Color then
					XenithLibrary.Options[Flag]:SetColor(Utility:HexToColor3(Data.Color))

					if Data.Alpha then
						XenithLibrary.Options[Flag]:SetAlpha(Data.Alpha)
					end
				end
			end
		}
	}

	local XenithLibraryUI = UILibraryUtility:CreateInstance("ScreenGui", {Name = "XenithLibrary", ResetOnSpawn = false, DisplayOrder = 100, IgnoreGuiInset = true, ScreenInsets = Enum.ScreenInsets.None, ZIndexBehavior = Enum.ZIndexBehavior.Sibling, Parent = gethui()})

	local Notifications = UILibraryUtility:CreateFrame({Name = "Notifications", BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Size = UDim2.fromScale(1, 1), Parent = XenithLibraryUI})
	Notifications.ZIndex = 2
	Notifications.Visible = not HiddenMode

	local StatusHub = UILibraryUtility:CreateFrame({Name = "ExteralStatus", AnchorPoint = Vector2.new(0.5, 0.5), BackgroundColor3 = Color3.fromRGB(15, 15, 15), BackgroundTransparency = XenithLibrary.AcrylicBlur and 0.05 or 0, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0.955, 0.5), Size = UDim2.fromOffset(200, 200), Parent = XenithLibraryUI})
	StatusHub.Visible = false

	local StatusDivider = UILibraryUtility:CreateFrame({Name = "Divider", AnchorPoint = Vector2.new(0, 1), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.9, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0.25, 0.15), Size = UDim2.new(0.5, 0, 0, 1), Parent = StatusHub})

	local StatusTitle = Instance.new("TextLabel")
	StatusTitle.Name = "StatusTitle"
	StatusTitle.FontFace = Font.new(
		Assets.InterFont,
		Enum.FontWeight.SemiBold,
		Enum.FontStyle.Normal
	)

	StatusTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
	StatusTitle.RichText = true
	StatusTitle.TextSize = 18
	StatusTitle.TextTransparency = 0.1
	StatusTitle.TextTruncate = Enum.TextTruncate.SplitWord
	StatusTitle.TextXAlignment = Enum.TextXAlignment.Center
	StatusTitle.TextYAlignment = Enum.TextYAlignment.Bottom
	StatusTitle.AutomaticSize = Enum.AutomaticSize.Y
	StatusTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	StatusTitle.BackgroundTransparency = 1
	StatusTitle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	StatusTitle.BorderSizePixel = 0
	StatusTitle.Position = UDim2.fromScale(0, 0.0255)
	StatusTitle.Size = UDim2.new(1, 0, 0, 0)
	StatusTitle.Parent = StatusHub

	local StatusHandle = UILibraryUtility:CreateFrame({Name = "Status", AnchorPoint = Vector2.new(0.5, 0.5), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.new(0.5, 0, 0.5, 15), Size = UDim2.new(1, 0, 1, -50), Parent = StatusHub})

	local StatusUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "StatusUIListLayout", Padding = UDim.new(0, 10), HorizontalAlignment = Enum.HorizontalAlignment.Center, SortOrder = Enum.SortOrder.LayoutOrder, VerticalAlignment = Enum.VerticalAlignment.Top, Parent = StatusHandle})

	UILibraryUtility:CreateUIStroke({Name = "StatusUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.9}, StatusHub)

	UILibraryUtility:CreateUICorner(10, StatusHub)

	local StatusWallpaper = UILibraryUtility:CreateImageLabel({Name = "Wallpaper", Image = Assets.StyleOneWallpaper, Size = UDim2.fromOffset(200, 200), Position = UDim2.new(0, 0, 0, 0), BackgroundTransparency = 1, ImageTransparency = 0.65, ImageColor3 = Color3.fromRGB(32, 32, 32), ScaleType = Enum.ScaleType.Crop, Parent = StatusHub})

	UILibraryUtility:CreateUICorner(10, StatusWallpaper)

	local NotificationsUIListLayout = UILibraryUtility:CreateInstance("UIListLayout", {Name = "NotificationsUIListLayout", Padding = UDim.new(0, 10), HorizontalAlignment = Enum.HorizontalAlignment.Right, SortOrder = Enum.SortOrder.LayoutOrder, VerticalAlignment = Enum.VerticalAlignment.Bottom, Parent = Notifications})

	UILibraryUtility:CreateUIPadding(10, 10, 10, 10, Notifications)

	local Base = Instance.new("Frame")
	Base.Name = "Base"
	Base.AnchorPoint = Vector2.new(0.5, 0.5)
	Base.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
	Base.BackgroundTransparency = XenithLibrary.AcrylicBlur and 1 or 0
	Base.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Base.BorderSizePixel = 0
	Base.Position = UDim2.fromScale(0.5, 0.5)
	Base.Size = Settings.Size or UDim2.fromOffset(868, 650)
	Base.Visible = false

	local BaseUIScale = UILibraryUtility:CreateInstance("UIScale", {Name = "BaseUIScale", Parent = Base})

	UILibraryUtility:CreateUICorner(10, Base)

	UILibraryUtility:CreateUIStroke({Name = "BaseUIStroke", ApplyStrokeMode = Enum.ApplyStrokeMode.Border, Color = Color3.fromRGB(255, 255, 255), Transparency = 0.7}, Base)

	XenithLibrary:CreateShadow({Object = XenithLibraryUI, ObjectReplication = Base, Gradient = {
		Color = ColorSequence.new({
			ColorSequenceKeypoint.new(0, Color3.new(0, 0, 0.2)),
			ColorSequenceKeypoint.new(1, Color3.new(0.4, 0.6, 1))
		}),
	}})

	local Sidebar = Instance.new("Frame")
	Sidebar.Name = "Sidebar"
	Sidebar.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Sidebar.BackgroundTransparency = 0.7
	Sidebar.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Sidebar.BorderSizePixel = 0
	Sidebar.AnchorPoint = Vector2.new(1, 0)
	Sidebar.Position = UDim2.fromScale(1, 0)
	Sidebar.Size = UDim2.fromScale(0.255, 1)

	UILibraryUtility:CreateUICorner(10, Sidebar)

	UILibraryUtility:CreateUIStroke({Name = "SidebarUIStroke", Thickness = 1, Transparency = 0.9, Color = Color3.fromRGB(255, 255, 255)}, Sidebar)

	local Divider = UILibraryUtility:CreateFrame({Name = "Divider", AnchorPoint = Vector2.new(0, 0), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, Position = UDim2.fromScale(0, 0), Size = UDim2.new(0, 1, 1, 0), Parent = Sidebar})

	local DividerInteract = UILibraryUtility:CreateTextButton({Name = "DividerInteract", AnchorPoint = Vector2.new(0.5, 0), BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 1, BorderColor3 = Color3.fromRGB(0, 0, 0), BorderSizePixel = 0, FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json"), Position = UDim2.fromScale(0.5, 0), Size = UDim2.new(1, 6, 1, 0), Text = "", TextColor3 = Color3.fromRGB(0, 0, 0), TextSize = 14, Parent = Divider})

	local WindowControls = Instance.new("Frame")
	WindowControls.Name = "WindowControls"
	WindowControls.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	WindowControls.BackgroundTransparency = 1
	WindowControls.BorderColor3 = Color3.fromRGB(0, 0, 0)
	WindowControls.BorderSizePixel = 0
	WindowControls.Size = UDim2.new(1, 0, 0, 31)

	local Controls = Instance.new("Frame")
	Controls.Name = "Controls"
	Controls.BackgroundColor3 = Color3.fromRGB(119, 174, 94)
	Controls.BackgroundTransparency = 1
	Controls.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Controls.BorderSizePixel = 0
	Controls.Size = UDim2.fromScale(1, 1)

	UILibraryUtility:CreateUIListLayout({Name = "UIListLayout", Padding = UDim.new(0, 5), FillDirection = Enum.FillDirection.Horizontal, VerticalAlignment = Enum.VerticalAlignment.Center}, Controls)

	local UIPadding = UILibraryUtility:CreateInstance("UIPadding", {Name = "UIPadding", PaddingLeft = UDim.new(0, 11), Parent = Controls})

	local WindowControlSettings = {
		Sizes = { Enabled = UDim2.fromOffset(8, 8), Disabled = UDim2.fromOffset(7, 7) },
		Transparencies = { Enabled = 0, Disabled = 1 },
		StrokeTransparency = 0.9,
	}

	local Stroke = Instance.new("UIStroke")
	Stroke.Name = "BaseUIStroke"
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(255, 255, 255)
	Stroke.Transparency = WindowControlSettings.StrokeTransparency

	local Exit = UILibraryUtility:CreateTextButton({
		Name = "Exit", 
		FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json"), 
		Text = "", 
		TextSize = 20,
		TextColor3 = Color3.fromRGB(255, 255, 255), 
		AutoButtonColor = false, 
		BackgroundColor3 = Color3.fromRGB(200, 50, 60), 
		BorderColor3 = Color3.fromRGB(0, 0, 0), 
		BorderSizePixel = 0, 
		LayoutOrder = 1, 
		Size = UDim2.fromOffset(20, 20),
		Parent = Controls
	})

	local ExitUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "UICorner", CornerRadius = UDim.new(0, 4), Parent = Exit})

	local Minimize = UILibraryUtility:CreateTextButton({
		Name = "Minimize", 
		FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json"), 
		Text = "", 
		TextSize = 18,
		TextColor3 = Color3.fromRGB(200, 200, 200), 
		AutoButtonColor = false, 
		BackgroundColor3 = Color3.fromRGB(60, 65, 75), 
		BorderColor3 = Color3.fromRGB(0, 0, 0), 
		BorderSizePixel = 0, 
		LayoutOrder = 2, 
		Size = UDim2.fromOffset(20, 20),
		Parent = Controls
	})

	local MinimizeUICorner = UILibraryUtility:CreateInstance("UICorner", {Name = "UICorner", CornerRadius = UDim.new(0, 4), Parent = Minimize})

	local function ApplyState(Button, Enabled)
		local Size = Enabled and WindowControlSettings.Sizes.Enabled or WindowControlSettings.Sizes.Disabled
		local Transparency = Enabled and WindowControlSettings.Transparencies.Enabled or WindowControlSettings.Transparencies.Disabled

		Button.Size = Size
		Button.BackgroundTransparency = Transparency
		Button.Active = Enabled
		Button.Interactable = Enabled

		for _, Child in ipairs(Button:GetChildren()) do
			if Child:IsA("UIStroke") then
				Child.Transparency = Transparency
			end
		end
		if not Enabled then
			Stroke:Clone().Parent = Button
		end
	end

	local ControlsList = {Exit, Minimize}
	for _, Button in pairs(ControlsList) do
		local ButtonName = Button.Name
		local IsEnabled = true

		if Settings.DisabledWindowControls and table.find(Settings.DisabledWindowControls, ButtonName) then
			IsEnabled = false
		end

		ApplyState(Button, IsEnabled)
	end

