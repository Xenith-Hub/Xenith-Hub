    },

    Potato = {
        BlurIntensity = 0,
        ShadowSize = 0,
        AnimationSpeed = 0.05,
        ParticlesEnabled = false,
        MotionBlurEnabled = false
    }
}]],
  ["UserInterface/Imports/Themes/PerformanceModes.lua"] = [[return {
    Quality = {
        BlurIntensity = 100,
        ShadowSize = 25,
        AnimationSpeed = 0.3,
        ParticlesEnabled = true,
        MotionBlurEnabled = true
    },

    Balanced = {
        BlurIntensity = 50,
        ShadowSize = 15,
        AnimationSpeed = 0.2,
        ParticlesEnabled = false,
        MotionBlurEnabled = false
    },

    Performance = {
        BlurIntensity = 20,
        ShadowSize = 5,
        AnimationSpeed = 0.1,
        ParticlesEnabled = false,
        MotionBlurEnabled = false
    },

    Potato = {
        BlurIntensity = 0,
        ShadowSize = 0,
        AnimationSpeed = 0.05,
        ParticlesEnabled = false,
        MotionBlurEnabled = false
    }
}]],
  ["Xenith/Xenith/UserInterface/Imports/Themes/ThemeColorSchemes.lua"] = [[return {
    Monochromatic = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(H, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(H, S * 0.6, V * 0.7)
        }
    end,

    Complementary = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        local CompH = (H + 0.5) % 1
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(CompH, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(CompH, S * 0.6, V * 0.7)
        }
    end,

    Triadic = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        local H2 = (H + 0.333) % 1
        local H3 = (H + 0.667) % 1
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H2, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H3, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(H2, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(H3, S * 0.6, V * 0.7)
        }
    end
}]],
  ["Xenith/UserInterface/Imports/Themes/ThemeColorSchemes.lua"] = [[return {
    Monochromatic = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(H, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(H, S * 0.6, V * 0.7)
        }
    end,

    Complementary = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        local CompH = (H + 0.5) % 1
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(CompH, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(CompH, S * 0.6, V * 0.7)
        }
    end,

    Triadic = function(BaseColor)
        local H, S, V = BaseColor:ToHSV()
        local H2 = (H + 0.333) % 1
        local H3 = (H + 0.667) % 1
        return {
            Background = Color3.fromHSV(H, S * 0.3, V * 0.2),
            Foreground = Color3.fromHSV(H2, S * 0.4, V * 0.3),
            Surface = Color3.fromHSV(H3, S * 0.5, V * 0.4),
            AccentPrimary = Color3.fromHSV(H2, S * 0.8, V * 0.9),
            AccentSecondary = Color3.fromHSV(H3, S * 0.6, V * 0.7)
        }
    end
}]],
