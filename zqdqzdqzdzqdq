  ["Xenith/Xenith/UserInterface/Imports/Settings/InsertGeneralSettings.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions

	function TabFunctions:InsertGeneralSettings(Side)
        local SettingsSection = TabFunctions:Section({Side = Side})
        
        local function GetKeybindPath()
            return XenithLibrary.Folder .. "/settings/startup/KeybindSaved.Xenith"
        end
        
        local function LoadSavedKeybind()
            local KeybindPath = GetKeybindPath()
            if isfile(KeybindPath) then
                local KeybindName = readfile(KeybindPath)
                return Enum.KeyCode[KeybindName]
            end
            return Enum.KeyCode.LeftAlt
        end
        
        local function ShowKeybindNotification(KeybindName)
            WindowFunctions:Notify({
                Title = WindowFunctions.Settings.Title,
                Description = "Successfully Binded Keybind to - " .. KeybindName,
                Lifetime = 3
            })
        end
        
        local SavedKeybind = LoadSavedKeybind()
        
        SettingsSection:Header({Name = "General Settings"})
        
        SettingsSection:Slider({
            Name = "Interface Scale",
            Default = 1,
            Minimum = 0.25,
            Maximum = 1.5,
            DisplayMethod = "Tenths",
            Callback = function(Value)
                WindowFunctions:SetScale(Value)
            end
        }, "Slider")
        
        SettingsSection:Keybind({
            Name = "Interface Keybind",
            Blacklist = false,
            Default = SavedKeybind,
            OnBinded = function(Bind)
                WindowFunctions:SetKeybind(Bind)
                ShowKeybindNotification(tostring(Bind.Name))
            end
        }, "Keybind")
    end
end]],
  ["Xenith/UserInterface/Imports/Settings/InsertGeneralSettings.lua"] = [[return function(Requirements)
	local WindowFunctions = Requirements.WindowFunctions
	local XenithLibrary = Requirements.XenithLibrary
	local TabFunctions = Requirements.TabFunctions

	function TabFunctions:InsertGeneralSettings(Side)
