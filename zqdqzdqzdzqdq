return ThemeMetaUtility]],
  ["Xenith/UserInterface/Imports/Themes/ThemeUtility.lua"] = [[local ThemeUtility = {}
ThemeUtility.__index = ThemeUtility

local ThemeMetaUtility = setmetatable({
	_initialized = false,
}, {
	__index = function(self, key)
		return ThemeUtility[key]
	end,

	__newindex = function(self, key, value)
		rawset(self, key, value)
	end,

	__call = function(self)
		return setmetatable({}, self)
	end
})

function ThemeUtility:AnimateIn(Parent, DelayTime, Duration)
	task.delay(DelayTime, function()
		for _, Child in ipairs(Parent:GetChildren()) do
			if Child:IsA("GuiObject") then
				if Child.Name == "Terminal" then
					continue
				end

				local Properties = {}

				if Child:IsA("TextLabel") or Child:IsA("TextButton") or Child:IsA("TextBox") then
					local OriginalTextTransparency = Child:GetAttribute("OriginalTextTransparency")
					local OriginalBackgroundTransparency = Child:GetAttribute("OriginalBackgroundTransparency")

					if OriginalTextTransparency ~= nil then
						Properties.TextTransparency = OriginalTextTransparency
					end

					if OriginalBackgroundTransparency ~= nil then
						Properties.BackgroundTransparency = OriginalBackgroundTransparency
					end
				elseif Child:IsA("ImageLabel") or Child:IsA("ImageButton") then
					local OriginalImageTransparency = Child:GetAttribute("OriginalImageTransparency")
					local OriginalBackgroundTransparency = Child:GetAttribute("OriginalBackgroundTransparency")

					if OriginalImageTransparency ~= nil then
						Properties.ImageTransparency = OriginalImageTransparency
					end

					if OriginalBackgroundTransparency ~= nil then
						Properties.BackgroundTransparency = OriginalBackgroundTransparency
					end
				else
					local OriginalBackgroundTransparency = Child:GetAttribute("OriginalBackgroundTransparency")

					if OriginalBackgroundTransparency ~= nil then
						Properties.BackgroundTransparency = OriginalBackgroundTransparency
					end
				end

				if next(Properties) ~= nil then
					Utility:PropertyTween(Child, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), Properties):Play()
				end

				local Stroke = Child:FindFirstChildOfClass("UIStroke")
				if Stroke then
					local OriginalTransparency = Stroke:GetAttribute("OriginalTransparency")

					if OriginalTransparency ~= nil then
						Utility:PropertyTween(Stroke, TweenInfo.new(Duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
							Transparency = OriginalTransparency
						}):Play()
					end
				end

				self:AnimateIn(Child, 0.02, Duration)
			end
		end
	end)
end

function ThemeUtility:MakeInvisible(Parent)
    for _, Child in ipairs(Parent:GetChildren()) do
        if Child:IsA("GuiObject") then
            if Child:IsA("TextLabel") or Child:IsA("TextButton") or Child:IsA("TextBox") then
                Child:SetAttribute("OriginalTextTransparency", Child.TextTransparency)
                Child:SetAttribute("OriginalBackgroundTransparency", Child.BackgroundTransparency)
                Child.TextTransparency = 1
                Child.BackgroundTransparency = 1
            elseif Child:IsA("ImageLabel") or Child:IsA("ImageButton") then
                Child:SetAttribute("OriginalImageTransparency", Child.ImageTransparency)
                Child:SetAttribute("OriginalBackgroundTransparency", Child.BackgroundTransparency)
                Child.ImageTransparency = 1
                Child.BackgroundTransparency = 1
            else
                Child:SetAttribute("OriginalBackgroundTransparency", Child.BackgroundTransparency)
                Child.BackgroundTransparency = 1
            end

            local Stroke = Child:FindFirstChildOfClass("UIStroke")
            if Stroke then
                Stroke:SetAttribute("OriginalTransparency", Stroke.Transparency)
                Stroke.Transparency = 1
            end

            self:MakeInvisible(Child)
        end
    end
end

