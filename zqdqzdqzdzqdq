  ["Xenith/Xenith/Modules/Utility.lua"] = [[local Utility = {}
Utility.__index = Utility

local MetaUtility = setmetatable({
	NameCallMethods = {},
	_initialized = false
}, {
	__index = function(self, key)
		return Utility[key]
	end,
	__newindex = function(self, key, value)
		rawset(self, key, value)
	end,
	__call = function(self)
		return setmetatable({}, self)
	end
})

function Utility:GetPlayer()
	return Services.Players.LocalPlayer
end

function Utility:GetPlayerScripts()
	return self:GetPlayer().PlayerScripts
end

function Utility:GetCharacter()
	return self:GetPlayer().Character or self:GetPlayer().CharacterAdded:Wait()
end

function Utility:GetCharacterTimeout()
	return self:GetPlayer().Character or false
end

function Utility:GetUserInterface()
	return self:GetPlayer().PlayerGui or self:GetPlayer():WaitForChild("PlayerGui")
end

function Utility:GetUserBackpack()
	return self:GetPlayer().Backpack or self:GetPlayer():WaitForChild("Backpack")
end

function Utility:GetHumanoidRootPart()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("HumanoidRootPart")
end

function Utility:GetHumanoid()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("Humanoid")
end

function Utility:GetMouse()
	return cloneref(self:GetPlayer():GetMouse())
end

function Utility:GetCamera()
	return Services.Workspace.CurrentCamera
end

function Utility:GetPing(Round)
	local Ping = self:GetPlayer():GetNetworkPing() * 2000
	return Round and math.round(Ping) or Ping
end

function Utility:GetAllPlayers(IncludeSelf)
	local TotalPlayers = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer then
			table.insert(TotalPlayers, Player)
		elseif IncludeSelf then
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:GetAllCharacters(IncludeSelf)
	local TotalCharacters = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		elseif IncludeSelf and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		end
	end

	return TotalCharacters
end

function Utility:GetAllCharactersHumanoidRootParts(IncludeSelf)
	local AllHumanoidRootParts = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player.Character then
			local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
			if HumanoidRootPart then
				if Player ~= LocalPlayer then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				elseif IncludeSelf then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				end
			end
		end
	end

	return AllHumanoidRootParts
end

function Utility:FindClosestPlayer(Settings)
	local MousePosition = Vector2.new(self:GetMouse().X, self:GetMouse().Y)
	local ShortestDistance = math.huge
	local ClosestPlayer = nil

	Settings = Settings or {}

	local MaxRange = Settings.AimbotRange or 100000
	local FieldRadius = Settings.FieldRadius or 10000
	local TeamState = Settings.TeamState or false
	local UseFieldCircle = Settings.UseFieldCircle or false

	for _, Player in ipairs(self:GetAllPlayers()) do
		if TeamState then
			if Settings.TeamCheckCallback and Settings.TeamCheckCallback(Player) then
				continue
			elseif self:GetPlayer().Team and Player.Team == self:GetPlayer().Team then
				continue
			end
		end

		if not Player.Character then
			continue
		end

		local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
		local Humanoid = Player.Character:FindFirstChild("Humanoid")

		if not HumanoidRootPart or (Humanoid and Humanoid.Health <= 0) then
			continue
		end

		local ScreenPosition, OnScreen = workspace.CurrentCamera:WorldToViewportPoint(HumanoidRootPart.Position)

		if OnScreen then
			local ScreenDistance = (MousePosition - Vector2.new(ScreenPosition.X, ScreenPosition.Y)).Magnitude
			local WorldDistance = (HumanoidRootPart.Position - workspace.CurrentCamera.CFrame.Position).Magnitude

			if UseFieldCircle and ScreenDistance > FieldRadius then
				continue
			end

			if WorldDistance > MaxRange then
				continue
			end

			if ScreenDistance < ShortestDistance then
				ShortestDistance = ScreenDistance
				ClosestPlayer = Player
			end
		end
	end

	return ClosestPlayer
end

function Utility:GeneratePlayerList(Properties)
	local UseTargetPlayers = Properties.UseTargetPlayers
	local UseSafePlayers = Properties.UseSafePlayers
	local TargetPlayers = Properties.TargetPlayers or {}
	local SafePlayers = Properties.SafePlayers or {}

	if UseTargetPlayers and #TargetPlayers == 0 then
		return {}
	end

	local TotalPlayers = {}

	for _, Player in pairs(self:GetAllPlayers()) do
		if UseSafePlayers and #SafePlayers > 0 and table.find(SafePlayers, Player.Name) then
			continue
		end

		if UseTargetPlayers then
			if table.find(TargetPlayers, Player) then
				table.insert(TotalPlayers, Player)
			end
		else
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:State(Flag)
	return WaveLibrary:ReturnFeatures()[Flag].State
end

function Utility:Value(Flag)
	return WaveLibrary:ReturnFeatures()[Flag] and WaveLibrary:ReturnFeatures()[Flag].Value or "Could not find Feature."
end

function Utility:OptionSelected(Flag)
	if typeof(Flag) == "table" then
		local Selected = {}
		local Count = 0

		for Key, IsSelected in pairs(Flag) do
			if IsSelected then
				Count = Count + 1
				Selected[Count] = Key
			end
		end

		if Count == 0 then
			return false
		elseif Count == 1 then
			return Selected[1]
		else
			return Selected
		end
	else
		return self:OptionSelected(WaveLibrary:ReturnFeatures()[Flag]:GetOptions())
	end
end

function Utility:PropertyTween(Instance, TweenInfo, PropertyTable)
	return Services.TweenService:Create(Instance, TweenInfo, PropertyTable)
end

function Utility:QuickTween(Instance, Properties)
	local QuickPlay = Services.TweenService:Create(Instance, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), Properties)
	QuickPlay:Play()
	return QuickPlay
end

function Utility:ProgressiveTween(Instance, Speed, Properties, WaitToComplete)
	if not Instance then
		return
	end

	local CurrentPosition = Instance.Position
	local TargetPosition = Properties.Position or (Properties.CFrame and Properties.CFrame.Position)

	if not TargetPosition then
		return
	end

	local Distance = (CurrentPosition - TargetPosition).Magnitude
	local TweenInformation = TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear)

	Maid.ProgressiveCreatedTween = Services.TweenService:Create(Instance, TweenInformation, Properties)
	Maid.ProgressiveCreatedTween:Play()

	if WaitToComplete then
		Maid.ProgressiveCreatedTween.Completed:Wait()
	end

	return Maid.ProgressiveCreatedTween
end

function Utility:AttachToBack(Toggle, Target, Custom)
	if not Toggle then
		if Maid.UtilAttachToBack then
			Maid.UtilAttachToBack:Disconnect()
		end
		Maid.UtilAttachToBack = nil
		return
	end

	local Offset = Custom or CFrame.new(0, 0, 5)

	Maid.UtilAttachToBack = Services.RunService.Heartbeat:Connect(function()
		if Target then
			local TargetHRP = Target:FindFirstChild("HumanoidRootPart")
			local MyHRP = self:GetHumanoidRootPart()

			if TargetHRP and MyHRP then
				MyHRP.CFrame = TargetHRP.CFrame * Offset
			end
		end
	end)
end

function Utility:LerpReturn(Alpha, Beta, Time)
	return Alpha + (Beta - Alpha) * Time
end

function Utility:Color3ToHex(Color)
	return string.format("#%02X%02X%02X", math.floor(Color.R * 255), math.floor(Color.G * 255), math.floor(Color.B * 255))
end

function Utility:HexToColor3(Hex)
	local R = tonumber(string.sub(Hex, 2, 3), 16) / 255
	local G = tonumber(string.sub(Hex, 4, 5), 16) / 255
	local B = tonumber(string.sub(Hex, 6, 7), 16) / 255
	return Color3.new(R, G, B)
end

function Utility:FindFirstDescendant(Location, Name)
	return Location:FindFirstChild(Name, true)
end

function Utility:FindFirstChildThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetChildren()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindFirstDescendantThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetDescendants()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindAndModifyInTable(Table, TargetKey, NewValue)
    if type(Table) ~= "table" then
        return
    end
    
    for Key, Value in pairs(Table) do
        if Key == TargetKey then
            Table[Key] = NewValue
        end
        
        if type(Value) == "table" then
            self:FindAndModifyInTable(Value, TargetKey, NewValue)
        end
    end
end

function Utility:Notify(NotifyTitle, NotifyDescription, NotifyLifetime)
	if not typeof(NotifyDescription) == "string" then
		NotifyDescription = tostring(NotifyDescription)
	end

	WaveLibrary:Notify({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Lifetime = NotifyLifetime or 5
	})
end

function Utility:Prompt(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.FirstCallback,
			},
			{
				Name = Options.SecondName,
				Callback = Options.SecondCallback,
			}
		}
	})
end

function Utility:Inform(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.Callback,
			},
		}
	})
end

function Utility:Encounter(Settings)
	return WaveLibrary:Encounter(Settings)
end

function Utility:PrintTable(Table, Index)
	Index = Index or 0
	local Spacing = string.rep("  ", Index)

	if type(Table) ~= "table" then
		print("The provided argument is not a table.")
		return
	end

	for Key, Value in pairs(Table) do
		if type(Value) == "table" then
			print(Spacing .. tostring(Key) .. ":")
			self:PrintTable(Value, Index + 1)
		else
			print(Spacing .. tostring(Key) .. " = " .. tostring(Value))
		end
	end
end

function Utility:CheckFile(File)
	return isfile("Wave-Hub/" .. File)
end

function Utility:ListFiles(Location)
	return listfiles("Wave-Hub/" .. Location)
end

function Utility:ListManager(Location, KeepExtension)
	local Files = listfiles("Wave-Hub/" .. Location)
	local Results = {}

	for _, Path in ipairs(Files) do
		local Name = string.match(Path, "[^\\/]+$") or Path

		if not KeepExtension then
			Name = string.match(Name, "^(.*)%.") or Name
		end

		Results[Name] = Path
	end

	return Results
end

function Utility:ReadFile(File, Parameter)
	local FilePath
	local FileName = string.match(File, "[^/\\]+$") or File

	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local Success, Result = pcall(function()
		return loadstring(readfile(FilePath) or nil)
	end)

	if not Success then
		print("Error in " .. FileName .. "| ReadFile:", Result)
		return nil
	end

	return Result
end

function Utility:LoadFile(File, Parameter)
	local FileName = string.match(File, "[^/\\]+$") or File
	local FilePath = (string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\")
		and File or "Wave-Hub/" .. File

	local FileContent
	local Success, ErrorMessage = pcall(function()
		FileContent = readfile(FilePath)
	end)

	if not Success then
		warn("Failed to read file:", FileName, "| Error:", ErrorMessage)
		return nil
	end

	if type(FileContent) == "table" then
		return FileContent
	elseif type(FileContent) ~= "string" then
		warn("Unsupported file content type for:", FileName, "| Got:", typeof(FileContent))
		return nil
	end

	local Chunk, LoadError = loadstring(FileContent, Parameter or FileName)
	if not Chunk then
		warn("Failed to compile file:", FileName, "| Error:", LoadError)
		return nil
	end

	local Result
	local ExecSuccess, ExecError = pcall(function()
		Result = Chunk()
	end)

	if not ExecSuccess then
		warn("Failed to execute file:", FileName, "| Error:", ExecError)
		return nil
	end

	return Result
end

function Utility:LoadAddon(AddonName)
	local FileDirectory = ("Wave-Hub/Games/Addons/%s Addons/"):format(GameLoading) .. AddonName .. ".lua"
	local FileToCheck = isfile(FileDirectory)

	if not FileToCheck then
		return nil
	end

	return loadstring(readfile(FileDirectory))()
end

function Utility:IsSetting(Setting, Globalize, Path)
	local FileDirectory = "WaveHubSettings/settings/startup/" .. Setting .. ".wave"
	local FileToCheck = isfile(FileDirectory)

	if Globalize then
		getgenv()[Setting] = FileToCheck
	end

	return Path and FileDirectory or FileToCheck
end

function Utility:SaveOpened(Check, Save)
	local FullPath = self:IsSetting("TerminalSaved", false, true)

	if Check then
		if not isfile(FullPath) and Save then
			writefile(FullPath, "")
		elseif isfile(FullPath) and not Save then
			delfile(FullPath)
		end
	end

	return isfile(FullPath)
end

function Utility:LoadActor(File, Actor)
	if getgenv().WaveRunningOnActor then
		return "Running"
	end

	if getgenv().WAVE_BUNDLE_MODE and getgenv().WAVE_ACTOR_INIT then
		local InitCode = getgenv().WAVE_ACTOR_INIT()
		run_on_actor(Actor, InitCode)

		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	else
		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	end

	local FilePath
	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local FileContent = readfile(FilePath)
	run_on_actor(Actor, FileContent)

	return "Initiated"
end

function Utility:SecureCallFunction(DesiredOutcome)
	local function SecuredFunction()
		setthreadidentity(2)
		DesiredOutcome()
		setthreadidentity(7)
	end
	
	return SecuredFunction()
end

function Utility:InitializeMethods()
	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end
	
	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	local GameRawMetatable = getrawmetatable(game)
	setreadonly(GameRawMetatable, false)

	if not Maid.InitializedNamecall then
		Maid.InitializedNamecall = true
		
		local InitialNameCall = GameRawMetatable.__namecall

		GameRawMetatable.__namecall = function(Self, ...)
			if checkcaller() then
				return InitialNameCall(Self, ...)
			end

			local Method = getnamecallmethod()

			for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
				if BlockedRemote.Method == Method and Self.Name == BlockedRemote.Server then
					if BlockedRemote.Condition ~= nil then
						if typeof(BlockRemote.Condition) == "function" then
							if not BlockRemote.Condition() then
								continue
							end
						elseif not BlockRemote.Condition then
							continue
						end
					end
					
					if BlockedRemote.CustomCallback then
						return BlockedRemote.CustomCallback
					end

					return 
				end
			end

			local Arguments = {...}

			for _, NameCallInfo in pairs(Utility.NameCallMethods) do
				if NameCallInfo.Method == Method and Self.Name == NameCallInfo.Server and (NameCallInfo.Condition and NameCallInfo.Condition() or not NameCallInfo.Condition) then
					if typeof(NameCallInfo.Arguments) == "table" then
						for Index, ArgumentIndex in ipairs(NameCallInfo.Arguments) do
							if Arguments[ArgumentIndex] ~= nil then
								local EditedValue

								if typeof(NameCallInfo.Edited) == "table" then
									EditedValue = NameCallInfo.Edited[Index]
								else
									EditedValue = NameCallInfo.Edited
								end

								if typeof(EditedValue) == "function" then
									EditedValue = EditedValue()
								end

								if EditedValue ~= nil then
									Arguments[ArgumentIndex] = EditedValue
								end
							end
						end
					elseif typeof(NameCallInfo.Arguments) == "number" then
						if Arguments[NameCallInfo.Arguments] ~= nil then
							local EditedValue = NameCallInfo.Edited

							if typeof(EditedValue) == "function" then
								EditedValue = EditedValue()
							end

							if EditedValue ~= nil then
								Arguments[NameCallInfo.Arguments] = EditedValue
							end
						end
					end

					return InitialNameCall(Self, unpack(Arguments))
				end
			end

			return InitialNameCall(Self, ...)
		end
	end

	setreadonly(GameRawMetatable, true)
	Utility._initialized = true
end

function Utility:BlockRemote(Method, Server, Condition, CustomCallback)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return false
		end
	end

	table.insert(Utility.BlockedRemotes, {
		Method = Method,
		Server = Server,
		Condition = Condition,
		CustomCallback = CustomCallback or nil,
	})

	return true
end

function Utility:UnblockRemote(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Server == Server then
			BlockedRemote.Server = " \nUnblocked"
		end
	end
end

function Utility:IsRemoteBlocked(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return true
		end
	end

	return false
end

function Utility:UpdateMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end

	local EditedSuccess = self:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if EditedSuccess then
		return "Edited"
	end

	table.insert(Utility.NameCallMethods, {
		Method = Method,
		Server = Server,
		Arguments = Arguments,
		Edited = Edited,
		Condition = Condition or false
	})

	if TemporarySeconds and TemporarySeconds > 0 then
		task.delay(TemporarySeconds, function()
			self:RemoveMethod(Method, Server)
		end)
	end

	return "Inserted"
end

function Utility:HookGetAttribute(Condition)
    if not Utility._initialized then
        self:InitializeMethods()
    end

    local GameRawMetatable = getrawmetatable(game)
    setreadonly(GameRawMetatable, false)

    if not Maid.OriginalIndex then
        Maid.OriginalIndex = GameRawMetatable.__index

        GameRawMetatable.__index = newcclosure(function(Self, Key)
            if Key == "GetAttribute" then
                return function(_, AttributeName)
                    if Condition and Condition(AttributeName) then
                        return nil
                    end

                    return Maid.OriginalIndex(Self, Key)(Self, AttributeName)
                end
            end

            return Maid.OriginalIndex(Self, Key)
        end)
    end

    setreadonly(GameRawMetatable, true)
end

function Utility:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			local Original = {
				Arguments = Info.Arguments,
				Edited = Info.Edited,
				Condition = Info.Condition
			}

			Utility.NameCallMethods[Index] = {
				Method = Method,
				Server = Server,
				Arguments = Arguments or Info.Arguments,
				Edited = Edited or Info.Edited,
				Condition = Condition ~= nil and Condition or Info.Condition,
				Original = Original
			}

			if TemporarySeconds and TemporarySeconds > 0 then
				task.delay(TemporarySeconds, function()
					if Utility.NameCallMethods[Index] and Utility.NameCallMethods[Index].Original then
						Utility.NameCallMethods[Index].Arguments = Original.Arguments
						Utility.NameCallMethods[Index].Edited = Original.Edited
						Utility.NameCallMethods[Index].Condition = Original.Condition
						Utility.NameCallMethods[Index].Original = nil
					end
				end)
			end

			return true
		end
	end

	return false
end

function Utility:RemoveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			table.remove(Utility.NameCallMethods, Index)
			return true
		end
	end

	return false
end

function Utility:IsActiveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for _, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			return true
		end
	end

	return false
end

function Utility:VirtualInputMachineClick(Element)
	local Inset = Services.Services.GuiService:GetGuiInset()
	local GuiElement = Element
	local AbsPos = GuiElement.AbsolutePosition
	local AbsSize = GuiElement.AbsoluteSize

	local ClickX = AbsPos.X + (AbsSize.X / 2) + Inset.X
	local ClickY = AbsPos.Y + (AbsSize.Y / 2) + Inset.Y

	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, true, game, 1)
	task.wait(0.1)
	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, false, game, 1)
end

function Utility:SendKey(Key, Time, EndTime)
	VirtualInputMachine:SendKeyEvent(true, Enum.KeyCode[Key], false, game)
	task.wait(Time or 0)
	VirtualInputMachine:SendKeyEvent(false, Enum.KeyCode[Key], false, game)
end

function Utility:FindBestServer(GameId)
	local Success, Result = pcall(function()
		return self:ExecuteHTTPGet("https://games.roblox.com/v1/games/" .. GameId .. "/servers/Public?sortOrder=Asc&limit=100")
	end)

	if not Success then
		return nil
	end

	local Data = Services.Services.HttpService:JSONDecode(Result)
	if not Data or not Data.data then
		return nil
	end

	for _, Server in ipairs(Data.data) do
		if Server.playing > 5 and Server.playing < Server.maxPlayers - 2 then
			return Server.id
		end
	end

	if #Data.data > 0 then
		return Data.data[1].id
	end

	return nil
end

function Utility:TeleportToGame(GameName, Options)
	Options = Options or {}

	local GameInfo = nil
	for _, Game in ipairs(GameData) do
		if Game.Name == GameName then
			GameInfo = Game
			break
		end
	end

	if not GameInfo then
		self:Notify("Teleport Error", "Game '" .. GameName .. "' not found in database.", 5)
		return false
	end

	if not GameInfo.Available then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	local GameId = GameInfo.Id
	if type(GameId) == "table" then
		GameId = GameId[1]
	end

	if GameId == 0 then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	self:Notify("Teleporting", "Preparing to teleport to " .. GameName .. "...", 3)

	wait(1)

	local TeleportOptions = Instance.new("TeleportOptions")

	if Options.ReserveServer then
		TeleportOptions.ReservedServerAccessCode = Options.ReserveServer
	end

	if Options.ServerJobId then
		TeleportOptions.ServerInstanceId = Options.ServerJobId
	elseif Options.FindBestServer then
		local BestServerId = self:FindBestServer(GameId)
		if BestServerId then
			TeleportOptions.ServerInstanceId = BestServerId
			self:Notify("Server Found", "Found a good server! Teleporting...", 2)
		end
	end

	local Success, ErrorMessage = pcall(function()
		Services.Services.TeleportService:TeleportAsync(GameId, {self:GetPlayer()}, TeleportOptions)
	end)

	if not Success then
		local FriendlyMessage = "Failed to teleport. Please check your internet connection."

		if string.find(ErrorMessage, "unauthorized") or string.find(ErrorMessage, "Unauthorized") then
			FriendlyMessage = "Please enable 'Allow Third Party Teleports' in your Roblox account settings."
		elseif string.find(ErrorMessage, "flooded") then
			FriendlyMessage = "Too many teleport requests. Please wait 30 seconds and try again."
		elseif string.find(ErrorMessage, "not found") then
			FriendlyMessage = "Game is currently unavailable. Please try again later."
		end

		self:Notify("Teleport Failed", FriendlyMessage, 7)
		return false
	end

	return true
end

function Utility:TeleportWithPrompt(GameName, Options)
	self:Prompt(false, "Are you sure you want to teleport to " .. GameName .. "?", {
		FirstName = "Yes",
		SecondName = "Cancel",
		FirstCallback = function()
			self:TeleportToGame(GameName, Options)
		end,
		SecondCallback = function()
			self:Notify("Cancelled", "Teleport cancelled.", 2)
		end,
	})
end

function Utility:GetGameLink(GameName)
	for _, GameInfo in ipairs(GameData) do
		if GameInfo.Name == GameName then
			return GameInfo.Link
		end
	end
	return nil
end

function Utility:CopyGameLink(GameName)
	local Link = self:GetGameLink(GameName)
	if Link then
		setclipboard(Link)
		self:Notify("Link Copied", "Game link copied to clipboard!", 3)
	else
		self:Notify("Error", "Game link not available.", 3)
	end
end

function Utility:QueueScript()
	if not getgenv().QueuedTeleport then
		queue_on_teleport(readfile("Wave-Hub/Script-Loader.lua"))
		getgenv().QueuedTeleport = true
	end
end

function Utility:DisableResetOnSpawn()
	Maid.ScreenGuisWithResetOnSpawn = {}
	local UserInterface = self:GetUserInterface()
	if UserInterface then
		for _, Object in pairs(UserInterface:GetChildren()) do
			if Object:IsA("ScreenGui") and Object.ResetOnSpawn then
				table.insert(Maid.ScreenGuisWithResetOnSpawn, Object)
				Object.ResetOnSpawn = false
			end
		end
	end
end

function Utility:EnableResetOnSpawn()
	for _, ScreenGui in pairs(Maid.ScreenGuisWithResetOnSpawn) do
		if ScreenGui and ScreenGui.Parent then
			ScreenGui.ResetOnSpawn = true
		end
	end
	Maid.ScreenGuisWithResetOnSpawn = {}
end

function Utility:CreateCommunicationChannel()
	local Identity, Channel = create_comm_channel()
	return Identity, Channel
end

function Utility:ExecuteFunction(Arguments)
	return function()
		local OnStart = Arguments.OnStart
		local Callback = Arguments.Callback
		local OnCompleted = Arguments.OnCompleted

		local Results = {
			OnStartResult = nil,
			CallbackResult = nil,
			OnCompletedResult = nil,
			StartingNotification = nil,
			Success = true,
			Errors = {},
		}

		if OnStart and type(OnStart) == "function" then
			local Success, Result = pcall(OnStart)
			if Success then
				Results.OnStartResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnStart", Error = Result})
			end
		elseif OnStart and type(OnStart) == "string" then
			local Notification = WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnStart)
			})

			if Arguments.StartingLoadingEffect then
				Results.StartingNotification = Notification

				task.spawn(function()
					local Dots = {"", ".", "..", "..."}
					local Index = 1

					while Arguments.StartingLoadingEffect do
						Notification:UpdateDescription(OnStart .. Dots[Index])
						Index = Index % #Dots + 1
						task.wait(0.25)
					end
				end)
			end
		end

		if Callback and type(Callback) == "function" then
			local Success, Result = pcall(Callback)
			if Success then
				Results.CallbackResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "Callback", Error = Result})
			end
		end

		if OnCompleted and type(OnCompleted) == "function" then
			local Success, Result = pcall(OnCompleted)
			if Success then
				Results.OnCompletedResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnCompleted", Error = Result})
			end
		elseif OnCompleted and type(OnCompleted) == "string" then
			if Results.StartingNotification then
				Results.StartingNotification:Cancel()
			end

			WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnCompleted)
			})
		end

		return Results
	end
end

function Utility:ExecuteHTTPGet(Link, Async)
	return Async and game:HttpGetAsync(Link) or game:HttpGet(Link)
end

function Utility:RenderWait(Time)
	return function()
		local Current = tick()
		Time = Time or 0

		repeat Services.RunService.Heartbeat:Wait()
		until tick() - Current >= Time

		return tick() - Current
	end
end

function Utility:KillMaidInstance(Name)
	if Maid[Name] then
		if type(Maid[Name]) == "thread" then
			task.cancel(Maid[Name])
		elseif type(Maid[Name]) == "RBXScriptConnection" then
			Maid[Name]:Disconnect()
		end
	end
	Maid[Name] = nil
end

function Utility:Reload()
	if getgenv().WaveLibrary then
		WaveLibrary:Unload()
	end
end

function Utility:IsWaveExecutor()
	return getexecutorname():match("Wave") ~= nil
end

function Utility:IsPremiumHubUser()
	return true
end

function Utility:CheckHubStatus(PrimaryAccentColor)
	if self:IsWaveExecutor() and not self:IsPremiumHubUser() then
		return '<font color="rgb(0,255,0)">Free</font>'
	elseif self:IsWaveExecutor() and self:IsPremiumHubUser() then
		return string.format('<font color="rgb(%d,%d,%d)">Premium</font>', PrimaryAccentColor.R * 255, PrimaryAccentColor.G * 255, PrimaryAccentColor.B * 255)
	end

	return '<font color="rgb(128,128,128)">Demo</font>'
end

Utility:LoadFile("Wave/Modules/Services.lua")

return MetaUtility]],
  ["Xenith/Modules/Utility.lua"] = [[local Utility = {}
Utility.__index = Utility

local MetaUtility = setmetatable({
	NameCallMethods = {},
	_initialized = false
}, {
	__index = function(self, key)
		return Utility[key]
	end,
	__newindex = function(self, key, value)
		rawset(self, key, value)
	end,
	__call = function(self)
		return setmetatable({}, self)
	end
})

function Utility:GetPlayer()
	return Services.Players.LocalPlayer
end

function Utility:GetPlayerScripts()
	return self:GetPlayer().PlayerScripts
end

function Utility:GetCharacter()
	return self:GetPlayer().Character or self:GetPlayer().CharacterAdded:Wait()
end

function Utility:GetCharacterTimeout()
	return self:GetPlayer().Character or false
end

function Utility:GetUserInterface()
	return self:GetPlayer().PlayerGui or self:GetPlayer():WaitForChild("PlayerGui")
end

function Utility:GetUserBackpack()
	return self:GetPlayer().Backpack or self:GetPlayer():WaitForChild("Backpack")
end

function Utility:GetHumanoidRootPart()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("HumanoidRootPart")
end

function Utility:GetHumanoid()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("Humanoid")
end

function Utility:GetMouse()
	return cloneref(self:GetPlayer():GetMouse())
end

function Utility:GetCamera()
	return Services.Workspace.CurrentCamera
end

function Utility:GetPing(Round)
	local Ping = self:GetPlayer():GetNetworkPing() * 2000
	return Round and math.round(Ping) or Ping
end

function Utility:GetAllPlayers(IncludeSelf)
	local TotalPlayers = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer then
			table.insert(TotalPlayers, Player)
		elseif IncludeSelf then
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:GetAllCharacters(IncludeSelf)
	local TotalCharacters = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		elseif IncludeSelf and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		end
	end

	return TotalCharacters
end

function Utility:GetAllCharactersHumanoidRootParts(IncludeSelf)
	local AllHumanoidRootParts = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player.Character then
			local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
			if HumanoidRootPart then
				if Player ~= LocalPlayer then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				elseif IncludeSelf then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				end
			end
		end
	end

	return AllHumanoidRootParts
end

function Utility:FindClosestPlayer(Settings)
	local MousePosition = Vector2.new(self:GetMouse().X, self:GetMouse().Y)
	local ShortestDistance = math.huge
	local ClosestPlayer = nil

	Settings = Settings or {}

	local MaxRange = Settings.AimbotRange or 100000
	local FieldRadius = Settings.FieldRadius or 10000
	local TeamState = Settings.TeamState or false
	local UseFieldCircle = Settings.UseFieldCircle or false

	for _, Player in ipairs(self:GetAllPlayers()) do
		if TeamState then
			if Settings.TeamCheckCallback and Settings.TeamCheckCallback(Player) then
				continue
			elseif self:GetPlayer().Team and Player.Team == self:GetPlayer().Team then
				continue
			end
		end

		if not Player.Character then
			continue
		end

		local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
		local Humanoid = Player.Character:FindFirstChild("Humanoid")

		if not HumanoidRootPart or (Humanoid and Humanoid.Health <= 0) then
			continue
		end

		local ScreenPosition, OnScreen = workspace.CurrentCamera:WorldToViewportPoint(HumanoidRootPart.Position)

		if OnScreen then
			local ScreenDistance = (MousePosition - Vector2.new(ScreenPosition.X, ScreenPosition.Y)).Magnitude
			local WorldDistance = (HumanoidRootPart.Position - workspace.CurrentCamera.CFrame.Position).Magnitude

			if UseFieldCircle and ScreenDistance > FieldRadius then
				continue
			end

			if WorldDistance > MaxRange then
				continue
			end

			if ScreenDistance < ShortestDistance then
				ShortestDistance = ScreenDistance
				ClosestPlayer = Player
			end
		end
	end

	return ClosestPlayer
end

function Utility:GeneratePlayerList(Properties)
	local UseTargetPlayers = Properties.UseTargetPlayers
	local UseSafePlayers = Properties.UseSafePlayers
	local TargetPlayers = Properties.TargetPlayers or {}
	local SafePlayers = Properties.SafePlayers or {}

	if UseTargetPlayers and #TargetPlayers == 0 then
		return {}
	end

	local TotalPlayers = {}

	for _, Player in pairs(self:GetAllPlayers()) do
		if UseSafePlayers and #SafePlayers > 0 and table.find(SafePlayers, Player.Name) then
			continue
		end

		if UseTargetPlayers then
			if table.find(TargetPlayers, Player) then
				table.insert(TotalPlayers, Player)
			end
		else
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:State(Flag)
	return WaveLibrary:ReturnFeatures()[Flag].State
end

function Utility:Value(Flag)
	return WaveLibrary:ReturnFeatures()[Flag] and WaveLibrary:ReturnFeatures()[Flag].Value or "Could not find Feature."
end

function Utility:OptionSelected(Flag)
	if typeof(Flag) == "table" then
		local Selected = {}
		local Count = 0

		for Key, IsSelected in pairs(Flag) do
			if IsSelected then
				Count = Count + 1
				Selected[Count] = Key
			end
		end

		if Count == 0 then
			return false
		elseif Count == 1 then
			return Selected[1]
		else
			return Selected
		end
	else
		return self:OptionSelected(WaveLibrary:ReturnFeatures()[Flag]:GetOptions())
	end
end

function Utility:PropertyTween(Instance, TweenInfo, PropertyTable)
	return Services.TweenService:Create(Instance, TweenInfo, PropertyTable)
end

function Utility:QuickTween(Instance, Properties)
	local QuickPlay = Services.TweenService:Create(Instance, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), Properties)
	QuickPlay:Play()
	return QuickPlay
end

function Utility:ProgressiveTween(Instance, Speed, Properties, WaitToComplete)
	if not Instance then
		return
	end

	local CurrentPosition = Instance.Position
	local TargetPosition = Properties.Position or (Properties.CFrame and Properties.CFrame.Position)

	if not TargetPosition then
		return
	end

	local Distance = (CurrentPosition - TargetPosition).Magnitude
	local TweenInformation = TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear)

	Maid.ProgressiveCreatedTween = Services.TweenService:Create(Instance, TweenInformation, Properties)
	Maid.ProgressiveCreatedTween:Play()

	if WaitToComplete then
		Maid.ProgressiveCreatedTween.Completed:Wait()
	end

	return Maid.ProgressiveCreatedTween
end

function Utility:AttachToBack(Toggle, Target, Custom)
	if not Toggle then
		if Maid.UtilAttachToBack then
			Maid.UtilAttachToBack:Disconnect()
		end
		Maid.UtilAttachToBack = nil
		return
	end

	local Offset = Custom or CFrame.new(0, 0, 5)

	Maid.UtilAttachToBack = Services.RunService.Heartbeat:Connect(function()
		if Target then
			local TargetHRP = Target:FindFirstChild("HumanoidRootPart")
			local MyHRP = self:GetHumanoidRootPart()

			if TargetHRP and MyHRP then
				MyHRP.CFrame = TargetHRP.CFrame * Offset
			end
		end
	end)
end

function Utility:LerpReturn(Alpha, Beta, Time)
	return Alpha + (Beta - Alpha) * Time
end

function Utility:Color3ToHex(Color)
	return string.format("#%02X%02X%02X", math.floor(Color.R * 255), math.floor(Color.G * 255), math.floor(Color.B * 255))
end

function Utility:HexToColor3(Hex)
	local R = tonumber(string.sub(Hex, 2, 3), 16) / 255
	local G = tonumber(string.sub(Hex, 4, 5), 16) / 255
	local B = tonumber(string.sub(Hex, 6, 7), 16) / 255
	return Color3.new(R, G, B)
end

function Utility:FindFirstDescendant(Location, Name)
	return Location:FindFirstChild(Name, true)
end

function Utility:FindFirstChildThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetChildren()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindFirstDescendantThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetDescendants()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindAndModifyInTable(Table, TargetKey, NewValue)
    if type(Table) ~= "table" then
        return
    end
    
    for Key, Value in pairs(Table) do
        if Key == TargetKey then
            Table[Key] = NewValue
        end
        
        if type(Value) == "table" then
            self:FindAndModifyInTable(Value, TargetKey, NewValue)
        end
    end
end

function Utility:Notify(NotifyTitle, NotifyDescription, NotifyLifetime)
	if not typeof(NotifyDescription) == "string" then
		NotifyDescription = tostring(NotifyDescription)
	end

	WaveLibrary:Notify({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Lifetime = NotifyLifetime or 5
	})
end

function Utility:Prompt(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.FirstCallback,
			},
			{
				Name = Options.SecondName,
				Callback = Options.SecondCallback,
			}
		}
	})
end

function Utility:Inform(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.Callback,
			},
		}
	})
end

function Utility:Encounter(Settings)
	return WaveLibrary:Encounter(Settings)
end

function Utility:PrintTable(Table, Index)
	Index = Index or 0
	local Spacing = string.rep("  ", Index)

	if type(Table) ~= "table" then
		print("The provided argument is not a table.")
		return
	end

	for Key, Value in pairs(Table) do
		if type(Value) == "table" then
			print(Spacing .. tostring(Key) .. ":")
			self:PrintTable(Value, Index + 1)
		else
			print(Spacing .. tostring(Key) .. " = " .. tostring(Value))
		end
	end
end

function Utility:CheckFile(File)
	return isfile("Wave-Hub/" .. File)
end

function Utility:ListFiles(Location)
	return listfiles("Wave-Hub/" .. Location)
end

function Utility:ListManager(Location, KeepExtension)
	local Files = listfiles("Wave-Hub/" .. Location)
	local Results = {}

	for _, Path in ipairs(Files) do
		local Name = string.match(Path, "[^\\/]+$") or Path

		if not KeepExtension then
			Name = string.match(Name, "^(.*)%.") or Name
		end

		Results[Name] = Path
	end

	return Results
end

function Utility:ReadFile(File, Parameter)
	local FilePath
	local FileName = string.match(File, "[^/\\]+$") or File

	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local Success, Result = pcall(function()
		return loadstring(readfile(FilePath) or nil)
	end)

	if not Success then
		print("Error in " .. FileName .. "| ReadFile:", Result)
		return nil
	end

	return Result
end

function Utility:LoadFile(File, Parameter)
	local FileName = string.match(File, "[^/\\]+$") or File
	local FilePath = (string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\")
		and File or "Wave-Hub/" .. File

	local FileContent
	local Success, ErrorMessage = pcall(function()
		FileContent = readfile(FilePath)
	end)

	if not Success then
		warn("Failed to read file:", FileName, "| Error:", ErrorMessage)
		return nil
	end

	if type(FileContent) == "table" then
		return FileContent
	elseif type(FileContent) ~= "string" then
		warn("Unsupported file content type for:", FileName, "| Got:", typeof(FileContent))
		return nil
	end

	local Chunk, LoadError = loadstring(FileContent, Parameter or FileName)
	if not Chunk then
		warn("Failed to compile file:", FileName, "| Error:", LoadError)
		return nil
	end

	local Result
	local ExecSuccess, ExecError = pcall(function()
		Result = Chunk()
	end)

	if not ExecSuccess then
		warn("Failed to execute file:", FileName, "| Error:", ExecError)
		return nil
	end

	return Result
end

function Utility:LoadAddon(AddonName)
	local FileDirectory = ("Wave-Hub/Games/Addons/%s Addons/"):format(GameLoading) .. AddonName .. ".lua"
	local FileToCheck = isfile(FileDirectory)

	if not FileToCheck then
		return nil
	end

	return loadstring(readfile(FileDirectory))()
end

function Utility:IsSetting(Setting, Globalize, Path)
	local FileDirectory = "WaveHubSettings/settings/startup/" .. Setting .. ".wave"
	local FileToCheck = isfile(FileDirectory)

	if Globalize then
		getgenv()[Setting] = FileToCheck
	end

	return Path and FileDirectory or FileToCheck
end

function Utility:SaveOpened(Check, Save)
	local FullPath = self:IsSetting("TerminalSaved", false, true)

	if Check then
		if not isfile(FullPath) and Save then
			writefile(FullPath, "")
		elseif isfile(FullPath) and not Save then
			delfile(FullPath)
		end
	end

	return isfile(FullPath)
end

function Utility:LoadActor(File, Actor)
	if getgenv().WaveRunningOnActor then
		return "Running"
	end

	if getgenv().WAVE_BUNDLE_MODE and getgenv().WAVE_ACTOR_INIT then
		local InitCode = getgenv().WAVE_ACTOR_INIT()
		run_on_actor(Actor, InitCode)

		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	else
		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	end

	local FilePath
	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local FileContent = readfile(FilePath)
	run_on_actor(Actor, FileContent)

	return "Initiated"
end

function Utility:SecureCallFunction(DesiredOutcome)
	local function SecuredFunction()
		setthreadidentity(2)
		DesiredOutcome()
		setthreadidentity(7)
	end
	
	return SecuredFunction()
end

function Utility:InitializeMethods()
	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end
	
	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	local GameRawMetatable = getrawmetatable(game)
	setreadonly(GameRawMetatable, false)

	if not Maid.InitializedNamecall then
		Maid.InitializedNamecall = true
		
		local InitialNameCall = GameRawMetatable.__namecall

		GameRawMetatable.__namecall = function(Self, ...)
			if checkcaller() then
				return InitialNameCall(Self, ...)
			end

			local Method = getnamecallmethod()

			for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
				if BlockedRemote.Method == Method and Self.Name == BlockedRemote.Server then
					if BlockedRemote.Condition ~= nil then
						if typeof(BlockRemote.Condition) == "function" then
							if not BlockRemote.Condition() then
								continue
							end
						elseif not BlockRemote.Condition then
							continue
						end
					end
					
					if BlockedRemote.CustomCallback then
						return BlockedRemote.CustomCallback
					end

					return 
				end
			end

			local Arguments = {...}

			for _, NameCallInfo in pairs(Utility.NameCallMethods) do
				if NameCallInfo.Method == Method and Self.Name == NameCallInfo.Server and (NameCallInfo.Condition and NameCallInfo.Condition() or not NameCallInfo.Condition) then
					if typeof(NameCallInfo.Arguments) == "table" then
						for Index, ArgumentIndex in ipairs(NameCallInfo.Arguments) do
							if Arguments[ArgumentIndex] ~= nil then
								local EditedValue

								if typeof(NameCallInfo.Edited) == "table" then
									EditedValue = NameCallInfo.Edited[Index]
								else
									EditedValue = NameCallInfo.Edited
								end

								if typeof(EditedValue) == "function" then
									EditedValue = EditedValue()
								end

								if EditedValue ~= nil then
									Arguments[ArgumentIndex] = EditedValue
								end
							end
						end
					elseif typeof(NameCallInfo.Arguments) == "number" then
						if Arguments[NameCallInfo.Arguments] ~= nil then
							local EditedValue = NameCallInfo.Edited

							if typeof(EditedValue) == "function" then
								EditedValue = EditedValue()
							end

							if EditedValue ~= nil then
								Arguments[NameCallInfo.Arguments] = EditedValue
							end
						end
					end

					return InitialNameCall(Self, unpack(Arguments))
				end
			end

			return InitialNameCall(Self, ...)
		end
	end

	setreadonly(GameRawMetatable, true)
	Utility._initialized = true
end

function Utility:BlockRemote(Method, Server, Condition, CustomCallback)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return false
		end
	end

	table.insert(Utility.BlockedRemotes, {
		Method = Method,
		Server = Server,
		Condition = Condition,
		CustomCallback = CustomCallback or nil,
	})

	return true
end

function Utility:UnblockRemote(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Server == Server then
			BlockedRemote.Server = " \nUnblocked"
		end
	end
end

function Utility:IsRemoteBlocked(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return true
		end
	end

	return false
end

function Utility:UpdateMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end

	local EditedSuccess = self:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if EditedSuccess then
		return "Edited"
	end

	table.insert(Utility.NameCallMethods, {
		Method = Method,
		Server = Server,
		Arguments = Arguments,
		Edited = Edited,
		Condition = Condition or false
	})

	if TemporarySeconds and TemporarySeconds > 0 then
		task.delay(TemporarySeconds, function()
			self:RemoveMethod(Method, Server)
		end)
	end

	return "Inserted"
end

function Utility:HookGetAttribute(Condition)
    if not Utility._initialized then
        self:InitializeMethods()
    end

    local GameRawMetatable = getrawmetatable(game)
    setreadonly(GameRawMetatable, false)

    if not Maid.OriginalIndex then
        Maid.OriginalIndex = GameRawMetatable.__index

        GameRawMetatable.__index = newcclosure(function(Self, Key)
            if Key == "GetAttribute" then
                return function(_, AttributeName)
                    if Condition and Condition(AttributeName) then
                        return nil
                    end

                    return Maid.OriginalIndex(Self, Key)(Self, AttributeName)
                end
            end

            return Maid.OriginalIndex(Self, Key)
        end)
    end

    setreadonly(GameRawMetatable, true)
end

function Utility:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			local Original = {
				Arguments = Info.Arguments,
				Edited = Info.Edited,
				Condition = Info.Condition
			}

			Utility.NameCallMethods[Index] = {
				Method = Method,
				Server = Server,
				Arguments = Arguments or Info.Arguments,
				Edited = Edited or Info.Edited,
				Condition = Condition ~= nil and Condition or Info.Condition,
				Original = Original
			}

			if TemporarySeconds and TemporarySeconds > 0 then
				task.delay(TemporarySeconds, function()
					if Utility.NameCallMethods[Index] and Utility.NameCallMethods[Index].Original then
						Utility.NameCallMethods[Index].Arguments = Original.Arguments
						Utility.NameCallMethods[Index].Edited = Original.Edited
						Utility.NameCallMethods[Index].Condition = Original.Condition
						Utility.NameCallMethods[Index].Original = nil
					end
				end)
			end

			return true
		end
	end

	return false
end

function Utility:RemoveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			table.remove(Utility.NameCallMethods, Index)
			return true
		end
	end

	return false
end

function Utility:IsActiveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for _, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			return true
		end
	end

	return false
end

function Utility:VirtualInputMachineClick(Element)
	local Inset = Services.Services.GuiService:GetGuiInset()
	local GuiElement = Element
	local AbsPos = GuiElement.AbsolutePosition
	local AbsSize = GuiElement.AbsoluteSize

	local ClickX = AbsPos.X + (AbsSize.X / 2) + Inset.X
	local ClickY = AbsPos.Y + (AbsSize.Y / 2) + Inset.Y

	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, true, game, 1)
	task.wait(0.1)
	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, false, game, 1)
end

function Utility:SendKey(Key, Time, EndTime)
	VirtualInputMachine:SendKeyEvent(true, Enum.KeyCode[Key], false, game)
	task.wait(Time or 0)
	VirtualInputMachine:SendKeyEvent(false, Enum.KeyCode[Key], false, game)
end

function Utility:FindBestServer(GameId)
	local Success, Result = pcall(function()
		return self:ExecuteHTTPGet("https://games.roblox.com/v1/games/" .. GameId .. "/servers/Public?sortOrder=Asc&limit=100")
	end)

	if not Success then
		return nil
	end

	local Data = Services.Services.HttpService:JSONDecode(Result)
	if not Data or not Data.data then
		return nil
	end

	for _, Server in ipairs(Data.data) do
		if Server.playing > 5 and Server.playing < Server.maxPlayers - 2 then
			return Server.id
		end
	end

	if #Data.data > 0 then
		return Data.data[1].id
	end

	return nil
end

function Utility:TeleportToGame(GameName, Options)
	Options = Options or {}

	local GameInfo = nil
	for _, Game in ipairs(GameData) do
		if Game.Name == GameName then
			GameInfo = Game
			break
		end
	end

	if not GameInfo then
		self:Notify("Teleport Error", "Game '" .. GameName .. "' not found in database.", 5)
		return false
	end

	if not GameInfo.Available then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	local GameId = GameInfo.Id
	if type(GameId) == "table" then
		GameId = GameId[1]
	end

	if GameId == 0 then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	self:Notify("Teleporting", "Preparing to teleport to " .. GameName .. "...", 3)

	wait(1)

	local TeleportOptions = Instance.new("TeleportOptions")

	if Options.ReserveServer then
		TeleportOptions.ReservedServerAccessCode = Options.ReserveServer
	end

	if Options.ServerJobId then
		TeleportOptions.ServerInstanceId = Options.ServerJobId
	elseif Options.FindBestServer then
		local BestServerId = self:FindBestServer(GameId)
		if BestServerId then
			TeleportOptions.ServerInstanceId = BestServerId
			self:Notify("Server Found", "Found a good server! Teleporting...", 2)
		end
	end

	local Success, ErrorMessage = pcall(function()
		Services.Services.TeleportService:TeleportAsync(GameId, {self:GetPlayer()}, TeleportOptions)
	end)

	if not Success then
		local FriendlyMessage = "Failed to teleport. Please check your internet connection."

		if string.find(ErrorMessage, "unauthorized") or string.find(ErrorMessage, "Unauthorized") then
			FriendlyMessage = "Please enable 'Allow Third Party Teleports' in your Roblox account settings."
		elseif string.find(ErrorMessage, "flooded") then
			FriendlyMessage = "Too many teleport requests. Please wait 30 seconds and try again."
		elseif string.find(ErrorMessage, "not found") then
			FriendlyMessage = "Game is currently unavailable. Please try again later."
		end

		self:Notify("Teleport Failed", FriendlyMessage, 7)
		return false
	end

	return true
end

function Utility:TeleportWithPrompt(GameName, Options)
	self:Prompt(false, "Are you sure you want to teleport to " .. GameName .. "?", {
		FirstName = "Yes",
		SecondName = "Cancel",
		FirstCallback = function()
			self:TeleportToGame(GameName, Options)
		end,
		SecondCallback = function()
			self:Notify("Cancelled", "Teleport cancelled.", 2)
		end,
	})
end

function Utility:GetGameLink(GameName)
	for _, GameInfo in ipairs(GameData) do
		if GameInfo.Name == GameName then
			return GameInfo.Link
		end
	end
	return nil
end

function Utility:CopyGameLink(GameName)
	local Link = self:GetGameLink(GameName)
	if Link then
		setclipboard(Link)
		self:Notify("Link Copied", "Game link copied to clipboard!", 3)
	else
		self:Notify("Error", "Game link not available.", 3)
	end
end

function Utility:QueueScript()
	if not getgenv().QueuedTeleport then
		queue_on_teleport(readfile("Wave-Hub/Script-Loader.lua"))
		getgenv().QueuedTeleport = true
	end
end

function Utility:DisableResetOnSpawn()
	Maid.ScreenGuisWithResetOnSpawn = {}
	local UserInterface = self:GetUserInterface()
	if UserInterface then
		for _, Object in pairs(UserInterface:GetChildren()) do
			if Object:IsA("ScreenGui") and Object.ResetOnSpawn then
				table.insert(Maid.ScreenGuisWithResetOnSpawn, Object)
				Object.ResetOnSpawn = false
			end
		end
	end
end

function Utility:EnableResetOnSpawn()
	for _, ScreenGui in pairs(Maid.ScreenGuisWithResetOnSpawn) do
		if ScreenGui and ScreenGui.Parent then
			ScreenGui.ResetOnSpawn = true
		end
	end
	Maid.ScreenGuisWithResetOnSpawn = {}
end

function Utility:CreateCommunicationChannel()
	local Identity, Channel = create_comm_channel()
	return Identity, Channel
end

function Utility:ExecuteFunction(Arguments)
	return function()
		local OnStart = Arguments.OnStart
		local Callback = Arguments.Callback
		local OnCompleted = Arguments.OnCompleted

		local Results = {
			OnStartResult = nil,
			CallbackResult = nil,
			OnCompletedResult = nil,
			StartingNotification = nil,
			Success = true,
			Errors = {},
		}

		if OnStart and type(OnStart) == "function" then
			local Success, Result = pcall(OnStart)
			if Success then
				Results.OnStartResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnStart", Error = Result})
			end
		elseif OnStart and type(OnStart) == "string" then
			local Notification = WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnStart)
			})

			if Arguments.StartingLoadingEffect then
				Results.StartingNotification = Notification

				task.spawn(function()
					local Dots = {"", ".", "..", "..."}
					local Index = 1

					while Arguments.StartingLoadingEffect do
						Notification:UpdateDescription(OnStart .. Dots[Index])
						Index = Index % #Dots + 1
						task.wait(0.25)
					end
				end)
			end
		end

		if Callback and type(Callback) == "function" then
			local Success, Result = pcall(Callback)
			if Success then
				Results.CallbackResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "Callback", Error = Result})
			end
		end

		if OnCompleted and type(OnCompleted) == "function" then
			local Success, Result = pcall(OnCompleted)
			if Success then
				Results.OnCompletedResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnCompleted", Error = Result})
			end
		elseif OnCompleted and type(OnCompleted) == "string" then
			if Results.StartingNotification then
				Results.StartingNotification:Cancel()
			end

			WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnCompleted)
			})
		end

		return Results
	end
end

function Utility:ExecuteHTTPGet(Link, Async)
	return Async and game:HttpGetAsync(Link) or game:HttpGet(Link)
end

function Utility:RenderWait(Time)
	return function()
		local Current = tick()
		Time = Time or 0

		repeat Services.RunService.Heartbeat:Wait()
		until tick() - Current >= Time

		return tick() - Current
	end
end

function Utility:KillMaidInstance(Name)
	if Maid[Name] then
		if type(Maid[Name]) == "thread" then
			task.cancel(Maid[Name])
		elseif type(Maid[Name]) == "RBXScriptConnection" then
			Maid[Name]:Disconnect()
		end
	end
	Maid[Name] = nil
end

function Utility:Reload()
	if getgenv().WaveLibrary then
		WaveLibrary:Unload()
	end
end

function Utility:IsWaveExecutor()
	return getexecutorname():match("Wave") ~= nil
end

function Utility:IsPremiumHubUser()
	return true
end

function Utility:CheckHubStatus(PrimaryAccentColor)
	if self:IsWaveExecutor() and not self:IsPremiumHubUser() then
		return '<font color="rgb(0,255,0)">Free</font>'
	elseif self:IsWaveExecutor() and self:IsPremiumHubUser() then
		return string.format('<font color="rgb(%d,%d,%d)">Premium</font>', PrimaryAccentColor.R * 255, PrimaryAccentColor.G * 255, PrimaryAccentColor.B * 255)
	end

	return '<font color="rgb(128,128,128)">Demo</font>'
end

Utility:LoadFile("Wave/Modules/Services.lua")

return MetaUtility]],
  ["Modules/Utility.lua"] = [[local Utility = {}
Utility.__index = Utility

local MetaUtility = setmetatable({
	NameCallMethods = {},
	_initialized = false
}, {
	__index = function(self, key)
		return Utility[key]
	end,
	__newindex = function(self, key, value)
		rawset(self, key, value)
	end,
	__call = function(self)
		return setmetatable({}, self)
	end
})

function Utility:GetPlayer()
	return Services.Players.LocalPlayer
end

function Utility:GetPlayerScripts()
	return self:GetPlayer().PlayerScripts
end

function Utility:GetCharacter()
	return self:GetPlayer().Character or self:GetPlayer().CharacterAdded:Wait()
end

function Utility:GetCharacterTimeout()
	return self:GetPlayer().Character or false
end

function Utility:GetUserInterface()
	return self:GetPlayer().PlayerGui or self:GetPlayer():WaitForChild("PlayerGui")
end

function Utility:GetUserBackpack()
	return self:GetPlayer().Backpack or self:GetPlayer():WaitForChild("Backpack")
end

function Utility:GetHumanoidRootPart()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("HumanoidRootPart")
end

function Utility:GetHumanoid()
	local Character = self:GetCharacter()
	return Character and Character:FindFirstChild("Humanoid")
end

function Utility:GetMouse()
	return cloneref(self:GetPlayer():GetMouse())
end

function Utility:GetCamera()
	return Services.Workspace.CurrentCamera
end

function Utility:GetPing(Round)
	local Ping = self:GetPlayer():GetNetworkPing() * 2000
	return Round and math.round(Ping) or Ping
end

function Utility:GetAllPlayers(IncludeSelf)
	local TotalPlayers = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer then
			table.insert(TotalPlayers, Player)
		elseif IncludeSelf then
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:GetAllCharacters(IncludeSelf)
	local TotalCharacters = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player ~= LocalPlayer and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		elseif IncludeSelf and Player.Character then
			table.insert(TotalCharacters, Player.Character)
		end
	end

	return TotalCharacters
end

function Utility:GetAllCharactersHumanoidRootParts(IncludeSelf)
	local AllHumanoidRootParts = {}
	local LocalPlayer = self:GetPlayer()

	for _, Player in pairs(Services.Players:GetPlayers()) do
		if Player.Character then
			local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
			if HumanoidRootPart then
				if Player ~= LocalPlayer then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				elseif IncludeSelf then
					table.insert(AllHumanoidRootParts, HumanoidRootPart)
				end
			end
		end
	end

	return AllHumanoidRootParts
end

function Utility:FindClosestPlayer(Settings)
	local MousePosition = Vector2.new(self:GetMouse().X, self:GetMouse().Y)
	local ShortestDistance = math.huge
	local ClosestPlayer = nil

	Settings = Settings or {}

	local MaxRange = Settings.AimbotRange or 100000
	local FieldRadius = Settings.FieldRadius or 10000
	local TeamState = Settings.TeamState or false
	local UseFieldCircle = Settings.UseFieldCircle or false

	for _, Player in ipairs(self:GetAllPlayers()) do
		if TeamState then
			if Settings.TeamCheckCallback and Settings.TeamCheckCallback(Player) then
				continue
			elseif self:GetPlayer().Team and Player.Team == self:GetPlayer().Team then
				continue
			end
		end

		if not Player.Character then
			continue
		end

		local HumanoidRootPart = Player.Character:FindFirstChild("HumanoidRootPart")
		local Humanoid = Player.Character:FindFirstChild("Humanoid")

		if not HumanoidRootPart or (Humanoid and Humanoid.Health <= 0) then
			continue
		end

		local ScreenPosition, OnScreen = workspace.CurrentCamera:WorldToViewportPoint(HumanoidRootPart.Position)

		if OnScreen then
			local ScreenDistance = (MousePosition - Vector2.new(ScreenPosition.X, ScreenPosition.Y)).Magnitude
			local WorldDistance = (HumanoidRootPart.Position - workspace.CurrentCamera.CFrame.Position).Magnitude

			if UseFieldCircle and ScreenDistance > FieldRadius then
				continue
			end

			if WorldDistance > MaxRange then
				continue
			end

			if ScreenDistance < ShortestDistance then
				ShortestDistance = ScreenDistance
				ClosestPlayer = Player
			end
		end
	end

	return ClosestPlayer
end

function Utility:GeneratePlayerList(Properties)
	local UseTargetPlayers = Properties.UseTargetPlayers
	local UseSafePlayers = Properties.UseSafePlayers
	local TargetPlayers = Properties.TargetPlayers or {}
	local SafePlayers = Properties.SafePlayers or {}

	if UseTargetPlayers and #TargetPlayers == 0 then
		return {}
	end

	local TotalPlayers = {}

	for _, Player in pairs(self:GetAllPlayers()) do
		if UseSafePlayers and #SafePlayers > 0 and table.find(SafePlayers, Player.Name) then
			continue
		end

		if UseTargetPlayers then
			if table.find(TargetPlayers, Player) then
				table.insert(TotalPlayers, Player)
			end
		else
			table.insert(TotalPlayers, Player)
		end
	end

	return TotalPlayers
end

function Utility:State(Flag)
	return WaveLibrary:ReturnFeatures()[Flag].State
end

function Utility:Value(Flag)
	return WaveLibrary:ReturnFeatures()[Flag] and WaveLibrary:ReturnFeatures()[Flag].Value or "Could not find Feature."
end

function Utility:OptionSelected(Flag)
	if typeof(Flag) == "table" then
		local Selected = {}
		local Count = 0

		for Key, IsSelected in pairs(Flag) do
			if IsSelected then
				Count = Count + 1
				Selected[Count] = Key
			end
		end

		if Count == 0 then
			return false
		elseif Count == 1 then
			return Selected[1]
		else
			return Selected
		end
	else
		return self:OptionSelected(WaveLibrary:ReturnFeatures()[Flag]:GetOptions())
	end
end

function Utility:PropertyTween(Instance, TweenInfo, PropertyTable)
	return Services.TweenService:Create(Instance, TweenInfo, PropertyTable)
end

function Utility:QuickTween(Instance, Properties)
	local QuickPlay = Services.TweenService:Create(Instance, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), Properties)
	QuickPlay:Play()
	return QuickPlay
end

function Utility:ProgressiveTween(Instance, Speed, Properties, WaitToComplete)
	if not Instance then
		return
	end

	local CurrentPosition = Instance.Position
	local TargetPosition = Properties.Position or (Properties.CFrame and Properties.CFrame.Position)

	if not TargetPosition then
		return
	end

	local Distance = (CurrentPosition - TargetPosition).Magnitude
	local TweenInformation = TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear)

	Maid.ProgressiveCreatedTween = Services.TweenService:Create(Instance, TweenInformation, Properties)
	Maid.ProgressiveCreatedTween:Play()

	if WaitToComplete then
		Maid.ProgressiveCreatedTween.Completed:Wait()
	end

	return Maid.ProgressiveCreatedTween
end

function Utility:AttachToBack(Toggle, Target, Custom)
	if not Toggle then
		if Maid.UtilAttachToBack then
			Maid.UtilAttachToBack:Disconnect()
		end
		Maid.UtilAttachToBack = nil
		return
	end

	local Offset = Custom or CFrame.new(0, 0, 5)

	Maid.UtilAttachToBack = Services.RunService.Heartbeat:Connect(function()
		if Target then
			local TargetHRP = Target:FindFirstChild("HumanoidRootPart")
			local MyHRP = self:GetHumanoidRootPart()

			if TargetHRP and MyHRP then
				MyHRP.CFrame = TargetHRP.CFrame * Offset
			end
		end
	end)
end

function Utility:LerpReturn(Alpha, Beta, Time)
	return Alpha + (Beta - Alpha) * Time
end

function Utility:Color3ToHex(Color)
	return string.format("#%02X%02X%02X", math.floor(Color.R * 255), math.floor(Color.G * 255), math.floor(Color.B * 255))
end

function Utility:HexToColor3(Hex)
	local R = tonumber(string.sub(Hex, 2, 3), 16) / 255
	local G = tonumber(string.sub(Hex, 4, 5), 16) / 255
	local B = tonumber(string.sub(Hex, 6, 7), 16) / 255
	return Color3.new(R, G, B)
end

function Utility:FindFirstDescendant(Location, Name)
	return Location:FindFirstChild(Name, true)
end

function Utility:FindFirstChildThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetChildren()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindFirstDescendantThatMatchesWith(Parent, Condition)
	for _, Child in pairs(Parent:GetDescendants()) do
		if Child.Name:match(Condition) then
			return Child
		end
	end
	return nil
end

function Utility:FindAndModifyInTable(Table, TargetKey, NewValue)
    if type(Table) ~= "table" then
        return
    end
    
    for Key, Value in pairs(Table) do
        if Key == TargetKey then
            Table[Key] = NewValue
        end
        
        if type(Value) == "table" then
            self:FindAndModifyInTable(Value, TargetKey, NewValue)
        end
    end
end

function Utility:Notify(NotifyTitle, NotifyDescription, NotifyLifetime)
	if not typeof(NotifyDescription) == "string" then
		NotifyDescription = tostring(NotifyDescription)
	end

	WaveLibrary:Notify({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Lifetime = NotifyLifetime or 5
	})
end

function Utility:Prompt(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.FirstCallback,
			},
			{
				Name = Options.SecondName,
				Callback = Options.SecondCallback,
			}
		}
	})
end

function Utility:Inform(NotifyTitle, NotifyDescription, Options)
	WaveLibrary:Dialog({
		Title = NotifyTitle or WaveLibrary.Settings.Title,
		Description = NotifyDescription,
		Buttons = {
			{
				Name = Options.FirstName,
				Callback = Options.Callback,
			},
		}
	})
end

function Utility:Encounter(Settings)
	return WaveLibrary:Encounter(Settings)
end

function Utility:PrintTable(Table, Index)
	Index = Index or 0
	local Spacing = string.rep("  ", Index)

	if type(Table) ~= "table" then
		print("The provided argument is not a table.")
		return
	end

	for Key, Value in pairs(Table) do
		if type(Value) == "table" then
			print(Spacing .. tostring(Key) .. ":")
			self:PrintTable(Value, Index + 1)
		else
			print(Spacing .. tostring(Key) .. " = " .. tostring(Value))
		end
	end
end

function Utility:CheckFile(File)
	return isfile("Wave-Hub/" .. File)
end

function Utility:ListFiles(Location)
	return listfiles("Wave-Hub/" .. Location)
end

function Utility:ListManager(Location, KeepExtension)
	local Files = listfiles("Wave-Hub/" .. Location)
	local Results = {}

	for _, Path in ipairs(Files) do
		local Name = string.match(Path, "[^\\/]+$") or Path

		if not KeepExtension then
			Name = string.match(Name, "^(.*)%.") or Name
		end

		Results[Name] = Path
	end

	return Results
end

function Utility:ReadFile(File, Parameter)
	local FilePath
	local FileName = string.match(File, "[^/\\]+$") or File

	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local Success, Result = pcall(function()
		return loadstring(readfile(FilePath) or nil)
	end)

	if not Success then
		print("Error in " .. FileName .. "| ReadFile:", Result)
		return nil
	end

	return Result
end

function Utility:LoadFile(File, Parameter)
	local FileName = string.match(File, "[^/\\]+$") or File
	local FilePath = (string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\")
		and File or "Wave-Hub/" .. File

	local FileContent
	local Success, ErrorMessage = pcall(function()
		FileContent = readfile(FilePath)
	end)

	if not Success then
		warn("Failed to read file:", FileName, "| Error:", ErrorMessage)
		return nil
	end

	if type(FileContent) == "table" then
		return FileContent
	elseif type(FileContent) ~= "string" then
		warn("Unsupported file content type for:", FileName, "| Got:", typeof(FileContent))
		return nil
	end

	local Chunk, LoadError = loadstring(FileContent, Parameter or FileName)
	if not Chunk then
		warn("Failed to compile file:", FileName, "| Error:", LoadError)
		return nil
	end

	local Result
	local ExecSuccess, ExecError = pcall(function()
		Result = Chunk()
	end)

	if not ExecSuccess then
		warn("Failed to execute file:", FileName, "| Error:", ExecError)
		return nil
	end

	return Result
end

function Utility:LoadAddon(AddonName)
	local FileDirectory = ("Wave-Hub/Games/Addons/%s Addons/"):format(GameLoading) .. AddonName .. ".lua"
	local FileToCheck = isfile(FileDirectory)

	if not FileToCheck then
		return nil
	end

	return loadstring(readfile(FileDirectory))()
end

function Utility:IsSetting(Setting, Globalize, Path)
	local FileDirectory = "WaveHubSettings/settings/startup/" .. Setting .. ".wave"
	local FileToCheck = isfile(FileDirectory)

	if Globalize then
		getgenv()[Setting] = FileToCheck
	end

	return Path and FileDirectory or FileToCheck
end

function Utility:SaveOpened(Check, Save)
	local FullPath = self:IsSetting("TerminalSaved", false, true)

	if Check then
		if not isfile(FullPath) and Save then
			writefile(FullPath, "")
		elseif isfile(FullPath) and not Save then
			delfile(FullPath)
		end
	end

	return isfile(FullPath)
end

function Utility:LoadActor(File, Actor)
	if getgenv().WaveRunningOnActor then
		return "Running"
	end

	if getgenv().WAVE_BUNDLE_MODE and getgenv().WAVE_ACTOR_INIT then
		local InitCode = getgenv().WAVE_ACTOR_INIT()
		run_on_actor(Actor, InitCode)

		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	else
		run_on_actor(Actor, [=[
			getgenv().WaveRunningOnActor = true
		]=])
	end

	local FilePath
	if string.sub(File, 1, 9) == "Wave-Hub/" or string.sub(File, 1, 9) == "Wave-Hub\\" then
		FilePath = File
	else
		FilePath = "Wave-Hub/" .. File
	end

	local FileContent = readfile(FilePath)
	run_on_actor(Actor, FileContent)

	return "Initiated"
end

function Utility:SecureCallFunction(DesiredOutcome)
	local function SecuredFunction()
		setthreadidentity(2)
		DesiredOutcome()
		setthreadidentity(7)
	end
	
	return SecuredFunction()
end

function Utility:InitializeMethods()
	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end
	
	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	local GameRawMetatable = getrawmetatable(game)
	setreadonly(GameRawMetatable, false)

	if not Maid.InitializedNamecall then
		Maid.InitializedNamecall = true
		
		local InitialNameCall = GameRawMetatable.__namecall

		GameRawMetatable.__namecall = function(Self, ...)
			if checkcaller() then
				return InitialNameCall(Self, ...)
			end

			local Method = getnamecallmethod()

			for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
				if BlockedRemote.Method == Method and Self.Name == BlockedRemote.Server then
					if BlockedRemote.Condition ~= nil then
						if typeof(BlockRemote.Condition) == "function" then
							if not BlockRemote.Condition() then
								continue
							end
						elseif not BlockRemote.Condition then
							continue
						end
					end
					
					if BlockedRemote.CustomCallback then
						return BlockedRemote.CustomCallback
					end

					return 
				end
			end

			local Arguments = {...}

			for _, NameCallInfo in pairs(Utility.NameCallMethods) do
				if NameCallInfo.Method == Method and Self.Name == NameCallInfo.Server and (NameCallInfo.Condition and NameCallInfo.Condition() or not NameCallInfo.Condition) then
					if typeof(NameCallInfo.Arguments) == "table" then
						for Index, ArgumentIndex in ipairs(NameCallInfo.Arguments) do
							if Arguments[ArgumentIndex] ~= nil then
								local EditedValue

								if typeof(NameCallInfo.Edited) == "table" then
									EditedValue = NameCallInfo.Edited[Index]
								else
									EditedValue = NameCallInfo.Edited
								end

								if typeof(EditedValue) == "function" then
									EditedValue = EditedValue()
								end

								if EditedValue ~= nil then
									Arguments[ArgumentIndex] = EditedValue
								end
							end
						end
					elseif typeof(NameCallInfo.Arguments) == "number" then
						if Arguments[NameCallInfo.Arguments] ~= nil then
							local EditedValue = NameCallInfo.Edited

							if typeof(EditedValue) == "function" then
								EditedValue = EditedValue()
							end

							if EditedValue ~= nil then
								Arguments[NameCallInfo.Arguments] = EditedValue
							end
						end
					end

					return InitialNameCall(Self, unpack(Arguments))
				end
			end

			return InitialNameCall(Self, ...)
		end
	end

	setreadonly(GameRawMetatable, true)
	Utility._initialized = true
end

function Utility:BlockRemote(Method, Server, Condition, CustomCallback)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.BlockedRemotes then
		Utility.BlockedRemotes = {}
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return false
		end
	end

	table.insert(Utility.BlockedRemotes, {
		Method = Method,
		Server = Server,
		Condition = Condition,
		CustomCallback = CustomCallback or nil,
	})

	return true
end

function Utility:UnblockRemote(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Server == Server then
			BlockedRemote.Server = " \nUnblocked"
		end
	end
end

function Utility:IsRemoteBlocked(Method, Server)
	if not Utility.BlockedRemotes then
		return false
	end

	for _, BlockedRemote in pairs(Utility.BlockedRemotes) do
		if BlockedRemote.Method == Method and BlockedRemote.Server == Server then
			return true
		end
	end

	return false
end

function Utility:UpdateMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility._initialized then
		self:InitializeMethods()
	end

	if not Utility.NameCallMethods then
		Utility.NameCallMethods = {}
	end

	local EditedSuccess = self:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if EditedSuccess then
		return "Edited"
	end

	table.insert(Utility.NameCallMethods, {
		Method = Method,
		Server = Server,
		Arguments = Arguments,
		Edited = Edited,
		Condition = Condition or false
	})

	if TemporarySeconds and TemporarySeconds > 0 then
		task.delay(TemporarySeconds, function()
			self:RemoveMethod(Method, Server)
		end)
	end

	return "Inserted"
end

function Utility:HookGetAttribute(Condition)
    if not Utility._initialized then
        self:InitializeMethods()
    end

    local GameRawMetatable = getrawmetatable(game)
    setreadonly(GameRawMetatable, false)

    if not Maid.OriginalIndex then
        Maid.OriginalIndex = GameRawMetatable.__index

        GameRawMetatable.__index = newcclosure(function(Self, Key)
            if Key == "GetAttribute" then
                return function(_, AttributeName)
                    if Condition and Condition(AttributeName) then
                        return nil
                    end

                    return Maid.OriginalIndex(Self, Key)(Self, AttributeName)
                end
            end

            return Maid.OriginalIndex(Self, Key)
        end)
    end

    setreadonly(GameRawMetatable, true)
end

function Utility:EditMethod(Method, Server, Arguments, Edited, Condition, TemporarySeconds)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			local Original = {
				Arguments = Info.Arguments,
				Edited = Info.Edited,
				Condition = Info.Condition
			}

			Utility.NameCallMethods[Index] = {
				Method = Method,
				Server = Server,
				Arguments = Arguments or Info.Arguments,
				Edited = Edited or Info.Edited,
				Condition = Condition ~= nil and Condition or Info.Condition,
				Original = Original
			}

			if TemporarySeconds and TemporarySeconds > 0 then
				task.delay(TemporarySeconds, function()
					if Utility.NameCallMethods[Index] and Utility.NameCallMethods[Index].Original then
						Utility.NameCallMethods[Index].Arguments = Original.Arguments
						Utility.NameCallMethods[Index].Edited = Original.Edited
						Utility.NameCallMethods[Index].Condition = Original.Condition
						Utility.NameCallMethods[Index].Original = nil
					end
				end)
			end

			return true
		end
	end

	return false
end

function Utility:RemoveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for Index, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			table.remove(Utility.NameCallMethods, Index)
			return true
		end
	end

	return false
end

function Utility:IsActiveMethod(Method, Server)
	if not Utility.NameCallMethods then
		return false
	end

	for _, Info in ipairs(Utility.NameCallMethods) do
		if Info.Method == Method and Info.Server == Server then
			return true
		end
	end

	return false
end

function Utility:VirtualInputMachineClick(Element)
	local Inset = Services.Services.GuiService:GetGuiInset()
	local GuiElement = Element
	local AbsPos = GuiElement.AbsolutePosition
	local AbsSize = GuiElement.AbsoluteSize

	local ClickX = AbsPos.X + (AbsSize.X / 2) + Inset.X
	local ClickY = AbsPos.Y + (AbsSize.Y / 2) + Inset.Y

	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, true, game, 1)
	task.wait(0.1)
	VirtualInputMachine:SendMouseButtonEvent(ClickX or 0, ClickY or 0, 0, false, game, 1)
end

function Utility:SendKey(Key, Time, EndTime)
	VirtualInputMachine:SendKeyEvent(true, Enum.KeyCode[Key], false, game)
	task.wait(Time or 0)
	VirtualInputMachine:SendKeyEvent(false, Enum.KeyCode[Key], false, game)
end

function Utility:FindBestServer(GameId)
	local Success, Result = pcall(function()
		return self:ExecuteHTTPGet("https://games.roblox.com/v1/games/" .. GameId .. "/servers/Public?sortOrder=Asc&limit=100")
	end)

	if not Success then
		return nil
	end

	local Data = Services.Services.HttpService:JSONDecode(Result)
	if not Data or not Data.data then
		return nil
	end

	for _, Server in ipairs(Data.data) do
		if Server.playing > 5 and Server.playing < Server.maxPlayers - 2 then
			return Server.id
		end
	end

	if #Data.data > 0 then
		return Data.data[1].id
	end

	return nil
end

function Utility:TeleportToGame(GameName, Options)
	Options = Options or {}

	local GameInfo = nil
	for _, Game in ipairs(GameData) do
		if Game.Name == GameName then
			GameInfo = Game
			break
		end
	end

	if not GameInfo then
		self:Notify("Teleport Error", "Game '" .. GameName .. "' not found in database.", 5)
		return false
	end

	if not GameInfo.Available then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	local GameId = GameInfo.Id
	if type(GameId) == "table" then
		GameId = GameId[1]
	end

	if GameId == 0 then
		self:Notify("Coming Soon", GameName .. " teleport will be available soon!", 3)
		return false
	end

	self:Notify("Teleporting", "Preparing to teleport to " .. GameName .. "...", 3)

	wait(1)

	local TeleportOptions = Instance.new("TeleportOptions")

	if Options.ReserveServer then
		TeleportOptions.ReservedServerAccessCode = Options.ReserveServer
	end

	if Options.ServerJobId then
		TeleportOptions.ServerInstanceId = Options.ServerJobId
	elseif Options.FindBestServer then
		local BestServerId = self:FindBestServer(GameId)
		if BestServerId then
			TeleportOptions.ServerInstanceId = BestServerId
			self:Notify("Server Found", "Found a good server! Teleporting...", 2)
		end
	end

	local Success, ErrorMessage = pcall(function()
		Services.Services.TeleportService:TeleportAsync(GameId, {self:GetPlayer()}, TeleportOptions)
	end)

	if not Success then
		local FriendlyMessage = "Failed to teleport. Please check your internet connection."

		if string.find(ErrorMessage, "unauthorized") or string.find(ErrorMessage, "Unauthorized") then
			FriendlyMessage = "Please enable 'Allow Third Party Teleports' in your Roblox account settings."
		elseif string.find(ErrorMessage, "flooded") then
			FriendlyMessage = "Too many teleport requests. Please wait 30 seconds and try again."
		elseif string.find(ErrorMessage, "not found") then
			FriendlyMessage = "Game is currently unavailable. Please try again later."
		end

		self:Notify("Teleport Failed", FriendlyMessage, 7)
		return false
	end

	return true
end

function Utility:TeleportWithPrompt(GameName, Options)
	self:Prompt(false, "Are you sure you want to teleport to " .. GameName .. "?", {
		FirstName = "Yes",
		SecondName = "Cancel",
		FirstCallback = function()
			self:TeleportToGame(GameName, Options)
		end,
		SecondCallback = function()
			self:Notify("Cancelled", "Teleport cancelled.", 2)
		end,
	})
end

function Utility:GetGameLink(GameName)
	for _, GameInfo in ipairs(GameData) do
		if GameInfo.Name == GameName then
			return GameInfo.Link
		end
	end
	return nil
end

function Utility:CopyGameLink(GameName)
	local Link = self:GetGameLink(GameName)
	if Link then
		setclipboard(Link)
		self:Notify("Link Copied", "Game link copied to clipboard!", 3)
	else
		self:Notify("Error", "Game link not available.", 3)
	end
end

function Utility:QueueScript()
	if not getgenv().QueuedTeleport then
		queue_on_teleport(readfile("Wave-Hub/Script-Loader.lua"))
		getgenv().QueuedTeleport = true
	end
end

function Utility:DisableResetOnSpawn()
	Maid.ScreenGuisWithResetOnSpawn = {}
	local UserInterface = self:GetUserInterface()
	if UserInterface then
		for _, Object in pairs(UserInterface:GetChildren()) do
			if Object:IsA("ScreenGui") and Object.ResetOnSpawn then
				table.insert(Maid.ScreenGuisWithResetOnSpawn, Object)
				Object.ResetOnSpawn = false
			end
		end
	end
end

function Utility:EnableResetOnSpawn()
	for _, ScreenGui in pairs(Maid.ScreenGuisWithResetOnSpawn) do
		if ScreenGui and ScreenGui.Parent then
			ScreenGui.ResetOnSpawn = true
		end
	end
	Maid.ScreenGuisWithResetOnSpawn = {}
end

function Utility:CreateCommunicationChannel()
	local Identity, Channel = create_comm_channel()
	return Identity, Channel
end

function Utility:ExecuteFunction(Arguments)
	return function()
		local OnStart = Arguments.OnStart
		local Callback = Arguments.Callback
		local OnCompleted = Arguments.OnCompleted

		local Results = {
			OnStartResult = nil,
			CallbackResult = nil,
			OnCompletedResult = nil,
			StartingNotification = nil,
			Success = true,
			Errors = {},
		}

		if OnStart and type(OnStart) == "function" then
			local Success, Result = pcall(OnStart)
			if Success then
				Results.OnStartResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnStart", Error = Result})
			end
		elseif OnStart and type(OnStart) == "string" then
			local Notification = WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnStart)
			})

			if Arguments.StartingLoadingEffect then
				Results.StartingNotification = Notification

				task.spawn(function()
					local Dots = {"", ".", "..", "..."}
					local Index = 1

					while Arguments.StartingLoadingEffect do
						Notification:UpdateDescription(OnStart .. Dots[Index])
						Index = Index % #Dots + 1
						task.wait(0.25)
					end
				end)
			end
		end

		if Callback and type(Callback) == "function" then
			local Success, Result = pcall(Callback)
			if Success then
				Results.CallbackResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "Callback", Error = Result})
			end
		end

		if OnCompleted and type(OnCompleted) == "function" then
			local Success, Result = pcall(OnCompleted)
			if Success then
				Results.OnCompletedResult = Result
			else
				Results.Success = false
				table.insert(Results.Errors, {Stage = "OnCompleted", Error = Result})
			end
		elseif OnCompleted and type(OnCompleted) == "string" then
			if Results.StartingNotification then
				Results.StartingNotification:Cancel()
			end

			WaveLibrary:Notify({
				Title = getgenv().WaveLibrary.Settings.Title,
				Description = (OnCompleted)
			})
		end

		return Results
	end
end

function Utility:ExecuteHTTPGet(Link, Async)
	return Async and game:HttpGetAsync(Link) or game:HttpGet(Link)
end

function Utility:RenderWait(Time)
	return function()
		local Current = tick()
		Time = Time or 0

		repeat Services.RunService.Heartbeat:Wait()
		until tick() - Current >= Time

		return tick() - Current
	end
end

function Utility:KillMaidInstance(Name)
	if Maid[Name] then
		if type(Maid[Name]) == "thread" then
			task.cancel(Maid[Name])
		elseif type(Maid[Name]) == "RBXScriptConnection" then
			Maid[Name]:Disconnect()
		end
	end
	Maid[Name] = nil
end

function Utility:Reload()
	if getgenv().WaveLibrary then
		WaveLibrary:Unload()
	end
end

function Utility:IsWaveExecutor()
	return getexecutorname():match("Wave") ~= nil
end

function Utility:IsPremiumHubUser()
	return true
end

function Utility:CheckHubStatus(PrimaryAccentColor)
	if self:IsWaveExecutor() and not self:IsPremiumHubUser() then
		return '<font color="rgb(0,255,0)">Free</font>'
	elseif self:IsWaveExecutor() and self:IsPremiumHubUser() then
		return string.format('<font color="rgb(%d,%d,%d)">Premium</font>', PrimaryAccentColor.R * 255, PrimaryAccentColor.G * 255, PrimaryAccentColor.B * 255)
	end

	return '<font color="rgb(128,128,128)">Demo</font>'
end

Utility:LoadFile("Wave/Modules/Services.lua")

return MetaUtility]],
  ["Xenith/Xenith/UserInterface/Components/Button.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Button(Settings, Flag)
        local ButtonFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "Button",
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local function CreateElements()
            local Elements = {}

            Elements.Button = Instance.new("Frame")
            Elements.Button.Name = "Button"
            Elements.Button.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Button.BackgroundTransparency = 1
            Elements.Button.BorderSizePixel = 0
            Elements.Button.Size = UDim2.new(1, 0, 0, 38)
            Elements.Button.Parent = Section

            Elements.ButtonInteract = Instance.new("TextButton")
            Elements.ButtonInteract.Name = "ButtonInteract"
            Elements.ButtonInteract.FontFace = Font.new(Assets.InterFont)
            Elements.ButtonInteract.RichText = true
            Elements.ButtonInteract.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ButtonInteract.TextSize = 13
            Elements.ButtonInteract.TextTransparency = TRANSPARENCY.Default
            Elements.ButtonInteract.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ButtonInteract.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ButtonInteract.BackgroundTransparency = 1
            Elements.ButtonInteract.BorderSizePixel = 0
            Elements.ButtonInteract.Size = UDim2.fromScale(1, 1)
            Elements.ButtonInteract.Text = Settings.Name
            Elements.ButtonInteract.Parent = Elements.Button

            if Settings.PremiumFeature then
                Elements.ButtonInteract.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ButtonInteract
            end

            Elements.ButtonImage = Instance.new("ImageLabel")
            Elements.ButtonImage.Name = "ButtonImage"
            Elements.ButtonImage.Image = Assets.ButtonImage
            Elements.ButtonImage.ImageTransparency = TRANSPARENCY.Default
            Elements.ButtonImage.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ButtonImage.BackgroundTransparency = 1
            Elements.ButtonImage.BorderSizePixel = 0
            Elements.ButtonImage.Position = UDim2.fromScale(1, 0.5)
            Elements.ButtonImage.Size = UDim2.fromOffset(15, 15)
            Elements.ButtonImage.Parent = Elements.Button

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ButtonFunctions.Elements.Button,
                Class = ButtonFunctions.Class,
                Maid = ButtonFunctions.Maid,
                Functions = ButtonFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ButtonFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ButtonFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function HandleButtonClick()
            if ButtonFunctions.IsHidden then return end
            if Settings.Callback then
                Settings.Callback()
            end
        end

        local function ChangeState(State)
            if ButtonFunctions.IsHidden then return end

            local TextTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default
            local ImageTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default

            if not Settings.PremiumFeature then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {
                    TextTransparency = TextTransparency
                }):Play()
            end

            if Utility:IsPremiumHubUser() then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {
                    ImageTransparency = ImageTransparency
                }):Play()
            end
        end

        local function SetupConnections()
            ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)

            ButtonFunctions.Maid.ButtonHover = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(function()
                if ButtonFunctions.IsHidden then return end
                ChangeState("Hover")
            end)

            ButtonFunctions.Maid.ButtonLeave = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(function()
                ChangeState("Idle")
                if XenithLibraryUI:FindFirstChild("HoverFrame") then
                    XenithLibraryUI:FindFirstChild("HoverFrame"):Destroy()
                end
            end)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ButtonFunctions:UpdateName(Name)
            ButtonFunctions.Elements.ButtonInteract.Text = Name
        end

        function ButtonFunctions:SetVisibility(State)
            ButtonFunctions.Elements.Button.Visible = State
        end

        function ButtonFunctions:GetState()
            return not ButtonFunctions.IsHidden
        end
        
        function ButtonFunctions:ReturnHidden()
            return ButtonFunctions.IsHidden
        end

        function ButtonFunctions:ReturnSettings()
            return Settings
        end

        function ButtonFunctions:ReturnClass()
            return ButtonFunctions.Class
        end

        function ButtonFunctions:ReturnTab()
            return ButtonFunctions.Tab
        end

        function ButtonFunctions:RunCallback()
            HandleButtonClick()
        end

        function ButtonFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ButtonFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ButtonFunctions.Elements.KeybindButton,
                KeybindLabel = ButtonFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ButtonFunctions.Maid.ButtonClick then
                    ButtonFunctions.Maid.ButtonClick:Disconnect()
                    ButtonFunctions.Maid.ButtonClick = nil
                end
            else
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Default}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)
            end
        end

        function ButtonFunctions:SetToolTip(Tip)
            if ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame") then
                ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ButtonFunctions.Elements.Button

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            ButtonFunctions.Maid.TooltipShow = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(ShowTooltip)
            ButtonFunctions.Maid.TooltipHide = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(HideTooltip)
        end

        function ButtonFunctions:Destroy()
            for ConnectionName, Connection in pairs(ButtonFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(ButtonFunctions.Maid)

            if ButtonFunctions.Elements.Button then
                ButtonFunctions.Elements.Button:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ButtonFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ButtonFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ButtonFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.ToolTip then
            ButtonFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ButtonFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ButtonFunctions
        end

        return ButtonFunctions
    end
end]],
  ["Xenith/UserInterface/Components/Button.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Button(Settings, Flag)
        local ButtonFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "Button",
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local function CreateElements()
            local Elements = {}

            Elements.Button = Instance.new("Frame")
            Elements.Button.Name = "Button"
            Elements.Button.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Button.BackgroundTransparency = 1
            Elements.Button.BorderSizePixel = 0
            Elements.Button.Size = UDim2.new(1, 0, 0, 38)
            Elements.Button.Parent = Section

            Elements.ButtonInteract = Instance.new("TextButton")
            Elements.ButtonInteract.Name = "ButtonInteract"
            Elements.ButtonInteract.FontFace = Font.new(Assets.InterFont)
            Elements.ButtonInteract.RichText = true
            Elements.ButtonInteract.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ButtonInteract.TextSize = 13
            Elements.ButtonInteract.TextTransparency = TRANSPARENCY.Default
            Elements.ButtonInteract.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ButtonInteract.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ButtonInteract.BackgroundTransparency = 1
            Elements.ButtonInteract.BorderSizePixel = 0
            Elements.ButtonInteract.Size = UDim2.fromScale(1, 1)
            Elements.ButtonInteract.Text = Settings.Name
            Elements.ButtonInteract.Parent = Elements.Button

            if Settings.PremiumFeature then
                Elements.ButtonInteract.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ButtonInteract
            end

            Elements.ButtonImage = Instance.new("ImageLabel")
            Elements.ButtonImage.Name = "ButtonImage"
            Elements.ButtonImage.Image = Assets.ButtonImage
            Elements.ButtonImage.ImageTransparency = TRANSPARENCY.Default
            Elements.ButtonImage.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ButtonImage.BackgroundTransparency = 1
            Elements.ButtonImage.BorderSizePixel = 0
            Elements.ButtonImage.Position = UDim2.fromScale(1, 0.5)
            Elements.ButtonImage.Size = UDim2.fromOffset(15, 15)
            Elements.ButtonImage.Parent = Elements.Button

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ButtonFunctions.Elements.Button,
                Class = ButtonFunctions.Class,
                Maid = ButtonFunctions.Maid,
                Functions = ButtonFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ButtonFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ButtonFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function HandleButtonClick()
            if ButtonFunctions.IsHidden then return end
            if Settings.Callback then
                Settings.Callback()
            end
        end

        local function ChangeState(State)
            if ButtonFunctions.IsHidden then return end

            local TextTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default
            local ImageTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default

            if not Settings.PremiumFeature then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {
                    TextTransparency = TextTransparency
                }):Play()
            end

            if Utility:IsPremiumHubUser() then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {
                    ImageTransparency = ImageTransparency
                }):Play()
            end
        end

        local function SetupConnections()
            ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)

            ButtonFunctions.Maid.ButtonHover = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(function()
                if ButtonFunctions.IsHidden then return end
                ChangeState("Hover")
            end)

            ButtonFunctions.Maid.ButtonLeave = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(function()
                ChangeState("Idle")
                if XenithLibraryUI:FindFirstChild("HoverFrame") then
                    XenithLibraryUI:FindFirstChild("HoverFrame"):Destroy()
                end
            end)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ButtonFunctions:UpdateName(Name)
            ButtonFunctions.Elements.ButtonInteract.Text = Name
        end

        function ButtonFunctions:SetVisibility(State)
            ButtonFunctions.Elements.Button.Visible = State
        end

        function ButtonFunctions:GetState()
            return not ButtonFunctions.IsHidden
        end
        
        function ButtonFunctions:ReturnHidden()
            return ButtonFunctions.IsHidden
        end

        function ButtonFunctions:ReturnSettings()
            return Settings
        end

        function ButtonFunctions:ReturnClass()
            return ButtonFunctions.Class
        end

        function ButtonFunctions:ReturnTab()
            return ButtonFunctions.Tab
        end

        function ButtonFunctions:RunCallback()
            HandleButtonClick()
        end

        function ButtonFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ButtonFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ButtonFunctions.Elements.KeybindButton,
                KeybindLabel = ButtonFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ButtonFunctions.Maid.ButtonClick then
                    ButtonFunctions.Maid.ButtonClick:Disconnect()
                    ButtonFunctions.Maid.ButtonClick = nil
                end
            else
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Default}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)
            end
        end

        function ButtonFunctions:SetToolTip(Tip)
            if ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame") then
                ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ButtonFunctions.Elements.Button

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            ButtonFunctions.Maid.TooltipShow = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(ShowTooltip)
            ButtonFunctions.Maid.TooltipHide = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(HideTooltip)
        end

        function ButtonFunctions:Destroy()
            for ConnectionName, Connection in pairs(ButtonFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(ButtonFunctions.Maid)

            if ButtonFunctions.Elements.Button then
                ButtonFunctions.Elements.Button:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ButtonFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ButtonFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ButtonFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.ToolTip then
            ButtonFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ButtonFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ButtonFunctions
        end

        return ButtonFunctions
    end
end]],
  ["UserInterface/Components/Button.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    local ComponentManager = Requirements.ComponentManager

    function SectionFunctions:Button(Settings, Flag)
        local ButtonFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "Button",
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local function CreateElements()
            local Elements = {}

            Elements.Button = Instance.new("Frame")
            Elements.Button.Name = "Button"
            Elements.Button.AutomaticSize = Enum.AutomaticSize.Y
            Elements.Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.Button.BackgroundTransparency = 1
            Elements.Button.BorderSizePixel = 0
            Elements.Button.Size = UDim2.new(1, 0, 0, 38)
            Elements.Button.Parent = Section

            Elements.ButtonInteract = Instance.new("TextButton")
            Elements.ButtonInteract.Name = "ButtonInteract"
            Elements.ButtonInteract.FontFace = Font.new(Assets.InterFont)
            Elements.ButtonInteract.RichText = true
            Elements.ButtonInteract.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ButtonInteract.TextSize = 13
            Elements.ButtonInteract.TextTransparency = TRANSPARENCY.Default
            Elements.ButtonInteract.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ButtonInteract.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ButtonInteract.BackgroundTransparency = 1
            Elements.ButtonInteract.BorderSizePixel = 0
            Elements.ButtonInteract.Size = UDim2.fromScale(1, 1)
            Elements.ButtonInteract.Text = Settings.Name
            Elements.ButtonInteract.Parent = Elements.Button

            if Settings.PremiumFeature then
                Elements.ButtonInteract.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ButtonInteract
            end

            Elements.ButtonImage = Instance.new("ImageLabel")
            Elements.ButtonImage.Name = "ButtonImage"
            Elements.ButtonImage.Image = Assets.ButtonImage
            Elements.ButtonImage.ImageTransparency = TRANSPARENCY.Default
            Elements.ButtonImage.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ButtonImage.BackgroundTransparency = 1
            Elements.ButtonImage.BorderSizePixel = 0
            Elements.ButtonImage.Position = UDim2.fromScale(1, 0.5)
            Elements.ButtonImage.Size = UDim2.fromOffset(15, 15)
            Elements.ButtonImage.Parent = Elements.Button

            return Elements
        end

        local function CreateKeybind()
            local KeybindComponents = ComponentManager.CreateKeybindComponents({
                Ancestor = ButtonFunctions.Elements.Button,
                Class = ButtonFunctions.Class,
                Maid = ButtonFunctions.Maid,
                Functions = ButtonFunctions,
                Settings = Settings,
                Flag = Flag
            })

            if KeybindComponents then
                ButtonFunctions.Elements.KeybindButton = KeybindComponents.KeybindButton
                ButtonFunctions.Elements.KeybindLabel = KeybindComponents.KeybindLabel
            end
        end

        local function HandleButtonClick()
            if ButtonFunctions.IsHidden then return end
            if Settings.Callback then
                Settings.Callback()
            end
        end

        local function ChangeState(State)
            if ButtonFunctions.IsHidden then return end

            local TextTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default
            local ImageTransparency = State == "Hover" and TRANSPARENCY.Hover or TRANSPARENCY.Default

            if not Settings.PremiumFeature then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {
                    TextTransparency = TextTransparency
                }):Play()
            end

            if Utility:IsPremiumHubUser() then
                Utility:PropertyTween(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {
                    ImageTransparency = ImageTransparency
                }):Play()
            end
        end

        local function SetupConnections()
            ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)

            ButtonFunctions.Maid.ButtonHover = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(function()
                if ButtonFunctions.IsHidden then return end
                ChangeState("Hover")
            end)

            ButtonFunctions.Maid.ButtonLeave = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(function()
                ChangeState("Idle")
                if XenithLibraryUI:FindFirstChild("HoverFrame") then
                    XenithLibraryUI:FindFirstChild("HoverFrame"):Destroy()
                end
            end)
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function ButtonFunctions:UpdateName(Name)
            ButtonFunctions.Elements.ButtonInteract.Text = Name
        end

        function ButtonFunctions:SetVisibility(State)
            ButtonFunctions.Elements.Button.Visible = State
        end

        function ButtonFunctions:GetState()
            return not ButtonFunctions.IsHidden
        end
        
        function ButtonFunctions:ReturnHidden()
            return ButtonFunctions.IsHidden
        end

        function ButtonFunctions:ReturnSettings()
            return Settings
        end

        function ButtonFunctions:ReturnClass()
            return ButtonFunctions.Class
        end

        function ButtonFunctions:ReturnTab()
            return ButtonFunctions.Tab
        end

        function ButtonFunctions:RunCallback()
            HandleButtonClick()
        end

        function ButtonFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            ButtonFunctions.IsHidden = State
            ComponentManager.UpdateKeybindDetection(State, Flag, {
                KeybindButton = ButtonFunctions.Elements.KeybindButton,
                KeybindLabel = ButtonFunctions.Elements.KeybindLabel,
            })

            if State then
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Hidden}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Hidden}):Play()
                end

                if ButtonFunctions.Maid.ButtonClick then
                    ButtonFunctions.Maid.ButtonClick:Disconnect()
                    ButtonFunctions.Maid.ButtonClick = nil
                end
            else
                Services.TweenService:Create(ButtonFunctions.Elements.ButtonImage, TWEEN_INFO, {ImageTransparency = TRANSPARENCY.Default}):Play()

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(ButtonFunctions.Elements.ButtonInteract, TWEEN_INFO, {TextTransparency = TRANSPARENCY.Default}):Play()
                end

                ButtonFunctions.Maid.ButtonClick = ButtonFunctions.Elements.ButtonInteract.MouseButton1Click:Connect(HandleButtonClick)
            end
        end

        function ButtonFunctions:SetToolTip(Tip)
            if ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame") then
                ButtonFunctions.Elements.Button:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = ButtonFunctions.Elements.Button

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            local function ShowTooltip()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end

            local function HideTooltip()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.spawn(function()
                    task.wait(0.2)
                    if ToolTipFrame and ToolTipFrame.Parent then
                        ToolTipFrame.Visible = false
                    end
                end)
            end

            ButtonFunctions.Maid.TooltipShow = ButtonFunctions.Elements.ButtonInteract.MouseEnter:Connect(ShowTooltip)
            ButtonFunctions.Maid.TooltipHide = ButtonFunctions.Elements.ButtonInteract.MouseLeave:Connect(HideTooltip)
        end

        function ButtonFunctions:Destroy()
            for ConnectionName, Connection in pairs(ButtonFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(ButtonFunctions.Maid)

            if ButtonFunctions.Elements.Button then
                ButtonFunctions.Elements.Button:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        ButtonFunctions.Elements = CreateElements()
        CreateKeybind()
        SetupConnections()

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                ButtonFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                ButtonFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.ToolTip then
            ButtonFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            ButtonFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            ButtonFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = ButtonFunctions
        end

        return ButtonFunctions
    end
end]],
  ["Xenith/Xenith/UserInterface/Components/CheckBox.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:CheckBox(Settings, Flag)
        local CheckBoxFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "CheckBox",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)

        local function CreateElements()
            local Elements = {}

            Elements.CheckBox = Instance.new("Frame")
            Elements.CheckBox.Name = "CheckBox"
            Elements.CheckBox.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.CheckBox.BackgroundTransparency = 1
            Elements.CheckBox.BorderSizePixel = 0
            Elements.CheckBox.Size = UDim2.new(1, 0, 0, 38)
            Elements.CheckBox.Parent = Section

            Elements.CheckBoxName = Instance.new("TextLabel")
            Elements.CheckBoxName.Name = "CheckBoxName"
            Elements.CheckBoxName.FontFace = Font.new(Assets.InterFont)
            Elements.CheckBoxName.Text = Settings.Name
            Elements.CheckBoxName.RichText = true
            Elements.CheckBoxName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.CheckBoxName.TextSize = 13
            Elements.CheckBoxName.TextTransparency = 0.5
            Elements.CheckBoxName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.CheckBoxName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.CheckBoxName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.CheckBoxName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.CheckBoxName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBoxName.BackgroundTransparency = 1
            Elements.CheckBoxName.Position = UDim2.fromScale(0, 0.5)
            Elements.CheckBoxName.Size = UDim2.new(1, -35, 0, 0)
            Elements.CheckBoxName.Parent = Elements.CheckBox

            if Settings.PremiumFeature then
                Elements.CheckBoxName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.CheckBoxName
            end

            Elements.CheckBoxButton = Instance.new("TextButton")
            Elements.CheckBoxButton.Name = "CheckBoxButton"
            Elements.CheckBoxButton.Text = ""
            Elements.CheckBoxButton.AutoButtonColor = false
            Elements.CheckBoxButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.CheckBoxButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Elements.CheckBoxButton.BackgroundTransparency = 0.9
            Elements.CheckBoxButton.BorderSizePixel = 0
            Elements.CheckBoxButton.Position = UDim2.fromScale(1, 0.5)
            Elements.CheckBoxButton.Size = UDim2.fromOffset(18, 18)
            Elements.CheckBoxButton.Parent = Elements.CheckBox

            local CheckBoxUICorner = Instance.new("UICorner")
            CheckBoxUICorner.CornerRadius = UDim.new(0, 4)
            CheckBoxUICorner.Parent = Elements.CheckBoxButton

            local CheckBoxUIStroke = Instance.new("UIStroke")
            CheckBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            CheckBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
            CheckBoxUIStroke.Thickness = 1
            CheckBoxUIStroke.Transparency = 0.8
            CheckBoxUIStroke.Parent = Elements.CheckBoxButton

            Elements.Checkmark = Instance.new("TextLabel")
            Elements.Checkmark.Name = "Checkmark"
            Elements.Checkmark.Text = ""
            Elements.Checkmark.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
            Elements.Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.Checkmark.TextSize = 12
            Elements.Checkmark.TextScaled = true
            Elements.Checkmark.TextTransparency = 1
            Elements.Checkmark.BackgroundTransparency = 1
            Elements.Checkmark.BorderSizePixel = 0
            Elements.Checkmark.Position = UDim2.fromScale(0, 0)
            Elements.Checkmark.Size = UDim2.fromScale(1, 1)
            Elements.Checkmark.TextXAlignment = Enum.TextXAlignment.Center
            Elements.Checkmark.TextYAlignment = Enum.TextYAlignment.Center
            Elements.Checkmark.ZIndex = 2
            Elements.Checkmark.Parent = Elements.CheckBoxButton

            return Elements
        end

        local function UpdateCheckBoxState(State)
            if State then
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.7,
                    BackgroundColor3 = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 1)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 0
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.3,
                    Color = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                })
                if StrokeTween then StrokeTween:Play() end
            else
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.9,
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 12)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 1
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.8,
                    Color = Color3.fromRGB(255, 255, 255)
                })
                if StrokeTween then StrokeTween:Play() end
            end

            CheckBoxFunctions.State = State
        end

        local function CheckBoxTrigger()
            if Settings.Fixed or CheckBoxFunctions.IsHidden then return end

            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)

            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        local function SetupConnections()
            CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency

                CheckBoxFunctions.Maid.CheckBoxHover = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = 0})
                end)

                CheckBoxFunctions.Maid.CheckBoxLeave = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function CheckBoxFunctions:Check()
            CheckBoxTrigger()
        end

        function CheckBoxFunctions:ServerCheck()
            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        function CheckBoxFunctions:UpdateState(State, Callback)
            CheckBoxFunctions.State = State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(CheckBoxFunctions.State)
                end
            end
        end

        function CheckBoxFunctions:GetState()
            return CheckBoxFunctions.State
        end

        function CheckBoxFunctions:ReturnSettings()
            return Settings
        end

        function CheckBoxFunctions:ReturnTab()
            return CheckBoxFunctions.Tab
        end

        function CheckBoxFunctions:ReturnClass()
            return CheckBoxFunctions.Class
        end

        function CheckBoxFunctions:UpdateName(Name)
            CheckBoxFunctions.Elements.CheckBoxName.Text = Name
        end

        function CheckBoxFunctions:SetToolTip(Tip)
            if CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame") then
                CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = CheckBoxFunctions.Elements.CheckBox

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            CheckBoxFunctions.Maid.TooltipShow = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            CheckBoxFunctions.Maid.TooltipHide = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function CheckBoxFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            CheckBoxFunctions.IsHidden = State

            if State then
                Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxButton, TweenInfo.new(0.2), {BackgroundTransparency = 0.99}):Play()
                Services.TweenService:Create(CheckBoxFunctions.Elements.Checkmark, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()

                local Stroke = CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke")
                if Stroke then
                    Services.TweenService:Create(Stroke, TweenInfo.new(0.2), {Transparency = 0.95}):Play()
                end

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if CheckBoxFunctions.Maid.CheckBoxClick then
                    CheckBoxFunctions.Maid.CheckBoxClick:Disconnect()
                    CheckBoxFunctions.Maid.CheckBoxClick = nil
                end
            else
                UpdateCheckBoxState(CheckBoxFunctions.State)

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)
            end
        end

        function CheckBoxFunctions:SetVisibility(State)
            CheckBoxFunctions.Elements.CheckBox.Visible = State
        end

        function CheckBoxFunctions:Destroy()
            for ConnectionName, Connection in pairs(CheckBoxFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(CheckBoxFunctions.Maid)

            if CheckBoxFunctions.Elements.CheckBox then
                CheckBoxFunctions.Elements.CheckBox:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        CheckBoxFunctions.Elements = CreateElements()
        SetupConnections()
        UpdateCheckBoxState(CheckBoxFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                CheckBoxFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                CheckBoxFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            CheckBoxFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            CheckBoxFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = CheckBoxFunctions
        end

        return CheckBoxFunctions
    end
end]],
  ["Xenith/UserInterface/Components/CheckBox.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:CheckBox(Settings, Flag)
        local CheckBoxFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "CheckBox",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)

        local function CreateElements()
            local Elements = {}

            Elements.CheckBox = Instance.new("Frame")
            Elements.CheckBox.Name = "CheckBox"
            Elements.CheckBox.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.CheckBox.BackgroundTransparency = 1
            Elements.CheckBox.BorderSizePixel = 0
            Elements.CheckBox.Size = UDim2.new(1, 0, 0, 38)
            Elements.CheckBox.Parent = Section

            Elements.CheckBoxName = Instance.new("TextLabel")
            Elements.CheckBoxName.Name = "CheckBoxName"
            Elements.CheckBoxName.FontFace = Font.new(Assets.InterFont)
            Elements.CheckBoxName.Text = Settings.Name
            Elements.CheckBoxName.RichText = true
            Elements.CheckBoxName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.CheckBoxName.TextSize = 13
            Elements.CheckBoxName.TextTransparency = 0.5
            Elements.CheckBoxName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.CheckBoxName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.CheckBoxName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.CheckBoxName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.CheckBoxName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBoxName.BackgroundTransparency = 1
            Elements.CheckBoxName.Position = UDim2.fromScale(0, 0.5)
            Elements.CheckBoxName.Size = UDim2.new(1, -35, 0, 0)
            Elements.CheckBoxName.Parent = Elements.CheckBox

            if Settings.PremiumFeature then
                Elements.CheckBoxName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.CheckBoxName
            end

            Elements.CheckBoxButton = Instance.new("TextButton")
            Elements.CheckBoxButton.Name = "CheckBoxButton"
            Elements.CheckBoxButton.Text = ""
            Elements.CheckBoxButton.AutoButtonColor = false
            Elements.CheckBoxButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.CheckBoxButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Elements.CheckBoxButton.BackgroundTransparency = 0.9
            Elements.CheckBoxButton.BorderSizePixel = 0
            Elements.CheckBoxButton.Position = UDim2.fromScale(1, 0.5)
            Elements.CheckBoxButton.Size = UDim2.fromOffset(18, 18)
            Elements.CheckBoxButton.Parent = Elements.CheckBox

            local CheckBoxUICorner = Instance.new("UICorner")
            CheckBoxUICorner.CornerRadius = UDim.new(0, 4)
            CheckBoxUICorner.Parent = Elements.CheckBoxButton

            local CheckBoxUIStroke = Instance.new("UIStroke")
            CheckBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            CheckBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
            CheckBoxUIStroke.Thickness = 1
            CheckBoxUIStroke.Transparency = 0.8
            CheckBoxUIStroke.Parent = Elements.CheckBoxButton

            Elements.Checkmark = Instance.new("TextLabel")
            Elements.Checkmark.Name = "Checkmark"
            Elements.Checkmark.Text = ""
            Elements.Checkmark.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
            Elements.Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.Checkmark.TextSize = 12
            Elements.Checkmark.TextScaled = true
            Elements.Checkmark.TextTransparency = 1
            Elements.Checkmark.BackgroundTransparency = 1
            Elements.Checkmark.BorderSizePixel = 0
            Elements.Checkmark.Position = UDim2.fromScale(0, 0)
            Elements.Checkmark.Size = UDim2.fromScale(1, 1)
            Elements.Checkmark.TextXAlignment = Enum.TextXAlignment.Center
            Elements.Checkmark.TextYAlignment = Enum.TextYAlignment.Center
            Elements.Checkmark.ZIndex = 2
            Elements.Checkmark.Parent = Elements.CheckBoxButton

            return Elements
        end

        local function UpdateCheckBoxState(State)
            if State then
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.7,
                    BackgroundColor3 = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 1)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 0
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.3,
                    Color = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                })
                if StrokeTween then StrokeTween:Play() end
            else
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.9,
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 12)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 1
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.8,
                    Color = Color3.fromRGB(255, 255, 255)
                })
                if StrokeTween then StrokeTween:Play() end
            end

            CheckBoxFunctions.State = State
        end

        local function CheckBoxTrigger()
            if Settings.Fixed or CheckBoxFunctions.IsHidden then return end

            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)

            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        local function SetupConnections()
            CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency

                CheckBoxFunctions.Maid.CheckBoxHover = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = 0})
                end)

                CheckBoxFunctions.Maid.CheckBoxLeave = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function CheckBoxFunctions:Check()
            CheckBoxTrigger()
        end

        function CheckBoxFunctions:ServerCheck()
            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        function CheckBoxFunctions:UpdateState(State, Callback)
            CheckBoxFunctions.State = State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(CheckBoxFunctions.State)
                end
            end
        end

        function CheckBoxFunctions:GetState()
            return CheckBoxFunctions.State
        end

        function CheckBoxFunctions:ReturnSettings()
            return Settings
        end

        function CheckBoxFunctions:ReturnTab()
            return CheckBoxFunctions.Tab
        end

        function CheckBoxFunctions:ReturnClass()
            return CheckBoxFunctions.Class
        end

        function CheckBoxFunctions:UpdateName(Name)
            CheckBoxFunctions.Elements.CheckBoxName.Text = Name
        end

        function CheckBoxFunctions:SetToolTip(Tip)
            if CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame") then
                CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = CheckBoxFunctions.Elements.CheckBox

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            CheckBoxFunctions.Maid.TooltipShow = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            CheckBoxFunctions.Maid.TooltipHide = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function CheckBoxFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            CheckBoxFunctions.IsHidden = State

            if State then
                Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxButton, TweenInfo.new(0.2), {BackgroundTransparency = 0.99}):Play()
                Services.TweenService:Create(CheckBoxFunctions.Elements.Checkmark, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()

                local Stroke = CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke")
                if Stroke then
                    Services.TweenService:Create(Stroke, TweenInfo.new(0.2), {Transparency = 0.95}):Play()
                end

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if CheckBoxFunctions.Maid.CheckBoxClick then
                    CheckBoxFunctions.Maid.CheckBoxClick:Disconnect()
                    CheckBoxFunctions.Maid.CheckBoxClick = nil
                end
            else
                UpdateCheckBoxState(CheckBoxFunctions.State)

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)
            end
        end

        function CheckBoxFunctions:SetVisibility(State)
            CheckBoxFunctions.Elements.CheckBox.Visible = State
        end

        function CheckBoxFunctions:Destroy()
            for ConnectionName, Connection in pairs(CheckBoxFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(CheckBoxFunctions.Maid)

            if CheckBoxFunctions.Elements.CheckBox then
                CheckBoxFunctions.Elements.CheckBox:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        CheckBoxFunctions.Elements = CreateElements()
        SetupConnections()
        UpdateCheckBoxState(CheckBoxFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                CheckBoxFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                CheckBoxFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            CheckBoxFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            CheckBoxFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = CheckBoxFunctions
        end

        return CheckBoxFunctions
    end
end]],
  ["UserInterface/Components/CheckBox.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale

    function SectionFunctions:CheckBox(Settings, Flag)
        local CheckBoxFunctions = {
            Settings = Settings,
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Class = "CheckBox",
            State = Settings.Default or false,
            IsHidden = false,
            Maid = {},
            Elements = {}
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)

        local function CreateElements()
            local Elements = {}

            Elements.CheckBox = Instance.new("Frame")
            Elements.CheckBox.Name = "CheckBox"
            Elements.CheckBox.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Elements.CheckBox.BackgroundTransparency = 1
            Elements.CheckBox.BorderSizePixel = 0
            Elements.CheckBox.Size = UDim2.new(1, 0, 0, 38)
            Elements.CheckBox.Parent = Section

            Elements.CheckBoxName = Instance.new("TextLabel")
            Elements.CheckBoxName.Name = "CheckBoxName"
            Elements.CheckBoxName.FontFace = Font.new(Assets.InterFont)
            Elements.CheckBoxName.Text = Settings.Name
            Elements.CheckBoxName.RichText = true
            Elements.CheckBoxName.TextColor3 = WindowFunctions:GetCurrentThemeAttribute("TextPrimary")
            Elements.CheckBoxName.TextSize = 13
            Elements.CheckBoxName.TextTransparency = 0.5
            Elements.CheckBoxName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.CheckBoxName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.CheckBoxName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.CheckBoxName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.CheckBoxName.AutomaticSize = Enum.AutomaticSize.Y
            Elements.CheckBoxName.BackgroundTransparency = 1
            Elements.CheckBoxName.Position = UDim2.fromScale(0, 0.5)
            Elements.CheckBoxName.Size = UDim2.new(1, -35, 0, 0)
            Elements.CheckBoxName.Parent = Elements.CheckBox

            if Settings.PremiumFeature then
                Elements.CheckBoxName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.CheckBoxName
            end

            Elements.CheckBoxButton = Instance.new("TextButton")
            Elements.CheckBoxButton.Name = "CheckBoxButton"
            Elements.CheckBoxButton.Text = ""
            Elements.CheckBoxButton.AutoButtonColor = false
            Elements.CheckBoxButton.AnchorPoint = Vector2.new(1, 0.5)
            Elements.CheckBoxButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Elements.CheckBoxButton.BackgroundTransparency = 0.9
            Elements.CheckBoxButton.BorderSizePixel = 0
            Elements.CheckBoxButton.Position = UDim2.fromScale(1, 0.5)
            Elements.CheckBoxButton.Size = UDim2.fromOffset(18, 18)
            Elements.CheckBoxButton.Parent = Elements.CheckBox

            local CheckBoxUICorner = Instance.new("UICorner")
            CheckBoxUICorner.CornerRadius = UDim.new(0, 4)
            CheckBoxUICorner.Parent = Elements.CheckBoxButton

            local CheckBoxUIStroke = Instance.new("UIStroke")
            CheckBoxUIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            CheckBoxUIStroke.Color = Color3.fromRGB(255, 255, 255)
            CheckBoxUIStroke.Thickness = 1
            CheckBoxUIStroke.Transparency = 0.8
            CheckBoxUIStroke.Parent = Elements.CheckBoxButton

            Elements.Checkmark = Instance.new("TextLabel")
            Elements.Checkmark.Name = "Checkmark"
            Elements.Checkmark.Text = ""
            Elements.Checkmark.FontFace = Font.new(Assets.InterFont, Enum.FontWeight.Bold)
            Elements.Checkmark.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.Checkmark.TextSize = 12
            Elements.Checkmark.TextScaled = true
            Elements.Checkmark.TextTransparency = 1
            Elements.Checkmark.BackgroundTransparency = 1
            Elements.Checkmark.BorderSizePixel = 0
            Elements.Checkmark.Position = UDim2.fromScale(0, 0)
            Elements.Checkmark.Size = UDim2.fromScale(1, 1)
            Elements.Checkmark.TextXAlignment = Enum.TextXAlignment.Center
            Elements.Checkmark.TextYAlignment = Enum.TextYAlignment.Center
            Elements.Checkmark.ZIndex = 2
            Elements.Checkmark.Parent = Elements.CheckBoxButton

            return Elements
        end

        local function UpdateCheckBoxState(State)
            if State then
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.7,
                    BackgroundColor3 = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 1)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 0
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.3,
                    Color = WindowFunctions:GetCurrentThemeAttribute("AccentPrimary")
                })
                if StrokeTween then StrokeTween:Play() end
            else
                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton, TWEEN_INFO, {
                    BackgroundTransparency = 0.9,
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton.UICorner, TWEEN_INFO, {
                    CornerRadius = UDim.new(0, 12)
                }):Play()

                Utility:PropertyTween(CheckBoxFunctions.Elements.Checkmark, TWEEN_INFO, {
                    TextTransparency = 1
                }):Play()

                local StrokeTween = Utility:PropertyTween(CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke"), TWEEN_INFO, {
                    Transparency = 0.8,
                    Color = Color3.fromRGB(255, 255, 255)
                })
                if StrokeTween then StrokeTween:Play() end
            end

            CheckBoxFunctions.State = State
        end

        local function CheckBoxTrigger()
            if Settings.Fixed or CheckBoxFunctions.IsHidden then return end

            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)

            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        local function SetupConnections()
            CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)

            if not Settings.PremiumFeature then
                local PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency

                CheckBoxFunctions.Maid.CheckBoxHover = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    PreviousTransparency = CheckBoxFunctions.Elements.CheckBoxName.TextTransparency
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = 0})
                end)

                CheckBoxFunctions.Maid.CheckBoxLeave = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                    if CheckBoxFunctions.IsHidden then return end
                    Utility:QuickTween(CheckBoxFunctions.Elements.CheckBoxName, {TextTransparency = PreviousTransparency})
                end)
            end
        end

        local function IsPlaceIdAllowed(PlaceIdSetting)
            if not PlaceIdSetting then return true end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        local function IsPlaceIdForbidden(PlaceIdSetting)
            if not PlaceIdSetting then return false end
            local CurrentPlaceId = game.PlaceId

            if type(PlaceIdSetting) == "table" then
                for _, PlaceId in pairs(PlaceIdSetting) do
                    if CurrentPlaceId == PlaceId then return true end
                end
                return false
            else
                return CurrentPlaceId == PlaceIdSetting
            end
        end

        function CheckBoxFunctions:Check()
            CheckBoxTrigger()
        end

        function CheckBoxFunctions:ServerCheck()
            CheckBoxFunctions.State = not CheckBoxFunctions.State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            if Settings.Callback then
                Settings.Callback(CheckBoxFunctions.State)
            end
        end

        function CheckBoxFunctions:UpdateState(State, Callback)
            CheckBoxFunctions.State = State
            UpdateCheckBoxState(CheckBoxFunctions.State)
            
            if Callback then
                if Settings.Callback then
                    Settings.Callback(CheckBoxFunctions.State)
                end
            end
        end

        function CheckBoxFunctions:GetState()
            return CheckBoxFunctions.State
        end

        function CheckBoxFunctions:ReturnSettings()
            return Settings
        end

        function CheckBoxFunctions:ReturnTab()
            return CheckBoxFunctions.Tab
        end

        function CheckBoxFunctions:ReturnClass()
            return CheckBoxFunctions.Class
        end

        function CheckBoxFunctions:UpdateName(Name)
            CheckBoxFunctions.Elements.CheckBoxName.Text = Name
        end

        function CheckBoxFunctions:SetToolTip(Tip)
            if CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame") then
                CheckBoxFunctions.Elements.CheckBox:FindFirstChild("ToolTipFrame"):Destroy()
            end

            if not Tip or Tip == "" then return end

            local ToolTipFrame = Instance.new("Frame")
            ToolTipFrame.Name = "ToolTipFrame"
            ToolTipFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
            ToolTipFrame.BackgroundTransparency = 1
            ToolTipFrame.BorderSizePixel = 0
            ToolTipFrame.AnchorPoint = Vector2.new(0.5, 1)
            ToolTipFrame.Position = UDim2.new(0.5, 0, 0, -5)
            ToolTipFrame.Visible = false
            ToolTipFrame.ZIndex = 200
            ToolTipFrame.Parent = CheckBoxFunctions.Elements.CheckBox

            local TooltipCorner = Instance.new("UICorner")
            TooltipCorner.CornerRadius = UDim.new(0, 6)
            TooltipCorner.Parent = ToolTipFrame

            local TooltipStroke = Instance.new("UIStroke")
            TooltipStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
            TooltipStroke.LineJoinMode = Enum.LineJoinMode.Round
            TooltipStroke.Color = Color3.fromRGB(255, 255, 255)
            TooltipStroke.Thickness = 0.1
            TooltipStroke.Transparency = 0.2
            TooltipStroke.Parent = ToolTipFrame

            local TooltipText = Instance.new("TextLabel")
            TooltipText.Text = Tip
            TooltipText.FontFace = Font.new(Assets.InterFont)
            TooltipText.TextColor3 = Color3.fromRGB(200, 220, 200)
            TooltipText.TextSize = 12
            TooltipText.TextTransparency = 1
            TooltipText.BackgroundTransparency = 1
            TooltipText.TextXAlignment = Enum.TextXAlignment.Center
            TooltipText.TextYAlignment = Enum.TextYAlignment.Center
            TooltipText.Size = UDim2.new(1, -12, 1, 0)
            TooltipText.Position = UDim2.new(0, 6, 0, 0)
            TooltipText.Parent = ToolTipFrame

            local TextBounds = Services.TextService:GetTextSize(Tip, TooltipText.TextSize, Enum.Font.SourceSansBold, Vector2.new(math.huge, math.huge))
            local TooltipWidth = math.max(80, TextBounds.X + 50)
            local TooltipHeight = math.max(28, TextBounds.Y + 12)
            ToolTipFrame.Size = UDim2.new(0, TooltipWidth, 0, TooltipHeight)

            CheckBoxFunctions.Maid.TooltipShow = CheckBoxFunctions.Elements.CheckBoxButton.MouseEnter:Connect(function()
                ToolTipFrame.Visible = true
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 0.1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 0.8}):Play()
            end)

            CheckBoxFunctions.Maid.TooltipHide = CheckBoxFunctions.Elements.CheckBoxButton.MouseLeave:Connect(function()
                Services.TweenService:Create(ToolTipFrame, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                Services.TweenService:Create(TooltipText, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
                Services.TweenService:Create(TooltipStroke, TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Transparency = 1}):Play()
                task.wait(0.2)
                if ToolTipFrame and ToolTipFrame.Parent then
                    ToolTipFrame.Visible = false
                end
            end)
        end

        function CheckBoxFunctions:SetHidden(State, Reason)
            if Reason then
                self:SetToolTip(Reason)
            end

            CheckBoxFunctions.IsHidden = State

            if State then
                Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxButton, TweenInfo.new(0.2), {BackgroundTransparency = 0.99}):Play()
                Services.TweenService:Create(CheckBoxFunctions.Elements.Checkmark, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()

                local Stroke = CheckBoxFunctions.Elements.CheckBoxButton:FindFirstChild("UIStroke")
                if Stroke then
                    Services.TweenService:Create(Stroke, TweenInfo.new(0.2), {Transparency = 0.95}):Play()
                end

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.9}):Play()
                end

                if CheckBoxFunctions.Maid.CheckBoxClick then
                    CheckBoxFunctions.Maid.CheckBoxClick:Disconnect()
                    CheckBoxFunctions.Maid.CheckBoxClick = nil
                end
            else
                UpdateCheckBoxState(CheckBoxFunctions.State)

                if not Settings.PremiumFeature then
                    Services.TweenService:Create(CheckBoxFunctions.Elements.CheckBoxName, TweenInfo.new(0.2), {TextTransparency = 0.5}):Play()
                end

                CheckBoxFunctions.Maid.CheckBoxClick = CheckBoxFunctions.Elements.CheckBoxButton.MouseButton1Click:Connect(CheckBoxTrigger)
            end
        end

        function CheckBoxFunctions:SetVisibility(State)
            CheckBoxFunctions.Elements.CheckBox.Visible = State
        end

        function CheckBoxFunctions:Destroy()
            for ConnectionName, Connection in pairs(CheckBoxFunctions.Maid) do
                if type(Connection) == "userdata" and Connection.Disconnect then
                    Connection:Disconnect()
                end
            end
            table.clear(CheckBoxFunctions.Maid)

            if CheckBoxFunctions.Elements.CheckBox then
                CheckBoxFunctions.Elements.CheckBox:Destroy()
            end

            if Flag and XenithLibrary.Options[Flag] then
                XenithLibrary.Options[Flag] = nil
            end
        end

        CheckBoxFunctions.Elements = CreateElements()
        SetupConnections()
        UpdateCheckBoxState(CheckBoxFunctions.State)

        if Settings.Hidden then
            if typeof(Settings.Hidden) == "table" then
                CheckBoxFunctions:SetHidden(Settings.Hidden[1], Settings.Hidden[2])
            else
                CheckBoxFunctions:SetHidden(Settings.Hidden)
            end
        end

        if Settings.Default == true and Settings.Callback then
            task.defer(function()
                Settings.Callback(Settings.Default)
            end)
        end

        if Settings.ToolTip then
            CheckBoxFunctions:SetToolTip(Settings.ToolTip)
        end

        if Settings.PremiumFeature and not Utility:IsPremiumHubUser() then
            CheckBoxFunctions:SetHidden(true, "Premium Feature")
        end

        if not IsPlaceIdAllowed(Settings.AllowedPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if IsPlaceIdForbidden(Settings.ForbiddenPlaceId) then
            CheckBoxFunctions:SetHidden(true, "Not in the right PlaceId to use this feature.")
        end

        if Flag then
            XenithLibrary.Options[Flag] = CheckBoxFunctions
        end

        return CheckBoxFunctions
    end
end]],
  ["Xenith/Xenith/UserInterface/Components/ColorPicker.lua"] = [[return function(Requirements)
    local Section = Requirements.Section
    local WindowFunctions = Requirements.WindowFunctions
    local XenithLibraryUI = Requirements.XenithLibraryUI
    local XenithLibrary = Requirements.XenithLibrary
    local SectionFunctions = Requirements.SectionFunctions
    local BaseUIScale = Requirements.BaseUIScale
    local Base = Requirements.Base
    
    function SectionFunctions:ColorPicker(Settings, Flag)
        local ColorPickerFunctions = {
            Settings = Settings,
            Class = "ColorPicker",
            IgnoreConfig = Settings.IgnoreConfig,
            Tab = Requirements.TabName,
            Color = Settings.Default or Color3.new(1, 1, 1),
            Alpha = Settings.Alpha or 0,
            IsHidden = false,
            Maid = {},
            Elements = {},
            State = {
                Hue = 0,
                Saturation = 0,
                Value = 1,
                WheelDown = false,
                SlideDown = false
            }
        }

        if not Flag or Flag == "WriteHere" or Flag == "AutoWriteFlag" then
            Flag = string.gsub(Settings.Name, " ", "_")
        end

        local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
        local TRANSPARENCY = {
            Default = 0.5,
            Hover = 0,
            Hidden = 0.9
        }

        local UI_CONSTANTS = {
            MAIN_HEIGHT = 38,
            COLOR_SIZE = 21,
            CORNER_RADIUS = {
                Small = UDim.new(0, 4),
                Medium = UDim.new(0, 6),
                Large = UDim.new(0, 10)
            },
            PADDING = {
                Small = UDim.new(0, 5),
                Medium = UDim.new(0, 10),
                Large = UDim.new(0, 20)
            }
        }

        local function CreateMainElements()
            local Elements = {}

            Elements.ColorPicker = Instance.new("Frame")
            Elements.ColorPicker.Name = "ColorPicker"
            Elements.ColorPicker.AutomaticSize = Enum.AutomaticSize.Y
            Elements.ColorPicker.BackgroundTransparency = 1
            Elements.ColorPicker.BorderSizePixel = 0
            Elements.ColorPicker.Size = UDim2.new(1, 0, 0, UI_CONSTANTS.MAIN_HEIGHT)
            Elements.ColorPicker.Parent = Section

            Elements.ColorPickerName = Instance.new("TextLabel")
            Elements.ColorPickerName.Name = "ColorPickerName"
            Elements.ColorPickerName.FontFace = Font.new(Assets.InterFont)
            Elements.ColorPickerName.Text = Settings.Name
            Elements.ColorPickerName.RichText = true
            Elements.ColorPickerName.TextColor3 = Color3.fromRGB(255, 255, 255)
            Elements.ColorPickerName.TextSize = 13
            Elements.ColorPickerName.TextTransparency = TRANSPARENCY.Default
            Elements.ColorPickerName.TextTruncate = Enum.TextTruncate.AtEnd
            Elements.ColorPickerName.TextXAlignment = Enum.TextXAlignment.Left
            Elements.ColorPickerName.TextYAlignment = Enum.TextYAlignment.Top
            Elements.ColorPickerName.AnchorPoint = Vector2.new(0, 0.5)
            Elements.ColorPickerName.AutomaticSize = Enum.AutomaticSize.XY
            Elements.ColorPickerName.BackgroundTransparency = 1
            Elements.ColorPickerName.Position = UDim2.fromScale(0, 0.5)
            Elements.ColorPickerName.Parent = Elements.ColorPicker

            if Settings.PremiumFeature then
                Elements.ColorPickerName.TextTransparency = 0
                local PremiumGradient = Instance.new("UIGradient")
                PremiumGradient.Color = WindowFunctions:GetCurrentThemeAttribute("PremiumGradientColor") or ColorSequence.new({
                    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 106, 255)),
                    ColorSequenceKeypoint.new(0.2, Color3.fromRGB(163, 201, 255)),
                    ColorSequenceKeypoint.new(0.4, Color3.fromRGB(255, 255, 255)),
                    ColorSequenceKeypoint.new(1, Color3.fromRGB(163, 201, 255))
                })
                PremiumGradient.Parent = Elements.ColorPickerName
            end

            Elements.ColorBackground = Instance.new("ImageLabel")
            Elements.ColorBackground.Name = "ColorBackground"
            Elements.ColorBackground.Image = Assets.Grid
            Elements.ColorBackground.ScaleType = Enum.ScaleType.Tile
            Elements.ColorBackground.TileSize = UDim2.fromOffset(500, 500)
            Elements.ColorBackground.AnchorPoint = Vector2.new(1, 0.5)
            Elements.ColorBackground.BackgroundTransparency = 1
            Elements.ColorBackground.Position = UDim2.fromScale(1, 0.5)
            Elements.ColorBackground.Size = UDim2.fromOffset(UI_CONSTANTS.COLOR_SIZE, UI_CONSTANTS.COLOR_SIZE)
            Elements.ColorBackground.Parent = Elements.ColorPicker

            Elements.ColorDisplay = Instance.new("Frame")
            Elements.ColorDisplay.Name = "ColorDisplay"
            Elements.ColorDisplay.AnchorPoint = Vector2.new(0.5, 0.5)
            Elements.ColorDisplay.BackgroundColor3 = ColorPickerFunctions.Color
            Elements.ColorDisplay.BorderSizePixel = 0
            Elements.ColorDisplay.Position = UDim2.fromScale(0.5, 0.5)
            Elements.ColorDisplay.Size = UDim2.fromScale(1, 1)
            Elements.ColorDisplay.BackgroundTransparency = ColorPickerFunctions.Alpha
            Elements.ColorDisplay.Parent = Elements.ColorBackground

            Elements.ColorInteract = Instance.new("TextButton")
            Elements.ColorInteract.Name = "ColorInteract"
            Elements.ColorInteract.Text = ""
            Elements.ColorInteract.BackgroundTransparency = 1
            Elements.ColorInteract.Size = UDim2.fromScale(1, 1)
            Elements.ColorInteract.Parent = Elements.ColorDisplay

            local DisplayCorner = Instance.new("UICorner")
            DisplayCorner.CornerRadius = UI_CONSTANTS.CORNER_RADIUS.Medium
            DisplayCorner.Parent = Elements.ColorDisplay

            local BackgroundCorner = Instance.new("UICorner")
            BackgroundCorner.CornerRadius = UDim.new(0, 8)
            BackgroundCorner.Parent = Elements.ColorBackground

            return Elements
        end

        local function CreateColorDialog()
            local Dialog = {}

            Dialog.Overlay = Instance.new("Frame")
            Dialog.Overlay.Name = "ColorPickerOverlay"
            Dialog.Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Dialog.Overlay.BackgroundTransparency = 0.5
