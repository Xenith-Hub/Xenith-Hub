            AntiAliasing = true,
            SubPixelRendering = true,
            DynamicTypeScaling = false,
            HighContrastMode = false,
            ReducedMotion = false,
            ColorBlindMode = "None",
            ThemeVariant = "Light",
            AccentColorSource = "Manual",
            AutoDarkMode = true,
            ScheduledThemes = {},
            CustomCSSOverrides = "",
            MenuPrimaryColor = Color3.fromRGB(2, 132, 199),
            MinimizeColor = Color3.fromRGB(2, 132, 199),
            ExitColor = Color3.fromRGB(3, 105, 161),
            Version = "2.0"
        },

        Custom = {}
    }
end

return ThemeMetaUtility]],
  ["Xenith/Xenith/UserInterface/Managers/ComponentManager.lua"] = [[local ComponentManager = {}
ComponentManager.__index = ComponentManager


local ActiveKeybindListeners = {}
local RegisteredKeybinds = {}
local KeybindElements = {}

local KeyShortcuts = {
    Space = "SP",
    LeftShift = "LS",
    RightShift = "RS",
    LeftControl = "LC",
    RightControl = "RC",
    LeftAlt = "LA",
    RightAlt = "RA",

    MouseButton1 = "M1",
    MouseButton2 = "M2",
    MouseButton3 = "M3",

    CapsLock = "CL",
    NumLock = "NL",
    ScrollLock = "SL",

    Backspace = "BS",
    Return = "RT",
    Escape = "ES",
    Delete = "DL",
    Insert = "IN",
    PageUp = "PU",
    PageDown = "PD",
    Home = "HM",
    End = "EN",

    F1 = "F1",
    F2 = "F2",
    F3 = "F3",
    F4 = "F4",
    F5 = "F5",
    F6 = "F6",
    F7 = "F7",
    F8 = "F8",
    F9 = "F9",
    F10 = "F10",
    F11 = "F11",
    F12 = "F12",

    KeypadEnter = "KE",
    KeypadPlus = "KP+",
    KeypadMinus = "KP-",
    KeypadMultiply = "KP*",
    KeypadDivide = "KP/",
    KeypadZero = "K0",
    KeypadOne = "K1",
    KeypadTwo = "K2",
    KeypadThree = "K3",
    KeypadFour = "K4",
    KeypadFive = "K5",
    KeypadSix = "K6",
    KeypadSeven = "K7",
    KeypadEight = "K8",
    KeypadNine = "K9",

    Up = "UP",
    Down = "DN",
    Left = "LT",
    Right = "RT",

    Print = "PR",
    Pause = "PA",
    Menu = "MN",

    Unknown = "NA"
}

local ClassOffsets = {
    Button = 0.9,
    Toggle = 0.825,
}

local function CleanupConnection(ConnectionTable, Key)
    if ConnectionTable[Key] then
        if type(ConnectionTable[Key]) == "userdata" and ConnectionTable[Key].Disconnect then
            ConnectionTable[Key]:Disconnect()
        end

        ConnectionTable[Key] = nil
    end
end

local function FormatKeyDisplay(InputDescription)
    local DisplayKey = InputDescription:gsub("Enum.KeyCode.", ""):gsub("Enum.UserInputType.", "")
    return KeyShortcuts[DisplayKey] or DisplayKey
end

function ComponentManager.RemoveComponentBind(KeybindButton, KeybindLabel, Flag)
    if not RegisteredKeybinds[Flag] then
        Utility:Notify("Keybind Action", "No Keybind set for this feature.", 1)
        return
    end

    Utility:Notify("Keybind Action", "Keep holding to Unbind.", 1)

    task.delay(1, function()
        if Services.UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) then
            RegisteredKeybinds[Flag] = nil

            Utility:Notify("Keybind Action", "Unbounded Key " .. Flag, 1)

            local TextTween = Utility:PropertyTween(
                KeybindLabel,
                TweenInfo.new(0.25, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out),
                {TextTransparency = 1}
            )

            TextTween:Play()
            TextTween.Completed:Wait()
            KeybindLabel.Text = ""
        end
    end)
end

function ComponentManager.RegisterComponentKeybind(KeybindButton, KeybindLabel, Flag, MaidTable)
    if ActiveKeybindListeners[Flag] then
        return
    end

    ActiveKeybindListeners[Flag] = true
    
    Utility:Notify("Keybind Action", "The next key you click will be the keybind!", 3)
    
    MaidTable.KeybindInput = Services.UserInputService.InputBegan:Connect(function(Key, Typing)
        if Typing then
            return
        end

        local InputData = {}

        if Key.KeyCode ~= Enum.KeyCode.Unknown then
            InputData.Input = Key.KeyCode
            InputData.Description = tostring(Key.KeyCode.Name)
        elseif Key.UserInputType and string.find(tostring(Key.UserInputType), "MouseButton") then
            InputData.Input = Key.UserInputType
            InputData.Description = tostring(Key.UserInputType.Name)
        else
            InputData.Input = Key
            InputData.Description = tostring(Key)
        end

        InputData.KeybindSetTime = os.time()

        RegisteredKeybinds[Flag] = InputData

        if KeybindLabel == "" then
            KeybindLabel.TextTransparency = 1
        end

        Utility:Notify("Keybind Action", "Set Keybind to " .. InputData.Description, 3)
        KeybindLabel.Text = FormatKeyDisplay(InputData.Description)

        Utility:PropertyTween(
            KeybindLabel,
            TweenInfo.new(0.25, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out),
            {TextTransparency = 0.45}
        ):Play()

        CleanupConnection(MaidTable, "KeybindInput")
        ActiveKeybindListeners[Flag] = nil
    end)
end

function ComponentManager.CreateKeybindAssignments(KeybindButton, KeybindLabel, Arguments)
    Arguments.Maid.KeybindClick = KeybindButton.MouseButton1Click:Connect(function()
        if not Arguments.Functions:ReturnHidden() then
            ComponentManager.RegisterComponentKeybind(KeybindButton, KeybindLabel, Arguments.Flag, Arguments.Maid)
        end
    end)
    
    Arguments.Maid.KeybindRightClick = KeybindButton.MouseButton2Down:Connect(function()
        if not Arguments.Functions:ReturnHidden() then
            ComponentManager.RemoveComponentBind(KeybindButton, KeybindLabel, Arguments.Flag, Arguments.Maid)
        end
    end)

    local PreviousKeybindTransparency = KeybindButton.ImageTransparency
    
    Arguments.Maid.KeybindHover = KeybindButton.MouseEnter:Connect(function()
        if not Arguments.Functions:ReturnHidden() then
            PreviousKeybindTransparency = KeybindButton.ImageTransparency
            Utility:QuickTween(KeybindButton, {ImageTransparency = 0})
        end
    end)

    Arguments.Maid.KeybindLeave = KeybindButton.MouseLeave:Connect(function()
        if not Arguments.Functions:ReturnHidden() then
            Utility:QuickTween(KeybindButton, {ImageTransparency = PreviousKeybindTransparency})
        end
    end)
end

function ComponentManager.CreateKeybindComponents(Arguments)
    if not Arguments.Settings.KeybindEnabled then
        return
    end

    local KeybindButton = Instance.new("ImageButton")
    KeybindButton.Name = "KeybindButton"
    KeybindButton.Image = Assets.Fingerprint
    KeybindButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
    KeybindButton.AutoButtonColor = false
    KeybindButton.AnchorPoint = Vector2.new(1, 0.5)
    KeybindButton.BackgroundTransparency = 1
    KeybindButton.BorderSizePixel = 0
    KeybindButton.Position = UDim2.fromScale(ClassOffsets[Arguments.Class], 0.5)
    KeybindButton.Size = UDim2.fromOffset(20, 20)
    KeybindButton.ImageTransparency = 0.45
    KeybindButton.Parent = Arguments.Ancestor

    local KeybindLabel = Instance.new("TextLabel")
    KeybindLabel.Name = "KeybindLabel"
    KeybindLabel.Size = UDim2.fromScale(1, 1)
    KeybindLabel.TextTransparency = 1
    KeybindLabel.TextStrokeTransparency = 0
    KeybindLabel.BackgroundTransparency = 1
    KeybindLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    KeybindLabel.TextScaled = true
    KeybindLabel.Font = Enum.Font.SourceSansBold
    KeybindLabel.Text = ""
    KeybindLabel.Parent = KeybindButton

    ComponentManager.CreateKeybindAssignments(KeybindButton, KeybindLabel, Arguments)

    local Elements = {
        KeybindButton = KeybindButton,
        KeybindLabel = KeybindLabel
    }

    KeybindElements[Arguments.Flag] = Elements

    return Elements
end

function ComponentManager.UpdateKeybindDetection(State, Flag, Elements)
    if RegisteredKeybinds[Flag] then
        RegisteredKeybinds[Flag].DetectionState = State
    end

    local TWEEN_INFO = TweenInfo.new(0.2, Enum.EasingStyle.Sine)

    if not Elements.KeybindButton or not Elements.KeybindLabel then
        return
    end

    if State then
        Services.TweenService:Create(Elements.KeybindButton, TWEEN_INFO, {ImageTransparency = 0.9}):Play()
        Services.TweenService:Create(Elements.KeybindLabel, TWEEN_INFO, {TextTransparency = 0.8}):Play()
    else
        Services.TweenService:Create(Elements.KeybindButton, TWEEN_INFO, {ImageTransparency = 0.45}):Play()
        Services.TweenService:Create(Elements.KeybindLabel, TWEEN_INFO, {TextTransparency = 0.45}):Play()
    end
end

function ComponentManager.AddFlagKeybind(Flag, Input)
    if not Flag or not Input then
        return false
    end

    local InputData = {}

    if typeof(Input) == "string" then
        local Keycode, UserInput
        
        pcall(function()
            Keycode = Enum.KeyCode[Input]
        end)

        pcall(function()
            UserInput = Enum.UserInputType[Input]
        end)

        if Keycode then
            InputData.Input = Keycode
            InputData.Description = Keycode.Name
        elseif UserInput then
            InputData.Input = UserInput
            InputData.Description = UserInput.Name
        else
            return false
        end
    elseif typeof(Input) == "EnumItem" then
        InputData.Input = Input
        InputData.Description = Input.Name
    else
        return false
    end

    InputData.KeybindSetTime = os.time()
    InputData.DetectionState = true

    RegisteredKeybinds[Flag] = InputData

    if KeybindElements[Flag] and KeybindElements[Flag].KeybindLabel then
        local KeybindLabel = KeybindElements[Flag].KeybindLabel
        KeybindLabel.Text = FormatKeyDisplay(InputData.Description)
        Utility:PropertyTween(
            KeybindLabel,
            TweenInfo.new(0.25, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out),
            {TextTransparency = 0.45}
        ):Play()
    end

    return true
end

function ComponentManager:CheckFlagKeybind(Flag)
    if not Flag then
        return
    end

    if RegisteredKeybinds[Flag] then
        Utility:PrintTable(RegisteredKeybinds[Flag])
        return RegisteredKeybinds[Flag].Description
    end

    return nil
end

function ComponentManager:Unload()
    if ComponentManager.KeybindHandler ~= nil then
        ComponentManager.KeybindHandler:Disconnect()
    end

    ComponentManager.KeybindHandler = nil
end

ComponentManager.KeybindHandler = Services.UserInputService.InputBegan:Connect(function(Key, Typing)
    if Typing then
        return
    end

    local InputData = {}

    if Key.KeyCode ~= Enum.KeyCode.Unknown then
        InputData.Input = Key.KeyCode
    elseif Key.UserInputType and string.find(tostring(Key.UserInputType), "MouseButton") then
        InputData.Input = Key.UserInputType
    else
        InputData.Input = Key
    end

    InputData.KeybindHit = os.time()

    for Flag, Data in pairs(RegisteredKeybinds) do
        if Data.DetectionState ~= nil and not Data.DetectionState then
            continue
        end
        
        if Data.Input == InputData.Input and (InputData.KeybindHit - Data.KeybindSetTime) ~= 0 then
            local ClassType, Component = XenithLibrary:ReturnFeatures()[Flag]:ReturnClass(), XenithLibrary:ReturnFeatures()[Flag]

            if ClassType == "Toggle" then
                Component:Toggle()
            elseif ClassType == "Button" then
                Component:RunCallback()
            end
        end
    end
end)

return ComponentManager]],
  ["Xenith/UserInterface/Managers/ComponentManager.lua"] = [[local ComponentManager = {}
